[164]={
vo={
{"vo",file="man_yuk_00333",ch="yuk"},
},
name={name="行人"},
ja={
{
"「什么玩意？」",
},
},
},
[165]={
vo={
{"vo",file="fem_yas_00013",ch="yas"},
