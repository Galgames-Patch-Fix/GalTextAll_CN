		<play_sound @l="185" buf="5" fadeout="0" type="1" />
		<msg @l="187" char="3" flags="0" index="104" line="187" name="礼羽" text="「很随便地从书上哦……？ＪＣ带过来的女性杂志……」" timeout="" voices="rai13621" />
