		<play_sound @l="198" buf="5" fadeout="0" intro="0" src="rai80002" type="6" volume="1." />
		<msg @l="199" char="11" flags="0" index="112" line="199" name="亮" text="「唔，啊……啊，突然放进嘴里舔的话，唔咕！？」" timeout="" voices="" />
		<msg @l="201" char="-1" flags="0" index="114" line="201" name="" text="莱莱一边观察着我的反应一边上下的动着头。" timeout="" voices="" />
		<msg @l="202" char="-1" flags="0" index="116" line="202" name="" text="虽然她的动作本身很笨拙又缓慢，但对于第一次被人口交的我来说已经足够舒服了。" timeout="" voices="" />
		<play_sound @l="203" buf="5" fadeout="0" type="1" />
		<msg @l="205" char="3" flags="0" index="118" line="205" name="礼羽" text="「嗯吸溜，吸溜……嗯，嗯嗯……吸溜，吸溜，吸溜……吸溜嗯\x;hearts:」" timeout="" voices="rai13623" />
		<play_sound @l="207" buf="5" fadeout="0" intro="0" src="rai80001" type="6" volume="1." />
		<msg @l="208" char="-1" flags="0" index="120" line="208" name="" text="看了我的反应，知道我被这样做感觉很好了吗。她没有像先前那样询问我并继续这样行动着。" timeout="" voices="" />
		<msg @l="209" char="-1" flags="0" index="122" line="209" name="" text="用唾液发出的水声，回响在只有我与她在的店内。" timeout="" voices="" />
		<msg @l="210" char="-1" flags="0" index="124" line="210" name="" text="因为在工作场所做出淫乱行为而感觉到的背德感，我的脊梁不禁打起了寒颤。" timeout="" voices="" />
		<play_sound @l="211" buf="5" fadeout="0" type="1" />
		<chgbg @l="213" duration="300" flags="256" flip="-1" flip_flags="0" param="16" screen="1" src="evrai03_04e" type="99" />
		<msg @l="215" char="3" flags="0" index="126" line="215" name="礼羽" text="「嗯嗯……欧○金，比起用手做的时候更大了……嗯，吸溜……\x;heart:」" timeout="" voices="rai13624" />
		<play_sound @l="217" buf="5" fadeout="0" intro="0" src="rai80003" type="6" volume="1." />
		<msg @l="218" char="11" flags="0" index="128" line="218" name="亮" text="「因为莱莱的嘴太舒服了啊……哈啊、哈啊……所以擅自就……啊，唔唔……」" timeout="" voices="" />
		<msg @l="220" char="-1" flags="0" index="130" line="220" name="" text="在逐渐强烈起来的快感与兴奋下，我对她为何会做这种事情的疑问也烟消云散了。" timeout="" voices="" />
		<msg @l="221" char="-1" flags="0" index="132" line="221" name="" text="现在，我只想把自己置身于她给予的快感中——" timeout="" voices="" />
		<play_sound @l="222" buf="5" fadeout="0" type="1" />
		<chgbg @l="224" duration="300" flags="256" flip="-1" flip_flags="0" param="16" screen="1" src="evrai03_04a" type="99" />
		<msg @l="226" char="3" flags="0" index="134" line="226" name="礼羽" text="「吸溜，呲噗……除此之外，书上好像还写着这种东西来着……吸溜，吸溜吸溜，吸溜……」" timeout="" voices="rai13625" />
		<play_sound @l="228" buf="5" fadeout="0" intro="0" src="rai80002" type="6" volume="1." />
		<msg @l="229" char="-1" flags="0" index="136" line="229" name="" text="像是轻抚着龟头的表面一样，在口内用舌尖蠕动刺激着。" timeout="" voices="" />
		<msg @l="231" char="11" flags="0" index="138" line="231" name="亮" text="「唔啊啊啊……！这个舌头的动作，太、太厉害了……！！」" timeout="" voices="" />
		<play_sound @l="232" buf="5" fadeout="0" type="1" />
		<chgbg @l="234" duration="300" flags="256" flip="-1" flip_flags="0" param="16" screen="1" src="evrai03_04c" type="99" />
		<msg @l="236" char="3" flags="0" index="140" line="236" name="礼羽" text="「嗯嗯……！欧○金，变得跳来跳去的……呼呼，这样很舒服吧……？啊嗯，嗯吸溜，吸溜……吸溜，嗯嗯……！」" timeout="" voices="rai13626" />
		<play_sound @l="238" buf="5" fadeout="0" intro="0" src="rai80003" type="6" volume="1." />
		<msg @l="239" char="-1" flags="0" index="142" line="239" name="" text="一知道这个动作会让我变得很有感觉，她舌头的动作变得大胆了起来，不断对我发起猛攻。" timeout="" voices="" />
		<play_sound @l="240" buf="5" fadeout="0" type="1" />
		<msg @l="242" char="3" flags="0" index="144" line="242" name="礼羽" text="「吸溜、吸溜……！嗯嗯、嗯……嗯唔，吸溜、吸溜吸溜……\x;heart:」" timeout="" voices="rai13627" />
