		<play_sound @l="2446" buf="0" fadeout="500" intro="-1" src="bgm21" type="0" volume="1." />
		<msg @l="2448" char="1" flags="0" index="1484" line="2448" name="汐" text="「已经非常晚了的样子呢」" timeout="" voices="usi17709" />
		<msg @l="2450" char="11" flags="0" index="1486" line="2450" name="亮" text="「这都是因为那之后，朝霞学姐睡着了」" timeout="" voices="" />
		<chgbg @l="2454" duration="300" flags="256" flip="-1" flip_flags="0" param="16" screen="1" src="evusi01_01a" type="99" />
		<msg @l="2455" char="1" flags="0" index="1488" line="2455" name="汐" text="「那是因为谁那么激烈的呢」" timeout="" voices="usi17710" />
		<msg @l="2457" char="11" flags="0" index="1490" line="2457" name="亮" text="「那是因为谁那么可爱的呢」" timeout="" voices="" />
		<chgbg @l="2461" duration="300" flags="256" flip="-1" flip_flags="0" param="16" screen="1" src="evusi01_01d" type="99" />
		<msg @l="2462" char="1" flags="0" index="1492" line="2462" name="汐" text="「那是因为谁射了一堆出来的呢」" timeout="" voices="usi17711" />
		<msg @l="2464" char="11" flags="0" index="1494" line="2464" name="亮" text="「那是谁喊着说出来吧，满满的出来吧！　的呢——」" timeout="" voices="" />
		<chgbg @l="2466" duration="300" flags="256" flip="-1" flip_flags="0" param="16" screen="1" src="evusi01_02c" type="99" />
		<msg @l="2467" char="1" flags="0" index="1496" line="2467" name="汐" text="\h;60:\s;40:「等等？！　声音太大了……！」\s:\h:" timeout="" voices="usi17712" />
		<msg @l="2469" char="-1" flags="0" index="1498" line="2469" name="" text="朝霞学姐脸变红着观察着四周，发现谁也不再后，安心的叹了一口气。" timeout="" voices="" />
		<chgbg @l="2471" duration="300" flags="256" flip="-1" flip_flags="0" param="16" screen="1" src="evusi01_01d" type="99" />
		<msg @l="2472" char="1" flags="0" index="1500" line="2472" name="汐" text="「……明明今天是要去做蛋糕的，结果做了孩子呢」" timeout="" voices="usi17713" />
		<msg @l="2474" char="11" flags="0" index="1502" line="2474" name="亮" text="「但是、也成了又甜又特别的一天不是吗」" timeout="" voices="" />
		<msg @l="2476" char="1" flags="0" index="1504" line="2476" name="汐" text="「确实，因为肚子现在也还是满满的呢……对吧？」" timeout="" voices="usi17714" />
		<msg @l="2478" char="-1" flags="0" index="1506" line="2478" name="" text="带着放弃继续话题的表情，小声念叨着的朝霞学姐，她拿着的洗浴用的桶里，正好放着小鸭鸭。" timeout="" voices="" />
		<msg @l="2480" char="11" flags="0" index="1508" line="2480" name="亮" text="「你也很享受的样子呢，小鸭鸭」" timeout="" voices="" />
		<msg @l="2482" char="11" flags="0" index="1510" line="2482" name="亮" text="「嗯、我也非常的开心哦（假声）」" timeout="" voices="" />
		<msg @l="2484" char="1" flags="0" index="1512" line="2484" name="汐" text="「噗、啊哈哈。真是的、突然间的怎么了吗？」" timeout="" voices="usi17715" />
		<msg @l="2486" char="11" flags="0" index="1514" line="2486" name="亮" text="「今天的做爱能这么舒服，我觉得这其中也有这孩子的功劳哦」" timeout="" voices="" />
		<msg @l="2488" char="1" flags="0" index="1516" line="2488" name="汐" text="「虽然我也认同这点……但是、这个孩子是女生哦？　不是用我、而是要用人家哦」" timeout="" voices="usi17716" />
		<msg @l="2490" char="11" flags="0" index="1518" line="2490" name="亮" text="「啊、对啊……。而且你真是幸福啊，每天晚上都能和朝霞学姐一起入浴」" timeout="" voices="" />
		<msg @l="2492" char="1" flags="0" index="1520" line="2492" name="汐" text="「不用那么慌张，你也会有那样的一天哦？」" timeout="" voices="usi17717" />
		<msg @l="2494" char="11" flags="0" index="1522" line="2494" name="亮" text="「虽然是这样……。但是、体验过像今天这样子后，总感觉已经等不下去了呢」" timeout="" voices="" />
		<msg @l="2496" char="-1" flags="0" index="1524" line="2496" name="" text="即使之后肯定会在一起生活，但现在离那一天还很远，远到我还得继续焦急的等待着。" timeout="" voices="" />
		<msg @l="2498" char="1" flags="0" index="1526" line="2498" name="汐" text="「……那么、这之后也一起入浴吧」" timeout="" voices="usi17718" />
		<msg @l="2500" char="11" flags="0" index="1528" line="2500" name="亮" text="「一起……诶、难道说一起在男浴场！？」" timeout="" voices="" />
		<chgbg @l="2502" duration="300" flags="256" flip="-1" flip_flags="0" param="16" screen="1" src="evusi01_01a" type="99" />
		<msg @l="2503" char="1" flags="0" index="1530" line="2503" name="汐" text="「才不ー是。真是的、我不是说的很明白了吗？　在我家的浴室哦、朝霞家的浴室」" timeout="" voices="usi17719" />
		<msg @l="2505" char="11" flags="0" index="1532" line="2505" name="亮" text="「可以吗？」" timeout="" voices="" />
		<chgbg @l="2507" duration="300" flags="256" flip="-1" flip_flags="0" param="16" screen="1" src="evusi01_01d" type="99" />
		<msg @l="2508" char="1" flags="0" index="1534" line="2508" name="汐" text="「虽然父母都在的时候，你也应该也会很在意，所以不行。但就我一个人的时候，ＯＫ哟」" timeout="" voices="usi17720" />
		<msg @l="2510" char="11" flags="0" index="1536" line="2510" name="亮" text="「那样说的话，请一定和我一起！！」" timeout="" voices="" />
		<msg @l="2512" char="1" flags="0" index="1538" line="2512" name="汐" text="「虽然话是这么说了，但因为那里可不像澡堂那样那么大，所以像今天这样的事可做不到哦？　那样也可以吗？」" timeout="" voices="usi17721" />
		<msg @l="2514" char="11" flags="0" index="1540" line="2514" name="亮" text="「那是当然的！　我们一起洗澡就可以了」" timeout="" voices="" />
		<msg @l="2516" char="-1" flags="0" index="1542" line="2516" name="" text="和学姐一起面对面的进到家庭用的浴缸里，然后从她的后面抱住她……像是那样的事，也是我十分憧憬的。" timeout="" voices="" />
		<msg @l="2518" char="1" flags="0" index="1544" line="2518" name="汐" text="「唔嗯、十分狭窄的话就会变得要两人贴的紧紧的窄窄的样子了呢」" timeout="" voices="usi17722" />
		<play_sound @l="2523" buf="0" fadeout="2500" type="0" />
		<showmsg @l="2524" duration="500" show="0" />
		<chgbg @l="2525" duration="1000" flags="256" flip="-1" flip_flags="0" param="16" screen="1" src="black" type="201" />
