		<scroll @l="20" async="0" center_x="45" center_y="45" duration="0" flags="0" rev="0." screen="1" transparency="0" v0="0." v1="0." wait_flags="0" x="0" y="0" zoom="1." />
		<chgscrn @l="21" duration="2500" flags="4608" flip="-1" flip_flags="0" param="0" screen="1" type="10" />
		<play_sound @l="22" buf="0" fadeout="500" intro="-1" src="bgm18" type="0" volume="1." />
		<msg @l="29" char="3" flags="0" index="0" line="29" name="礼羽" text="「哼哼。居然把我带到这种一般人不会进来的房间，还让我穿成这样」" timeout="" voices="rai17300" />
		<chgbg @l="31" duration="300" flags="256" flip="-1" flip_flags="0" param="16" screen="1" src="evrai15_11c" type="99" />
		<msg @l="32" char="3" flags="0" index="2" line="32" name="礼羽" text="「那么，这也就是认同我了？」" timeout="" voices="rai17301" />
		<msg @l="34" char="-1" flags="0" index="4" line="34" name="" text="她用充满期待的眼神开心地看着我。" timeout="" voices="" />
		<msg @l="35" char="-1" flags="0" index="6" line="35" name="" text="她那圆溜溜的肚子里正装着我俩的孩子。" timeout="" voices="" />
		<msg @l="37" char="11" flags="0" index="8" line="37" name="亮" text="「应该是我想问你为什么穿成这样吧？　半脱着衣服，成何体统啊」" timeout="" voices="" />
		<msg @l="39" char="-1" flags="0" index="10" line="39" name="" text="从镶着刺绣蕾丝边的睡袍看，不仅是乳头，甚至那一圈乳环都能看得清清楚楚。" timeout="" voices="" />
		<msg @l="40" char="-1" flags="0" index="12" line="40" name="" text="不像是孕妇，肌肤透明淫荡的设计与性感的礼羽巧妙地契合，光是看着就觉得邪气上涌。" timeout="" voices="" />
		<msg @l="42" char="3" flags="0" index="14" line="42" name="礼羽" text="「因为医生说，刺激乳头能够促进子宫收缩。所以我买了这件衣服……」" timeout="" voices="rai17302" />
		<chgbg @l="44" duration="300" flags="256" flip="-1" flip_flags="0" param="16" screen="1" src="evrai15_11d" type="99" />
		<msg @l="45" char="3" flags="0" index="16" line="45" name="礼羽" text="「如何？　很有情趣的衣服吧？　你也开始有反应了吗？」" timeout="" voices="rai17303" />
		<msg @l="47" char="11" flags="0" index="18" line="47" name="亮" text="「就色情程度来说实在太棒了！」" timeout="" voices="" />
		<chgbg @l="49" duration="300" flags="256" flip="-1" flip_flags="0" param="16" screen="1" src="evrai15_11a" type="99" />
		<msg @l="50" char="3" flags="0" index="20" line="50" name="礼羽" text="「啊哈。幸好买了这件衣服，要是你不喜欢就没意义了」" timeout="" voices="rai17304" />
		<msg @l="52" char="11" flags="0" index="22" line="52" name="亮" text="「但是穿这种衣服，还是开着灯比较好吧……」" timeout="" voices="" />
		<msg @l="54" char="-1" flags="0" index="24" line="54" name="" text="今天下着雪，虽然是白天，但不开灯的话实在有点黑。" timeout="" voices="" />
		<msg @l="55" char="-1" flags="0" index="26" line="55" name="" text="北欧的日照时间很短，即使是白天，天黑也是很平常的事。" timeout="" voices="" />
		<chgbg @l="57" duration="300" flags="256" flip="-1" flip_flags="0" param="16" screen="1" src="evrai15_11c" type="99" />
		<msg @l="58" char="3" flags="0" index="28" line="58" name="礼羽" text="「但暖炉还开着。总感觉火光就很温暖了，而且还很有氛围」" timeout="" voices="rai17305" />
		<msg @l="60" char="11" flags="0" index="30" line="60" name="亮" text="「那个主要还是用来温暖房间，以防肚子着凉吧。你现在身体才是最重要的」" timeout="" voices="" />
		<chgbg @l="62" duration="300" flags="256" flip="-1" flip_flags="0" param="16" screen="1" src="evrai15_11a" type="99" />
		<msg @l="63" char="3" flags="0" index="32" line="63" name="礼羽" text="「虽然你还是一如既往的温柔，但其实不用那么担心啦」" timeout="" voices="rai17306" />
		<msg @l="65" char="3" flags="0" index="34" line="65" name="礼羽" text="「医生说过，做爱不会直接导致流产或早产，所以不用那么在意」" timeout="" voices="rai17307" />
		<msg @l="67" char="-1" flags="0" index="36" line="67" name="" text="而且进入安定期后，就算是怀孕中做爱其实也没问题。" timeout="" voices="" />
		<msg @l="68" char="-1" flags="0" index="38" line="68" name="" text="只要做出对肚子没什么负担的姿势，应该就能毫无问题地做爱了。" timeout="" voices="" />
		<chgbg @l="70" duration="300" flags="256" flip="-1" flip_flags="0" param="16" screen="1" src="evrai15_11c" type="99" />
		<msg @l="71" char="3" flags="0" index="40" line="71" name="礼羽" text="「所以来尽情做爱吧，将至今为止忍耐的份一起……来吧」" timeout="" voices="rai17308" />
		<msg @l="73" char="-1" flags="0" index="42" line="73" name="" text="礼羽摆出一个突显大肚子的姿势，并且温柔地微笑着。" timeout="" voices="" />
		<msg @l="74" char="-1" flags="0" index="44" line="74" name="" text="比之前大了一圈的乳头稍微变黑了一点，看起来反而更色气了。" timeout="" voices="" />
		<chgbg @l="76" duration="300" flags="256" flip="-1" flip_flags="0" param="16" screen="1" src="evrai15_11a" type="99" />
		<msg @l="77" char="3" flags="0" index="46" line="77" name="礼羽" text="「以后乳头应该会越变越黑吧。听说婴儿最容易发现黑色的东西，所以颜色会变化」" timeout="" voices="rai17309" />
		<msg @l="79" char="11" flags="0" index="48" line="79" name="亮" text="「哦—，人体真是神秘啊」" timeout="" voices="" />
		<msg @l="81" char="-1" flags="0" index="50" line="81" name="" text="虽然变黑了，但即便如此礼羽的乳头还是很漂亮，很诱人。" timeout="" voices="" />
		<chgbg @l="86" duration="300" flags="256" flip="-1" flip_flags="0" param="16" screen="1" src="evrai15_06a" type="99" />
		<msg @l="88" char="11" flags="0" index="52" line="88" name="亮" text="「哎呀？　是我的错觉吗，好像乳头湿了啊……？」" timeout="" voices="" />
		<chgbg @l="90" duration="300" flags="256" flip="-1" flip_flags="0" param="16" screen="1" src="evrai15_06c" type="99" />
		<msg @l="91" char="3" flags="0" index="54" line="91" name="礼羽" text="「啊！？　为什么已经流出奶水了啊！？？」" timeout="" voices="rai17310" />
		<showmsg @l="93" duration="500" show="0" />
		<scroll @l="94" async="-1" center_x="45" center_y="200" duration="1000" flags="0" rev="0." screen="0" transparency="0" v0="0." v1="0." wait_flags="0" x="0" y="0" zoom="1.5" />
		<msg @l="96" char="-1" flags="0" index="56" line="96" name="" text="从乳头处突然流出了雪白而黏稠的母乳。" timeout="" voices="" />
		<msg @l="98" char="3" flags="0" index="58" line="98" name="礼羽" text="「根据体质不同，似乎这会在生产前产出……但没想到居然会这么早」" timeout="" voices="rai17311" />
		<msg @l="100" char="-1" flags="0" index="60" line="100" name="" text="溢出来的奶水不断汇集，然后完全沿着胸部的曲线下流。" timeout="" voices="" />
		<msg @l="101" char="-1" flags="0" index="62" line="101" name="" text="这个样子实在是色气满满，我根本无法从胸部移开视线。" timeout="" voices="" />
		<showmsg @l="104" duration="500" show="0" />
		<loadimage @l="105" bg="0" fit="0" screen="1" src="evrai15_06d" />
		<scroll @l="106" async="0" center_x="45" center_y="45" duration="0" flags="0" rev="0." screen="1" transparency="0" v0="0." v1="0." wait_flags="0" x="0" y="0" zoom="1." />
		<chgscrn @l="107" duration="1000" flags="256" flip="-1" flip_flags="0" param="16" screen="1" type="99" />
		<msg @l="110" char="3" flags="0" index="64" line="110" name="礼羽" text="「呵呵，你怎么用这么火热的眼神看着我？」" timeout="" voices="rai17312" />
		<msg @l="112" char="11" flags="0" index="66" line="112" name="亮" text="「因为礼羽的胸部可是流出了奶水啊……咕噜」" timeout="" voices="" />
		<msg @l="114" char="-1" flags="0" index="68" line="114" name="" text="最爱的女人在我面前流出奶水的样子，让我不由得咽了口唾沫。" timeout="" voices="" />
		<msg @l="115" char="-1" flags="0" index="70" line="115" name="" text="甜蜜的气味充满了此处，几乎要将我的大脑融化。" timeout="" voices="" />
		<chgbg @l="117" duration="300" flags="256" flip="-1" flip_flags="0" param="16" screen="1" src="evrai15_06c" type="99" />
		<msg @l="118" char="3" flags="0" index="72" line="118" name="礼羽" text="「对乳头这样……挤一下的话……」" timeout="" voices="rai17313" />
		<msg @l="121" char="11" flags="0" index="74" line="121" name="亮" text="「哦哦！！！　奶水挤出来了，好厉害！！！！！」" timeout="" voices="" />
		<msg @l="123" char="-1" flags="0" index="76" line="123" name="" text="礼羽捏住乳头后，奶水便随即溢出来。" timeout="" voices="" />
		<msg @l="124" char="-1" flags="0" index="78" line="124" name="" text="溢出来越来越多，而且从乳头往乳房下方流下的液体又白又浓。" timeout="" voices="" />
		<msg @l="126" char="11" flags="0" index="80" line="126" name="亮" text="「我可以舔吗？」" timeout="" voices="" />
		<chgbg @l="128" duration="300" flags="256" flip="-1" flip_flags="0" param="16" screen="1" src="evrai15_06d" type="99" />
		<msg @l="129" char="3" flags="0" index="82" line="129" name="礼羽" text="「啊哈，你的眼睛都兴奋地充血了……真开心。看来我得到了一个能让你兴奋的新武器呢」" timeout="" voices="rai17314" />
		<showmsg @l="135" duration="500" show="0" />
		<loadimage @l="136" bg="0" fit="0" screen="1" src="evrai15_06b" />
		<scroll @l="137" async="0" center_x="45" center_y="200" duration="0" flags="0" rev="0." screen="1" transparency="0" v0="0." v1="0." wait_flags="0" x="0" y="0" zoom="1.5" />
		<chgscrn @l="138" duration="1000" flags="256" flip="-1" flip_flags="0" param="16" screen="1" type="99" />
		<msg @l="141" char="-1" flags="0" index="84" line="141" name="" text="没等她回应，我立马含住那又甜又香的乳头。" timeout="" voices="" />
		<msg @l="144" char="3" flags="0" index="86" line="144" name="礼羽" text="「啊，呀啊啊！！？」" timeout="" voices="rai17315" />
		<msg @l="146" char="3" flags="0" index="88" line="146" name="礼羽" text="「啊，等一下……啊，啊啊……吸得这么多，即将出生的孩子怎么办……」" timeout="" voices="rai17316" />
		<play_sound @l="148" buf="5" fadeout="0" intro="0" src="rai80016" type="6" volume="1." />
		<msg @l="149" char="-1" flags="0" index="90" line="149" name="" text="母乳和我想的一样，口感非常顺滑。" timeout="" voices="" />
		<msg @l="150" char="-1" flags="0" index="92" line="150" name="" text="这浓度与其说是奶，不如说根本是水了，非常容易喝下去。" timeout="" voices="" />
		<msg @l="152" char="11" flags="0" index="94" line="152" name="亮" text="「味道确实很甜，但却有些铁味……怎么说呢，总觉得让我有些迷惑」" timeout="" voices="" />
		<play_sound @l="153" buf="5" fadeout="0" type="1" />
		<showmsg @l="156" duration="500" show="0" />
		<loadimage @l="157" bg="0" fit="0" screen="1" src="evrai15_06a" />
		<scroll @l="158" async="0" center_x="45" center_y="45" duration="0" flags="0" rev="0." screen="1" transparency="0" v0="0." v1="0." wait_flags="0" x="0" y="0" zoom="1." />
		<chgscrn @l="159" duration="1000" flags="256" flip="-1" flip_flags="0" param="16" screen="1" type="99" />
		<msg @l="162" char="3" flags="0" index="96" line="162" name="礼羽" text="「那当然，毕竟是从血液里产生的，可没有像市面里的牛奶一样被加工过」" timeout="" voices="rai17317" />
		<play_sound @l="164" buf="5" fadeout="0" intro="0" src="rai80015" type="6" volume="1." />
		<msg @l="165" char="11" flags="0" index="98" line="165" name="亮" text="「确实是这样没错。这味道更像是野生的……不对，是母性的感觉」" timeout="" voices="" />
		<msg @l="167" char="-1" flags="0" index="100" line="167" name="" text="虽然一开始和香味不符的味道吓了我一跳，但细细品味后却发现其实非常甜滑爽润。" timeout="" voices="" />
		<msg @l="168" char="-1" flags="0" index="102" line="168" name="" text="我大张鼻孔尽力吸收奶水香味，等察觉到的时候，自己已经变成了母乳的俘虏了。" timeout="" voices="" />
		<msg @l="170" char="11" flags="0" index="104" line="170" name="亮" text="「嗯咕，嗯咕，嗯咕……礼羽的胸部好香，太香了！」" timeout="" voices="" />
		<play_sound @l="171" buf="5" fadeout="0" type="1" />
		<chgbg @l="173" duration="300" flags="256" flip="-1" flip_flags="0" param="16" screen="1" src="evrai15_06d" type="99" />
		<msg @l="174" char="3" flags="0" index="106" line="174" name="礼羽" text="「真是的，你又不是小宝宝……真拿你没办法」" timeout="" voices="rai17318" />
		<play_sound @l="176" buf="5" fadeout="0" intro="0" src="rai80016" type="6" volume="1." />
		<msg @l="177" char="-1" flags="0" index="108" line="177" name="" text="然后我用空着的手去揉捏乳房，并且继续含住乳头以求更多奶水。" timeout="" voices="" />
		<msg @l="178" char="-1" flags="0" index="110" line="178" name="" text="随后礼羽摸着我的头不断安抚。" timeout="" voices="" />
		<play_sound @l="179" buf="5" fadeout="0" type="1" />
		<msg @l="181" char="3" flags="0" index="112" line="181" name="礼羽" text="「真是个调皮鬼。真有这么喜欢胸部吗～？」" timeout="" voices="rai17319" />
		<play_sound @l="183" buf="5" fadeout="0" intro="0" src="rai80015" type="6" volume="1." />
		<msg @l="184" char="11" flags="0" index="114" line="184" name="亮" text="「嗯……那是……」" timeout="" voices="" />
		<msg @l="186" char="-1" flags="0" index="116" line="186" name="" text="我当然喜欢从礼羽胸部流出来的奶水……但我感觉好像不只是这样。" timeout="" voices="" />
		<msg @l="187" char="-1" flags="0" index="118" line="187" name="" text="是什么呢？　胸部——奶水到底代表着什么。" timeout="" voices="" />
		<msg @l="189" char="11" flags="0" index="120" line="189" name="亮" text="「是母性……吗？」" timeout="" voices="" />
		<play_sound @l="190" buf="5" fadeout="0" type="1" />
		<chgbg @l="192" duration="300" flags="256" flip="-1" flip_flags="0" param="16" screen="1" src="evrai15_06c" type="99" />
		<msg @l="193" char="3" flags="0" index="122" line="193" name="礼羽" text="「母性？」" timeout="" voices="rai17320" />
		<play_sound @l="195" buf="5" fadeout="0" intro="0" src="rai80017" type="6" volume="1." />
		<msg @l="196" char="-1" flags="0" index="124" line="196" name="" text="这似乎是礼羽意料之外的回答，让她愣了一下。" timeout="" voices="" />
		<msg @l="198" char="11" flags="0" index="126" line="198" name="亮" text="「因为我是孤儿，从小就不认识父母，所以一直在寻求母性——」" timeout="" voices="" />
		<msg @l="200" char="-1" flags="0" index="128" line="200" name="" text="在我说完之前，礼羽便伸出双手抱住了我。" timeout="" voices="" />
		<msg @l="201" char="-1" flags="0" index="130" line="201" name="" text="然后双颊相贴，她对我温柔地低语。" timeout="" voices="" />
		<play_sound @l="202" buf="5" fadeout="0" type="1" />
		<chgbg @l="204" duration="300" flags="256" flip="-1" flip_flags="0" param="16" screen="1" src="evrai15_06a" type="99" />
		<msg @l="205" char="3" flags="0" index="132" line="205" name="礼羽" text="「那么，尽情享用我的胸吧。小宝宝的那份我会重新再生产出来的。现在这些都是你的」" timeout="" voices="rai17321" />
		<msg @l="207" char="3" flags="0" index="134" line="207" name="礼羽" text="「想怎么撒娇、亲吻都行哦？」" timeout="" voices="rai17322" />
		<play_sound @l="209" buf="5" fadeout="0" intro="0" src="rai80016" type="6" volume="1." />
		<msg @l="210" char="-1" flags="0" index="136" line="210" name="" text="礼羽非常慈爱地用温柔的眼神对我微笑。" timeout="" voices="" />
		<msg @l="211" char="-1" flags="0" index="138" line="211" name="" text="光是这样都让我产生了一种似乎是她生下来我一般的错觉，让我心中一热。" timeout="" voices="" />
		<msg @l="213" char="11" flags="0" index="140" line="213" name="亮" text="「……啾，我舔。啾―啾——」" timeout="" voices="" />
		<play_sound @l="214" buf="5" fadeout="0" type="1" />
		<chgbg @l="216" duration="300" flags="256" flip="-1" flip_flags="0" param="16" screen="1" src="evrai15_06b" type="99" />
		<msg @l="217" char="3" flags="0" index="142" line="217" name="礼羽" text="「嗯……啊，啊啊……我的胸很好吃吧？」" timeout="" voices="rai17323" />
		<play_sound @l="219" buf="5" fadeout="0" intro="0" src="rai80017" type="6" volume="1." />
		<msg @l="220" char="11" flags="0" index="144" line="220" name="亮" text="「啾，啾……嗯，我舔我舔」" timeout="" voices="" />
		<msg @l="222" char="-1" flags="0" index="146" line="222" name="" text="我沉迷于吮吸母乳，尽情地摇晃脑袋。" timeout="" voices="" />
		<play_sound @l="223" buf="5" fadeout="0" type="1" />
		<chgbg @l="225" duration="300" flags="256" flip="-1" flip_flags="0" param="16" screen="1" src="evrai15_06a" type="99" />
		<msg @l="226" char="3" flags="0" index="148" line="226" name="礼羽" text="「呵呵，太好了。就连答话的时候也不把嘴从胸部放开，你真是喜欢那里呀」" timeout="" voices="rai17324" />
		<play_sound @l="228" buf="5" fadeout="0" intro="0" src="rai80017" type="6" volume="1." />
		<msg @l="229" char="11" flags="0" index="150" line="229" name="亮" text="「因为、礼羽的母乳、啾、太美味了……啾，好吃好吃」" timeout="" voices="" />
		<play_sound @l="230" buf="5" fadeout="0" type="1" />
		<chgbg @l="232" duration="300" flags="256" flip="-1" flip_flags="0" param="16" screen="1" src="evrai15_06d" type="99" />
		<msg @l="233" char="3" flags="0" index="152" line="233" name="礼羽" text="「啊！　边吸边说话的话，也会让乳头颤动的啊」" timeout="" voices="rai17325" />
