		<msg @l="1962" char="0" flags="0" index="909" line="1962" name="花音" text="「话说，没想到ＪＣ小姐那么敏感啊」" timeout="" voices="kan12521" />
		<msg @l="1964" char="3" flags="0" index="911" line="1964" name="礼羽" text="「稍微摸到就好大反应，真可爱\x;note:」" timeout="" voices="rai12073" />
		<msg @l="1966" char="4" flags="0" index="913" line="1966" name="ＪＣ" text="「已经不能嫁人了……哭哭」" timeout="" voices="jei10924" />
		<msg @l="1968" char="2" flags="0" index="915" line="1968" name="纱纱" text="「其实还有颗少女心！！？」" timeout="" voices="sas11620" />
		<msg @l="1970" char="8" flags="0" index="917" line="1970" name="静香" text="「这种时候，就有种插不进去的寂寞感呢。嗯嗯我懂的」" timeout="" voices="siz03719" />
		<msg @l="1972" char="1" flags="0" index="919" line="1972" name="汐" text="「所以说不要用怜悯的眼光看着我啊——！！」" timeout="" voices="usi13617" />
		<msg @l="1974" char="8" flags="0" index="921" line="1974" name="静香" text="「不要紧的哦，一定会有狂热者在的。就在世界的某处」" timeout="" voices="siz03720" />
		<msg @l="1976" char="1" flags="0" index="923" line="1976" name="汐" text="「小静香也和我没差多少吧？　再说了，还是我胸更大一点吧」" timeout="" voices="usi13618" />
		<msg @l="1979" char="8" flags="0" index="925" line="1979" name="静香" text="「唔……我总有一天会成长的」" timeout="" voices="siz03721" />
		<msg @l="1981" char="1" flags="0" index="927" line="1981" name="汐" text="「王婆卖瓜！？　也不是谁都能变那么大的哦！？　我以前也以为它会自己变大的！！」" timeout="" voices="usi13619" />
		<msg @l="1983" char="8" flags="0" index="929" line="1983" name="静香" text="「什、什么……！？　说起来是这样啊」" timeout="" voices="siz03722" />
		<msg @l="1985" char="8" flags="0" index="931" line="1985" name="静香" text="「礼羽大姐姐，怎么样才能长得那么大呢？」" timeout="" voices="siz03723" />
		<msg @l="1987" char="1" flags="0" index="933" line="1987" name="汐" text="「那边几位！　请讲详细一点！！　顺便把现在也还来得及的方法教给我！！」" timeout="" voices="usi13620" />
		<msg @l="1989" char="3" flags="0" index="935" line="1989" name="礼羽" text="「不是，我也没干什么特别的啦……」" timeout="" voices="rai12074" />
		<msg @l="1991" char="8" flags="0" index="937" line="1991" name="静香" text="「那ＪＣ小姐！！」" timeout="" voices="siz03724" />
		<msg @l="1993" char="4" flags="0" index="939" line="1993" name="ＪＣ" text="「多吃，多睡！」" timeout="" voices="jei10925" />
		<msg @l="1996" char="2" flags="0" index="941" line="1996" name="纱纱" text="「那只会横向变大吧？」" timeout="" voices="sas11621" />
		<msg @l="1998" char="8" flags="0" index="943" line="1998" name="静香" text="「那纱纱大姐……。花音大姐姐，怎么样胸部才能变大呢？」" timeout="" voices="siz03725" />
		<msg @l="2000" char="2" flags="0" index="945" line="2000" name="纱纱" text="「我的尺寸就直接被无视了！！？」" timeout="" voices="sas11622" />
