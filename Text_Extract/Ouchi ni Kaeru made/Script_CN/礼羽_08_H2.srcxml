		<chgbg @l="128" duration="300" flags="256" flip="-1" flip_flags="0" param="16" screen="1" src="evrai05_02a" type="99" />
		<msg @l="130" char="3" flags="0" index="74" line="130" name="礼羽" text="「你是对我发情了对不对……？嗯呼呼～，跟计划的一样\x;hearts:」" timeout="" voices="rai13717" />
		<play_sound @l="132" buf="5" fadeout="0" intro="0" src="rai80001" type="6" volume="1." />
		<msg @l="133" char="11" flags="0" index="76" line="133" name="亮" text="「跟计划的一样什么的……难道你一开始就打算这么做的吗！？」" timeout="" voices="" />
		<play_sound @l="134" buf="5" fadeout="0" type="1" />
		<chgbg @l="136" duration="300" flags="256" flip="-1" flip_flags="0" param="16" screen="1" src="evrai05_02e" type="99" />
		<msg @l="138" char="3" flags="0" index="78" line="138" name="礼羽" text="「……呼呼，保密\x;heart: 嗯嗯……吸溜、吸溜吸溜……！吸溜、吸溜吸溜……！吸溜噗、吸溜吸溜……！！」" timeout="" voices="rai13718" />
		<play_sound @l="140" buf="5" fadeout="0" intro="0" src="rai80002" type="6" volume="1." />
		<msg @l="141" char="11" flags="0" index="80" line="141" name="亮" text="「又像这样蒙混过去……！啊、咕唔唔……！唔、啊啊……！」" timeout="" voices="" />
		<msg @l="143" char="-1" flags="0" index="82" line="143" name="" text="像是为了要打断我的话一般，她不留余力地吮吸着阴茎。" timeout="" voices="" />
		<play_sound @l="144" buf="5" fadeout="0" type="1" />
		<msg @l="146" char="3" flags="0" index="84" line="146" name="礼羽" text="「吸溜噗、吸溜……！喜欢被用舌头玩弄前端对吧……？吸溜、吸溜吸溜、吸溜……！」" timeout="" voices="rai13719" />
		<play_sound @l="148" buf="5" fadeout="0" intro="0" src="rai80003" type="6" volume="1." />
		<msg @l="149" char="-1" flags="0" index="86" line="149" name="" text="是因为已经经历过口交呢，还是因为本来就有这方面的才能呢，好像完全没有任何难受的样子。" timeout="" voices="" />
		<msg @l="150" char="-1" flags="0" index="88" line="150" name="" text="技术本身也有所提高，确实是很巧妙地运用着舌尖刺激着我有感觉的地方。" timeout="" voices="" />
		<msg @l="152" char="11" flags="0" index="90" line="152" name="亮" text="「啊、啊啊啊……！！这样不行……！」" timeout="" voices="" />
