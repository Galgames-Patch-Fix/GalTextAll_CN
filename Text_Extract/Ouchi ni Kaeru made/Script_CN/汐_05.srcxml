		<msg @l="479" char="11" flags="0" index="227" line="479" name="亮" text="「好厉害啊」" timeout="" voices="" />
		<chgbg @l="481" duration="300" flags="256" flip="-1" flip_flags="0" param="16" screen="1" src="evetc01_02b" type="99" />
		<msg @l="483" char="2" flags="0" index="229" line="483" name="纱纱" text="「每个蛋糕都好可爱，吃掉好浪费啊」" timeout="" voices="sas09801" />
		<msg @l="485" char="1" flags="0" index="231" line="485" name="汐" text="「别这么说，别客气，吃吧。因为是试吃会，所以必须要听听味道的感想」" timeout="" voices="usi10427" />
		<msg @l="487" char="0" flags="0" index="233" line="487" name="花音" text="「咦？　礼羽小姐不吃吗？」" timeout="" voices="kan10802" />
		<msg @l="489" char="3" flags="0" index="235" line="489" name="礼羽" text="「因为是边尝边做的，所以今天已经不想吃甜的东西了」" timeout="" voices="rai10300" />
		<msg @l="491" char="0" flags="0" index="237" line="491" name="花音" text="「那，小汐也是吗？」" timeout="" voices="kan10803" />
		<msg @l="493" char="1" flags="0" index="239" line="493" name="汐" text="「我是以装饰为主，所以还没怎么吃」" timeout="" voices="usi10428" />
		<msg @l="495" char="4" flags="0" index="241" line="495" name="ＪＣ" text="「那辈前也快吃啊。来，张嘴，啊——」" timeout="" voices="jei09600" />
		<msg @l="497" char="1" flags="0" index="243" line="497" name="汐" text="「诶？！　不、不用了，我又不是小孩子！」" timeout="" voices="usi10429" />
		<msg @l="499" char="4" flags="0" index="245" line="499" name="ＪＣ" text="「不是小孩子了啊——？」" timeout="" voices="jei09601" />
		<msg @l="501" char="1" flags="0" index="247" line="501" name="汐" text="「才不是啊！　姐姐自己吃也没问题的！」" timeout="" voices="usi10430" />
		<msg @l="503" char="2" flags="0" index="249" line="503" name="纱纱" text="「那个，前辈愿意的话，也来试试“啊——”吗？」" timeout="" voices="sas09802" />
		<msg @l="505" char="11" flags="0" index="251" line="505" name="亮" text="「你的好意我心领了」" timeout="" voices="" />
		<chgbg @l="507" duration="300" flags="256" flip="-1" flip_flags="0" param="16" screen="1" src="evetc01_02a" type="99" />
		<msg @l="509" char="2" flags="0" index="253" line="509" name="纱纱" text="「？？」" timeout="" voices="sas09803" />
		<msg @l="511" char="4" flags="0" index="255" line="511" name="ＪＣ" text="「好了好了纱纱亲！　别管那家伙抓紧吃吧！」" timeout="" voices="jei09602" />
		<msg @l="513" char="-1" flags="0" index="257" line="513" name="" text="抱着小纱纱肩膀的ＪＣ小姐，向我使了个眼色。" timeout="" voices="" />
		<msg @l="514" char="-1" flags="0" index="259" line="514" name="" text="这么说来，小纱纱好像不知道我不能吃蛋糕啊。" timeout="" voices="" />
		<msg @l="516" char="11" flags="0" index="261" line="516" name="亮" text="（哈……要是能吃其中的任何一个就好了啊）" timeout="" voices="" />
		<msg @l="518" char="-1" flags="0" index="263" line="518" name="" text="排成一排的试制品看上去都很美味，但是我怎么也下不去手。" timeout="" voices="" />
		<chgbg @l="520" duration="300" flags="256" flip="-1" flip_flags="0" param="16" screen="1" src="evetc01_01a" type="99" />
		<msg @l="522" char="-1" flags="0" index="265" line="522" name="" text="……诶？　这也是蛋糕吗……？" timeout="" voices="" />
		<msg @l="524" char="11" flags="0" index="267" line="524" name="亮" text="（这个的话，我好像也能吃——）" timeout="" voices="" />
		<msg @l="526" char="1" flags="0" index="269" line="526" name="汐" text="「怎么样？　有中意的蛋糕吗？　我试着为你做了几个奇怪的变种」" timeout="" voices="usi10431" />
		<msg @l="528" char="11" flags="0" index="271" line="528" name="亮" text="「摆在这里的，有一个我好像能吃！」" timeout="" voices="" />
		<msg @l="530" char="1" flags="0" index="273" line="530" name="汐" text="「真的？！　哪个哪个？　那个蛋糕？！」" timeout="" voices="usi10432" />
		<msg @l="532" char="11" flags="0" index="275" line="532" name="亮" text="「这个看起来圆圆的，像馒头一样的东西。虽然很像日式点心，但那也是蛋糕吧？」" timeout="" voices="" />
		<msg @l="534" char="1" flags="0" index="277" line="534" name="汐" text="「啊……那是用多余的材料试着做的，算是西式和日式的结合吧」" timeout="" voices="usi10433" />
		<msg @l="536" char="11" flags="0" index="279" line="536" name="亮" text="「也就是说，这不算蛋糕……？」" timeout="" voices="" />
		<msg @l="538" char="-1" flags="0" index="281" line="538" name="" text="我还以为我说不定成长到能吃它了。" timeout="" voices="" />
		<msg @l="540" char="1" flags="0" index="283" line="540" name="汐" text="「嗯。是实验性的点心啊，虽然很可惜但确实不算蛋糕」" timeout="" voices="usi10434" />
		<msg @l="542" char="11" flags="0" index="285" line="542" name="亮" text="「深受打击」" timeout="" voices="" />
		<msg @l="544" char="1" flags="0" index="287" line="544" name="汐" text="「但是，这样也就再次确认了。你果然还是吃不了蛋糕这种东西啊」" timeout="" voices="usi10435" />
		<msg @l="546" char="-1" flags="0" index="289" line="546" name="" text="至今为止我都是把蛋糕捣碎吃的。" timeout="" voices="" />
		<msg @l="547" char="-1" flags="0" index="291" line="547" name="" text="所以我并不是不喜欢甜食和西点。当然吃奶油泡芙还是可以吃的。" timeout="" voices="" />
		<msg @l="549" char="1" flags="0" index="293" line="549" name="汐" text="「如果一直朝这方向努力的话，总有一天能做出你能吃的蛋糕的！」" timeout="" voices="usi10436" />
