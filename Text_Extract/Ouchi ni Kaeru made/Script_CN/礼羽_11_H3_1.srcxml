<xscript>
	<code>
		<msg @l="737" char="3" flags="0" index="423" line="737" name="礼羽" text="「吸溜溜溜溜溜\x;heart:\x;hearts:　啊哈……要留下唇印了、噗啾、吸溜。吸出来好多啊\x;heart:」" timeout="" voices="rai00571" />
		<chgbg @l="739" duration="300" flags="256" flip="-1" flip_flags="0" param="16" screen="1" src="evrai08_05c" type="99" />
		<msg @l="741" char="3" flags="0" index="425" line="741" name="礼羽" text="「弄痛你了吗？　抱歉啊。太高兴了一不小心就忘我了……啾、嘶溜、吸溜溜！」" timeout="" voices="rai00572" />
		<play_sound @l="743" buf="5" fadeout="0" intro="0" src="rai80028" type="6" volume="1." />
		<msg @l="744" char="-1" flags="0" index="427" line="744" name="" text="为了对强烈的吸吮表示歉意，这次她温柔地亲了一下乳头。" timeout="" voices="" />
		<msg @l="745" char="-1" flags="0" index="429" line="745" name="" text="时缓时急的手法，难道她是天才吗？　不愧是做蛋糕的顶尖人物理解能力太厉害了。" timeout="" voices="" />
		<play_sound @l="746" buf="5" fadeout="0" type="1" />
		<chgbg @l="748" duration="300" flags="256" flip="-1" flip_flags="0" param="16" screen="1" src="evrai08_04b" type="99" />
		<msg @l="750" char="3" flags="0" index="431" line="750" name="礼羽" text="「对了、肉棒不套弄一下的话也太可怜了。看招、我捋我捋\x;heart:」" timeout="" voices="rai00573" />
		<msg @l="752" char="3" flags="0" index="433" line="752" name="礼羽" text="「怎么样？　从根部……到前端、我捋我捋……不停地来来回回地、套弄着哦……」" timeout="" voices="rai00574" />
		<play_sound @l="754" buf="5" fadeout="0" intro="0" src="rai80029" type="6" volume="1." />
		<msg @l="755" char="11" flags="0" index="435" line="755" name="亮" text="「咕……那里、就保持这样……拜托了」" timeout="" voices="" />
		<msg @l="757" char="-1" flags="0" index="437" line="757" name="" text="一边刺激着龟头一边舔着乳头、再加上语言的煽动、我的射精感一下子就变强了。" timeout="" voices="" />
		<play_sound @l="758" buf="5" fadeout="0" type="1" />
		<chgbg @l="760" duration="300" flags="256" flip="-1" flip_flags="0" param="16" screen="1" src="evrai08_04d" type="99" />
		<msg @l="762" char="3" flags="0" index="439" line="762" name="礼羽" text="「嘻嘻。已经想要射了啊。鸡鸡这样抽动着，在说想要咻咻地射出来哦\x;heart:」" timeout="" voices="rai00575" />
		<play_sound @l="764" buf="5" fadeout="0" intro="0" src="rai80028" type="6" volume="1." />
		<msg @l="765" char="-1" flags="0" index="441" line="765" name="" text="礼羽通过我的语气和手感受着我的界限、大力地握紧了阴茎。" timeout="" voices="" />
		<play_sound @l="766" buf="5" fadeout="0" type="1" />
		<chgbg @l="768" duration="300" flags="256" flip="-1" flip_flags="0" param="16" screen="1" src="evrai08_04a" type="99" />
		<msg @l="770" char="3" flags="0" index="443" line="770" name="礼羽" text="「不过，现在还不行」" timeout="" voices="rai00576" />
		<msg @l="772" char="11" flags="0" index="445" line="772" name="亮" text="「为，为什么！？」" timeout="" voices="" />
		<msg @l="774" char="-1" flags="0" index="447" line="774" name="" text="她的手仅仅握着肉棒的根部一动也不动。" timeout="" voices="" />
		<msg @l="776" char="11" flags="0" index="449" line="776" name="亮" text="「啊啊…………啊」" timeout="" voices="" />
		<msg @l="778" char="-1" flags="0" index="451" line="778" name="" text="由于太过惊讶、牙齿发出了咔嗒咔嗒的丢人的声音。" timeout="" voices="" />
		<chgbg @l="780" duration="300" flags="256" flip="-1" flip_flags="0" param="16" screen="1" src="evrai08_03a" type="99" />
		<msg @l="782" char="3" flags="0" index="453" line="782" name="礼羽" text="「如果想去的话，就必须好好求我。快说吧？　说请让我尽情地射精」" timeout="" voices="rai00577" />
		<play_sound @l="784" buf="1" fadeout="0" intro="-1" src="hse104" type="1" volume="1." />
		<msg @l="786" char="-1" flags="0" index="455" line="786" name="" text="这一次她像是故意让我着急，用指尖慢慢划过龟头的边缘。" timeout="" voices="" />
		<msg @l="787" char="-1" flags="0" index="457" line="787" name="" text="虽然这样很舒服，但这种程度的刺激还无法满足射精的欲望。" timeout="" voices="" />
		<play_sound @l="789" buf="1" fadeout="0" type="1" />
		<msg @l="791" char="11" flags="0" index="459" line="791" name="亮" text="「这不完全就是欺负人嘛，我已经被你玩弄到现在了，求求你做到最后吧」" timeout="" voices="" />
		<msg @l="793" char="3" flags="0" index="461" line="793" name="礼羽" text="「不行\x;heart:　我想听的不是这话哦！」" timeout="" voices="rai00578" />
		<msg @l="795" char="11" flags="17" index="463" line="795" name="亮" text="「呜咿！！？　不要挠啊！　要做不成女婿了！」" timeout="" voices="" />
		<shakescrn @l="796" amp="20" cycle="20" duration="300" flags="2" screen="0" />
		<pause @l="797" flags="0" index="465" timeout="" />
		<chgbg @l="799" duration="300" flags="256" flip="-1" flip_flags="0" param="16" screen="1" src="evrai08_03d" type="99" />
		<msg @l="802" char="3" flags="0" index="466" line="802" name="礼羽" text="「快说吧，好孩子。不要再固执了，试着说……吧？」" timeout="" voices="rai00579" />
		<msg @l="804" char="-1" flags="0" index="468" line="804" name="" text="就像教导孩子一样，温柔地在耳边轻声说着，力度慢慢地放松下来。" timeout="" voices="" />
		<msg @l="806" char="11" flags="0" index="470" line="806" name="亮" text="（看来已经完全中了她的圈套啊……）" timeout="" voices="" />
		<msg @l="808" char="-1" flags="0" index="472" line="808" name="" text="在绝妙的软硬兼施下，一切身为男人的自尊，都嘎啦嘎啦地崩塌了。" timeout="" voices="" />
		<msg @l="810" char="11" flags="0" index="474" line="810" name="亮" text="「请，请让我射精吧!   拜托了」" timeout="" voices="" />
		<chgbg @l="812" duration="300" flags="256" flip="-1" flip_flags="0" param="16" screen="1" src="evrai08_04c" type="99" />
		<msg @l="814" char="3" flags="0" index="476" line="814" name="礼羽" text="「嘻嘻\x;heart:　有好好地说了呢……我最喜欢诚实的孩子、啾\x;hearts:」" timeout="" voices="rai00580" />
		<play_sound @l="819" buf="1" fadeout="0" intro="0" src="hse106" type="1" volume="1." />
		<msg @l="821" char="-1" flags="0" index="478" line="821" name="" text="于是再次握住阴茎，上下捋了起来。" timeout="" voices="" />
		<msg @l="822" char="-1" flags="0" index="480" line="822" name="" text="感觉到在被很熟练的，很了解男人射精时间的高超技巧玩弄着，快感无法抗拒，难以言表。" timeout="" voices="" />
		<msg @l="824" char="11" flags="0" index="482" line="824" name="亮" text="「啊、咕呜呜呜……！？　你都让我这么难受了，乳头也舔一下吧」" timeout="" voices="" />
		<play_sound @l="826" buf="1" fadeout="300" type="1" />
		<chgbg @l="828" duration="300" flags="256" flip="-1" flip_flags="0" param="16" screen="1" src="evrai08_04a" type="99" />
		<msg @l="830" char="3" flags="0" index="484" line="830" name="礼羽" text="「嘻嘻、乳头、你好像已经很喜欢被玩弄了呢……叽啾、吸溜、啾……！　我捋我捋！　啾、啾！」" timeout="" voices="rai00581" />
		<play_sound @l="832" buf="5" fadeout="0" intro="0" src="rai80029" type="6" volume="1." />
		<msg @l="833" char="-1" flags="0" index="486" line="833" name="" text="乳头和阴茎同时被欺负着实在太舒服了，血流在咕嘟咕嘟地流入阴茎。" timeout="" voices="" />
		<play_sound @l="834" buf="5" fadeout="0" type="1" />
		<msg @l="836" char="3" flags="0" index="488" line="836" name="礼羽" text="「哈啊、哈啊……嗯、啾……仅仅是舔着肉棒就已经硬邦邦了，嗯哈……就像握着热铁一样」" timeout="" voices="rai00582" />
		<chgbg @l="839" duration="300" flags="256" flip="-1" flip_flags="0" param="16" screen="1" src="evrai08_04d" type="99" />
		<msg @l="841" char="3" flags="0" index="490" line="841" name="礼羽" text="「就是这个凶恶的肉棒把我的处女夺走了啊，哈啊……嗯啊，真是个坏孩子呢ー\x;heart:」" timeout="" voices="rai00583" />
		<play_sound @l="843" buf="1" fadeout="0" intro="0" src="hse106" type="1" volume="1." />
