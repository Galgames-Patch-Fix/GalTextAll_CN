		<chgscrn @l="806" duration="300" flags="256" flip="-1" flip_flags="0" param="16" screen="1" type="99" />
		<msg @l="808" char="1" flags="0" index="418" line="808" name="汐" text="「呜咕……嗯，嗯呜……啊啊，今天也，射出了这么多……呜啊，啊啊，好厉害……嗯嗯」" timeout="" voices="usi13497" />
		<play_sound @l="810" buf="1" fadeout="0" type="1" />
		<msg @l="812" char="1" flags="0" index="420" line="812" name="汐" text="「嗯呜呜，哈呜……诶嘿嘿\x;heart:咕嘟咕嘟地精子，这么多……嗯呼，嗯呵呵呵呵\x;heart:\x;hearts:」" timeout="" voices="usi13498" />
		<msg @l="814" char="-1" flags="0" index="422" line="814" name="" text="凝视着从秘裂里流出来的精子，陶醉地笑着。" timeout="" voices="" />
		<msg @l="815" char="-1" flags="0" index="424" line="815" name="" text="虽然在做爱时的撒娇和进攻都很强力，但做爱之后的这个表情，也很是撩人。" timeout="" voices="" />
		<msg @l="817" char="1" flags="0" index="426" line="817" name="汐" text="「真是在缆车里干了件不得了的事啊……」" timeout="" voices="usi13499" />
		<msg @l="819" char="11" flags="0" index="428" line="819" name="亮" text="「不用担心。我有带着纸巾和毛巾」" timeout="" voices="" />
		<msg @l="821" char="11" flags="0" index="430" line="821" name="亮" text="「总之，就请你放心的抓好我吧」" timeout="" voices="" />
		<loadimage @l="823" bg="0" fit="0" screen="1" src="evusi09_09b" />
		<scroll @l="824" async="0" center_x="0" center_y="1300" duration="0" flags="0" rev="0." screen="1" transparency="0" v0="0." v1="0." wait_flags="0" x="0" y="0" zoom="1." />
		<chgscrn @l="825" duration="300" flags="256" flip="-1" flip_flags="0" param="16" screen="1" type="99" />
		<msg @l="827" char="-1" flags="0" index="432" line="827" name="" text="重新将身子靠过去，让她紧紧地抱住我，在她的腰轻轻地抬起来间隙那里铺上毛巾。" timeout="" voices="" />
		<msg @l="828" char="-1" flags="0" index="434" line="828" name="" text="那个景象，就像是在给小宝宝换尿布一样，有种说不出的感觉。" timeout="" voices="" />
		<chgbg @l="830" duration="300" flags="256" flip="-1" flip_flags="0" param="16" screen="1" src="evusi09_09b" type="99" />
		<msg @l="832" char="11" flags="0" index="436" line="832" name="亮" text="「我要擦地板了，朝霞学姐就请坐到椅子上」" timeout="" voices="" />
		<chgbg @l="834" duration="300" flags="256" flip="-1" flip_flags="0" param="16" screen="1" src="evusi09_09a" type="99" />
		<msg @l="836" char="1" flags="0" index="438" line="836" name="汐" text="「嗯，好的」" timeout="" voices="usi13500" />
		<msg @l="838" char="-1" flags="0" index="440" line="838" name="" text="用余光看着她靠在椅子背上，活动着手，一口气将地板打扫干净。" timeout="" voices="" />
		<chgbg @l="840" duration="1000" flags="256" flip="-1" flip_flags="0" param="16" screen="1" src="evusi09_10f" type="99" />
