	<code>
		<label @l="5" id="scene_0_02" />
		<play_sound @l="7" buf="0" fadeout="2500" type="0" />
		<showmsg @l="8" duration="500" show="0" />
		<loadimage @l="15" bg="0" fit="0" screen="1" src="evkan16_01a" />
		<scroll @l="16" async="0" center_x="45" center_y="45" duration="0" flags="0" rev="0." screen="1" transparency="0" v0="0." v1="0." wait_flags="0" x="0" y="0" zoom="1." />
		<chgscrn @l="17" duration="2500" flags="4608" flip="-1" flip_flags="0" param="0" screen="1" type="10" />
		<play_sound @l="18" buf="0" fadeout="500" intro="-1" src="bgm18" type="0" volume="1." />
		<msg @l="33" char="11" flags="0" index="0" line="33" name="亮" text="「啊？　花音？　等等、你在……！！」" timeout="" voices="" />
		<chgbg @l="35" duration="300" flags="256" flip="-1" flip_flags="0" param="16" screen="1" src="evkan16_01c" type="99" />
		<msg @l="36" char="0" flags="0" index="2" line="36" name="花音" text="「因为，它不是很有精神，正要变大吗？　那么，必须快点让它冷静下来啊」" timeout="" voices="kan11600" />
		<msg @l="38" char="11" flags="0" index="4" line="38" name="亮" text="「那个是因为刚才的对话，或者说当时的气氛……诶、已经脱了！？」" timeout="" voices="" />
		<msg @l="40" char="0" flags="0" index="6" line="40" name="花音" text="「诶嘿嘿。全都交给我吧\x;heart:……嘿咻」" timeout="" voices="kan11601" />
		<play_sound @l="42" buf="1" fadeout="0" intro="-1" src="se511" type="1" volume="1." />
		<msg @l="44" char="-1" flags="0" index="8" line="44" name="" text="花音在我面前跪下，脱下制服露出丰满的胸部。" timeout="" voices="" />
		<msg @l="45" char="-1" flags="0" index="10" line="45" name="" text="仅仅这样，血液就一下都集中到两腿之间。" timeout="" voices="" />
		<play_sound @l="47" buf="1" fadeout="1000" type="1" />
		<chgbg @l="49" duration="300" flags="256" flip="-1" flip_flags="0" param="16" screen="1" src="evkan16_01b" type="99" />
		<msg @l="50" char="0" flags="0" index="12" line="50" name="花音" text="「哇，欧○金已经变得这么大了哦，果然是特制温泉蛋猪肉盖饭便当很有效吧」" timeout="" voices="kan11602" />
		<msg @l="52" char="0" flags="0" index="14" line="52" name="花音" text="「变得这么大了的话，就不能回教室了啊……所以交给我吧」" timeout="" voices="kan11603" />
		<msg @l="54" char="0" flags="0" index="16" line="54" name="花音" text="「像这样……誒咻、揉来揉去～的话……哇、变得越来越大了啊」" timeout="" voices="kan11604" />
		<msg @l="56" char="-1" flags="0" index="18" line="56" name="" text="花音把我的阴茎用惯用手抓住，轻捏着刺激着我。" timeout="" voices="" />
		<chgbg @l="58" duration="300" flags="256" flip="-1" flip_flags="0" param="16" screen="1" src="evkan16_01c" type="99" />
		<msg @l="59" char="0" flags="0" index="20" line="59" name="花音" text="「只是用手握着，就变得这么硬了……真是色色的欧○金啊\x;hearts:」" timeout="" voices="kan11605" />
		<msg @l="61" char="11" flags="0" index="22" line="61" name="亮" text="「我认为，花音你不摸的话，它也不会变得这么大啊」" timeout="" voices="" />
		<chgbg @l="63" duration="300" flags="256" flip="-1" flip_flags="0" param="16" screen="1" src="evkan16_01b" type="99" />
		<msg @l="64" char="0" flags="0" index="24" line="64" name="花音" text="「噗～、摆着那么舒服的表情，说这些话，完全没有说服力哦」" timeout="" voices="kan11606" />
		<msg @l="66" char="-1" flags="0" index="26" line="66" name="" text="被戳中要害，无法还嘴。" timeout="" voices="" />
		<msg @l="68" char="0" flags="0" index="28" line="68" name="花音" text="「所以、我来让你变得更加舒服吧？」" timeout="" voices="kan11607" />
		<msg @l="70" char="11" flags="0" index="30" line="70" name="亮" text="「更加……？」" timeout="" voices="" />
		<msg @l="72" char="0" flags="0" index="32" line="72" name="花音" text="「嗯。男人都最喜欢欧派对吧？　所以呢……嘿咻」" timeout="" voices="kan11608" />
		<play_sound @l="74" buf="1" fadeout="0" intro="-1" src="hse101" type="1" volume="1." />
		<chgbg @l="75" duration="300" flags="256" flip="-1" flip_flags="0" param="16" screen="1" src="evkan16_01d" type="99" />
		<msg @l="76" char="-1" flags="0" index="34" line="76" name="" text="花音用自己的胸部，夹住我的阴茎，就这样轻轻地压住。" timeout="" voices="" />
		<msg @l="77" char="-1" flags="0" index="36" line="77" name="" text="淋上汗水的胸部的谷间，就像○穴一样，把阴茎包裹进去。" timeout="" voices="" />
		<play_sound @l="79" buf="1" fadeout="0" type="1" />
		<msg @l="81" char="11" flags="0" index="38" line="81" name="亮" text="「呜呜！？」" timeout="" voices="" />
		<chgbg @l="83" duration="300" flags="256" flip="-1" flip_flags="0" param="16" screen="1" src="evkan16_01c" type="99" />
		<msg @l="84" char="0" flags="0" index="40" line="84" name="花音" text="「啊、跳了一下……也就是说，照这个感觉下去就可以吧？　怎么样？　有舒服起来吗？」" timeout="" voices="kan11609" />
		<msg @l="86" char="11" flags="0" index="42" line="86" name="亮" text="「咕……、啊啊、很舒服哦……就这样继续吧」" timeout="" voices="" />
		<chgbg @l="88" duration="300" flags="256" flip="-1" flip_flags="0" param="16" screen="1" src="evkan16_01a" type="99" />
		<msg @l="89" char="0" flags="0" index="44" line="89" name="花音" text="「誒咻、誒咻！　变舒服吧～，变舒服吧～」" timeout="" voices="kan11610" />
		<play_sound @l="91" buf="1" fadeout="0" intro="-1" src="hse102" type="1" volume="1." />
		<msg @l="93" char="-1" flags="0" index="46" line="93" name="" text="唱着像咒文一样的话语，身体和胸部整体，像画圆一样动起来。" timeout="" voices="" />
		<msg @l="94" char="-1" flags="0" index="48" line="94" name="" text="就像轻轻吸上来一样，花音的肌肤，紧贴着我的肉棒。" timeout="" voices="" />
		<play_sound @l="96" buf="1" fadeout="0" type="1" />
		<chgbg @l="97" duration="300" flags="256" flip="-1" flip_flags="0" param="16" screen="1" src="evkan16_01b" type="99" />
		<msg @l="98" char="0" flags="0" index="50" line="98" name="花音" text="「哇、欧派中间，欧○金在一跳一跳的……哈啊，我能清楚的感觉到哦」" timeout="" voices="kan11611" />
		<chgbg @l="100" duration="300" flags="256" flip="-1" flip_flags="0" param="16" screen="1" src="evkan16_01a" type="99" />
		<msg @l="101" char="0" flags="0" index="52" line="101" name="花音" text="「嗯、哈啊……每次运动欧派，先走汁都会黏糊糊的探出头来，嘿嘿、真可爱\x;heart:」" timeout="" voices="kan11612" />
		<msg @l="103" char="-1" flags="0" index="54" line="103" name="" text="用这种姿势，俯视地看着侍奉中的花音，一种类似征服感的满足感在我内心扩散开。" timeout="" voices="" />
		<msg @l="105" char="11" flags="0" index="56" line="105" name="亮" text="「胸真的好大啊……呜、哦哦，把我的，全都藏到里面了啊」" timeout="" voices="" />
		<chgbg @l="107" duration="300" flags="256" flip="-1" flip_flags="0" param="16" screen="1" src="evkan16_01d" type="99" />
		<msg @l="108" char="0" flags="0" index="58" line="108" name="花音" text="「像这样用欧派全部包起来，一般不会做这种事的吧？」" timeout="" voices="kan11613" />
		<msg @l="110" char="11" flags="0" index="60" line="110" name="亮" text="「并不是哦，又柔软又温暖，这种感觉最棒了！」" timeout="" voices="" />
		<chgbg @l="112" duration="300" flags="256" flip="-1" flip_flags="0" param="16" screen="1" src="evkan16_01a" type="99" />
		<msg @l="113" char="0" flags="0" index="62" line="113" name="花音" text="「好高兴！！　把它养这么大真是太好了！」" timeout="" voices="kan11614" />
		<msg @l="115" char="-1" flags="0" index="64" line="115" name="" text="花音一边用欧派夹着，一边天真烂漫地高兴着。" timeout="" voices="" />
		<chgbg @l="117" duration="300" flags="256" flip="-1" flip_flags="0" param="16" screen="1" src="evkan16_01c" type="99" />
		<msg @l="118" char="0" flags="0" index="66" line="118" name="花音" text="「最近一直在忙着学习，没有好好注意到，对不起啊」" timeout="" voices="kan11615" />
		<msg @l="120" char="11" flags="0" index="68" line="120" name="亮" text="「？？　学习？」" timeout="" voices="" />
		<chgbg @l="122" duration="300" flags="256" flip="-1" flip_flags="0" param="16" screen="1" src="evkan16_01d" type="99" />
		<msg @l="123" char="0" flags="0" index="70" line="123" name="花音" text="「嗯。看了ＪＣ小姐推荐的动画网站，努力学习了哦」" timeout="" voices="kan11616" />
		<chgbg @l="125" duration="300" flags="256" flip="-1" flip_flags="0" param="16" screen="1" src="evkan16_01b" type="99" />
		<msg @l="126" char="0" flags="0" index="72" line="126" name="花音" text="「因为，能让你高兴的话，我想去做任何事情\x;heart:」" timeout="" voices="kan11617" />
		<msg @l="128" char="-1" flags="0" index="74" line="128" name="" text="这些话语让我心中温暖了起来。" timeout="" voices="" />
		<msg @l="129" char="-1" flags="0" index="76" line="129" name="" text="作为学园的女神一样存在的她，能为了我做到这种程度。" timeout="" voices="" />
		<msg @l="131" char="11" flags="0" index="78" line="131" name="亮" text="（……但是这样的话，如果暴露给其他学生了的话不是很不妙吗？　我就算了，花音的形象可是会崩塌的啊）" timeout="" voices="" />
		<effect @l="133" cycle="0" flags="16896" param="0" screen="0" type="9" />
		<fade @l="134" async="-1" degree="150" duration="500" flags="16384" screen="0" wait_flags="0" />
		<msg @l="136" char="-1" flags="0" index="80" line="136" name="" text="虽说是没什么人经过的地方，姑且确认一下位置吧。" timeout="" voices="" />
		<msg @l="139" char="11" flags="0" index="82" line="139" name="亮" text="（而且是在树荫下，应该没问题吧？）" timeout="" voices="" />
		<fade @l="141" async="-1" degree="0" duration="500" flags="16384" screen="0" wait_flags="0" />
		<msg @l="143" char="0" flags="0" index="84" line="143" name="花音" text="「呣、好不容易用欧派夹住了，你一直看别处的话，我会很寂寞的啊」" timeout="" voices="kan11618" />
		<showmsg @l="145" duration="500" show="0" />
		<scroll @l="146" async="-1" center_x="45" center_y="260" duration="1000" flags="0" rev="0." screen="0" transparency="0" v0="0." v1="0." wait_flags="0" x="0" y="0" zoom="1.5" />
		<msg @l="149" char="0" flags="0" index="86" line="149" name="花音" text="「呐、欧○金？　你也想集中精力变得更舒服吧？」" timeout="" voices="kan11619" />
		<play_sound @l="151" buf="1" fadeout="0" intro="-1" src="hse104" type="1" volume="1." />
		<msg @l="153" char="-1" flags="0" index="88" line="153" name="" text="花音一边摇晃胸部刺激着阴茎，一边跟它说话。" timeout="" voices="" />
		<showmsg @l="156" duration="500" show="0" />
		<loadimage @l="157" bg="0" fit="0" screen="1" src="evkan16_01c" />
		<scroll @l="158" async="0" center_x="45" center_y="45" duration="0" flags="0" rev="0." screen="1" transparency="0" v0="0." v1="0." wait_flags="0" x="0" y="0" zoom="1." />
		<chgscrn @l="159" duration="1000" flags="256" flip="-1" flip_flags="0" param="16" screen="1" type="99" />
		<msg @l="163" char="0" flags="0" index="90" line="163" name="花音" text="「来吧来吧，这样的话你会把注意力集中过来吗？　用欧派挤一挤～……上下动一动～」" timeout="" voices="kan11620" />
		<play_sound @l="165" buf="1" fadeout="0" intro="-1" src="hse102" type="1" volume="1." />
		<msg @l="167" char="11" flags="0" index="92" line="167" name="亮" text="「呜哦哦！？」" timeout="" voices="" />
		<chgbg @l="169" duration="300" flags="256" flip="-1" flip_flags="0" param="16" screen="1" src="evkan16_01a" type="99" />
		<msg @l="170" char="0" flags="0" index="94" line="170" name="花音" text="「嘿嘿、好像很舒服啊……嘿咻、嗯、呜啊……、欧○金，一跳一跳的哦」" timeout="" voices="kan11621" />
		<msg @l="172" char="-1" flags="0" index="96" line="172" name="" text="每当巨乳肉垫包裹肉棒，脑海中都像有激烈的光芒闪过。" timeout="" voices="" />
		<msg @l="173" char="-1" flags="0" index="98" line="173" name="" text="虽然很快就会消失，但是现在，想要全身心来体味这种快感，这种心情越来越强烈。" timeout="" voices="" />
		<play_sound @l="176" buf="1" fadeout="0" type="1" />
		<play_sound @l="178" buf="1" fadeout="0" intro="-1" src="hse101" type="1" volume="1." />
		<chgbg @l="179" duration="300" flags="256" flip="-1" flip_flags="0" param="16" screen="1" src="evkan16_01c" type="99" />
		<msg @l="180" char="0" flags="0" index="100" line="180" name="花音" text="「呀、又变得更大了……那我也要再努力一些了……嗯哼、啊哈、啊哈哈！」" timeout="" voices="kan11622" />
		<play_sound @l="181" buf="1" fadeout="0" type="1" />
		<chgbg @l="182" duration="300" flags="256" flip="-1" flip_flags="0" param="16" screen="1" src="evkan16_01d" type="99" />
		<msg @l="183" char="0" flags="0" index="102" line="183" name="花音" text="「嗯啊……嗯嗯、呜嗯嗯……怎么样？　嗯！　嘿！！　啊啊嗯嗯……！」" timeout="" voices="kan11623" />
		<chgbg @l="185" duration="300" flags="256" flip="-1" flip_flags="0" param="16" screen="1" src="evkan16_01b" type="99" />
		<msg @l="186" char="0" flags="0" index="104" line="186" name="花音" text="「誒、哈、怎么样？　摩擦摩擦、摩擦摩擦——，我有在色色的动着吗？」" timeout="" voices="kan11624" />
		<play_sound @l="188" buf="5" fadeout="0" intro="0" src="kan80015" type="6" volume="1." />
		<msg @l="189" char="-1" flags="0" index="106" line="189" name="" text="花音一边仰视着我观察我的表情、一边积极的刺激着阴茎。" timeout="" voices="" />
		<msg @l="190" char="-1" flags="0" index="108" line="190" name="" text="双手揉来揉去，把胸部揉变形。一会儿上下移动，一会儿我紧紧挤压着，用不同的手段刺激着我。" timeout="" voices="" />
		<play_sound @l="191" buf="5" fadeout="0" type="1" />
		<msg @l="193" char="0" flags="0" index="110" line="193" name="花音" text="「动啊动啊，动啊动啊……啊哈、啊哈啊哈……，胸的谷间变成了欧○金的形状了，好下流啊」" timeout="" voices="kan11625" />
		<play_sound @l="195" buf="5" fadeout="0" intro="0" src="kan80017" type="6" volume="1." />
		<msg @l="196" char="11" flags="0" index="112" line="196" name="亮" text="「咕……！！　这个不妙，超级舒服啊」" timeout="" voices="" />
		<play_sound @l="197" buf="5" fadeout="0" type="1" />
		<chgbg @l="199" duration="300" flags="256" flip="-1" flip_flags="0" param="16" screen="1" src="evkan16_01d" type="99" />
		<msg @l="200" char="0" flags="0" index="114" line="200" name="花音" text="「诶嘿嘿，好高兴啊……哈啊、哈啊，好像，我也变得越来越舒服了……嗯、啊啊啊」" timeout="" voices="kan11626" />
		<chgbg @l="202" duration="300" flags="256" flip="-1" flip_flags="0" param="16" screen="1" src="evkan16_01b" type="99" />
		<msg @l="203" char="0" flags="0" index="116" line="203" name="花音" text="「欧派每次运动，欧○金的龟头进进出出……嗯、啊，我变得，越来越色了啊……嗯啊、哈啊！」" timeout="" voices="kan11627" />
		<msg @l="205" char="0" flags="0" index="118" line="205" name="花音" text="「哈啊、哈啊……，龟头，粉嫩嫩的，闪闪的发着光，好像很好吃……哈啊啊、哈啊，呐、我这样直接舔下去也没关系吧？」" timeout="" voices="kan11628" />
		<play_sound @l="207" buf="5" fadeout="0" intro="0" src="kan80016" type="6" volume="1." />
		<msg @l="208" char="-1" flags="0" index="120" line="208" name="" text="不等我的回答，就这样用乳交的姿势，把舌头伸了出来。" timeout="" voices="" />
		<play_sound @l="209" buf="5" fadeout="0" type="1" />
		<chgbg @l="213" duration="300" flags="256" flip="-1" flip_flags="0" param="16" screen="1" src="evkan16_02i" type="99" />
		<msg @l="215" char="0" flags="0" index="122" line="215" name="花音" text="「我开动了　\x;heart:吸溜吸溜……啾、嘶溜\x;heart:\x;hearts:」" timeout="" voices="kan11629" />
		<play_sound @l="217" buf="5" fadeout="0" intro="0" src="kan80000" type="6" volume="1." />
		<msg @l="218" char="-1" flags="0" index="124" line="218" name="" text="花音用小小的舌头，仔细地描绘着龟头的形状。" timeout="" voices="" />
		<msg @l="219" char="-1" flags="0" index="126" line="219" name="" text="每次舌头一点点的缠绕，唾液在舌头前面润湿，反射着下流的光。" timeout="" voices="" />
		<play_sound @l="220" buf="5" fadeout="0" type="1" />
		<chgbg @l="222" duration="300" flags="256" flip="-1" flip_flags="0" param="16" screen="1" src="evkan16_02j" type="99" />
		<msg @l="223" char="0" flags="0" index="128" line="223" name="花音" text="「啾、色色的味道扩散在嘴里，啊哈哈……，让我越来越想舔了，吸溜吸溜吸溜」" timeout="" voices="kan11630" />
		<chgbg @l="225" duration="300" flags="256" flip="-1" flip_flags="0" param="16" screen="1" src="evkan16_02l" type="99" />
		<msg @l="226" char="0" flags="0" index="130" line="226" name="花音" text="「嗯哈啊……啊啊嗯嗯……欧○金……真好吃，吸溜、吸溜，啾……」" timeout="" voices="kan11631" />
		<play_sound @l="228" buf="5" fadeout="0" intro="0" src="kan80001" type="6" volume="1." />
		<msg @l="229" char="11" flags="0" index="132" line="229" name="亮" text="「花音的舌头，太舒服了……呜呜！？」" timeout="" voices="" />
		<msg @l="232" char="-1" flags="0" index="134" line="232" name="" text="花音一边用胸夹着阴茎，一边灵巧的用舌头舔着。" timeout="" voices="" />
		<msg @l="233" char="-1" flags="0" index="136" line="233" name="" text="唾液也流到了肉茎上，黏糊糊的润湿着。" timeout="" voices="" />
