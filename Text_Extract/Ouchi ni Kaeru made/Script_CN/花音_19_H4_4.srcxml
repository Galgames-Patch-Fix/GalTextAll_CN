		<clr_allbs duration="200" screen="0" />
		<waitbs @l="3501" flags="0" screen="0" />
		<msg @l="3503" char="-1" flags="0" index="2051" line="3503" name="" text="虽说是保持着清纯并养育成色色的了……嘛，不管说什么我的女朋友最棒了（恍惚）" timeout="" voices="" />
		<play_sound @l="3504" buf="5" fadeout="0" type="1" />
		<msg @l="3506" char="0" flags="0" index="2053" line="3506" name="花音" text="「舌头伸出来～、啊——、舔、舔舔……干燥凝固的，用唾液浸湿……\n:舔舔、舔」" timeout="" voices="kan18810" />
		<msg @l="3507" char="0" flags="0" index="2055" line="3507" name="花音" text="「蛋蛋的皱纹里也流的有，舔、嗯、舔舔舔、啾啾……咕嘟、咕嘟」" timeout="" voices="kan18811" />
		<play_sound @l="3509" buf="5" fadeout="0" intro="0" src="kan80001" type="6" volume="1." />
		<msg @l="3510" char="-1" flags="0" index="2057" line="3510" name="" text="睾丸的皱纹也一条一条的仔细地用舌尖舔过。" timeout="" voices="" />
		<msg @l="3511" char="-1" flags="0" index="2059" line="3511" name="" text="残渣被黏黏的舌头取下，白色的喉咙发出响声咽下去。" timeout="" voices="" />
		<play_sound @l="3512" buf="5" fadeout="0" type="1" />
		<msg @l="3514" char="0" flags="0" index="2061" line="3514" name="花音" text="「用嘴唇抓住皱纹……哈姆　\x;hearts:认真的舔的话……啾、舔、很快就弄干净了……啾、舔、舔」" timeout="" voices="kan18812" />
		<msg @l="3516" char="0" flags="0" index="2063" line="3516" name="花音" text="「啊哈……蛋蛋变干净了呢。那么最后……啊——呣\x;heart:」" timeout="" voices="kan18813" />
		<play_sound @l="3518" buf="5" fadeout="0" intro="0" src="kan80002" type="6" volume="1." />
		<msg @l="3519" char="11" flags="0" index="2065" line="3519" name="亮" text="「哦哦！」" timeout="" voices="" />
		<msg @l="3521" char="-1" flags="0" index="2067" line="3521" name="" text="把睾丸含在嘴里，在嘴里爱抚着。" timeout="" voices="" />
		<msg @l="3522" char="-1" flags="0" index="2069" line="3522" name="" text="一边轻轻吸着，用舌尖转着阴囊。" timeout="" voices="" />
		<play_sound @l="3523" buf="5" fadeout="0" type="1" />
		<msg @l="3525" char="0" flags="0" index="2071" line="3525" name="花音" text="「哈姆哈姆……舔、舔舔……蛋蛋里面硬硬的东西，也用舌头轻轻地、舔舔、嗯——嘛、唔嘛唔嘛」" timeout="" voices="kan18814" />
		<msg @l="3527" char="0" flags="0" index="2073" line="3527" name="花音" text="「啾、嗯嘛\x;hearts:　哈……怎么样？　蛋蛋变干净了吗？」" timeout="" voices="kan18815" />
		<msg @l="3529" char="11" flags="0" index="2075" line="3529" name="亮" text="「啊啊，变得很干净都闪闪发光了」" timeout="" voices="" />
		<msg @l="3531" char="-1" flags="0" index="2077" line="3531" name="" text="沉重的快感在睾丸全体扩散开来。" timeout="" voices="" />
		<msg @l="3532" char="-1" flags="0" index="2079" line="3532" name="" text="从天窗照进的朝阳照到了大概股间的位置，花音的口水使阴囊闪闪发光。" timeout="" voices="" />
		<msg @l="3534" char="0" flags="0" index="2081" line="3534" name="花音" text="「现在舔龟头还太早吗……那，先从下面开始吧」" timeout="" voices="kan18816" />
		<msg @l="3536" char="0" flags="0" index="2083" line="3536" name="花音" text="「舔———\x;heart:　舔、舔舔舔舔————、舔、舔」" timeout="" voices="kan18817" />
		<play_sound @l="3538" buf="5" fadeout="0" intro="0" src="kan80000" type="6" volume="1." />
		<msg @l="3539" char="-1" flags="0" index="2085" line="3539" name="" text="从阴茎小带下面开始慢慢的往上舔。" timeout="" voices="" />
		<msg @l="3541" char="11" flags="0" index="2087" line="3541" name="亮" text="「啊呼，一震一震的脚自己就绷起来了」" timeout="" voices="" />
		<play_sound @l="3542" buf="5" fadeout="0" type="1" />
		<msg @l="3544" char="0" flags="0" index="2089" line="3544" name="花音" text="「诶嘿嘿，好像很喜欢呢……啾、啾、吸溜吸溜」" timeout="" voices="kan18818" />
		<play_sound @l="3546" buf="5" fadeout="0" intro="0" src="kan80001" type="6" volume="1." />
		<msg @l="3547" char="-1" flags="0" index="2091" line="3547" name="" text="一边确认这我的反应，一边用舌尖在阴茎小带上转圈。" timeout="" voices="" />
		<play_sound @l="3548" buf="5" fadeout="0" type="1" />
		<msg @l="3550" char="0" flags="0" index="2093" line="3550" name="花音" text="「舔、舔……血管浮出的下面也流的有……舔、吸溜溜」" timeout="" voices="kan18819" />
		<msg @l="3552" char="0" flags="0" index="2095" line="3552" name="花音" text="「这个血管喜欢哈姆哈姆的呢？　哈姆、唔嘛唔嘛、舔、舔舔」" timeout="" voices="kan18820" />
		<play_sound @l="3554" buf="5" fadeout="0" intro="0" src="kan80000" type="6" volume="1." />
		<msg @l="3555" char="11" flags="0" index="2097" line="3555" name="亮" text="「哈啊哈啊……太……、舒服了、咕、唔唔！？」" timeout="" voices="" />
		<msg @l="3557" char="-1" flags="0" index="2099" line="3557" name="" text="血液大量集中到阴茎，让神经变得更敏感。" timeout="" voices="" />
		<msg @l="3558" char="-1" flags="0" index="2101" line="3558" name="" text="甜美的快感，让我的后背一阵阵的颤抖。" timeout="" voices="" />
		<play_sound @l="3559" buf="5" fadeout="0" type="1" />
		<msg @l="3561" char="0" flags="0" index="2103" line="3561" name="花音" text="「这个向外凸起的部分，也积攒了很多呢……总是把我的阴道挠动着舒服起来的突起，你辛苦了，啾、啾啾……舔舔」" timeout="" voices="kan18821" />
		<msg @l="3563" char="0" flags="0" index="2105" line="3563" name="花音" text="「啊哈、刚才，它好像很高兴似的动了一下哦……诶嘿嘿，扫除值得了呢、舔、舔舔\x;hearts:」" timeout="" voices="kan18822" />
		<play_sound @l="3565" buf="5" fadeout="0" intro="0" src="kan80001" type="6" volume="1." />
		<msg @l="3566" char="11" flags="0" index="2107" line="3566" name="亮" text="「冠状沟被舔着，真的超级舒服……」" timeout="" voices="" />
		<msg @l="3568" char="-1" flags="0" index="2109" line="3568" name="" text="真的是小心的、仔细地舔着，这样一来能够感受到由于敏感，快感比平时上升的更多。" timeout="" voices="" />
		<play_sound @l="3569" buf="5" fadeout="0" type="1" />
		<msg @l="3571" char="0" flags="0" index="2111" line="3571" name="花音" text="「啊啊……突出的部分和外面的皮之间白色的残渣残留了好多，哈、哈……舔掉也没问题吧？」" timeout="" voices="kan18823" />
		<msg @l="3573" char="-1" flags="0" index="2113" line="3573" name="" text="大概也有一整晚做爱流了汗的原因在内，残留下来了大量的污垢。" timeout="" voices="" />
		<msg @l="3574" char="-1" flags="0" index="2115" line="3574" name="" text="羞耻感也促使着快感上升，已经不剩下一点抵抗了。" timeout="" voices="" />
		<msg @l="3576" char="11" flags="0" index="2117" line="3576" name="亮" text="「你很喜欢舔这个啊……哈啊哈啊、可以哦，就这样舔走吧」" timeout="" voices="" />
		<msg @l="3578" char="0" flags="0" index="2119" line="3578" name="花音" text="「谢谢——！　这个喜欢、啾、舔……你的味道很浓厚，脑袋里面晕乎乎的……哈、啊」" timeout="" voices="kan18824" />
		<msg @l="3580" char="0" flags="0" index="2121" line="3580" name="花音" text="「舔舔、呼啊……比以前的还要浓厚……舔、吸溜溜、好吃，在嘴里味道好浓郁\x;heart:」" timeout="" voices="kan18825" />
		<play_sound @l="3582" buf="5" fadeout="0" intro="0" src="kan80000" type="6" volume="1." />
		<msg @l="3583" char="-1" flags="0" index="2123" line="3583" name="" text="一边用舌头把污垢剥落，一边舔着。" timeout="" voices="" />
		<msg @l="3584" char="-1" flags="0" index="2125" line="3584" name="" text="持续用手揉着已经变干净了的睾丸，缓缓地把快感送进肉棒。" timeout="" voices="" />
		<play_sound @l="3585" buf="5" fadeout="0" type="1" />
		<msg @l="3587" char="0" flags="0" index="2127" line="3587" name="花音" text="「和凝固了的精液一起送进嘴里，呼啊——！！　非常好吃啊，香气充满鼻腔，这是新发现啊！！」" timeout="" voices="kan18826" />
		<msg @l="3589" char="0" flags="0" index="2129" line="3589" name="花音" text="「哈啊、哈啊……你的味道在嘴里有好多，最棒了啊，这个！」" timeout="" voices="kan18827" />
		<play_sound @l="3591" buf="5" fadeout="0" intro="0" src="kan80001" type="6" volume="1." />
		<msg @l="3592" char="-1" flags="0" index="2131" line="3592" name="" text="花音露出沉醉的表情，把嘴里舔掉的残渣，和凝固起来干巴巴的精液吞下。" timeout="" voices="" />
		<msg @l="3593" char="-1" flags="0" index="2133" line="3593" name="" text="这之后也，用舌尖穿进冠状沟和皮相连的部分。" timeout="" voices="" />
		<play_sound @l="3594" buf="5" fadeout="0" type="1" />
		<msg @l="3596" char="0" flags="0" index="2135" line="3596" name="花音" text="「舔、舔……吸溜吸溜溜。突起，好可爱啊……滑滑的水润润的，很有光泽，好色啊」" timeout="" voices="kan18828" />
		<msg @l="3598" char="0" flags="0" index="2137" line="3598" name="花音" text="「我会认真的帮你舔哦……舔、舔、啾、所以以后再让我舒服起来吧，啾\x;heart:」" timeout="" voices="kan18829" />
		<play_sound @l="3600" buf="5" fadeout="0" intro="0" src="kan80000" type="6" volume="1." />
		<msg @l="3601" char="-1" flags="0" index="2139" line="3601" name="" text="摩擦着冠状沟头，舌头表面些许的丝滑感让我无法忍耐。" timeout="" voices="" />
		<msg @l="3602" char="-1" flags="0" index="2141" line="3602" name="" text="噗噜噗噜地震动着肥硕的安产型的臀部，一刻不停的持续侍奉着。" timeout="" voices="" />
