		<movebs duration="0" flags="0" id="hevkan11_cut02b" rev="0." screen="0" transparency="256" v0="0." v1="0." x="2147483648" y="-100" />
		<waitbs @l="3642" flags="0" screen="0" />
		<movebs duration="800" flags="0" id="hevkan11_cut02b" rev="0." screen="0" transparency="0" v0="0." v1="0." x="2147483648" y="0" />
		<waitbs @l="3644" flags="0" screen="0" />
		<set_cg_flag @l="3646" cg="hevkan11_cut02b.eri" />
		<msg @l="3648" char="0" flags="0" index="2169" line="3648" name="花音" text="「嗯呣嗯呣\x;hearts:……啾……舔、啊哈！　从龟头里，透明的汁水出来的越来越多了哦」" timeout="" voices="kan18835" />
		<cutbs async="0" duration="200" priority="0" screen="0" src="hevkan11_cut02a" transparency="0" x="2147483648" />
		<waitbs @l="3650" flags="0" screen="0" />
		<set_cg_flag @l="3651" cg="hevkan11_cut02a.eri" />
		<msg @l="3652" char="0" flags="0" index="2171" line="3652" name="花音" text="「继续舔了哦？　啾……吸溜……嗯呣嗯呣、啾、好吃……哈姆哈姆」" timeout="" voices="kan18836" />
		<play_sound @l="3654" buf="5" fadeout="0" intro="0" src="kan80001" type="6" volume="1." />
		<msg @l="3655" char="-1" flags="0" index="2173" line="3655" name="" text="花音用像是舔着奶嘴的婴儿一样的嘴巴，舔着肉○。" timeout="" voices="" />
		<msg @l="3656" char="-1" flags="0" index="2175" line="3656" name="" text="在把龟头完全含进去的嘴里，就像要穿过尿道口一样，舌尖细细地动着。" timeout="" voices="" />
		<msg @l="3658" char="11" flags="0" index="2177" line="3658" name="亮" text="「很舒服哦，花音……就照这样继续舔吧」" timeout="" voices="" />
		<play_sound @l="3659" buf="5" fadeout="0" type="1" />
		<cutbs async="0" duration="200" priority="0" screen="0" src="hevkan11_cut02b" transparency="0" x="2147483648" />
		<waitbs @l="3661" flags="0" screen="0" />
		<msg @l="3663" char="0" flags="0" index="2179" line="3663" name="花音" text="「嗯、啾……吸溜……尿道球腺液真好喝，再让我多喝点……哈姆、吸溜、啾、啾、吸溜溜」" timeout="" voices="kan18837" />
		<play_sound @l="3665" buf="5" fadeout="0" intro="0" src="kan80002" type="6" volume="1." />
		<msg @l="3666" char="11" flags="0" index="2181" line="3666" name="亮" text="「咕啊啊！？　这样一口气吸的话，腰就要飘起来了啊，唔咕唔唔！？」" timeout="" voices="" />
		<play_sound @l="3667" buf="5" fadeout="0" type="1" />
		<cutbs async="0" duration="200" priority="0" screen="0" src="hevkan11_cut02a" transparency="0" x="2147483648" />
		<waitbs @l="3669" flags="0" screen="0" />
		<msg @l="3670" char="0" flags="0" index="2183" line="3670" name="花音" text="「这么，啾、吸溜……舒服吗？」" timeout="" voices="kan18838" />
		<play_sound @l="3672" buf="5" fadeout="0" intro="0" src="kan80003" type="6" volume="1." />
		<msg @l="3673" char="-1" flags="0" index="2185" line="3673" name="" text="只是被轻轻地吸着龟头，像是要把腰挺起来的快感奔上头顶。" timeout="" voices="" />
		<msg @l="3674" char="-1" flags="0" index="2187" line="3674" name="" text="眼前一闪一闪的，反射性地挺起了腰。" timeout="" voices="" />
		<play_sound @l="3675" buf="5" fadeout="0" type="1" />
		<cutbs async="0" duration="200" priority="0" screen="0" src="hevkan11_cut02c" transparency="0" x="2147483648" />
		<waitbs @l="3677" flags="0" screen="0" />
		<set_cg_flag @l="3678" cg="hevkan11_cut02c.eri" />
		<msg @l="3679" char="0" flags="0" index="2189" line="3679" name="花音" text="「啊！？　挺起来了。刺激太强了吗？　再慢点会更好呢」" timeout="" voices="kan18839" />
		<msg @l="3681" char="11" flags="0" index="2191" line="3681" name="亮" text="「不、抱歉。实在太舒服了，不小心动了一下」" timeout="" voices="" />
		<cutbs async="0" duration="200" priority="0" screen="0" src="hevkan11_cut02a" transparency="0" x="2147483648" />
		<waitbs @l="3683" flags="0" screen="0" />
		<msg @l="3684" char="0" flags="0" index="2193" line="3684" name="花音" text="「欸嘿。那再一次，让你舒服起来哦……舔\x;heart:」" timeout="" voices="kan18840" />
		<play_sound @l="3686" buf="5" fadeout="0" intro="0" src="kan80000" type="6" volume="1." />
		<msg @l="3687" char="-1" flags="0" index="2195" line="3687" name="" text="所以说这已经不叫扫除了吧……？　虽然这么想，但我要保持沉默。" timeout="" voices="" />
		<msg @l="3688" char="-1" flags="0" index="2197" line="3688" name="" text="把全身全心，全部交给花音献身的侍奉中。" timeout="" voices="" />
		<play_sound @l="3689" buf="5" fadeout="0" type="1" />
		<cutbs async="0" duration="200" priority="0" screen="0" src="hevkan11_cut02b" transparency="0" x="2147483648" />
		<waitbs @l="3691" flags="0" screen="0" />
		<msg @l="3692" char="0" flags="0" index="2199" line="3692" name="花音" text="「嗅嗅……欧○金的深处，好像残留的有精液？」" timeout="" voices="kan18841" />
		<msg @l="3694" char="11" flags="0" index="2201" line="3694" name="亮" text="「感觉是全都射出来了，可能是尿道里稍微残留了一点」" timeout="" voices="" />
		<cutbs async="0" duration="200" priority="0" screen="0" src="hevkan11_cut02a" transparency="0" x="2147483648" />
		<waitbs @l="3696" flags="0" screen="0" />
		<msg @l="3697" char="0" flags="0" index="2203" line="3697" name="花音" text="「那么，我把那些吸出来……啾、啾啾！　吸溜！」" timeout="" voices="kan18842" />
		<play_sound @l="3699" buf="5" fadeout="0" intro="0" src="kan80004" type="6" volume="1." />
		<msg @l="3700" char="11" flags="0" index="2205" line="3700" name="亮" text="「啊、咕唔唔……这、这么突然！　唔啊！」" timeout="" voices="" />
		<play_sound @l="3701" buf="5" fadeout="0" type="1" />
		<cutbs async="0" duration="200" priority="0" screen="0" src="hevkan11_cut02d" transparency="0" x="2147483648" />
		<waitbs @l="3703" flags="0" screen="0" />
		<set_cg_flag @l="3704" cg="hevkan11_cut02d.eri" />
		<msg @l="3705" char="0" flags="0" index="2207" line="3705" name="花音" text="「嘶溜溜溜溜！！　吸溜……啾呼、啾……嘶溜、啾……」" timeout="" voices="kan18843" />
		<play_sound @l="3707" buf="5" fadeout="0" intro="0" src="kan80003" type="6" volume="1." />
		<msg @l="3708" char="-1" flags="0" index="2209" line="3708" name="" text="花音一边把脸颊缩小发出声音一边吸着阴茎。" timeout="" voices="" />
		<msg @l="3709" char="-1" flags="0" index="2211" line="3709" name="" text="龟头被尽全力压迫着，一颤一颤的快感经过背肌。" timeout="" voices="" />
		<play_sound @l="3710" buf="5" fadeout="0" type="1" />
		<cutbs async="0" duration="200" priority="0" screen="0" src="hevkan11_cut02a" transparency="0" x="2147483648" />
		<waitbs @l="3712" flags="0" screen="0" />
		<msg @l="3713" char="0" flags="0" index="2213" line="3713" name="花音" text="「诶嘿嘿、果然还残留着呢……嗯、舔。稍微有一点精液的味道哦？　啾、啾！」" timeout="" voices="kan18844" />
		<play_sound @l="3715" buf="5" fadeout="0" intro="0" src="kan80006" type="6" volume="1." />
		<msg @l="3716" char="-1" flags="0" index="2215" line="3716" name="" text="用像猎人一样的嗅觉嗅出残存的精液，像强力的真空吸引器一样吸溜吸溜的吸上来。" timeout="" voices="" />
		<msg @l="3717" char="-1" flags="0" index="2217" line="3717" name="" text="双手抓住我的腰，头纵向的震动着，开始用弹弹的柔软嘴唇往上拔。" timeout="" voices="" />
		<play_sound @l="3718" buf="5" fadeout="0" type="1" />
		<msg @l="3720" char="0" flags="0" index="2219" line="3720" name="花音" text="「接着，来舔舔欧○金的龟头……舔、\n:啾、吸溜！　舔、舔」" timeout="" voices="kan18845" />
		<cutbs async="0" duration="200" priority="0" screen="0" src="hevkan11_cut02c" transparency="0" x="2147483648" />
		<waitbs @l="3722" flags="0" screen="0" />
		<msg @l="3723" char="0" flags="0" index="2221" line="3723" name="花音" text="「嗯！　嗯嗯！！　啾……啾、吸溜……吸溜溜溜」" timeout="" voices="kan18846" />
		<play_sound @l="3725" buf="5" fadeout="0" intro="0" src="kan80005" type="6" volume="1." />
		<msg @l="3726" char="-1" flags="0" index="2223" line="3726" name="" text="嘴里的温暖在龟头全体缓缓地扩散开，舒适感把腰都被麻痹了。" timeout="" voices="" />
		<msg @l="3727" char="-1" flags="0" index="2225" line="3727" name="" text="淫荡的水声不停的从嘴里漏出来，在屋里回响。" timeout="" voices="" />
		<msg @l="3729" char="11" flags="0" index="2227" line="3729" name="亮" text="「花音的舌头，虽然口水黏黏的，但也能感觉到滑滑的……咕、嗯嗯！？」" timeout="" voices="" />
		<play_sound @l="3730" buf="5" fadeout="0" type="1" />
		<cutbs async="0" duration="200" priority="0" screen="0" src="hevkan11_cut02a" transparency="0" x="2147483648" />
		<waitbs @l="3732" flags="0" screen="0" />
		<msg @l="3733" char="0" flags="0" index="2229" line="3733" name="花音" text="「很舒服啊？　那么凹下去的部分也细心的……\n:舔、啾！　啾、啾、吸溜吸溜！」" timeout="" voices="kan18847" />
		<play_sound @l="3735" buf="5" fadeout="0" intro="0" src="kan80006" type="6" volume="1." />
		<msg @l="3736" char="-1" flags="0" index="2231" line="3736" name="" text="舌头够到冠状沟的沟里，在凹陷处舔取着。" timeout="" voices="" />
		<play_sound @l="3737" buf="5" fadeout="0" type="1" />
		<cutbs async="0" duration="200" priority="0" screen="0" src="hevkan11_cut02b" transparency="0" x="2147483648" />
		<waitbs @l="3739" flags="0" screen="0" />
		<msg @l="3740" char="0" flags="0" index="2233" line="3740" name="花音" text="「这里，果然有欧○金浓郁的味道\x;hearts:……啾、啾啾！」" timeout="" voices="kan18848" />
		<play_sound @l="3742" buf="5" fadeout="0" intro="0" src="kan80005" type="6" volume="1." />
		<msg @l="3743" char="-1" flags="0" index="2235" line="3743" name="" text="别说露出厌恶感了，反而很高兴的舔取着污垢。" timeout="" voices="" />
		<msg @l="3744" char="-1" flags="0" index="2237" line="3744" name="" text="实感着我被花音爱着，胸口炙热了起来。" timeout="" voices="" />
		<play_sound @l="3745" buf="5" fadeout="0" type="1" />
		<cutbs async="0" duration="200" priority="0" screen="0" src="hevkan11_cut02a" transparency="0" x="2147483648" />
		<waitbs @l="3747" flags="0" screen="0" />
		<msg @l="3748" char="0" flags="0" index="2239" line="3748" name="花音" text="「嗯啾、吸溜……、诶嘿嘿，口水都留下来了一副很色情的表情哦，舔舔」" timeout="" voices="kan18849" />
		<play_sound @l="3750" buf="5" fadeout="0" intro="0" src="kan80006" type="6" volume="1." />
		<msg @l="3751" char="11" flags="0" index="2241" line="3751" name="亮" text="「那当然，因为花音的舌头很舒服啊……唔哦哦」" timeout="" voices="" />
		<play_sound @l="3752" buf="5" fadeout="0" type="1" />
		<msg @l="3754" char="0" flags="0" index="2243" line="3754" name="花音" text="「啊呣、吸溜吸溜……、这么舒服？　我做的还好吧？」" timeout="" voices="kan18850" />
		<play_sound @l="3756" buf="5" fadeout="0" intro="0" src="kan80004" type="6" volume="1." />
		<msg @l="3757" char="-1" flags="0" index="2245" line="3757" name="" text="这么说的时候也在注意着舌头的动作和速度，关注着我的样子。" timeout="" voices="" />
		<msg @l="3758" char="-1" flags="0" index="2247" line="3758" name="" text="明明已经舔过好多回了，还保持着这样的探索精神，为她的学习热情深感佩服。" timeout="" voices="" />
		<msg @l="3760" char="11" flags="0" index="2249" line="3760" name="亮" text="「啊啊、非常舒服……好像再激烈一点也没问题」" timeout="" voices="" />
		<play_sound @l="3761" buf="5" fadeout="0" type="1" />
		<msg @l="3763" char="0" flags="0" index="2251" line="3763" name="花音" text="「啊哈，我知道了！　那么，我再努把力喽，啾噜噜噜\x;heart:」" timeout="" voices="kan18851" />
		<play_sound @l="3765" buf="5" fadeout="0" intro="0" src="kan80006" type="6" volume="1." />
		<msg @l="3766" char="-1" flags="0" index="2253" line="3766" name="" text="一副早就等着这句话的态度，花音再次把阴茎含进嘴里。" timeout="" voices="" />
		<msg @l="3767" char="-1" flags="0" index="2255" line="3767" name="" text="吸溜吸溜的用舌头缠绕着阴茎，脸上下移动，嘴唇抓着阴茎。" timeout="" voices="" />
		<play_sound @l="3768" buf="5" fadeout="0" type="1" />
		<msg @l="3770" char="0" flags="0" index="2257" line="3770" name="花音" text="「诶嘿嘿，欧○金在嘴里又变大了哦，这样的话怎么样呢……啾、啾噗！　啾噜、啾、吸溜溜！」" timeout="" voices="kan18852" />
		<cutbs async="0" duration="200" priority="0" screen="0" src="hevkan11_cut02d" transparency="0" x="2147483648" />
		<waitbs @l="3772" flags="0" screen="0" />
		<play_sound @l="3773" buf="5" fadeout="0" intro="0" src="kan80007" type="6" volume="1." />
		<msg @l="3774" char="11" flags="0" index="2259" line="3774" name="亮" text="「唔哦哦！？　嘴里面好激烈，太激烈了……、库唔唔！　啊啊！」" timeout="" voices="" />
		<play_sound @l="3775" buf="5" fadeout="0" type="1" />
		<msg @l="3777" char="0" flags="0" index="2261" line="3777" name="花音" text="「呜哼哼、冒出声音了呢……嗯哼、那再来一次，啾、啾噗、啾噜噜、吸溜吸溜、溜溜溜！！」" timeout="" voices="kan18853" />
		<cutbs async="0" duration="200" priority="0" screen="0" src="hevkan11_cut02a" transparency="0" x="2147483648" />
		<waitbs @l="3779" flags="0" screen="0" />
		<msg @l="3780" char="0" flags="0" index="2263" line="3780" name="花音" text="「舔舔舔、你有感觉时的声音，又可爱又色色的……我最喜欢了，舔舔、舔吸溜吸溜\x;heart:\x;heart:\x;hearts:」" timeout="" voices="kan18854" />
		<play_sound @l="3782" buf="5" fadeout="0" intro="0" src="kan80006" type="6" volume="1." />
		<msg @l="3783" char="-1" flags="0" index="2265" line="3783" name="" text="花音一边笑眯眯的笑着，一边高兴地玩弄着肉棒。" timeout="" voices="" />
		<msg @l="3784" char="-1" flags="0" index="2267" line="3784" name="" text="对我的敏感点探索出来舔着、探索出来舔着这样重复，就像寻宝的小孩子一样让人会心一笑。" timeout="" voices="" />
		<play_sound @l="3785" buf="5" fadeout="0" type="1" />
