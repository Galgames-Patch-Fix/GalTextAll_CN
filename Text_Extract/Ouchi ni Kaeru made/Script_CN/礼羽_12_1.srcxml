		<msg @l="1196" char="11" flags="0" index="559" line="1196" name="亮" text="（呼——！　好软啊——！！）" timeout="" voices="" />
		<msg @l="1198" char="-1" flags="0" index="561" line="1198" name="" text="与胸部不同，有着更好的缓冲性和体温。" timeout="" voices="" />
		<msg @l="1199" char="-1" flags="0" index="563" line="1199" name="" text="不仅既纤细又漂亮，还具备着女孩子柔软的肉感。" timeout="" voices="" />
		<msg @l="1201" char="11" flags="0" index="565" line="1201" name="亮" text="\h;70:\s;70:（大腿最———棒了！！）\s:\h:" timeout="" voices="" />
		<chgbg @l="1203" duration="300" flags="256" flip="-1" flip_flags="0" param="16" screen="1" src="evrai21_01c" type="99" />
		<msg @l="1205" char="3" flags="0" index="567" line="1205" name="礼羽" text="「唔\x;note:　不要把脸贴上来蹭啦，这样会很痒的哦？」" timeout="" voices="rai15702" />
		<msg @l="1207" char="11" flags="0" index="569" line="1207" name="亮" text="「因为大腿滑溜溜的，实在是太舒服了啊」" timeout="" voices="" />
		<chgbg @l="1209" duration="300" flags="256" flip="-1" flip_flags="0" param="16" screen="1" src="evrai21_01a" type="99" />
		<msg @l="1210" char="-1" flags="0" index="571" line="1210" name="" text="我将脸埋进了连汗毛都没有的光滑柔软的肌肤中。" timeout="" voices="" />
		<msg @l="1211" char="-1" flags="0" index="573" line="1211" name="" text="是该说有安全感呢还是说有种放心的感觉呢……总之感觉被治愈了。" timeout="" voices="" />
		<msg @l="1212" char="-1" flags="0" index="575" line="1212" name="" text="而且不知是不是因为仰视的原因，两只硕大的胸部迫力十足地不断跳动着。" timeout="" voices="" />
		<msg @l="1214" char="11" flags="0" index="577" line="1214" name="亮" text="（伸出手的话，立刻就能摸到——）" timeout="" voices="" />
		<chgbg @l="1216" duration="300" flags="256" flip="-1" flip_flags="0" param="16" screen="1" src="evrai21_01c" type="99" />
		<msg @l="1217" char="3" flags="0" index="579" line="1217" name="礼羽" text="「哎呀！　不可以调皮的哦\x;note:」" timeout="" voices="rai15703" />
		<msg @l="1219" char="-1" flags="0" index="581" line="1219" name="" text="我的手刚伸出去就被温柔地挡了下来。" timeout="" voices="" />
		<msg @l="1221" char="3" flags="0" index="583" line="1221" name="礼羽" text="「真是的，这么快就想摸我的胸部了。你是有多喜欢胸部啊？」" timeout="" voices="rai15704" />
		<msg @l="1223" char="11" flags="0" index="585" line="1223" name="亮" text="「不、不是啦。这么大的就在眼前却不摸一下才更加失礼吧?」" timeout="" voices="" />
		<msg @l="1225" char="3" flags="0" index="587" line="1225" name="礼羽" text="「哧哧。你在说什么呀」" timeout="" voices="rai15705" />
		<msg @l="1227" char="11" flags="0" index="589" line="1227" name="亮" text="「况且这还是礼羽的胸部呢」" timeout="" voices="" />
		<chgbg @l="1231" duration="300" flags="256" flip="-1" flip_flags="0" param="16" screen="1" src="evrai21_02a" type="99" />
		<play_sound @l="1232" buf="1" fadeout="0" intro="-1" src="se511" type="1" volume="0.7" />
		<msg @l="1234" char="3" flags="0" index="591" line="1234" name="礼羽" text="「因为是我的胸部才会想摸的吗。虽然这让我很开心，但是现在得老实待着哦」" timeout="" voices="rai15706" />
		<msg @l="1236" char="-1" flags="0" index="593" line="1236" name="" text="礼羽这么说着温柔地抚摸着我的头。" timeout="" voices="" />
		<msg @l="1237" char="-1" flags="0" index="595" line="1237" name="" text="被这么做的话我也就只能老老实实地待着了。" timeout="" voices="" />
		<play_sound @l="1239" buf="1" fadeout="0" type="1" />
		<msg @l="1241" char="3" flags="0" index="597" line="1241" name="礼羽" text="「好孩子好孩子。就这样好好待着哦，现在开始我要给你清理耳朵」" timeout="" voices="rai15707" />
		<msg @l="1243" char="11" flags="0" index="599" line="1243" name="亮" text="「喔喔，真是无微不至呢」" timeout="" voices="" />
		<msg @l="1245" char="3" flags="0" index="601" line="1245" name="礼羽" text="「哼哼。这是专门为你准备的，要好好享受哦」" timeout="" voices="rai15708" />
		<msg @l="1247" char="-1" flags="0" index="603" line="1247" name="" text="膝枕的同时给我清理耳朵。活着真棒啊～！" timeout="" voices="" />
		<msg @l="1248" char="-1" flags="0" index="605" line="1248" name="" text="卿卿我我的感觉也出来了！" timeout="" voices="" />
		<chgbg @l="1250" duration="300" flags="256" flip="-1" flip_flags="0" param="16" screen="1" src="evrai21_01e" type="99" />
		<msg @l="1252" char="3" flags="0" index="607" line="1252" name="礼羽" text="「在海外，很难能买到挖耳勺呢」" timeout="" voices="rai15709" />
		<msg @l="1254" char="11" flags="0" index="609" line="1254" name="亮" text="「诶？　那要怎么清理呢？」" timeout="" voices="" />
		<msg @l="1257" char="3" flags="0" index="611" line="1257" name="礼羽" text="「在芬兰基本都是用棉签的哦。没地方卖日本这样品质很好又有特色的挖耳勺的」" timeout="" voices="rai15710" />
		<chgbg @l="1259" duration="300" flags="256" flip="-1" flip_flags="0" param="16" screen="1" src="evrai21_01b" type="99" />
		<msg @l="1261" char="3" flags="0" index="613" line="1261" name="礼羽" text="「但是就想试试呢。因为清洁耳朵的时候如果不放松下来把自己交给我的话会很危险呢」" timeout="" voices="rai15711" />
		<msg @l="1263" char="11" flags="17" index="615" line="1263" name="亮" text="「你这小恶魔般的笑容算什么啊！？」" timeout="" voices="" />
		<shakescrn @l="1264" amp="20" cycle="20" duration="500" flags="2" screen="0" />
		<pause @l="1265" flags="0" index="617" timeout="" />
		<msg @l="1267" char="-1" flags="0" index="618" line="1267" name="" text="我一边说着一边伸出耳朵。" timeout="" voices="" />
		<chgbg @l="1269" duration="300" flags="256" flip="-1" flip_flags="0" param="16" screen="1" src="evrai21_01a" type="99" />
		<msg @l="1271" char="3" flags="0" index="620" line="1271" name="礼羽" text="「嘻嘻。真的全部都交给我吗，真开心啊——」" timeout="" voices="rai15712" />
		<msg @l="1273" char="11" flags="0" index="622" line="1273" name="亮" text="「我、我是第一次所以请温柔一些哦」" timeout="" voices="" />
		<msg @l="1275" char="3" flags="0" index="624" line="1275" name="礼羽" text="「你是少女吗！？  会让人心动的啦，真是的——」" timeout="" voices="rai15713" />
		<msg @l="1277" char="-1" flags="0" index="626" line="1277" name="" text="我的脸被噗噗地戳着。" timeout="" voices="" />
		<chgbg @l="1279" duration="300" flags="256" flip="-1" flip_flags="0" param="16" screen="1" src="evrai21_01e" type="99" />
		<msg @l="1281" char="3" flags="0" index="628" line="1281" name="礼羽" text="「我也是第一次所以会温柔地来的。而且，我是有在网络上调查过的，放心吧」" timeout="" voices="rai15714" />
		<msg @l="1283" char="11" flags="0" index="630" line="1283" name="亮" text="「为了我，连调查都做了吗」" timeout="" voices="" />
		<chgbg @l="1285" duration="300" flags="256" flip="-1" flip_flags="0" param="16" screen="1" src="evrai21_01c" type="99" />
		<msg @l="1287" char="3" flags="0" index="632" line="1287" name="礼羽" text="「调查之后的结果是，鲸之类哺乳类的耳垢即使放着不管也不会自己掉出来」" timeout="" voices="rai15715" />
		<msg @l="1289" char="3" flags="0" index="634" line="1289" name="礼羽" text="「所以耳垢就会像年轮一样，从这上面还能推算出鲸鱼的年龄哦」" timeout="" voices="rai15716" />
		<msg @l="1291" char="11" flags="0" index="636" line="1291" name="亮" text="「这种知识，现在和我有关系吗！？　这可不行啊。这不就是在调查的时候又对其他的知识感兴趣，然后就这样不停地查看其他网站的情况吗……」" timeout="" voices="" />
		<chgbg @l="1293" duration="300" flags="256" flip="-1" flip_flags="0" param="16" screen="1" src="evrai21_01a" type="99" />
		<msg @l="1295" char="3" flags="0" index="638" line="1295" name="礼羽" text="「哎呀没关系的。嘿嘿、那我来咯」" timeout="" voices="rai15717" />
		<chgbg @l="1299" duration="300" flags="256" flip="-1" flip_flags="0" param="16" screen="1" src="evrai21_03a" type="99" />
		<msg @l="1301" char="-1" flags="0" index="640" line="1301" name="" text="礼羽缓慢又小心地将挖耳勺的前端部分伸入我的耳朵。" timeout="" voices="" />
		<msg @l="1302" char="-1" flags="0" index="642" line="1302" name="" text="虽然刚开始感觉很可怕，但这深度和力度却都恰到好处。" timeout="" voices="" />
		<msg @l="1304" char="3" flags="0" index="644" line="1304" name="礼羽" text="「怎么样？　痛的话要说出来哦？」" timeout="" voices="rai15718" />
		<msg @l="1306" char="11" flags="0" index="646" line="1306" name="亮" text="「不、感觉特别的舒服。这还是第一次有人为我清理耳朵，没想到是这么舒服的一件事啊」" timeout="" voices="" />
		<msg @l="1308" char="3" flags="0" index="648" line="1308" name="礼羽" text="「哼哼。那真是太好了。喜欢的话下次还帮你清理哦，记得告诉我」" timeout="" voices="rai15719" />
		<msg @l="1310" char="11" flags="0" index="650" line="1310" name="亮" text="「好的，下次再拜托你了哟」" timeout="" voices="" />
		<msg @l="1312" char="-1" flags="0" index="652" line="1312" name="" text="像这样边膝枕，边清理耳朵真是幸福呢。" timeout="" voices="" />
		<msg @l="1314" char="11" flags="0" index="654" line="1314" name="亮" text="「咦？　说起来礼羽的委托这样就可以了吗？」" timeout="" voices="" />
		<chgbg @l="1316" duration="300" flags="256" flip="-1" flip_flags="0" param="16" screen="1" src="evrai21_03e" type="99" />
		<msg @l="1318" char="3" flags="0" index="656" line="1318" name="礼羽" text="「嗯，我就是想这么做呢。谢谢你实现我的愿望」" timeout="" voices="rai15720" />
		<msg @l="1320" char="11" flags="0" index="658" line="1320" name="亮" text="「总觉得只有我这么舒服不太好吧？　所以说还有其他的愿望吗？　能够实现的那种」" timeout="" voices="" />
		<chgbg @l="1322" duration="300" flags="256" flip="-1" flip_flags="0" param="16" screen="1" src="evrai21_03a" type="99" />
		<msg @l="1324" char="3" flags="0" index="660" line="1324" name="礼羽" text="「那就前段时间在电视上放的约会场所特别选集！我想去那里面介绍的游乐园」" timeout="" voices="rai15721" />
		<msg @l="1326" char="-1" flags="0" index="662" line="1326" name="" text="说起来电视上着重介绍了在情侣之间很有人气的游乐园呢。" timeout="" voices="" />
		<msg @l="1328" char="11" flags="0" index="664" line="1328" name="亮" text="（嗯？　你想和我一起去在情侣之间颇有人气的游乐园……？）" timeout="" voices="" />
		<msg @l="1330" char="11" flags="0" index="666" line="1330" name="亮" text="「也就是说这是约会呢！」" timeout="" voices="" />
		<msg @l="1332" char="3" flags="0" index="668" line="1332" name="礼羽" text="「嗯。是约会哦」" timeout="" voices="rai15722" />
		<msg @l="1334" char="11" flags="17" index="670" line="1334" name="亮" text="「呀吼——！！　你终于肯跟我一起约会了——」" timeout="" voices="" />
		<shakescrn @l="1335" amp="20" cycle="20" duration="800" flags="2" screen="0" />
		<pause @l="1336" flags="0" index="672" timeout="" />
		<chgbg @l="1338" duration="300" flags="256" flip="-1" flip_flags="0" param="16" screen="1" src="evrai21_03c" type="99" />
		<msg @l="1340" char="3" flags="0" index="673" line="1340" name="礼羽" text="「开玩笑的」" timeout="" voices="rai15723" />
		<msg @l="1342" char="11" flags="0" index="675" line="1342" name="亮" text="「诶！？　不是吗？　到底去不去！？　这很重要啊！！　喂、到底去不去啊！？」" timeout="" voices="" />
		<chgbg @l="1344" duration="300" flags="256" flip="-1" flip_flags="0" param="16" screen="1" src="evrai21_03a" type="99" />
		<msg @l="1346" char="3" flags="0" index="677" line="1346" name="礼羽" text="「这就要看你够不够努力了哦～？」" timeout="" voices="rai15724" />
		<msg @l="1348" char="11" flags="0" index="679" line="1348" name="亮" text="「我会努力的，会超级努力的！！」" timeout="" voices="" />
		<msg @l="1350" char="3" flags="0" index="681" line="1350" name="礼羽" text="「哼哼。从现在起就好期待呢」" timeout="" voices="rai15725" />
		<msg @l="1352" char="3" flags="0" index="683" line="1352" name="礼羽" text="「到底要怎么做才能取悦到我呢？你会说出比去游乐园还要有趣的话题的吧？」" timeout="" voices="rai15726" />
		<msg @l="1354" char="11" flags="0" index="685" line="1354" name="亮" text="「等下啦！　和游乐园比压力太大了，别这样啊！！」" timeout="" voices="" />
		<chgbg @l="1356" duration="300" flags="256" flip="-1" flip_flags="0" param="16" screen="1" src="evrai21_03b" type="99" />
		<msg @l="1358" char="3" flags="0" index="687" line="1358" name="礼羽" text="「要加油哦——\x;note:」" timeout="" voices="rai15727" />
		<msg @l="1360" char="11" flags="0" index="689" line="1360" name="亮" text="（呜呜呜。看来必须得拿出艺人级别的段子了啊？）" timeout="" voices="" />
		<chgbg @l="1362" duration="300" flags="256" flip="-1" flip_flags="0" param="16" screen="1" src="evrai21_03a" type="99" />
		<msg @l="1364" char="3" flags="0" index="691" line="1364" name="礼羽" text="「呵呵。和亮一起约会，游乐园约会\x;note:」" timeout="" voices="rai15728" />
		<msg @l="1366" char="11" flags="0" index="693" line="1366" name="亮" text="「怎么看你这么开心！？」" timeout="" voices="" />
		<msg @l="1368" char="-1" flags="0" index="695" line="1368" name="" text="不过，礼羽看上去也挺期待的，就这样吧。" timeout="" voices="" />
		<msg @l="1369" char="-1" flags="0" index="697" line="1369" name="" text="就在决心改变成为艺人宫原的瞬间，我的耳朵被轻柔地吹了一下。" timeout="" voices="" />
		<msg @l="1371" char="3" flags="17" index="699" line="1371" name="礼羽" text="「呼———」" timeout="" voices="rai15729" />
		<shakescrn @l="1373" amp="20" cycle="100" duration="300" flags="2" screen="0" />
		<pause @l="1374" flags="0" index="701" timeout="" />
		<play_sound @l="1375" buf="1" fadeout="0" type="1" />
		<msg @l="1378" char="11" flags="0" index="702" line="1378" name="亮" text="「！？？」" timeout="" voices="" />
		<msg @l="1380" char="-1" flags="0" index="704" line="1380" name="" text="感觉到全身软绵绵的，力量都被抽走了。" timeout="" voices="" />
		<chgbg @l="1382" duration="300" flags="256" flip="-1" flip_flags="0" param="16" screen="1" src="evrai21_03e" type="99" />
		<msg @l="1384" char="3" flags="0" index="706" line="1384" name="礼羽" text="「好，清理好了。接下来是另一边的耳朵哦」" timeout="" voices="rai15730" />
		<msg @l="1386" char="-1" flags="0" index="708" line="1386" name="" text="感觉自己恍如梦境中一般，翻身将另一边的耳朵朝向上方。" timeout="" voices="" />
