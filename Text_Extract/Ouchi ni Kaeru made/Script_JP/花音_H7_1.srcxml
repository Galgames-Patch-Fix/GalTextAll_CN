<?xml version="1.0" encoding="utf-8"?>
<xscript>
	<code>
		<msg @l="822" char="0" flags="0" index="437" line="822" name="花音"
		   text="「あっ、ダメだよ。赤ちゃんなんだから、ばぶぅって\n:言わなくっちゃ」" timeout="" voices="kan17784"/>
		<msg @l="824" char="11" flags="0" index="439" line="824" name="亮"
		   text="「わかってるけど……でも、エプロンから甘い味がするんだ」" timeout="" voices=""/>
		<chgbg @l="826" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evkan18_03b" type="99"/>
		<msg @l="828" char="0" flags="0" index="441" line="828" name="花音"
		   text="「えっ……？　って、まさか――――」" timeout="" voices="kan17785"/>
		<play_sound @l="830" buf="1" fadeout="0" intro="-1" src="se511" type="1"
		   volume="0.6"/>
		<msg @l="832" char="-1" flags="0" index="443" line="832" name=""
		   text="胸の谷間を覗きこむようにエプロンの下を確かめる花音。" timeout="" voices=""/>
		<msg @l="833" char="-1" flags="0" index="445" line="833" name=""
		   text="その表情が、なんともいえない驚きの色に染まる。" timeout="" voices=""/>
		<play_sound @l="835" buf="1" fadeout="0" type="1"/>
		<showmsg @l="841" duration="500" show="0"/>
		<scroll @l="842" async="-1" center_x="70" center_y="430" duration="1000"
		   flags="0" rev="0." screen="0" transparency="0" v0="0." v1="0."
		   wait_flags="0" x="0" y="0" zoom="1.5"/>
		<msg @l="845" char="0" flags="0" index="447" line="845" name="花音"
		   text="「……ほんとに、おっぱい出ちゃってる！？」" timeout="" voices="kan17786"/>
		<msg @l="847" char="11" flags="0" index="449" line="847" name="亮"
		   text="「えっ……？　ってことはこの味は……」" timeout="" voices=""/>
		<msg @l="849" char="0" flags="0" index="451" line="849" name="花音"
		   text="「うん……。正真正銘、私の母乳みたい……なんだけど………」" timeout="" voices="kan17787"/>
		<msg @l="851" char="-1" flags="0" index="453" line="851" name=""
		   text="今まで感じたことのない甘い味の正体が花音の母乳！？" timeout="" voices=""/>
		<showmsg @l="853" duration="500" show="0"/>
		<loadimage @l="854" bg="0" fit="0" screen="1" src="evkan18_03b"/>
		<scroll @l="855" async="0" center_x="45" center_y="45" duration="0"
		   flags="0" rev="0." screen="1" transparency="0" v0="0." v1="0."
		   wait_flags="0" x="0" y="0" zoom="1."/>
		<chgscrn @l="856" duration="1000" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" type="99"/>
		<msg @l="859" char="0" flags="0" index="455" line="859" name="花音"
		   text="「でもどうして急に……。あのサプリが今になって\n:効いちゃったのかな？」" timeout="" voices="kan17788"/>
		<msg @l="861" char="11" flags="0" index="457" line="861" name="亮"
		   text="「かもしれないし……僕がずっと強く吸ってたせいかもしれない」" timeout="" voices=""/>
		<msg @l="863" char="0" flags="0" index="459" line="863" name="花音"
		   text="「でも、こんな風に乳首を舌で転がされたり、ちゅうちゅう\n:吸われたこと何度もあるよね？」" timeout=""
		   voices="kan17789"/>
		<msg @l="865" char="11" flags="0" index="461" line="865" name="亮"
		   text="「たしかに……」" timeout="" voices=""/>
		<msg @l="867" char="-1" flags="0" index="463" line="867" name=""
		   text="と、いうことは、原因はサプリってことになる。" timeout="" voices=""/>
		<msg @l="869" char="11" flags="0" index="465" line="869" name="亮"
		   text="「すげえ！　サプリすげえよ！！」" timeout="" voices=""/>
		<chgbg @l="871" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evkan18_03a" type="99"/>
		<msg @l="873" char="0" flags="0" index="467" line="873" name="花音"
		   text="「赤ちゃんを産まなくても、こうやってお乳が出ちゃうことって\n:あるんだね」" timeout=""
		   voices="kan17790"/>
		<msg @l="875" char="0" flags="0" index="469" line="875" name="花音"
		   text="「そういう子もいるって聞いたことはあったけど……まさか自分がそうなっちゃうなんて思ってもいなかったよ」" timeout=""
		   voices="kan17791"/>
		<msg @l="877" char="-1" flags="0" index="471" line="877" name=""
		   text="理由はともあれ、花音の母乳が飲めるとわかったわけで……。" timeout="" voices=""/>
		<msg @l="878" char="-1" flags="0" index="473" line="878" name=""
		   text="ならばもっと飲みたいと思うのが、自然な欲求ってものだろう。" timeout="" voices=""/>
		<chgbg @l="880" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evkan18_03c" type="99"/>
		<msg @l="882" char="0" flags="0" index="475" line="882" name="花音"
		   text="「ね？　私のおっぱい、飲んでくれるよね？」" timeout="" voices="kan17792"/>
		<msg @l="884" char="11" flags="0" index="477" line="884" name="亮"
		   text="「もちろん！　はむっ、れろっ、れるるっ……！」" timeout="" voices=""/>
		<chgbg @l="886" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evkan18_03d" type="99"/>
		<msg @l="888" char="0" flags="0" index="479" line="888" name="花音"
		   text="「ひゃんっ？！　やあっ、あ、ああっ……\x;hearts:」" timeout="" voices="kan17793"/>
		<play_sound @l="890" buf="5" fadeout="0" intro="0" src="kan80016" type="6"
		   volume="1."/>
		<msg @l="891" char="-1" flags="0" index="481" line="891" name=""
		   text="勢いよくしゃぶりつくと、ミルクがエプロンを通して口の中\n:いっぱいに広がる。" timeout="" voices=""/>
		<msg @l="893" char="11" flags="0" index="483" line="893" name="亮"
		   text="「んむ、ちゅるるっ……甘くてさらっとしてて……んむっ、んむっ……いくらでも飲めそうだ」" timeout="" voices=""/>
		<play_sound @l="894" buf="5" fadeout="0" type="1"/>
		<chgbg @l="896" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evkan18_03b" type="99"/>
		<msg @l="898" char="0" flags="0" index="485" line="898" name="花音"
		   text="「わわっ……。おち○ちんがすごいびくびくって……母乳で\n:興奮してくれたんだね」" timeout=""
		   voices="kan17794"/>
		<play_sound @l="900" buf="5" fadeout="0" intro="0" src="kan80015" type="6"
		   volume="1."/>
		<msg @l="901" char="11" flags="0" index="487" line="901" name="亮"
		   text="「赤ちゃんだからな。おっぱいが好きで当たり前なんだ……\n:んぐっ、んぐっ」" timeout="" voices=""/>
		<play_sound @l="902" buf="5" fadeout="0" type="1"/>
		<chgbg @l="904" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evkan18_03a" type="99"/>
		<msg @l="906" char="0" flags="0" index="489" line="906" name="花音"
		   text="「はぁ、はふんっ……えへへ、また赤ちゃんに戻ってくれたんだ」" timeout="" voices="kan17795"/>
		<play_sound @l="908" buf="5" fadeout="0" intro="0" src="kan80015" type="6"
		   volume="1."/>
		<msg @l="909" char="-1" flags="0" index="491" line="909" name=""
		   text="また慈愛に満ちた笑顔で、股間に添えていた手を\n:動かし始める花音。" timeout="" voices=""/>
		<play_sound @l="910" buf="5" fadeout="0" type="1"/>
		<msg @l="912" char="0" flags="0" index="493" line="912" name="花音"
		   text="「じゃ、私にもあなたのミルクをご馳走してね」" timeout="" voices="kan17796"/>
		<msg @l="914" char="0" flags="0" index="495" line="914" name="花音"
		   text="「ママのおっぱいちゅーちゅしながら、おち○ちんミルク、\n:いっぱいぴゅっぴゅっしまちょうねー？」" timeout=""
		   voices="kan17797"/>
		<msg @l="916" char="-1" flags="0" index="497" line="916" name=""
		   text="再び親子のような関係に戻って、赤ちゃんのように答える。" timeout="" voices=""/>
		<msg @l="918" char="11" flags="0" index="499" line="918" name="亮"
		   text="「だぁ、んむっ、んむむっ」" timeout="" voices=""/>
		<msg @l="920" char="0" flags="0" index="501" line="920" name="花音"
		   text="「えへへ。ママのミルクよりずっとずっと、あまーいあまーい\n:甘えん坊さんだね」" timeout=""
		   voices="kan17798"/>
		<msg @l="922" char="0" flags="0" index="503" line="922" name="花音"
		   text="「ほぅら僕ちゃん？　おいちーおいちーママのおっぱい、たっぷりごくごくしていいでちゅよ？」" timeout=""
		   voices="kan17799"/>
		<play_sound @l="924" buf="5" fadeout="0" intro="0" src="kan80015" type="6"
		   volume="1."/>
		<msg @l="925" char="-1" flags="0" index="505" line="925" name=""
		   text="口の中に広がる母乳をすぐに飲んでみたり、ある程度貯めてから\n:一気に飲み干してみたり。" timeout="" voices=""/>
		<msg @l="926" char="-1" flags="0" index="507" line="926" name=""
		   text="そのうちに、エプロン越しでは少しもどかしくなってきてしまう。" timeout="" voices=""/>
		<msg @l="928" char="11" flags="0" index="509" line="928" name="亮"
		   text="「ふぅ、んぐっ……あむっ、あむっ」" timeout="" voices=""/>
		<play_sound @l="929" buf="5" fadeout="0" type="1"/>
		<chgbg @l="931" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evkan18_03b" type="99"/>
		<msg @l="933" char="0" flags="0" index="511" line="933" name="花音"
		   text="「んぅ？　どうしたの？　必死にお口をぱくぱくして……」" timeout="" voices="kan17800"/>
		<msg @l="935" char="0" flags="0" index="513" line="935" name="花音"
		   text="「あっ……もしかして、直接ちゅーちゅーしたい？」" timeout="" voices="kan17801"/>
		<msg @l="937" char="-1" flags="0" index="515" line="937" name=""
		   text="察してくれた花音に、コクコクと首を縦に振って応える。" timeout="" voices=""/>
		<chgbg @l="939" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evkan18_03a" type="99"/>
		<msg @l="941" char="0" flags="0" index="517" line="941" name="花音"
		   text="「くすっ、いいでちゅよ？　じゃあ……んっと、エプロンを\n:こうして――」" timeout="" voices="kan17802"/>
		<play_sound @l="945" buf="1" fadeout="0" intro="-1" src="se511" type="1"
		   volume="1."/>
		<play_sound @l="946" buf="1" fadeout="2000" type="1"/>
		<chgbg @l="947" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evkan18_04a" type="99"/>
		<msg @l="949" char="0" flags="0" index="519" line="949" name="花音"
		   text="「……ほらっ、ママのおっぱい丸見えになっちゃった」" timeout="" voices="kan17803"/>
		<msg @l="951" char="-1" flags="0" index="521" line="951" name=""
		   text="花音ママがエプロンをズラすと、ふわっと甘い匂いが漂う。" timeout="" voices=""/>
		<msg @l="952" char="-1" flags="0" index="523" line="952" name=""
		   text="おっぱいは綺麗な乳白色の照りを帯びていて、その突端からは\n:ぽたぽたと母乳がしたたり落ちている。" timeout=""
		   voices=""/>
		<msg @l="954" char="11" flags="0" index="525" line="954" name="亮"
		   text="「あっ、ああっ……」" timeout="" voices=""/>
		<chgbg @l="956" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evkan18_04b" type="99"/>
		<msg @l="958" char="0" flags="0" index="527" line="958" name="花音"
		   text="「……どうしたの？　舐めないんでちゅか？」" timeout="" voices="kan17804"/>
		<msg @l="960" char="-1" flags="0" index="529" line="960" name=""
		   text="まるで誰も足を踏み入れてない雪原を見たような気分になり、\n:思わず一瞬固まってしまっていた。" timeout=""
		   voices=""/>
		<msg @l="961" char="-1" flags="0" index="531" line="961" name=""
		   text="しかし次の瞬間にはそんな光景に吸いこまれるように口を運ぶ。" timeout="" voices=""/>
		<showmsg @l="965" duration="500" show="0"/>
		<scroll @l="966" async="-1" center_x="70" center_y="430" duration="1000"
		   flags="0" rev="0." screen="0" transparency="0" v0="0." v1="0."
		   wait_flags="0" x="0" y="0" zoom="1.5"/>
		<msg @l="969" char="0" flags="0" index="533" line="969" name="花音"
		   text="「ん、そうそう。赤ちゃんはおっぱい飲むのがしごと\n:でちゅからねー」" timeout="" voices="kan17805"/>
		<msg @l="971" char="0" flags="0" index="535" line="971" name="花音"
		   text="「いっぱいちゅーちゅーちましょうねー」" timeout="" voices="kan17806"/>
		<msg @l="973" char="11" flags="0" index="537" line="973" name="亮"
		   text="「あーん……んむっ」" timeout="" voices=""/>
		<msg @l="975" char="-1" flags="0" index="539" line="975" name=""
		   text="花音は僕の竿を握ったまま、乳首をさらに押しつけてくる。" timeout="" voices=""/>
		<msg @l="976" char="-1" flags="0" index="541" line="976" name=""
		   text="それに夢中になって吸い付く。" timeout="" voices=""/>
		<msg @l="978" char="11" flags="0" index="543" line="978" name="亮"
		   text="「んぐっ、んぐっ……はふっ、んぐぐっ」" timeout="" voices=""/>
		<showmsg @l="983" duration="500" show="0"/>
		<loadimage @l="984" bg="0" fit="0" screen="1" src="evkan18_04c"/>
		<scroll @l="985" async="0" center_x="45" center_y="45" duration="0"
		   flags="0" rev="0." screen="1" transparency="0" v0="0." v1="0."
		   wait_flags="0" x="0" y="0" zoom="1."/>
		<chgscrn @l="986" duration="1000" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" type="99"/>
		<msg @l="990" char="0" flags="0" index="545" line="990" name="花音"
		   text="「んふぅ、はぁ……あぁ、本当に私のおっぱい飲んでる……\x;hearts:」" timeout=""
		   voices="kan17807"/>
		<play_sound @l="992" buf="5" fadeout="0" intro="0" src="kan80015" type="6"
		   volume="1."/>
		<msg @l="993" char="-1" flags="0" index="547" line="993" name=""
		   text="母乳と同じくらい甘い声でささやかれると、腰が熱くなり男根が\n:さらに勃起していく。" timeout="" voices=""/>
		<play_sound @l="994" buf="5" fadeout="0" type="1"/>
		<msg @l="996" char="0" flags="0" index="549" line="996" name="花音"
		   text="「搾りたてのお乳をこんなに夢中になって、ごくごくって……はぁ、ああっ」" timeout="" voices="kan17808"/>
		<chgbg @l="998" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evkan18_04a" type="99"/>
		<msg @l="1000" char="0" flags="0" index="551" line="1000" name="花音"
		   text="「どう？　ママの本物おっぱいおいちーい？」" timeout="" voices="kan17809"/>
		<msg @l="1002" char="11" flags="0" index="553" line="1002" name="亮"
		   text="「んぅっ……」" timeout="" voices=""/>
		<msg @l="1004" char="-1" flags="0" index="555" line="1004" name=""
		   text="頷くと、目が合ったママはにこっと微笑む。" timeout="" voices=""/>
		<msg @l="1006" char="0" flags="0" index="557" line="1006" name="花音"
		   text="「えへへっ……\x;hearts:　それならママも、おち○ちんめいっぱい、\n:しゅこしゅこしてあげまちゅね」" timeout=""
		   voices="kan17810"/>
		<msg @l="1008" char="0" flags="0" index="559" line="1008" name="花音"
		   text="「ママのお手々でふわっと軽めに握ったら、そのままやさーしく\n:……こすこす、しこしこっ……\x;hearts:」"
		   timeout="" voices="kan17811"/>
		<msg @l="1010" char="0" flags="0" index="561" line="1010" name="花音"
		   text="「こしょこしょ、しゅこしゅこ……じっくりゆっくり、\n:おち○ちんがとろっとろになるように……っと\x;hearts:」"
		   timeout="" voices="kan17812"/>
		<play_sound @l="1012" buf="1" fadeout="0" intro="-1" src="hse103" type="1"
		   volume="1."/>
		<play_sound @l="1014" buf="5" fadeout="0" intro="0" src="kan80015" type="6"
		   volume="1."/>
		<msg @l="1015" char="-1" flags="0" index="563" line="1015" name=""
		   text="花音の指が、先端から根元まで肉棒にねっとりと絡みつく。" timeout="" voices=""/>
		<msg @l="1016" char="-1" flags="0" index="565" line="1016" name=""
		   text="それは心地良さともどかしさのバランスが絶妙な手つきで、\n:つい自ら腰を動かしてしまう。" timeout="" voices=""/>
		<play_sound @l="1017" buf="5" fadeout="0" type="1"/>
		<play_sound @l="1019" buf="1" fadeout="0" type="1"/>
		<chgbg @l="1021" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evkan18_04b" type="99"/>
		<msg @l="1023" char="0" flags="0" index="567" line="1023" name="花音"
		   text="「あっ……んもう、自分から腰を動かしちゃうなんてエッチで\n:おませな赤ちゃんなんだからぁ……\x;hearts:」"
		   timeout="" voices="kan17813"/>
		<msg @l="1025" char="0" flags="0" index="569" line="1025" name="花音"
		   text="「おち○ちんもっとぎゅーぎゅーしてほちかったの？」" timeout="" voices="kan17814"/>
		<msg @l="1027" char="11" flags="0" index="571" line="1027" name="亮"
		   text="「んぅ、ばぶぅ……」" timeout="" voices=""/>
		<chgbg @l="1029" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evkan18_04a" type="99"/>
		<msg @l="1031" char="0" flags="0" index="573" line="1031" name="花音"
		   text="「じゃ、おち○ちんのことはママにぜーんぶ任せて、僕ちゃんは\n:力を抜いてくだちゃいねー？」" timeout=""
		   voices="kan17815"/>
		<msg @l="1033" char="-1" flags="0" index="575" line="1033" name=""
		   text="優しく諭すように言われ、腰の動きを止めて口を動かす\n:ことに集中する。" timeout="" voices=""/>
		<msg @l="1035" char="0" flags="0" index="577" line="1035" name="花音"
		   text="「そうそう。いい子でちゅねー」" timeout="" voices="kan17816"/>
		<msg @l="1037" char="0" flags="0" index="579" line="1037" name="花音"
		   text="「そのままおっぱいをちゅーちゅーすることだけ考えて……お口をおっきくあーんして、れろっと舌を伸ばして――」" timeout=""
		   voices="kan17817"/>
		<chgbg @l="1039" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evkan18_04c" type="99"/>
		<msg @l="1041" char="0" flags="0" index="581" line="1041" name="花音"
		   text="「あっ、はんっ……そう、そうやって乳首をちゅぱちゅぱっ……はぁ、ふぅ、はあっ……\x;hearts:」" timeout=""
		   voices="kan17818"/>
		<play_sound @l="1043" buf="5" fadeout="0" intro="0" src="kan80015" type="6"
		   volume="1."/>
		<msg @l="1044" char="-1" flags="0" index="583" line="1044" name=""
		   text="ささやきはまるで催眠術のように僕の身体を動かし、大きくあけた口で乳首とミルクを堪能する。" timeout="" voices=""/>
		<msg @l="1045" char="-1" flags="0" index="585" line="1045" name=""
		   text="全てを花音ママに委ねている感覚にどんどん溺れていく。" timeout="" voices=""/>
