<?xml version="1.0" encoding="utf-8"?>
<xscript>
	<code>
		<msg @l="1382" char="1" flags="0" index="820" line="1382" name="汐"
		   text="「んぅ、へーき。いざとなったら、底に手もつけるし……」" timeout="" voices="usi17565"/>
		<msg @l="1384" char="1" flags="0" index="822" line="1384" name="汐"
		   text="「それに、こんなにしっかり繋がってるんだよ？　\n:溺れたりなんかしないもんっ……\x;heart:」" timeout=""
		   voices="usi17566"/>
		<msg @l="1386" char="1" flags="0" index="824" line="1386" name="汐"
		   text="「キミには溺れてるけどね……とか言ってみたり？」" timeout="" voices="usi17567"/>
		<play_sound @l="1388" buf="5" fadeout="0" intro="0" src="usi80019" type="6"
		   volume="1."/>
		<msg @l="1389" char="-1" flags="0" index="826" line="1389" name=""
		   text="照れたように笑う朝霞さんの目を見てしっかり応える。" timeout="" voices=""/>
		<msg @l="1391" char="11" flags="0" index="828" line="1391" name="亮"
		   text="「それなら僕だって一緒ですよ」" timeout="" voices=""/>
		<play_sound @l="1392" buf="5" fadeout="0" type="1"/>
		<chgbg @l="1394" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evusi16_03c" type="99"/>
		<msg @l="1395" char="1" flags="0" index="830" line="1395" name="汐"
		   text="「……うん。だったら離さないようにしっかりぎゅーしてね？」" timeout="" voices="usi17568"/>
		<chgbg @l="1397" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evusi16_03d" type="99"/>
		<msg @l="1398" char="1" flags="0" index="832" line="1398" name="汐"
		   text="「たとえば、こんなふぅにぃ……ん、ぐぐっ」" timeout="" voices="usi17569"/>
		<play_sound @l="1399" buf="1" fadeout="0" intro="-1" src="hse104" type="1"
		   volume="1."/>
		<play_sound @l="1400" buf="5" fadeout="0" intro="0" src="usi80020" type="6"
		   volume="1."/>
		<msg @l="1401" char="-1" flags="0" index="834" line="1401" name=""
		   text="キュッキュッとペニスをハグしてきた膣に驚き、\n:思わず腰が震える。" timeout="" voices=""/>
		<play_sound @l="1402" buf="1" fadeout="0" type="1"/>
		<msg @l="1403" char="11" flags="0" index="836" line="1403" name="亮"
		   text="「んんッ」" timeout="" voices=""/>
		<play_sound @l="1405" buf="1" fadeout="0" intro="-1" src="se753" type="1"
		   volume="1."/>
		<msg @l="1406" char="-1" flags="0" index="838" line="1406" name=""
		   text="お湯が波打つと、アルちゃんが朝霞さんに少し近寄った。" timeout="" voices=""/>
		<play_sound @l="1407" buf="5" fadeout="0" type="1"/>
		<play_sound @l="1408" buf="1" fadeout="0" type="1"/>
		<chgbg @l="1410" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evusi16_03b" type="99"/>
		<msg @l="1411" char="1" flags="0" index="840" line="1411" name="汐"
		   text="「やぁ……アルちゃんが見に来ちゃったよぉ」" timeout="" voices="usi17570"/>
		<msg @l="1413" char="1" flags="0" index="842" line="1413" name="汐"
		   text="「あぁ、はあっ……なんか、アルちゃんに見られてると思うと、すっごいドキドキしちゃうっ……」" timeout=""
		   voices="usi17571"/>
		<chgbg @l="1415" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evusi16_03d" type="99"/>
		<msg @l="1417" char="1" flags="0" index="844" line="1417" name="汐"
		   text="「見られて感じちゃうなんて……はぁ、はぁ……\n:私、ちょっと変態さんなのかもっ……んふぅ、んぐぅ」" timeout=""
		   voices="usi17572"/>
		<play_sound @l="1419" buf="5" fadeout="0" intro="0" src="usi80018" type="6"
		   volume="1."/>
		<msg @l="1420" char="-1" flags="0" index="846" line="1420" name=""
		   text="そういえば観覧車のときとか、\n:誰かに見られそうな場所だとすごく感じてたっけ。" timeout="" voices=""/>
		<msg @l="1421" char="-1" flags="0" index="848" line="1421" name=""
		   text="誰にも見せたくないけど、誰かに見られたい――\n:そんな難しい願いを叶えてくれる存在がこんな所にいたなんて。" timeout=""
		   voices=""/>
		<msg @l="1423" char="11" flags="0" index="850" line="1423" name="亮"
		   text="「朝霞さん。アルちゃんがじーっと見てますよ？」" timeout="" voices=""/>
		<play_sound @l="1424" buf="5" fadeout="0" type="1"/>
		<chgbg @l="1426" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evusi16_03a" type="99"/>
		<msg @l="1427" char="1" flags="0" index="852" line="1427" name="汐"
		   text="「ひぐっ……やっ、ズルいよぅ。\n:そういうこと言ってドキドキさせるのっ……はぁ、ああっ」" timeout=""
		   voices="usi17573"/>
		<msg @l="1429" char="1" flags="0" index="854" line="1429" name="汐"
		   text="「ただでさえお風呂の反響でエッチな声が響いて、\n:いつもよりドキドキしてるのに……はぁ、ああっ」" timeout=""
		   voices="usi17574"/>
		<play_sound @l="1431" buf="5" fadeout="0" intro="0" src="usi80021" type="6"
		   volume="1."/>
		<msg @l="1432" char="11" flags="0" index="856" line="1432" name="亮"
		   text="「それを言うなら僕だってっ……」" timeout="" voices=""/>
		<msg @l="1434" char="-1" flags="0" index="858" line="1434" name=""
		   text="普段と違って聞こえてくる朝霞さんの嬌声や、\n:湯の熱気に脳がとかされるようだ。" timeout="" voices=""/>
		<play_sound @l="1435" buf="5" fadeout="0" type="1"/>
		<play_sound @l="1436" buf="1" fadeout="0" intro="-1" src="hse104" type="1"
		   volume="1."/>
		<chgbg @l="1437" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evusi16_03b" type="99"/>
		<msg @l="1438" char="1" flags="0" index="860" line="1438" name="汐"
		   text="「あっ、おち○ちん、まだおっきくっ……んぐぅ」" timeout="" voices="usi17575"/>
		<play_sound @l="1439" buf="1" fadeout="0" type="1"/>
		<msg @l="1440" char="1" flags="0" index="862" line="1440" name="汐"
		   text="「そんなに勃起したら、出し入れだけでぐりぐりこすれてっ……ひぅ、はぁんっ、気持ちいいよぅっ」" timeout=""
		   voices="usi17576"/>
		<chgbg @l="1442" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evusi16_03a" type="99"/>
		<play_sound @l="1443" buf="5" fadeout="0" intro="0" src="usi80022" type="6"
		   volume="1."/>
		<msg @l="1444" char="-1" flags="0" index="864" line="1444" name=""
		   text="揺れるお湯が、波となって朝霞さんの腋に収まる。" timeout="" voices=""/>
		<msg @l="1445" char="-1" flags="0" index="866" line="1445" name=""
		   text="ついさっきまでそこで僕のを挟んでくれていたかと思うと、\n:見慣れている腋がたまらなく魅力的に思えてくる。" timeout=""
		   voices=""/>
		<msg @l="1447" char="11" flags="0" index="868" line="1447" name="亮"
		   text="「はぁ、はぁ……朝霞さん可愛い、すっごく可愛い、\n:めちゃ可愛い、マジ可愛いっ……」" timeout="" voices=""/>
		<play_sound @l="1448" buf="5" fadeout="0" type="1"/>
		<chgbg @l="1450" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evusi16_03b" type="99"/>
		<msg @l="1451" char="1" flags="0" index="870" line="1451" name="汐"
		   text="「んぐぅ……？！　そんなに言われたら、\n:さすがに照れちゃうっ……あっ、はぅ、はぁんっ」" timeout=""
		   voices="usi17577"/>
		<play_sound @l="1453" buf="5" fadeout="0" intro="0" src="usi80023" type="6"
		   volume="1."/>
		<msg @l="1454" char="11" flags="0" index="872" line="1454" name="亮"
		   text="「でも、アルちゃんにしっかりと、\n:僕たちのラブラブっぷりを見せないと」" timeout="" voices=""/>
		<play_sound @l="1455" buf="5" fadeout="0" type="1"/>
		<chgbg @l="1457" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evusi16_03a" type="99"/>
		<msg @l="1458" char="1" flags="0" index="874" line="1458" name="汐"
		   text="「でも、あんまり恥ずかしすぎると、のぼせちゃうよっ」" timeout="" voices="usi17578"/>
		<play_sound @l="1460" buf="5" fadeout="0" intro="0" src="usi80022" type="6"
		   volume="1."/>
		<msg @l="1461" char="11" flags="0" index="876" line="1461" name="亮"
		   text="「そしたら僕が介抱しますよ」" timeout="" voices=""/>
		<play_sound @l="1462" buf="5" fadeout="0" type="1"/>
		<msg @l="1464" char="1" flags="0" index="878" line="1464" name="汐"
		   text="「んぅ、もうっ……はぁ、んぐっ、ああんッ」" timeout="" voices="usi17579"/>
		<play_sound @l="1466" buf="5" fadeout="0" intro="0" src="usi80021" type="6"
		   volume="1."/>
		<msg @l="1467" char="-1" flags="0" index="880" line="1467" name=""
		   text="まんざらでもない顔で微笑む朝霞さんが可愛くて、\n:腰をぐぐっと引き寄せて弓なりに反らせる。" timeout=""
		   voices=""/>
		<msg @l="1468" char="-1" flags="0" index="882" line="1468" name=""
		   text="入り口は相変わらずキツキツだが、奥がぽよんと広がって\n:僕の肉棒を根元まで受け入れられるように変化していた。" timeout=""
		   voices=""/>
		<play_sound @l="1469" buf="5" fadeout="0" type="1"/>
		<chgbg @l="1471" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evusi16_03b" type="99"/>
		<msg @l="1472" char="1" flags="0" index="884" line="1472" name="汐"
		   text="「はふぅ、はあっ。ああっ、あ、そこっ、んぅ……\n:そこ、そこもっとっ……あふぅ、はぁんっ」" timeout=""
		   voices="usi17580"/>
		<chgbg @l="1474" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evusi16_03a" type="99"/>
		<msg @l="1475" char="1" flags="0" index="886" line="1475" name="汐"
		   text="「いまのこと、さきっぽでもっとぐにぐにこすって……\n:はあっ、あ、こしゅって、こしゅってぇ\x;heart:」"
		   timeout="" voices="usi17581"/>
		<play_sound @l="1477" buf="5" fadeout="0" intro="0" src="usi80022" type="6"
		   volume="1."/>
		<msg @l="1478" char="11" flags="0" index="888" line="1478" name="亮"
		   text="「ん、はいっ……」" timeout="" voices=""/>
		<msg @l="1480" char="-1" flags="0" index="890" line="1480" name=""
		   text="ぷにぷにの肌に、もっちりとした割れ目。" timeout="" voices=""/>
		<msg @l="1481" char="-1" flags="0" index="892" line="1481" name=""
		   text="朝霞さんのミニマムな身体を揺さぶり、\n:挿抜を少しずつ早くしていく。" timeout="" voices=""/>
		<msg @l="1482" char="-1" flags="0" index="894" line="1482" name=""
		   text="だんだん湯船の波が高くなるけれど、\n:それでもアルちゃんはずっと朝霞さんの側にいた。" timeout="" voices=""/>
		<play_sound @l="1483" buf="5" fadeout="0" type="1"/>
		<play_sound @l="1485" buf="1" fadeout="0" intro="-1" src="se753" type="1"
		   volume="1."/>
		<chgbg @l="1487" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evusi16_03b" type="99"/>
		<msg @l="1488" char="1" flags="0" index="896" line="1488" name="汐"
		   text="「はあっ、はあっ……いいっ、気持ちいいっ……」" timeout="" voices="usi17582"/>
		<play_sound @l="1490" buf="1" fadeout="0" type="1"/>
		<msg @l="1492" char="1" flags="0" index="898" line="1492" name="汐"
		   text="「ずんずんされるたび、お湯がちゃぽちゃぽしてっ……\n:はぁ、ああっ、本当に雲の上にいるみたいっ……」" timeout=""
		   voices="usi17583"/>
		<chgbg @l="1494" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evusi16_03a" type="99"/>
		<msg @l="1495" char="1" flags="0" index="900" line="1495" name="汐"
		   text="「おねがい、おち○ちんもっと……ぐぐぐぅって、\n:奥までじゅっぽり挿れてっ……いっ、はぁ、ああっ」" timeout=""
		   voices="usi17584"/>
		<play_sound @l="1497" buf="5" fadeout="0" intro="0" src="usi80023" type="6"
		   volume="1."/>
		<msg @l="1498" char="-1" flags="0" index="902" line="1498" name=""
		   text="リクエストに応えるため、背筋を伸ばして腰を突き出す。" timeout="" voices=""/>
		<msg @l="1499" char="-1" flags="0" index="904" line="1499" name=""
		   text="細い腰を支えたまま、子宮を意識しながら挿抜を繰り返す。" timeout="" voices=""/>
		<play_sound @l="1500" buf="5" fadeout="0" type="1"/>
		<play_sound @l="1502" buf="1" fadeout="0" intro="-1" src="hse102" type="1"
		   volume="1."/>
		<chgbg @l="1503" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evusi16_03b" type="99"/>
		<msg @l="1504" char="1" flags="0" index="906" line="1504" name="汐"
		   text="「はぁんっ、ああっ、しゅごひっ……\n:お願いしたところを、ちゃんとぐりぐりっ……やふっ、はあっ」" timeout=""
		   voices="usi17585"/>
		<play_sound @l="1505" buf="1" fadeout="0" type="1"/>
		<chgbg @l="1507" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evusi16_03a" type="99"/>
		<msg @l="1508" char="1" flags="0" index="908" line="1508" name="汐"
		   text="「はぁ、はぁ……ほら見てアルちゃん。\n:私の大好きな人、すごいっ……はあっ、ね？　\n:すごいでしょっ……？」"
		   timeout="" voices="usi17586"/>
		<msg @l="1510" char="1" flags="0" index="910" line="1510" name="汐"
		   text="「こぉんなに私のこと愛してっ……\n:気持ちよくしてくれちゃうんだよ？　あっ、はぁんっ\x;heart:」" timeout=""
		   voices="usi17587"/>
		<play_sound @l="1512" buf="5" fadeout="0" intro="0" src="usi80021" type="6"
		   volume="1."/>
		<msg @l="1513" char="11" flags="0" index="912" line="1513" name="亮"
		   text="「あああっ、はあっ、よろしくアルちゃん……ッ」" timeout="" voices=""/>
		<msg @l="1515" char="-1" flags="0" index="914" line="1515" name=""
		   text="思いがけないのろけ攻撃に照れるやら嬉しいやらで、\n:思わず力が入ってしまう。" timeout="" voices=""/>
		<msg @l="1516" char="-1" flags="0" index="916" line="1516" name=""
		   text="流れる汗をいっさい気にせず、\n:火照った身体同士をしっかりとぶつけ合う。" timeout="" voices=""/>
		<msg @l="1517" char="-1" flags="0" index="918" line="1517" name=""
		   text="濡れた肌がぴたぴたと音をたて、\n:そこに湯船のぱちゃぱちゃという音が混ざる。" timeout="" voices=""/>
		<msg @l="1519" char="11" flags="0" index="920" line="1519" name="亮"
		   text="「朝霞さんのおま○こ、いつもよりぬるぬるで、\n:どんどん熱くなって、すっごく気持ちいいっ……」" timeout=""
		   voices=""/>
		<play_sound @l="1520" buf="5" fadeout="0" type="1"/>
		<chgbg @l="1522" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evusi16_03c" type="99"/>
		<msg @l="1523" char="1" flags="0" index="922" line="1523" name="汐"
		   text="「私も……んはうっ、いいっ、気持ちいいっ、きもちいよぉっ\x;heart:」" timeout=""
		   voices="usi17588"/>
		<msg @l="1525" char="1" flags="0" index="924" line="1525" name="汐"
		   text="「だいしゅきなキミとエッチしてるとこ見られて……\n:はぁん、しゅごいこーふんすりゅぅ、んっ……ああっ」" timeout=""
		   voices="usi17589"/>
		<play_sound @l="1527" buf="5" fadeout="0" intro="0" src="usi80023" type="6"
		   volume="1."/>
		<msg @l="1528" char="11" flags="0" index="926" line="1528" name="亮"
		   text="「くっ！　そんなに締め付けたら、すぐに出ちゃいますよっ！」" timeout="" voices=""/>
		<msg @l="1530" char="-1" flags="0" index="928" line="1530" name=""
		   text="ねっとりと絡みつく粘膜が、一気に竿を包み込む。" timeout="" voices=""/>
		<msg @l="1531" char="-1" flags="0" index="930" line="1531" name=""
		   text="際限なく射精できる身体だったら挿抜のたびに\n:精子が溢れてしまいそうなぐらい、狭い膣内は容赦がなかった。" timeout=""
		   voices=""/>
		<play_sound @l="1532" buf="5" fadeout="0" type="1"/>
		<chgbg @l="1534" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evusi16_03d" type="99"/>
		<msg @l="1535" char="1" flags="0" index="932" line="1535" name="汐"
		   text="「お風呂でひびきゅ音と、\n:お湯から伝わっれくる音が重なっれぇ……\n:頭もぽーっとしてきひゃったぁ」" timeout=""
		   voices="usi17590"/>
		<chgbg @l="1537" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evusi16_03b" type="99"/>
		<msg @l="1538" char="1" flags="0" index="934" line="1538" name="汐"
		   text="「いひよ、出ひちゃっへ！　アルちゃんに見らへて、\n:もうがみゃんでひなくなっちゃっへるきゃら」" timeout=""
		   voices="usi17591"/>
		<play_sound @l="1540" buf="5" fadeout="0" intro="0" src="usi80024" type="6"
		   volume="1."/>
		<msg @l="1541" char="-1" flags="0" index="936" line="1541" name=""
		   text="ろれつがまわらなくなった朝霞さんが上半身をくねらせる度に、\n:膣内に当たってる場所が変わる。" timeout=""
		   voices=""/>
		<msg @l="1542" char="-1" flags="0" index="938" line="1542" name=""
		   text="膣肉が射精を促す収縮を繰り返し、\n:それを受けた陰嚢がグイッとせり上がってきた。" timeout="" voices=""/>
		<msg @l="1544" char="11" flags="0" index="940" line="1544" name="亮"
		   text="「あっ、ああっ……もうすぐっ……！」" timeout="" voices=""/>
		<play_sound @l="1545" buf="5" fadeout="0" type="1"/>
		<chgbg @l="1547" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evusi16_03d" type="99"/>
		<msg @l="1548" char="1" flags="0" index="942" line="1548" name="汐"
		   text="「私も、もうしゅぐっ……はあっ、ひぐぅんっ」" timeout="" voices="usi17592"/>
		<play_sound @l="1550" buf="5" fadeout="0" intro="0" src="usi80023" type="6"
		   volume="1."/>
		<msg @l="1551" char="11" flags="0" index="944" line="1551" name="亮"
		   text="「なら、今日も一緒にっ……」" timeout="" voices=""/>
		<play_sound @l="1552" buf="5" fadeout="0" type="1"/>
		<chgbg @l="1554" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evusi16_03a" type="99"/>
		<msg @l="1555" char="1" flags="0" index="946" line="1555" name="汐"
		   text="「はあんっ\x;heart:　いっしょ！　いっしょがいいのぉ！　\n:すきっ！　すきっ、だいしゅき」" timeout=""
		   voices="usi17593"/>
