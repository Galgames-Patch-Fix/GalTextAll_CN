		<msg @l="1533" char="1" flags="0" index="826" line="1533" name="汐"
		   text="「はぅ、今日もこんなに潮吹きしちゃった……」" timeout="" voices="usi11286"/>
		<msg @l="1535" char="1" flags="0" index="828" line="1535" name="汐"
		   text="「お漏らしじゃないってわかってても、あのぷしゃーって感覚は、\n:やっぱりちょっと恥ずかしいなぁ……」" timeout=""
		   voices="usi11287"/>
		<msg @l="1537" char="11" flags="0" index="830" line="1537" name="亮"
		   text="「僕、潮吹き好きですよ」" timeout="" voices=""/>
		<chgbg @l="1539" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evusi12_05a" type="99"/>
		<msg @l="1541" char="1" flags="0" index="832" line="1541" name="汐"
		   text="「そうなの！？」" timeout="" voices="usi11288"/>
		<msg @l="1543" char="11" flags="0" index="834" line="1543" name="亮"
		   text="「だって、いつも僕の射精に対して、いっぱい出たね、\n:たくさん出してくれてありがとうって言うでしょ？」" timeout=""
		   voices=""/>
		<msg @l="1545" char="11" flags="0" index="836" line="1545" name="亮"
		   text="「だから、朝霞さんがこんなふうに潮吹きしてくれたら、\n:僕も同じことを言ってあげられるなって」" timeout=""
		   voices=""/>
		<msg @l="1547" char="1" flags="0" index="838" line="1547" name="汐"
		   text="「へへっ\x;heart:　なら、褒めて？」" timeout="" voices="usi11289"/>
		<msg @l="1549" char="1" flags="0" index="840" line="1549" name="汐"
		   text="「こんなにいっぱい潮吹きしてくれて、\n:すっごく嬉しかったって、言って？」" timeout=""
		   voices="usi11290"/>
		<msg @l="1551" char="11" flags="0" index="842" line="1551" name="亮"
		   text="「こんなにいっぱい、ぷしゃーって潮吹きしてくれて、\n:すっごく嬉しかったです。超最高でした」" timeout=""
		   voices=""/>
		<chgbg @l="1553" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evusi12_05b" type="99"/>
		<msg @l="1555" char="1" flags="0" index="844" line="1555" name="汐"
		   text="「あと、なでなでも」" timeout="" voices="usi11291"/>
		<msg @l="1557" char="11" flags="0" index="846" line="1557" name="亮"
