		<waitbs @l="1404" flags="0" screen="0"/>
		<msg @l="1406" char="-1" flags="0" index="825" line="1406" name=""
		   text="邪魔されてから日数が経ってるし、今が安全だという保証はない。" timeout="" voices=""/>
		<msg @l="1407" char="-1" flags="0" index="827" line="1407" name=""
		   text="ああいうのって、\n:何日くらいの周期でＯＫとダメが入れ替わるものなんだろう？" timeout="" voices=""/>
		<play_sound @l="1408" buf="5" fadeout="0" type="1"/>
		<msg @l="1410" char="0" flags="0" index="829" line="1410" name="花音"
		   text="「大丈夫だよ。色々、えっちのお勉強したって言ったでしょ？　\n:だから大丈夫」" timeout=""
		   voices="kan18159"/>
		<play_sound @l="1412" buf="5" fadeout="0" intro="0" src="kan80017" type="6"
		   volume="1."/>
		<msg @l="1413" char="11" flags="0" index="831" line="1413" name="亮"
		   text="「そうか、それなら安心だな」" timeout="" voices=""/>
		<play_sound @l="1414" buf="5" fadeout="0" type="1"/>
		<chgbg @l="1417" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evkan13_04c" type="99"/>
		<msg @l="1418" char="0" flags="0" index="833" line="1418" name="花音"
		   text="「責任とってなんて言わないから大丈夫だよ。えへへ\x;heart:」" timeout="" voices="kan18160"/>
		<play_sound @l="1420" buf="5" fadeout="0" intro="0" src="kan80015" type="6"
		   volume="1."/>
		<msg @l="1421" char="11" flags="0" index="835" line="1421" name="亮"
		   text="「そっちの大丈夫かーい！！？」" timeout="" voices=""/>
		<msg @l="1423" char="-1" flags="0" index="837" line="1423" name=""
		   text="って、あれ？　それってヤバい日ってこと？？" timeout="" voices=""/>
		<play_sound @l="1424" buf="5" fadeout="0" type="1"/>
		<msg @l="1426" char="0" flags="0" index="839" line="1426" name="花音"
		   text="「いわゆる危険日って月に２、３日しかないんだ。\n:でも膣内に入った精子が元気だったら\n:１週間くらいは持つんだって」"
		   timeout="" voices="kan18161"/>
		<play_sound @l="1428" buf="5" fadeout="0" intro="0" src="kan80016" type="6"
		   volume="1."/>
		<msg @l="1429" char="11" flags="0" index="841" line="1429" name="亮"
		   text="「もし出来たら……出来なくても同じことだけど、\n:ずっと一緒にいてほしいって思ってる」" timeout="" voices=""/>
		<play_sound @l="1430" buf="5" fadeout="0" type="1"/>
		<chgbg @l="1432" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evkan13_04a" type="99"/>
		<msg @l="1433" char="0" flags="0" index="843" line="1433" name="花音"
		   text="「それってプロポーズだったり！？」" timeout="" voices="kan18162"/>
		<play_sound @l="1435" buf="5" fadeout="0" intro="0" src="kan80017" type="6"
		   volume="1."/>
		<msg @l="1436" char="11" flags="0" index="845" line="1436" name="亮"
		   text="「ま、まあ、解釈は任せるよ。\n:とにかくそうなってもいいって覚悟があるから、\n:僕は花音とセックスを続けたい」"
		   timeout="" voices=""/>
		<play_sound @l="1437" buf="5" fadeout="0" type="1"/>
		<chgbg @l="1439" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evkan13_04c" type="99"/>
		<msg @l="1440" char="0" flags="0" index="847" line="1440" name="花音"
		   text="「キュンキュンしちゃう\x;heart:　好きすきっ\x;heart:　\n:だいちゅきっ\x;heart:\x;hearts:」"
		   timeout="" voices="kan18163"/>
		<play_sound @l="1442" buf="5" fadeout="0" intro="0" src="kan80018" type="6"
		   volume="1."/>
		<msg @l="1443" char="-1" flags="0" index="849" line="1443" name=""
		   text="上に覆い被さるように花音が迫ってくると、\n:お腹にかかる圧力が微妙に変化してきた。" timeout="" voices=""/>
		<msg @l="1445" char="11" flags="0" index="851" line="1445" name="亮"
		   text="「くおおっ！！？　おま○こキュッってしまって、\n:気を抜くとすぐに出ちまいそうだっ！」" timeout="" voices=""/>
		<msg @l="1447" char="-1" flags="0" index="853" line="1447" name=""
		   text="あまりの締め付けにち○こがビクンビクンと胎内で跳ねる。" timeout="" voices=""/>
		<play_sound @l="1448" buf="5" fadeout="0" type="1"/>
		<chgbg @l="1451" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evkan13_04b" type="99"/>
		<msg @l="1452" char="0" flags="0" index="855" line="1452" name="花音"
		   text="「ご、ごめん！　\n:嬉しくってついおま○こに力が入っちゃったみたい」" timeout="" voices="kan18164"/>
		<play_sound @l="1454" buf="5" fadeout="0" intro="0" src="kan80017" type="6"
		   volume="1."/>
		<msg @l="1455" char="11" flags="0" index="857" line="1455" name="亮"
		   text="「それって、自分の意思で締め付けられるってこと？」" timeout="" voices=""/>
		<play_sound @l="1456" buf="5" fadeout="0" type="1"/>
		<msg @l="1458" char="0" flags="0" index="859" line="1458" name="花音"
		   text="「ううん、それは練習してないんだ。\n:膣内はあなた専用でまったく触ってないから」" timeout=""
		   voices="kan18165"/>
		<play_sound @l="1460" buf="5" fadeout="0" intro="0" src="kan80015" type="6"
		   volume="1."/>
		<msg @l="1461" char="-1" flags="0" index="861" line="1461" name=""
		   text="それじゃ、ナチュラルにあんなエロい動きが出来たってこと？　\n:なんて末恐ろしいおま○こなんだ！" timeout=""
		   voices=""/>
		<play_sound @l="1462" buf="5" fadeout="0" type="1"/>
		<chgbg @l="1465" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evkan13_04d" type="99"/>
		<msg @l="1466" char="0" flags="0" index="863" line="1466" name="花音"
		   text="「ひゃん！？　今おち○ちんがピクって跳ねたよ？」" timeout="" voices="kan18166"/>
		<play_sound @l="1468" buf="5" fadeout="0" intro="0" src="kan80016" type="6"
		   volume="1."/>
		<msg @l="1469" char="11" flags="0" index="865" line="1469" name="亮"
		   text="「ゴメン。まだ痛みが残ってるだろうけど、\n:動いて欲しくなってる」" timeout="" voices=""/>
		<msg @l="1471" char="-1" flags="0" index="867" line="1471" name=""
		   text="入れてるだけなのに、\n:ヒダがさわさわと亀頭に刺激を与え続けている。" timeout="" voices=""/>
		<msg @l="1472" char="-1" flags="0" index="869" line="1472" name=""
		   text="まるで焦らされているかのような甘い感触が\n:肉棒全体にじわーっと広がっていく。" timeout="" voices=""/>
		<play_sound @l="1473" buf="5" fadeout="0" type="1"/>
		<chgbg @l="1476" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evkan13_04a" type="99"/>
		<msg @l="1477" char="0" flags="0" index="871" line="1477" name="花音"
		   text="「そうだよね。\n:動いた方が、もっとおち○ちん気持ちよくできるもんね」" timeout="" voices="kan18167"/>
		<play_sound @l="1479" buf="5" fadeout="0" intro="0" src="kan80015" type="6"
		   volume="1."/>
		<msg @l="1480" char="11" flags="0" index="873" line="1480" name="亮"
		   text="「本当にゴメン。我慢できない彼氏で」" timeout="" voices=""/>
		<play_sound @l="1481" buf="5" fadeout="0" type="1"/>
		<msg @l="1483" char="0" flags="0" index="875" line="1483" name="花音"
		   text="「我慢なんてしないで、\n:ずっと入ってたからもうだいぶ馴染んできたと思うし」" timeout=""
		   voices="kan18168"/>
		<chgbg @l="1486" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evkan13_04c" type="99"/>
		<msg @l="1487" char="0" flags="0" index="877" line="1487" name="花音"
		   text="「私のおま○こでいっぱい気持ちよくなってねっ！　えへへ\x;heart:」" timeout="" voices="kan18169"/>
		<play_sound @l="1489" buf="5" fadeout="0" intro="0" src="kan80016" type="6"
		   volume="1."/>
		<msg @l="1490" char="-1" flags="0" index="879" line="1490" name=""
		   text="天使か何かか僕の彼女は！？　\n:ああもう、花音への愛おしさがストップ高だ。" timeout="" voices=""/>
		<play_sound @l="1491" buf="5" fadeout="0" type="1"/>
		<chgbg @l="1494" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evkan13_04a" type="99"/>
		<msg @l="1495" char="0" flags="0" index="881" line="1495" name="花音"
		   text="「それじゃあ動くからね……あっ、ううんっ」" timeout="" voices="kan18170"/>
		<play_sound @l="1497" buf="1" fadeout="0" intro="-1" src="hse104" type="1"
		   volume="1."/>
		<play_sound @l="1498" buf="5" fadeout="0" intro="0" src="kan80017" type="6"
		   volume="1."/>
		<msg @l="1499" char="-1" flags="0" index="883" line="1499" name=""
		   text="恐る恐るといった感じに腰を上げて静かに下ろす。" timeout="" voices=""/>
		<play_sound @l="1500" buf="5" fadeout="0" type="1"/>
		<play_sound @l="1501" buf="1" fadeout="0" type="1"/>
		<chgbg @l="1503" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evkan13_04b" type="99"/>
		<msg @l="1504" char="0" flags="0" index="885" line="1504" name="花音"
		   text="「はぁ、はぁ……おち○ちんが私の中にぴったりくっついて、\n:ゴリゴリって擦れてるよぉ」" timeout=""
		   voices="kan18171"/>
		<msg @l="1506" char="0" flags="0" index="887" line="1506" name="花音"
		   text="「私も何か……はぁ、はぁっ、んっ、ああぁ。\n:気持ち良くなってきちゃったかも\x;hearts:」" timeout=""
		   voices="kan18172"/>
		<play_sound @l="1508" buf="5" fadeout="0" intro="0" src="kan80018" type="6"
		   volume="1."/>
		<msg @l="1509" char="-1" flags="0" index="889" line="1509" name=""
		   text="ゆっくりと自分の体になじませるよう、\n:身体を前後に動かしている花音。" timeout="" voices=""/>
		<msg @l="1510" char="-1" flags="0" index="891" line="1510" name=""
		   text="その動きが数往復しただけで、甘さの混じる声を漏らしだした。" timeout="" voices=""/>
		<msg @l="1512" char="11" flags="0" index="893" line="1512" name="亮"
		   text="「そんなに早く気持ち良くなれるものなのか？　んっ、んんっ」" timeout="" voices=""/>
		<play_sound @l="1513" buf="5" fadeout="0" type="1"/>
		<chgbg @l="1516" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evkan13_04d" type="99"/>
		<msg @l="1517" char="0" flags="0" index="895" line="1517" name="花音"
		   text="「自由に動けるから、気持ちいいところにおち○ちんあたって……はぁ、はぁっ！」" timeout=""
		   voices="kan18173"/>
		<msg @l="1519" char="0" flags="0" index="897" line="1519" name="花音"
		   text="「それに、あなたと繋がれたよろこびで……んっ、あぁっん……\n:それで気持ち良くなれてるみたいかも」" timeout=""
		   voices="kan18174"/>
		<play_sound @l="1521" buf="5" fadeout="0" intro="0" src="kan80019" type="6"
		   volume="1."/>
		<msg @l="1522" char="-1" flags="0" index="899" line="1522" name=""
		   text="僕が嬉しくなるようなことをナチュラルに言ってくれる。" timeout="" voices=""/>
		<play_sound @l="1523" buf="5" fadeout="0" type="1"/>
		<chgbg @l="1526" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evkan13_04b" type="99"/>
		<msg @l="1527" char="0" flags="0" index="901" line="1527" name="花音"
		   text="「ごめんね。私ばっかり気持ちよくなっちゃって」" timeout="" voices="kan18175"/>
		<play_sound @l="1529" buf="5" fadeout="0" intro="0" src="kan80018" type="6"
		   volume="1."/>
		<msg @l="1530" char="11" flags="0" index="903" line="1530" name="亮"
		   text="「や、花音がクネクネいやらしく腰をくねらせてるところを\n:見るだけで、興奮してくるからな」" timeout=""
		   voices=""/>
		<play_sound @l="1531" buf="5" fadeout="0" type="1"/>
		<msg @l="1533" char="0" flags="0" index="905" line="1533" name="花音"
		   text="「もうっ、えっちなことばかり言ってぇ。\n:でも、これだけスムーズになってれば、\n:あなたが動いても大丈夫そうだよ……あ、あぁんっ……」"
		   timeout="" voices="kan18176"/>
		<play_sound @l="1535" buf="5" fadeout="0" intro="0" src="kan80019" type="6"
		   volume="1."/>
		<msg @l="1536" char="11" flags="0" index="907" line="1536" name="亮"
		   text="「……いいのか？」" timeout="" voices=""/>
		<play_sound @l="1537" buf="5" fadeout="0" type="1"/>
		<chgbg @l="1539" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evkan13_04a" type="99"/>
		<msg @l="1540" char="0" flags="0" index="909" line="1540" name="花音"
		   text="「んっ、ふぅ……ダメな理由が見つからないよ、\n:だって、あなたにもっと気持ちよくなって欲しいもん\x;heart:」"
		   timeout="" voices="kan18177"/>
		<play_sound @l="1542" buf="5" fadeout="0" intro="0" src="kan80016" type="6"
		   volume="1."/>
		<msg @l="1543" char="-1" flags="0" index="911" line="1543" name=""
		   text="僕だって彼女にも、もっとよろこんで欲しい。" timeout="" voices=""/>
		<msg @l="1544" char="-1" flags="0" index="913" line="1544" name=""
		   text="下から花音のくびれた腰をつかみ、下腹部にグッと力を込める。" timeout="" voices=""/>
		<msg @l="1546" char="11" flags="0" index="915" line="1546" name="亮"
		   text="「じゃ、動くからな」" timeout="" voices=""/>
		<msg @l="1548" char="-1" flags="0" index="917" line="1548" name=""
		   text="花音を気遣いつつも、\n:それでも少しの勢いをつけて股間を下から突き上げる。" timeout="" voices=""/>
		<play_sound @l="1550" buf="1" fadeout="0" intro="-1" src="hse110" type="1"
		   volume="1."/>
		<msg @l="1551" char="-1" flags="0" index="919" line="1551" name=""
		   text="すると、パンという肌を打つ乾いた音が浴場に鳴り響いていく。" timeout="" voices=""/>
		<play_sound @l="1552" buf="5" fadeout="0" type="1"/>
		<play_sound @l="1553" buf="1" fadeout="0" type="1"/>
		<chgbg @l="1555" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evkan13_04d" type="99"/>
		<msg @l="1556" char="0" flags="0" index="921" line="1556" name="花音"
		   text="「ああぁぁっ！！？　はぁはぁっ、\n:おち○ちんが下からグイグイッて突き上げてくるぅっ！！」" timeout=""
		   voices="kan18178"/>
		<msg @l="1558" char="0" flags="0" index="923" line="1558" name="花音"
		   text="「はぁ、はぁっ……思ってもないところに当たってきて、\n:すごいよこれ……ふぁ、あぁぁ\x;hearts:」" timeout=""
		   voices="kan18179"/>
		<play_sound @l="1560" buf="5" fadeout="0" intro="0" src="kan80020" type="6"
		   volume="1."/>
		<msg @l="1561" char="-1" flags="0" index="925" line="1561" name=""
		   text="あまりの嬌声に一瞬ためらったが、\n:このまま大きく動いても大丈夫そうだ。" timeout="" voices=""/>
		<msg @l="1562" char="-1" flags="0" index="927" line="1562" name=""
		   text="グッと下腹部に力をこめて、\n:そのまま上へ上へと腰を打ち付けていく。" timeout="" voices=""/>
		<play_sound @l="1563" buf="5" fadeout="0" type="1"/>
		<chgbg @l="1566" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evkan13_04e" type="99"/>
		<msg @l="1567" char="0" flags="0" index="929" line="1567" name="花音"
		   text="「激しっ！？　ひうっ！　\n:おち○ちん奥までどんどん来てるぅっ！！！」" timeout="" voices="kan18180"/>
		<msg @l="1569" char="0" flags="0" index="931" line="1569" name="花音"
		   text="「何もしてないのにずぶずぶって私の中に入ってきてぇ、\n:はぁはぁっ！　一番奥をコツンって叩いてビリッってするぅ！」"
		   timeout="" voices="kan18181"/>
		<play_sound @l="1571" buf="5" fadeout="0" intro="0" src="kan80022" type="6"
		   volume="1."/>
		<msg @l="1572" char="-1" flags="0" index="933" line="1572" name=""
