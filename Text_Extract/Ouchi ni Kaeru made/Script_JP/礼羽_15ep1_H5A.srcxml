<?xml version="1.0" encoding="utf-8"?>
<xscript>
	<code>
		<label @l="1" id="scene_3_04"/>
		<showmsg @l="13" duration="500" show="0"/>
		<chgbg @l="14" duration="1000" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="black" type="99"/>
		<chgbg @l="15" duration="1000" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="bg17a" type="99"/>
		<face src=""/>
		<msg @l="17" char="-1" flags="0" index="0" line="17" name=""
		   text="礼羽のお爺さんに認められ、その後はお父さんも\n:渋々ながら納得してくれた。" timeout="" voices=""/>
		<face src=""/>
		<msg @l="18" char="-1" flags="0" index="2" line="18" name=""
		   text="これからはこの家で婿入り修行をしながら暮らす許可も貰い、\n:僕は彼女とずっと一緒にいられることになった。" timeout=""
		   voices=""/>
		<play_sound @l="23" buf="0" fadeout="2500" type="0"/>
		<showmsg @l="24" duration="500" show="0"/>
		<loadimage @l="30" bg="0" fit="0" screen="1" src="evrai14_01a"/>
		<scroll @l="31" async="0" center_x="45" center_y="45" duration="0" flags="0"
		   rev="0." screen="1" transparency="0" v0="0." v1="0." wait_flags="0" x="0"
		   y="0" zoom="1."/>
		<chgscrn @l="32" duration="2500" flags="4608" flip="-1" flip_flags="0"
		   param="0" screen="1" type="10"/>
		<play_sound @l="33" buf="0" fadeout="500" intro="-1" src="bgm18" type="0"
		   volume="1."/>
		<msg @l="37" char="-1" flags="0" index="4" line="37" name=""
		   text="窓に手をつけて、突き出すお尻に指を喰い込ます。" timeout="" voices=""/>
		<msg @l="38" char="-1" flags="0" index="6" line="38" name=""
		   text="以前と変わらない白い肌は、柔らかさと女性らしさを\n:しっかりと感じることができた。" timeout="" voices=""/>
		<msg @l="40" char="3" flags="0" index="8" line="40" name="礼羽"
		   text="「もう、いきなりこんな格好をさせるなんて……」" timeout="" voices="rai17100"/>
		<msg @l="42" char="11" flags="0" index="10" line="42" name="亮"
		   text="「だってやっとふたりきりになれたんだからな。\n:ガマンなんてできないさ」" timeout="" voices=""/>
		<msg @l="44" char="3" flags="0" index="12" line="44" name="礼羽"
		   text="「ここなら誰のジャマも入らないからね。それに、\n:もうガマンする必要なんてないんだもんね」" timeout=""
		   voices="rai17101"/>
		<msg @l="46" char="-1" flags="0" index="14" line="46" name=""
		   text="礼羽の部屋にお呼ばれして訪ねると、すでに着替えた\n:礼羽の姿があった。" timeout="" voices=""/>
		<msg @l="48" char="11" flags="0" index="16" line="48" name="亮"
		   text="「その服ってもしかして……」" timeout="" voices=""/>
		<msg @l="50" char="3" flags="0" index="18" line="50" name="礼羽"
		   text="「私のこっちでの生活が見たいってライヌをくれた時に送った、\n:自撮り写真と一緒のやつだよ」" timeout=""
		   voices="rai17102"/>
		<msg @l="52" char="3" flags="0" index="20" line="52" name="礼羽"
		   text="「よく似合ってるって言ってくれたから、直接見てほしくって。\n:どうかな？」" timeout=""
		   voices="rai17103"/>
		<msg @l="54" char="11" flags="0" index="22" line="54" name="亮"
		   text="「その帽子はシャープカだっけ？　ふわふわもこもこが\n:かわいくって、思わず触りたくなるよ」" timeout=""
		   voices=""/>
		<chgbg @l="56" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evrai14_01c" type="99"/>
		<msg @l="58" char="3" flags="0" index="24" line="58" name="礼羽"
		   text="「触って触って。今までの分もいっぱいなでなでして！」" timeout="" voices="rai17104"/>
		<msg @l="60" char="-1" flags="0" index="26" line="60" name=""
		   text="彼女の頭に手を置くと、僕に身を委ねたまま\n:うっとりした顔で目を細める。" timeout="" voices=""/>
		<msg @l="61" char="-1" flags="0" index="28" line="61" name=""
		   text="撫でると思った以上にモワモワで温かく、\n:寒い地域での必需品だということを実感する。" timeout="" voices=""/>
		<msg @l="63" char="11" flags="0" index="30" line="63" name="亮"
		   text="「それにしても礼羽は黒い服がよく似合うな。金髪も映えるし\n:クールな雰囲気とも合ってる」" timeout="" voices=""/>
		<chgbg @l="65" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evrai14_01a" type="99"/>
		<msg @l="66" char="3" flags="0" index="32" line="66" name="礼羽"
		   text="「ふふっ、それだけ？　頭をなでなでされたりしてるうちに、\n:クールじゃなくなってるとこがあるんだけど」" timeout=""
		   voices="rai17105"/>
		<msg @l="68" char="-1" flags="0" index="34" line="68" name=""
		   text="気がつくと、女の子特有の少し甘い匂いが部屋に漂いだしていた。" timeout="" voices=""/>
		<msg @l="69" char="-1" flags="0" index="36" line="69" name=""
		   text="頭から手を離して、よく知る匂いの元を見る。" timeout="" voices=""/>
		<msg @l="71" char="11" flags="0" index="38" line="71" name="亮"
		   text="「もうこんなに濡れてるのか……」" timeout="" voices=""/>
		<chgbg @l="73" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evrai14_01c" type="99"/>
		<msg @l="75" char="3" flags="0" index="40" line="75" name="礼羽"
		   text="「こんなにって言われてもわかんないよ。だって\n:久しぶりにおま○こ濡れちゃってるんだもん\x;hearts:」"
		   timeout="" voices="rai17106"/>
		<msg @l="77" char="11" flags="0" index="42" line="77" name="亮"
		   text="「ねばねばの愛液がパンツにまで垂れたまま糸引いてるぐらい、\n:シミもいっぱいできてる」" timeout="" voices=""/>
		<msg @l="79" char="-1" flags="0" index="44" line="79" name=""
		   text="紫色の大人っぽくてセクシーな下着に濃いシミを作り、\n:愛液の匂いも濃厚になってきている。" timeout="" voices=""/>
		<msg @l="80" char="-1" flags="0" index="46" line="80" name=""
		   text="久々の雌の匂いに、股間に血液が集まっていく。" timeout="" voices=""/>
		<chgbg @l="82" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evrai14_01a" type="99"/>
		<msg @l="84" char="3" flags="0" index="48" line="84" name="礼羽"
		   text="「ふふっ、貴方もパンパンにふくらまして苦しそう\x;hearts:」" timeout="" voices="rai17107"/>
		<msg @l="86" char="11" flags="0" index="50" line="86" name="亮"
		   text="「礼羽がエロすぎるからな」" timeout="" voices=""/>
		<msg @l="88" char="3" flags="0" index="52" line="88" name="礼羽"
		   text="「くすっ。また褒められちゃった。じゃそのエロい身体に\n:ち○ぽちょうだい\x;heart:」" timeout=""
		   voices="rai17108"/>
		<msg @l="90" char="11" flags="0" index="54" line="90" name="亮"
		   text="「ああ、たっぷりしてやるからな」" timeout="" voices=""/>
		<msg @l="92" char="-1" flags="0" index="56" line="92" name=""
		   text="そう言って、ズボンからペニスを取り出す。" timeout="" voices=""/>
		<play_sound @l="97" buf="1" fadeout="0" intro="-1" src="se511" type="1"
		   volume="1."/>
		<chgbg @l="98" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evrai14_02d" type="99"/>
		<msg @l="100" char="3" flags="0" index="58" line="100" name="礼羽"
		   text="「ふふっ、貴方も久しぶりね。ち○ぽくん\x;heart:」" timeout="" voices="rai17109"/>
		<play_sound @l="101" buf="1" fadeout="0" type="1"/>
		<msg @l="102" char="11" flags="0" index="60" line="102" name="亮"
		   text="「そこにも挨拶を欠かさないなんて礼羽は相変わらず\n:エッチだなぁ」" timeout="" voices=""/>
		<msg @l="104" char="3" flags="0" index="62" line="104" name="礼羽"
		   text="「そんなこと言う貴方もお部屋に入るなり、いきなり\n:こんな体勢にしたじゃない」" timeout=""
		   voices="rai17110"/>
		<msg @l="106" char="3" flags="0" index="64" line="106" name="礼羽"
		   text="「しかも脱ぎかけがいいだなんて、十分に\n:エッチだと思うんだけど？」" timeout="" voices="rai17111"/>
		<msg @l="108" char="11" flags="0" index="66" line="108" name="亮"
		   text="「セックスフレンドの教育のたまものだよ。\n:それに全部脱いだら寒いだろ」" timeout="" voices=""/>
		<msg @l="110" char="-1" flags="0" index="68" line="110" name=""
		   text="ふと窓の外を見ると、いつの間にかしんしんと雪が降っていた。" timeout="" voices=""/>
		<msg @l="111" char="-1" flags="0" index="70" line="111" name=""
		   text="建物も調度品も異国の雰囲気バッチリで、こんな場所で\n:これから礼羽とセックス出来ると思うだけでワクワクしてくる。"
		   timeout="" voices=""/>
		<chgbg @l="113" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evrai14_02c" type="99"/>
		<msg @l="115" char="3" flags="0" index="72" line="115" name="礼羽"
		   text="「ん？　外が気になるの？　大丈夫だよ。防犯上\n:ここを覗けるような建物はないから」" timeout=""
		   voices="rai17112"/>
		<msg @l="117" char="3" flags="0" index="74" line="117" name="礼羽"
		   text="「それに前の道も敷地内で誰も通らない。だから\n:こっちに集中して？」" timeout="" voices="rai17113"/>
		<msg @l="119" char="11" flags="0" index="76" line="119" name="亮"
		   text="「ははっ。そういう配慮は相変わらずしっかりしてるんだな」" timeout="" voices=""/>
		<msg @l="121" char="3" flags="0" index="78" line="121" name="礼羽"
		   text="「セックスフレンドの教育のたまものかな。色んな場所で\n:セックスしたからね、しかも誰にも見つからないように」" timeout=""
		   voices="rai17114"/>
		<msg @l="123" char="11" flags="0" index="80" line="123" name="亮"
		   text="「それはそれは。お互い、いい人と巡り合えたらしい」" timeout="" voices=""/>
		<chgbg @l="125" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evrai14_02d" type="99"/>
		<msg @l="127" char="3" flags="0" index="82" line="127" name="礼羽"
		   text="「くすっ。そうだね最高の人と巡り会えたよ\x;heart:」" timeout="" voices="rai17115"/>
		<msg @l="129" char="-1" flags="0" index="84" line="129" name=""
		   text="礼羽がにっこり笑いながら、ち○こに腰を擦りつけてくる。" timeout="" voices=""/>
		<msg @l="130" char="-1" flags="0" index="86" line="130" name=""
		   text="ジワリと秘部から粘ついた愛液が溢れ出て、それが裏スジを擦り\n:くちゅっと音を立てた。" timeout="" voices=""/>
		<chgbg @l="132" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evrai14_02b" type="99"/>
		<msg @l="134" char="3" flags="0" index="88" line="134" name="礼羽"
		   text="「はぁんっ！！」" timeout="" voices="rai17116"/>
		<msg @l="136" char="-1" flags="0" index="90" line="136" name=""
		   text="粘り気のある液体はまるで蜂蜜のような濃さで、\n:擦れる度にクチクチと鳴る。" timeout="" voices=""/>
		<msg @l="138" char="11" flags="0" index="92" line="138" name="亮"
		   text="「こんなに濡れて……すごく礼羽が興奮してる匂いがするよ」" timeout="" voices=""/>
		<chgbg @l="140" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evrai14_02a" type="99"/>
		<msg @l="142" char="3" flags="0" index="94" line="142" name="礼羽"
		   text="「だって、やっと貴方とセックス出来るって考えただけで\n:軽くイっちゃうくらい気持ちが昂ぶってるんだもん\x;hearts:」"
		   timeout="" voices="rai17117"/>
		<msg @l="144" char="-1" flags="0" index="96" line="144" name=""
		   text="突き出したお尻をさらにくねらせ、ペニスに擦りつけてくる。" timeout="" voices=""/>
		<msg @l="145" char="-1" flags="0" index="98" line="145" name=""
		   text="まるで素股のような行為に、挿入前から肉棒が小さく弾ける。" timeout="" voices=""/>
		<msg @l="147" char="3" flags="0" index="100" line="147" name="礼羽"
		   text="「お尻に擦れたち○ぽが、ビクビクって震えてるよ」" timeout="" voices="rai17118"/>
		<msg @l="149" char="11" flags="0" index="102" line="149" name="亮"
		   text="「そりゃ、僕だって期待しちゃってるからな」" timeout="" voices=""/>
		<chgbg @l="151" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evrai14_02d" type="99"/>
		<msg @l="153" char="3" flags="0" index="104" line="153" name="礼羽"
		   text="「くすっ、嬉しいこと言ってくれちゃってもう\x;heart:\n:んふぅ……はっ、はぁぁ\x;hearts:」" timeout=""
		   voices="rai17119"/>
		<play_sound @l="155" buf="1" fadeout="0" intro="-1" src="hse104" type="1"
		   volume="1."/>
		<play_sound @l="156" buf="5" fadeout="0" intro="0" src="rai80016" type="6"
		   volume="1."/>
		<msg @l="157" char="-1" flags="0" index="106" line="157" name=""
		   text="彼女が肉棒の上を滑るように、腰を前後に動かしていく。" timeout="" voices=""/>
		<msg @l="158" char="-1" flags="0" index="108" line="158" name=""
		   text="ま○こから染み出した愛液が肉棒の上にぬらぬらと光る筋を作る。" timeout="" voices=""/>
		<play_sound @l="159" buf="5" fadeout="0" type="1"/>
		<play_sound @l="160" buf="1" fadeout="0" type="1"/>
		<msg @l="162" char="3" flags="0" index="110" line="162" name="礼羽"
		   text="「あはぁ、ち○ぽすっごく熱くて、硬くなってる\x;heart:\n:こうして擦ってるだけで浮き出た血管が\n:気持ちいいとこに当たるの、はぁはぁ\x;hearts:」"
		   timeout="" voices="rai17120"/>
		<play_sound @l="164" buf="5" fadeout="0" intro="0" src="rai80015" type="6"
		   volume="1."/>
		<msg @l="165" char="11" flags="0" index="112" line="165" name="亮"
		   text="「くっ、うぅ……っ、礼羽のぬるぬるが気持ちいい」" timeout="" voices=""/>
		<play_sound @l="166" buf="5" fadeout="0" type="1"/>
		<chgbg @l="168" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evrai14_02c" type="99"/>
		<msg @l="170" char="3" flags="0" index="114" line="170" name="礼羽"
		   text="「貴方の先っぽからもぬるぬるが出てきたよ、\n:んぅっ、んぅぅ……っ、私のと混ざって、はぁはぁ、いやらしい」" timeout=""
		   voices="rai17121"/>
		<play_sound @l="172" buf="5" fadeout="0" intro="0" src="rai80015" type="6"
		   volume="1."/>
		<msg @l="173" char="-1" flags="0" index="116" line="173" name=""
		   text="礼羽が腰を大きく動かすと、鈴口から出たカウパーが\n:秘裂と絡まって１本の大きな糸を引く。" timeout="" voices=""/>
		<msg @l="174" char="-1" flags="0" index="118" line="174" name=""
		   text="やがてカウパーと愛液が混ざり合い、ぐちゅぐちゅと\n:粘り気のある音へと変わっていく。" timeout="" voices=""/>
		<play_sound @l="175" buf="5" fadeout="0" type="1"/>
		<msg @l="177" char="3" flags="0" index="120" line="177" name="礼羽"
		   text="「はぁはぁ……準備も出来たことだし、\n:そろそろあったまろっか\x;heart:」" timeout=""
		   voices="rai17122"/>
		<chgbg @l="179" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evrai14_02a" type="99"/>
		<msg @l="181" char="3" flags="0" index="122" line="181" name="礼羽"
		   text="「この子も寒いって。だからほら、もう準備が出来て\n:ほかほかになってる、あつあつおま○こに入れてあげて？」" timeout=""
		   voices="rai17123"/>
		<play_sound @l="183" buf="5" fadeout="0" intro="0" src="rai80016" type="6"
		   volume="1."/>
		<msg @l="184" char="-1" flags="0" index="124" line="184" name=""
		   text="ヘソまで反り返るほどギンギンになったペニスを包み込むように\n:秘唇がピタッとはりついてくる。" timeout=""
		   voices=""/>
		<msg @l="186" char="11" flags="0" index="126" line="186" name="亮"
		   text="「熱い愛液がち○ぽに絡みついて、\n:ぬるぬるしっぱなしになってるな」" timeout="" voices=""/>
		<play_sound @l="187" buf="5" fadeout="0" type="1"/>
		<chgbg @l="189" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evrai14_02c" type="99"/>
		<msg @l="191" char="3" flags="0" index="128" line="191" name="礼羽"
		   text="「中はもっととろとろで、すごいことになってるから……\n:早くち○ぽで鎮めてぇ\x;heart:」" timeout=""
		   voices="rai17124"/>
		<play_sound @l="193" buf="5" fadeout="0" intro="0" src="rai80017" type="6"
		   volume="1."/>
		<msg @l="194" char="-1" flags="0" index="130" line="194" name=""
		   text="ガマンできないと言った風にペニスを掴まれ、\n:そのまま亀頭を入り口に持っていかれる。" timeout="" voices=""/>
		<msg @l="196" char="11" flags="0" index="132" line="196" name="亮"
		   text="「待った待った！　久しぶりなんだから、もっとゆっくり\n:ほぐしたほうがいいんじゃないか？」" timeout=""
		   voices=""/>
		<play_sound @l="197" buf="5" fadeout="0" type="1"/>
		<chgbg @l="199" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evrai14_02d" type="99"/>
		<msg @l="201" char="3" flags="0" index="134" line="201" name="礼羽"
		   text="「もう、じゅうぶんほぐれてるよ。それに\n:これ以上ガマンなんて無理、だって貴方が目の前にいるんだから」" timeout=""
		   voices="rai17125"/>
		<chgbg @l="203" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evrai14_02c" type="99"/>
		<msg @l="205" char="3" flags="0" index="136" line="205" name="礼羽"
		   text="「写真越しでも画面越しでもない。貴方がここに居るんだから！」" timeout="" voices="rai17126"/>
		<play_sound @l="207" buf="5" fadeout="0" intro="0" src="rai80016" type="6"
		   volume="1."/>
		<msg @l="208" char="-1" flags="0" index="138" line="208" name=""
		   text="感極まったように目の端に涙を溜める礼羽。" timeout="" voices=""/>
		<play_sound @l="209" buf="5" fadeout="0" type="1"/>
		<chgbg @l="211" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evrai14_02b" type="99"/>
		<msg @l="213" char="3" flags="0" index="140" line="213" name="礼羽"
		   text="「好き好き好き大好き！」" timeout="" voices="rai17127"/>
		<msg @l="215" char="3" flags="0" index="142" line="215" name="礼羽"
		   text="「貴方のことが好きで好きでたまらないの！」" timeout="" voices="rai17128"/>
		<play_sound @l="217" buf="5" fadeout="0" intro="0" src="rai80015" type="6"
		   volume="1."/>
		<msg @l="218" char="-1" flags="0" index="144" line="218" name=""
		   text="こんなにも僕を求め、待っていてくれたのか。" timeout="" voices=""/>
		<msg @l="219" char="-1" flags="0" index="146" line="219" name=""
