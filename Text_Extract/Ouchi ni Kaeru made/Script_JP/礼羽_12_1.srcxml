		   volume="1."/>
		<face src=""/>
		<msg @l="1163" char="-1" flags="0" index="546" line="1163" name=""
		   text="暖炉があるそばに礼羽が座り、手招きをする。" timeout="" voices=""/>
		<face src="f"/>
		<msg @l="1165" char="11" flags="0" index="548" line="1165" name="亮"
		   text="「それがお願いか？　お安い御用だ」" timeout="" voices=""/>
		<chgbs async="0" duration="200" priority="0" screen="0" src="rai22az"
		   transparency="0" x="2147483648"/>
		<waitbs @l="1167" flags="0" screen="0"/>
		<face src="frai22a"/>
		<msg @l="1168" char="3" flags="17" index="550" line="1168" name="礼羽"
		   text="「はい、じゃ次はここに頭を置いて」" timeout="" voices="rai15701"/>
		<movebs duration="200" flags="0" id="rai" rev="0." screen="0"
		   transparency="0" v0="0." v1="0." x="2147483648" y="45"/>
		<waitbs @l="1169" flags="0" screen="0"/>
		<movebs duration="200" flags="0" id="rai" rev="0." screen="0"
		   transparency="0" v0="0." v1="0." x="2147483648" y="0"/>
		<waitbs @l="1170" flags="0" screen="0"/>
		<movebs duration="200" flags="0" id="rai" rev="0." screen="0"
		   transparency="0" v0="0." v1="0." x="2147483648" y="45"/>
		<waitbs @l="1171" flags="0" screen="0"/>
		<movebs duration="200" flags="0" id="rai" rev="0." screen="0"
		   transparency="0" v0="0." v1="0." x="2147483648" y="0"/>
		<waitbs @l="1172" flags="0" screen="0"/>
		<pause @l="1173" flags="0" index="552" timeout=""/>
		<face src=""/>
		<msg @l="1175" char="-1" flags="0" index="553" line="1175" name=""
		   text="自分の太ももをポンポンと叩いて、アピールしてくる礼羽。" timeout="" voices=""/>
		<face src="f"/>
		<msg @l="1177" char="11" flags="0" index="555" line="1177" name="亮"
		   text="（それって膝枕ってことですかーー！？）" timeout="" voices=""/>
		<face src=""/>
		<msg @l="1179" char="-1" flags="0" index="557" line="1179" name=""
		   text="お願いと言う名のご褒美に、僕はすぐさま寝転がった。" timeout="" voices=""/>
		<play_sound @l="1183" buf="0" fadeout="2500" type="0"/>
		<showmsg @l="1184" duration="500" show="0"/>
		<loadimage @l="1186" bg="0" fit="0" screen="1" src="evrai21_01a"/>
		<scroll @l="1187" async="0" center_x="286" center_y="180" duration="0"
		   flags="0" rev="0." screen="1" transparency="0" v0="0." v1="0."
		   wait_flags="0" x="0" y="0" zoom="1.5"/>
		<chgscrn @l="1188" duration="1000" flags="4608" flip="-1" flip_flags="0"
		   param="0" screen="1" type="3"/>
		<scroll @l="1190" async="-1" center_x="45" center_y="45" duration="2500"
		   flags="0" rev="0." screen="0" transparency="0" v0="0." v1="0."
		   wait_flags="0" x="0" y="0" zoom="1."/>
		<chgbg @l="1191" duration="2500" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evrai21_01a" type="99"/>
		<play_sound @l="1192" buf="0" fadeout="500" intro="-1" src="bgm31" type="0"
		   volume="1."/>
		<msg @l="1196" char="11" flags="0" index="559" line="1196" name="亮"
		   text="（ふぁあーー！　やわらけーー！！）" timeout="" voices=""/>
		<msg @l="1198" char="-1" flags="0" index="561" line="1198" name=""
		   text="おっぱいとも違う、ほど良いクッション性と体温。" timeout="" voices=""/>
		<msg @l="1199" char="-1" flags="0" index="563" line="1199" name=""
		   text="細くて綺麗なだけじゃなく、女の子らしい肉付きの\n:柔らかさもある。" timeout="" voices=""/>
		<msg @l="1201" char="11" flags="0" index="565" line="1201" name="亮"
		   text="\h;70:\s;70:（太もも最高でーーーす！！）\s:\h:" timeout="" voices=""/>
		<chgbg @l="1203" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evrai21_01c" type="99"/>
		<msg @l="1205" char="3" flags="0" index="567" line="1205" name="礼羽"
		   text="「ンッ\x;note:　こら頬ずりしないの、くすぐったいでしょ？」" timeout="" voices="rai15702"/>
		<msg @l="1207" char="11" flags="0" index="569" line="1207" name="亮"
		   text="「だって太ももがすべすべで、気持ちいいんだもん」" timeout="" voices=""/>
		<chgbg @l="1209" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evrai21_01a" type="99"/>
		<msg @l="1210" char="-1" flags="0" index="571" line="1210" name=""
		   text="産毛すらないすべすべの柔肌に顔を埋める。" timeout="" voices=""/>
		<msg @l="1211" char="-1" flags="0" index="573" line="1211" name=""
		   text="安心感と言うべきか安堵感と言うべきか……\n:とにかく癒されるなあ。" timeout="" voices=""/>
		<msg @l="1212" char="-1" flags="0" index="575" line="1212" name=""
		   text="しかも下から見上げる形になっているせいか、２つの\n:大きなたわわが大迫力でゆさゆさと揺れていた。" timeout=""
		   voices=""/>
		<msg @l="1214" char="11" flags="0" index="577" line="1214" name="亮"
		   text="（手を伸ばせば、すぐにとど――）" timeout="" voices=""/>
		<chgbg @l="1216" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evrai21_01c" type="99"/>
		<msg @l="1217" char="3" flags="0" index="579" line="1217" name="礼羽"
		   text="「ンもう！　おいたはダーメっ\x;note:」" timeout="" voices="rai15703"/>
		<msg @l="1219" char="-1" flags="0" index="581" line="1219" name=""
		   text="手を伸ばしかけたところで優しく制されてしまった。" timeout="" voices=""/>
		<msg @l="1221" char="3" flags="0" index="583" line="1221" name="礼羽"
		   text="「まったく、すぐそうやっておっぱい触ろうとするんだから。\n:どんだけおっぱいが好きなの？」" timeout=""
		   voices="rai15704"/>
		<msg @l="1223" char="11" flags="0" index="585" line="1223" name="亮"
		   text="「や、だって。こんな大きいのが目の前にあったら\n:触らない方が失礼ってもんだろ？」" timeout="" voices=""/>
		<msg @l="1225" char="3" flags="0" index="587" line="1225" name="礼羽"
		   text="「くすくすっ。なにそれ」" timeout="" voices="rai15705"/>
		<msg @l="1227" char="11" flags="0" index="589" line="1227" name="亮"
		   text="「なにしろ礼羽のおっぱいだからな」" timeout="" voices=""/>
		<chgbg @l="1231" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evrai21_02a" type="99"/>
		<play_sound @l="1232" buf="1" fadeout="0" intro="-1" src="se511" type="1"
		   volume="0.7"/>
		<msg @l="1234" char="3" flags="0" index="591" line="1234" name="礼羽"
		   text="「私のだから触りたいんだ。それは嬉しいけど、今は\n:おとなしくしててね」" timeout="" voices="rai15706"/>
		<msg @l="1236" char="-1" flags="0" index="593" line="1236" name=""
		   text="そう言って優しく頭を撫でられる。" timeout="" voices=""/>
		<msg @l="1237" char="-1" flags="0" index="595" line="1237" name=""
		   text="こうされてはさすがに大人しくするしかない。" timeout="" voices=""/>
		<play_sound @l="1239" buf="1" fadeout="0" type="1"/>
		<msg @l="1241" char="3" flags="0" index="597" line="1241" name="礼羽"
		   text="「いい子いい子。そのままおとなしくしてるんだよ、\n:今から耳かきをしてあげるから」" timeout=""
		   voices="rai15707"/>
		<msg @l="1243" char="11" flags="0" index="599" line="1243" name="亮"
		   text="「おお、至れり尽くせりだな」" timeout="" voices=""/>
		<msg @l="1245" char="3" flags="0" index="601" line="1245" name="礼羽"
		   text="「ふふっ。貴方のために用意したんだからじっくり堪能してね」" timeout="" voices="rai15708"/>
		<msg @l="1247" char="-1" flags="0" index="603" line="1247" name=""
		   text="膝枕されながら耳かきとか。生きててよかったぁ～！" timeout="" voices=""/>
		<msg @l="1248" char="-1" flags="0" index="605" line="1248" name=""
		   text="めっちゃラブラブな感じが出てますやん！" timeout="" voices=""/>
		<chgbg @l="1250" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evrai21_01e" type="99"/>
		<msg @l="1252" char="3" flags="0" index="607" line="1252" name="礼羽"
		   text="「耳かきって海外では、めったに手にはいらないんだよねー」" timeout="" voices="rai15709"/>
		<msg @l="1254" char="11" flags="0" index="609" line="1254" name="亮"
		   text="「え？　じゃどうやって掃除してるんだ？」" timeout="" voices=""/>
		<msg @l="1257" char="3" flags="0" index="611" line="1257" name="礼羽"
		   text="「フィンランドでは綿棒派がほとんどだよ。日本のみたいに\n:質のいいこだわりの耳かきってまず置いてないんだ」" timeout=""
		   voices="rai15710"/>
		<chgbg @l="1259" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evrai21_01b" type="99"/>
		<msg @l="1261" char="3" flags="0" index="613" line="1261" name="礼羽"
		   text="「でもしてみたかったの。だって耳掃除って\n:私に全てを委ねないと危ないもんねー」" timeout=""
		   voices="rai15711"/>
		<msg @l="1263" char="11" flags="17" index="615" line="1263" name="亮"
		   text="「なにその小悪魔的笑顔！？」" timeout="" voices=""/>
		<shakescrn @l="1264" amp="20" cycle="20" duration="500" flags="2" screen="0"/>
		<pause @l="1265" flags="0" index="617" timeout=""/>
		<msg @l="1267" char="-1" flags="0" index="618" line="1267" name=""
		   text="と言いつつ耳を差しだす。" timeout="" voices=""/>
		<chgbg @l="1269" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evrai21_01a" type="99"/>
		<msg @l="1271" char="3" flags="0" index="620" line="1271" name="礼羽"
		   text="「ふふっ。全部委ねてくれるんだ、嬉しいなー」" timeout="" voices="rai15712"/>
		<msg @l="1273" char="11" flags="0" index="622" line="1273" name="亮"
		   text="「は、初めてだから優しくしてね」" timeout="" voices=""/>
		<msg @l="1275" char="3" flags="0" index="624" line="1275" name="礼羽"
		   text="「なにその乙女！？　キュンってしちゃうでしょ、もうー」" timeout="" voices="rai15713"/>
		<msg @l="1277" char="-1" flags="0" index="626" line="1277" name=""
		   text="ふにふにと頬を突かれてしまった。" timeout="" voices=""/>
		<chgbg @l="1279" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evrai21_01e" type="99"/>
		<msg @l="1281" char="3" flags="0" index="628" line="1281" name="礼羽"
		   text="「私も初めてだから優しくするよ。ま、ネットで調べたから\n:そこは安心して」" timeout=""
		   voices="rai15714"/>
		<msg @l="1283" char="11" flags="0" index="630" line="1283" name="亮"
		   text="「僕のために、そんなことまでしてくれてたのか」" timeout="" voices=""/>
		<chgbg @l="1285" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evrai21_01c" type="99"/>
		<msg @l="1287" char="3" flags="0" index="632" line="1287" name="礼羽"
		   text="「調べた結果、クジラって哺乳類で唯一ほっといても耳垢が\n:勝手に出ていかないんだって」" timeout=""
		   voices="rai15715"/>
		<msg @l="1289" char="3" flags="0" index="634" line="1289" name="礼羽"
		   text="「だから耳垢が年輪みたいになって、そこから\n:クジラの年齢がわかるってことも学んだよ」" timeout=""
		   voices="rai15716"/>
		<msg @l="1291" char="11" flags="0" index="636" line="1291" name="亮"
		   text="「そのウンチク今、関係あるか！？　これアカンやつや。\n:調べてるうちに他のことに興味がいって気の赴くまま\n:次々他のサイトに行くやつや……」"
		   timeout="" voices=""/>
		<chgbg @l="1293" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evrai21_01a" type="99"/>
		<msg @l="1295" char="3" flags="0" index="638" line="1295" name="礼羽"
		   text="「まあいいじゃない。ふふっ、じゃあいくね」" timeout="" voices="rai15717"/>
		<chgbg @l="1299" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evrai21_03a" type="99"/>
		<msg @l="1301" char="-1" flags="0" index="640" line="1301" name=""
		   text="ゆっくり丁寧に耳かきの先の部分が挿入される。" timeout="" voices=""/>
		<msg @l="1302" char="-1" flags="0" index="642" line="1302" name=""
		   text="最初こそ怖かったが、これが程よい深さと力加減。" timeout="" voices=""/>
		<msg @l="1304" char="3" flags="0" index="644" line="1304" name="礼羽"
		   text="「どう？　痛かったら言ってね？」" timeout="" voices="rai15718"/>
		<msg @l="1306" char="11" flags="0" index="646" line="1306" name="亮"
		   text="「や、すっげー気持ちいい。人にしてもらったの初めてだけど、\n:耳掃除がこんなにも気持ちいいものだなんて知らなかったぜ」"
		   timeout="" voices=""/>
		<msg @l="1308" char="3" flags="0" index="648" line="1308" name="礼羽"
		   text="「ふふっ。それはよかった。気にいったんなら\n:またやってあげるから、いつでも言ってね」" timeout=""
		   voices="rai15719"/>
		<msg @l="1310" char="11" flags="0" index="650" line="1310" name="亮"
		   text="「ああ、またお願いするよ」" timeout="" voices=""/>
		<msg @l="1312" char="-1" flags="0" index="652" line="1312" name=""
		   text="こうやって膝枕されながら、耳掃除とか幸せだなー。" timeout="" voices=""/>
		<msg @l="1314" char="11" flags="0" index="654" line="1314" name="亮"
		   text="「あれ？　そういや礼羽のお願いってこれでいいのか？」" timeout="" voices=""/>
		<chgbg @l="1316" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evrai21_03e" type="99"/>
		<msg @l="1318" char="3" flags="0" index="656" line="1318" name="礼羽"
		   text="「うん、こうしたかったからね。私のお願いを叶えてくれて\n:ありがと」" timeout="" voices="rai15720"/>
		<msg @l="1320" char="11" flags="0" index="658" line="1320" name="亮"
		   text="「なんか僕だけ気持ちよくなって悪いような？　ってわけで\n:他もなんかない？　叶えられるヤツ」" timeout=""
		   voices=""/>
		<chgbg @l="1322" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evrai21_03a" type="99"/>
		<msg @l="1324" char="3" flags="0" index="660" line="1324" name="礼羽"
		   text="「だったら、この前テレビやってたデートスポットの特集！\n:あの遊園地に行ってみたいんだ」" timeout=""
		   voices="rai15721"/>
		<msg @l="1326" char="-1" flags="0" index="662" line="1326" name=""
		   text="そういえばカップルに人気の遊園地があるって、\n:大々的にピックアップされていたな。" timeout="" voices=""/>
		<msg @l="1328" char="11" flags="0" index="664" line="1328" name="亮"
		   text="（ん？　カップルに人気の遊園地に僕と行きたい……？）" timeout="" voices=""/>
		<msg @l="1330" char="11" flags="0" index="666" line="1330" name="亮"
		   text="「つまりはデートだな！」" timeout="" voices=""/>
		<msg @l="1332" char="3" flags="0" index="668" line="1332" name="礼羽"
		   text="「うん。デートだよ」" timeout="" voices="rai15722"/>
		<msg @l="1334" char="11" flags="17" index="670" line="1334" name="亮"
		   text="「いやっほぉぉっ！！　ついに\n:デートしてくれる気になったんだー」" timeout="" voices=""/>
		<shakescrn @l="1335" amp="20" cycle="20" duration="800" flags="2" screen="0"/>
		<pause @l="1336" flags="0" index="672" timeout=""/>
		<chgbg @l="1338" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evrai21_03c" type="99"/>
		<msg @l="1340" char="3" flags="0" index="673" line="1340" name="礼羽"
		   text="「つってねー」" timeout="" voices="rai15723"/>
		<msg @l="1342" char="11" flags="0" index="675" line="1342" name="亮"
		   text="「えっ！？　違うの？　どっち！？　そこ大事だから！！\n:ねぇ、どっちなの！？」" timeout="" voices=""/>
		<chgbg @l="1344" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evrai21_03a" type="99"/>
		<msg @l="1346" char="3" flags="0" index="677" line="1346" name="礼羽"
		   text="「それは貴方の頑張りしだいかな～？」" timeout="" voices="rai15724"/>
		<msg @l="1348" char="11" flags="0" index="679" line="1348" name="亮"
		   text="「頑張るから、超頑張るから！！」" timeout="" voices=""/>
		<msg @l="1350" char="3" flags="0" index="681" line="1350" name="礼羽"
		   text="「ふふっ。それは今から楽しみだね」" timeout="" voices="rai15725"/>
		<msg @l="1352" char="3" flags="0" index="683" line="1352" name="礼羽"
		   text="「いったいどうやって私を楽しませてくれるんだろ？\n:遊園地よりおもしろいことを言ってくれるんだろーなー？」" timeout=""
		   voices="rai15726"/>
		<msg @l="1354" char="11" flags="0" index="685" line="1354" name="亮"
		   text="「ちょっ！　アミューズメントパークと比べるとか、\n:プレッシャー半端ないからやめてっ！！」" timeout=""
		   voices=""/>
		<chgbg @l="1356" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evrai21_03b" type="99"/>
		<msg @l="1358" char="3" flags="0" index="687" line="1358" name="礼羽"
		   text="「がんばってねー\x;note:」" timeout="" voices="rai15727"/>
		<msg @l="1360" char="11" flags="0" index="689" line="1360" name="亮"
		   text="（むむむ。これは芸人になれるくらいの笑いを\n:提供しなければならないのか？）" timeout="" voices=""/>
		<chgbg @l="1362" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evrai21_03a" type="99"/>
		<msg @l="1364" char="3" flags="0" index="691" line="1364" name="礼羽"
		   text="「うふふっ。亮とデート、遊園地デートっ\x;note:」" timeout="" voices="rai15728"/>
		<msg @l="1366" char="11" flags="0" index="693" line="1366" name="亮"
		   text="「ルンルン気分なんですけどー！？」" timeout="" voices=""/>
		<msg @l="1368" char="-1" flags="0" index="695" line="1368" name=""
		   text="まあ、礼羽も楽しみにしてるみたいだし、いっか。" timeout="" voices=""/>
		<msg @l="1369" char="-1" flags="0" index="697" line="1369" name=""
		   text="テクニシャン改め芸人宮原になる覚悟を決めた瞬間。\n:耳に軽く息を吹きかけられる。" timeout="" voices=""/>
		<msg @l="1371" char="3" flags="17" index="699" line="1371" name="礼羽"
		   text="「ふーーーっ」" timeout="" voices="rai15729"/>
		<shakescrn @l="1373" amp="20" cycle="100" duration="300" flags="2"
		   screen="0"/>
		<pause @l="1374" flags="0" index="701" timeout=""/>
		<play_sound @l="1375" buf="1" fadeout="0" type="1"/>
