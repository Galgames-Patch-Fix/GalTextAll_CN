		<waitbs @l="1036" flags="0" screen="0"/>
		<msg @l="1037" char="0" flags="17" index="393" line="1037" name="花音"
		   text="「ダメだよ、すぐに手当てしないと、もっと悪化してケーキ作る\n:どころじゃなくなっちゃうよ！」" timeout=""
