		<waitbs @l="1054" flags="0" screen="0"/>
		<msg @l="1055" char="0" flags="0" index="405" line="1055" name="花音"
		   text="「ごめんね。自分のことばっかりいっぱいいっぱいで、汐ちゃんの気持ちなんて考えてなかったね」" timeout=""
		   voices="kan00635"/>
		<chgbs async="0" duration="200" priority="0" screen="0" src="usi04bz"
		   transparency="0" x="2147483648"/>
		<waitbs @l="1057" flags="0" screen="0"/>
		<msg @l="1058" char="1" flags="17" index="407" line="1058" name="汐"
		   text="「あなたが気にすることじゃないってば。全部、私が勝手に\n:やってるだけなんだから。ね？　だからお願い泣かないで？」"
