		<waitbs @l="990" flags="0" screen="0"/>
		<msg @l="991" char="1" flags="17" index="369" line="991" name="汐"
		   text="「あなたがずっと落ち込んでるから、ケーキでも食べれば\n:昔みたいに元気になるかなって思ったんだけど……全然上手く\n:いかないね」"
		   timeout="" voices="usi00519"/>
		<wait @l="994" flags="5" time="7200"/>
		<chgbs async="0" duration="200" priority="0" screen="0" src="usi04bz"
