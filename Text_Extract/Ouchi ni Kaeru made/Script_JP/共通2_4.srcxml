		<waitbs @l="986" flags="0" screen="0"/>
		<msg @l="988" char="-1" flags="0" index="367" line="988" name=""
		   text="目に涙を浮かべ悔しさを滲ませながら、必死でクリームを\n:かき混ぜる汐ちゃん。" timeout="" voices=""/>
		<chgbs async="0" duration="200" priority="0" screen="0" src="usi00bz"
