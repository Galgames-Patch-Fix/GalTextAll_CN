<?xml version="1.0" encoding="utf-8"?>
<xscript>
	<code>
		<label @l="6" id="scene_0_03"/>
		<play_sound @l="10" buf="0" fadeout="2500" type="0"/>
		<showmsg @l="11" duration="500" show="0"/>
		<loadimage @l="17" bg="0" fit="0" screen="1" src="evkan08_01a"/>
		<scroll @l="18" async="0" center_x="45" center_y="45" duration="0" flags="0"
		   rev="0." screen="1" transparency="0" v0="0." v1="0." wait_flags="0" x="0"
		   y="0" zoom="1."/>
		<chgscrn @l="19" duration="2500" flags="4608" flip="-1" flip_flags="0"
		   param="0" screen="1" type="10"/>
		<play_sound @l="20" buf="0" fadeout="500" intro="-1" src="bgm19" type="0"
		   volume="1."/>
		<play_sound @l="28" buf="1" fadeout="0" intro="-1" src="se511" type="1"
		   volume="1."/>
		<msg @l="30" char="-1" flags="0" index="0" line="30" name=""
		   text="大きなおっぱいを僕に押しつけるように身体を寄せてくる花音。" timeout="" voices=""/>
		<msg @l="31" char="-1" flags="0" index="2" line="31" name=""
		   text="そして僕のシャツのボタンを外し、ズボンを剥ぎ取ってきた。" timeout="" voices=""/>
		<play_sound @l="33" buf="1" fadeout="300" type="1"/>
		<chgbg @l="34" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evkan08_01c" type="99"/>
		<msg @l="35" char="0" flags="0" index="4" line="35" name="花音"
		   text="「あなたもしたかったんだね。おち○ちん、\n:こんなに大きくなってる……はぁはぁ」" timeout=""
		   voices="kan18400"/>
		<msg @l="37" char="11" flags="0" index="6" line="37" name="亮"
		   text="「こんなかわいい花嫁とキスしてたんだ、当然だろ？」" timeout="" voices=""/>
		<chgbg @l="39" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evkan08_01c" type="99"/>
		<msg @l="40" char="0" flags="0" index="8" line="40" name="花音"
		   text="「かわいいだなんて、照れちゃうよぉ\x;hearts:」" timeout="" voices="kan18401"/>
		<msg @l="42" char="-1" flags="0" index="10" line="42" name=""
		   text="照れ隠しのつもりなのか、ペニスにそっと手を寄せてくる花音。" timeout="" voices=""/>
		<msg @l="43" char="-1" flags="0" index="12" line="43" name=""
		   text="ウェディング姿の花音が新婚初夜のように、おずおずと\n:ペニスに手を伸ばし、亀頭をさわさわと撫でている。" timeout=""
		   voices=""/>
		<chgbg @l="45" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evkan08_01c" type="99"/>
		<msg @l="46" char="0" flags="0" index="14" line="46" name="花音"
		   text="「わぁ、凄いね。ビクビクって血管まで浮き上がってて……\n:これも、私のせいなんだよね」" timeout=""
		   voices="kan18402"/>
		<msg @l="48" char="11" flags="0" index="16" line="48" name="亮"
		   text="「かわいすぎるお嫁さんのせいだな」" timeout="" voices=""/>
		<msg @l="50" char="0" flags="0" index="18" line="50" name="花音"
		   text="「それじゃあ、しこしこってしてすぐ楽にしてあげるね……\x;heart:」" timeout="" voices="kan18403"/>
		<chgbg @l="52" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evkan08_01d" type="99"/>
		<msg @l="53" char="0" flags="0" index="20" line="53" name="花音"
		   text="「おち○ちん、しーこしーこぉ……先っぽ、くにくに……\n:はぁはぁ、しーこしこ、こしゅこしゅ」" timeout=""
		   voices="kan18404"/>
		<play_sound @l="55" buf="5" fadeout="0" intro="0" src="kan80015" type="6"
		   volume="1."/>
		<msg @l="56" char="-1" flags="0" index="22" line="56" name=""
		   text="純白の手袋に包まれた手で、反り返っているペニスを\n:キュッと握り、ゆっくりと上下に動かし始める。" timeout=""
		   voices=""/>
		<msg @l="57" char="-1" flags="0" index="24" line="57" name=""
		   text="サテン生地のウェディンググローブはつるつるサラサラで、\n:ストレッチ性が高くてペニスにぴったりとフィットしてくる。"
		   timeout="" voices=""/>
		<msg @l="59" char="11" flags="0" index="26" line="59" name="亮"
		   text="「手袋の感触が気持ち、い……」" timeout="" voices=""/>
		<msg @l="61" char="-1" flags="0" index="28" line="61" name=""
		   text="僕だけの花嫁をこれから汚す。その背徳感と独占欲に\n:背筋がゾクゾクッとしてくる。" timeout="" voices=""/>
		<msg @l="62" char="-1" flags="0" index="30" line="62" name=""
		   text="手でするのとはまた違った手袋独特の感覚に、\n:思わず息も荒くなる。" timeout="" voices=""/>
		<play_sound @l="63" buf="5" fadeout="0" type="1"/>
		<chgbg @l="65" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evkan08_01c" type="99"/>
		<msg @l="66" char="0" flags="0" index="32" line="66" name="花音"
		   text="「ふぁ、あぁぁ……っ、もっと、手でしこしこしてあげるから、\n:はぁっ、はぁはぁっ、びゅーびゅーって気持ちいい射精\n:しようねぇ」"
		   timeout="" voices="kan18405"/>
		<msg @l="68" char="0" flags="0" index="34" line="68" name="花音"
		   text="「ウェディングドレスにぶっかけて、あなた色に\n:染めてほしいの……はぁはぁ、あなたのお嫁さんだって\n:証を刻みつけて欲しい」"
		   timeout="" voices="kan18406"/>
		<play_sound @l="70" buf="5" fadeout="0" intro="0" src="kan80016" type="6"
		   volume="1."/>
		<msg @l="71" char="-1" flags="0" index="36" line="71" name=""
		   text="胸がはだけて大きなおっぱいが露出し、僕の胸板の上に\n:ふにゅんと乗っかってくる。" timeout="" voices=""/>
		<msg @l="72" char="-1" flags="0" index="38" line="72" name=""
		   text="乳首が変形するほど押しつけられ、乳房がむにゅむにゅと\n:形を変えていく。" timeout="" voices=""/>
		<play_sound @l="73" buf="5" fadeout="0" type="1"/>
		<chgbg @l="75" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evkan08_01d" type="99"/>
		<msg @l="76" char="0" flags="0" index="40" line="76" name="花音"
		   text="「はっ、はふっ、はぁっ、こしゅこしゅぐりぐり……っ、\n:一気にぐいーん！」" timeout=""
		   voices="kan18407"/>
		<msg @l="78" char="11" flags="0" index="42" line="78" name="亮"
		   text="「ちょっ！？　亀頭から下までっ！！！！？」" timeout="" voices=""/>
		<play_sound @l="80" buf="5" fadeout="0" intro="0" src="kan80015" type="6"
		   volume="1."/>
		<msg @l="81" char="-1" flags="0" index="44" line="81" name=""
		   text="手袋のツルツルした感触に裏筋を強ばらせて耐える。" timeout="" voices=""/>
		<msg @l="82" char="-1" flags="0" index="46" line="82" name=""
		   text="一気に下まで降りた手がまた亀頭に戻ってきて、\n:カリと先端を指の腹でくにくにといじってきた。" timeout=""
		   voices=""/>
		<play_sound @l="83" buf="5" fadeout="0" type="1"/>
		<msg @l="85" char="0" flags="0" index="48" line="85" name="花音"
		   text="「先っぽ、くりくりー……はぁはぁ、はふぅ、\n:ぷにぷにしててかわいくて、テカテカしてるねぇ」" timeout=""
		   voices="kan18408"/>
		<chgbg @l="87" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evkan08_01a" type="99"/>
		<msg @l="88" char="0" flags="0" index="50" line="88" name="花音"
		   text="「あなたの汗の匂いが、はふぅ……はぁ、んはぁ、ステキだよ。\n:棒の部分も、こしゅこしゅ、しこ、しこ」" timeout=""
		   voices="kan18409"/>
		<play_sound @l="90" buf="5" fadeout="0" intro="0" src="kan80015" type="6"
		   volume="1."/>
		<msg @l="91" char="-1" flags="0" index="52" line="91" name=""
		   text="くんくんしながら僕の汗を嗅ぎつつ、\n:丁寧に指先で愛撫を続けてくる。" timeout="" voices=""/>
		<msg @l="92" char="-1" flags="0" index="54" line="92" name=""
		   text="カリを引っかけたと思ったら、裏筋を撫でたりと\n:多彩な責めで男根を翻弄してくる。" timeout="" voices=""/>
		<msg @l="94" char="11" flags="0" index="56" line="94" name="亮"
		   text="「くぁぁ……はぁはぁ、ご奉仕されてるみたいでいい気分だ」" timeout="" voices=""/>
		<play_sound @l="95" buf="5" fadeout="0" type="1"/>
		<chgbg @l="97" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evkan08_01c" type="99"/>
		<msg @l="98" char="0" flags="0" index="58" line="98" name="花音"
		   text="「えへへ、いつもおっぱいですることが多かったけど、\n:たまにはこういうのもいいでしょ？」" timeout=""
		   voices="kan18410"/>
		<play_sound @l="100" buf="5" fadeout="0" intro="0" src="kan80017" type="6"
		   volume="1."/>
		<msg @l="101" char="-1" flags="0" index="60" line="101" name=""
		   text="僕の返答に安心したのか、花音が少しずつ手の動きを\n:加速させていく。" timeout="" voices=""/>
		<play_sound @l="103" buf="1" fadeout="0" intro="-1" src="se511" type="1"
		   volume="1."/>
		<msg @l="104" char="-1" flags="0" index="62" line="104" name=""
		   text="その動きに合わせて、しゅるしゅるとした、衣擦れの音が\n:僕の耳をくすぐる。" timeout="" voices=""/>
		<play_sound @l="105" buf="5" fadeout="0" type="1"/>
		<play_sound @l="106" buf="1" fadeout="0" type="1"/>
		<chgbg @l="109" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evkan08_02c" type="99"/>
		<msg @l="111" char="0" flags="0" index="64" line="111" name="花音"
		   text="「わぁ！　おち○ちんの先っぽからどんどんねばねばが\n:出てくるよ……はぁはぁ……垂れてきて、手袋に染みついちゃう」"
		   timeout="" voices="kan18411"/>
		<msg @l="113" char="0" flags="0" index="66" line="113" name="花音"
		   text="「ほら見て、手袋の指先がこんなにねちゃねちゃして、\n:糸引いちゃってるよぉ」" timeout=""
		   voices="kan18412"/>
		<play_sound @l="115" buf="1" fadeout="0" intro="-1" src="hse104" type="1"
		   volume="1."/>
		<play_sound @l="117" buf="5" fadeout="0" intro="0" src="kan80015" type="6"
		   volume="1."/>
		<msg @l="118" char="-1" flags="0" index="68" line="118" name=""
		   text="カウパーでねばねばになった指先を擦り合わせ、\n:糸を引いて見せる。" timeout="" voices=""/>
		<msg @l="119" char="-1" flags="0" index="70" line="119" name=""
		   text="直接触れないから付ける手袋。汚い物を触るときに付ける手袋。" timeout="" voices=""/>
		<msg @l="120" char="-1" flags="0" index="72" line="120" name=""
		   text="“手袋”というシチュのおかげで、ペニスがいつもより\n:グロテスクに見え、そのギャップがより興奮度合いを高めていく。"
		   timeout="" voices=""/>
		<play_sound @l="121" buf="5" fadeout="0" type="1"/>
		<play_sound @l="123" buf="1" fadeout="0" type="1"/>
		<chgbg @l="125" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evkan08_02d" type="99"/>
		<msg @l="126" char="0" flags="0" index="74" line="126" name="花音"
		   text="「手袋つけたままこんなことをしちゃうなんて。\n:んはぁ、はぁはぁ……私、えっちな花嫁だよね\x;hearts:\n:……んっ、んふぅっ」"
		   timeout="" voices="kan18413"/>
		<play_sound @l="128" buf="5" fadeout="0" intro="0" src="kan80016" type="6"
		   volume="1."/>
		<msg @l="129" char="11" flags="0" index="76" line="129" name="亮"
		   text="「こんなに可愛くてえっちな花嫁なら、はぁはぁ、\n:いくらでも大歓迎だ」" timeout="" voices=""/>
		<play_sound @l="130" buf="5" fadeout="0" type="1"/>
		<msg @l="132" char="0" flags="0" index="78" line="132" name="花音"
		   text="「えへへ、大歓迎されちゃった。じゃ、もっともーっと\n:歓迎されるために、こういうのはどうかな？」" timeout=""
		   voices="kan18414"/>
		<play_sound @l="134" buf="5" fadeout="0" intro="0" src="kan80015" type="6"
		   volume="1."/>
		<msg @l="135" char="-1" flags="0" index="80" line="135" name=""
		   text="そう言って更に身体を寄せ、露骨におっぱいを\n:押し当ててくる花音。" timeout="" voices=""/>
		<msg @l="136" char="-1" flags="0" index="82" line="136" name=""
		   text="そのままペニスを刺激する動きに合わせて柔らかい乳房を、\n:僕の身体にふにふにと擦りつけていく。" timeout=""
		   voices=""/>
		<msg @l="138" char="11" flags="0" index="84" line="138" name="亮"
		   text="「はぁはぁ、花音のおっぱいは大きくて柔らかくて最高だな。\n:それに乳首も硬くなって、凄くエッチだ」" timeout=""
		   voices=""/>
		<play_sound @l="139" buf="5" fadeout="0" type="1"/>
		<chgbg @l="141" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evkan08_02c" type="99"/>
		<msg @l="142" char="0" flags="0" index="86" line="142" name="花音"
		   text="「おっぱい好きだもんね。私も……はぁ、あぁん。\n:おっぱいの先っぽが、あなたの胸板に擦れて気持ちいいよぉ」" timeout=""
		   voices="kan18415"/>
		<play_sound @l="144" buf="5" fadeout="0" intro="0" src="kan80016" type="6"
		   volume="1."/>
		<msg @l="145" char="-1" flags="0" index="88" line="145" name=""
		   text="手を動かすと一緒に彼女の体も揺れ動き、バージンピンクの乳首が\n:僕の乳首とキスをする。" timeout="" voices=""/>
		<msg @l="146" char="-1" flags="0" index="90" line="146" name=""
		   text="お互い硬度を増した乳首同士が擦れ合い、快感を分け合っていく。" timeout="" voices=""/>
		<play_sound @l="147" buf="5" fadeout="0" type="1"/>
		<chgbg @l="149" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evkan08_02b" type="99"/>
		<msg @l="150" char="0" flags="0" index="92" line="150" name="花音"
		   text="「んはぁ！　あなたに気持ちよくなって欲しいのに……\n:んはぁ、はぁ、おっぱい感じてっ、ダメぇ！」" timeout=""
		   voices="kan18416"/>
		<chgbg @l="152" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evkan08_02d" type="99"/>
		<msg @l="153" char="0" flags="0" index="94" line="153" name="花音"
		   text="「ちゃんとおち○ちんにご奉仕して、りっぱなお嫁さんに\n:ならなくちゃいけないのにぃ、あひぃっ！」" timeout=""
		   voices="kan18417"/>
		<play_sound @l="155" buf="5" fadeout="0" intro="0" src="kan80018" type="6"
		   volume="1."/>
		<msg @l="156" char="-1" flags="0" index="96" line="156" name=""
		   text="熱い息を吐きながら、花音が身体を擦り付けてくる。" timeout="" voices=""/>
		<msg @l="157" char="-1" flags="0" index="98" line="157" name=""
		   text="まるで僕の身体を使ってオナニーしているようで、\n:股間にも熱がこもってくる。" timeout="" voices=""/>
		<play_sound @l="158" buf="5" fadeout="0" type="1"/>
		<msg @l="160" char="0" flags="0" index="100" line="160" name="花音"
		   text="「すごぉい……はぁはぁ、おち○ちん、カチカチの\n:パンパンになって鉄の棒みたいだよぉ」" timeout=""
		   voices="kan18418"/>
		<msg @l="162" char="0" flags="0" index="102" line="162" name="花音"
		   text="「だらしなくて気持ちよさそうな顔してる……はぁはぁ、\n:んはぁ、その顔すきぃ……だいしゅきぃ\x;heart:」"
		   timeout="" voices="kan18419"/>
		<play_sound @l="164" buf="5" fadeout="0" intro="0" src="kan80016" type="6"
		   volume="1."/>
		<msg @l="165" char="-1" flags="0" index="104" line="165" name=""
		   text="とろーんとした目をした花音が、すりすりと頬ずりしてくる。" timeout="" voices=""/>
		<msg @l="166" char="-1" flags="0" index="106" line="166" name=""
		   text="もう！　かわいくて仕方がない。どんだけ可愛いお嫁さんなんだ。" timeout="" voices=""/>
		<msg @l="168" char="11" flags="0" index="108" line="168" name="亮"
		   text="「僕も、花音をきもちよくしてやりたい……」" timeout="" voices=""/>
		<msg @l="170" char="-1" flags="0" index="110" line="170" name=""
		   text="首と舌をのばし、ぷるぷるの瑞々しい舌に吸い付く。" timeout="" voices=""/>
		<play_sound @l="171" buf="5" fadeout="0" type="1"/>
		<chgbg @l="173" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evkan08_02c" type="99"/>
		<msg @l="174" char="0" flags="0" index="112" line="174" name="花音"
		   text="「きしゅきたぁ！　ちゅっ、れろっ、ちゅる、ぺろっ、\n:んはぁ、ちゅ……」" timeout="" voices="kan18420"/>
		<chgbg @l="176" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evkan08_02a" type="99"/>
		<msg @l="177" char="0" flags="0" index="114" line="177" name="花音"
		   text="「しこしこしながらキス、ちゅっ……れろっ、ちゅううううっ\x;heart:」" timeout=""
		   voices="kan18421"/>
		<play_sound @l="179" buf="5" fadeout="0" intro="0" src="kan80031" type="6"
		   volume="1."/>
		<msg @l="180" char="11" flags="0" index="116" line="180" name="亮"
		   text="「そんなに強く吸ったら、舌が伸びちゃうぞ……\n:れろっ、ちゅぅっ」" timeout="" voices=""/>
		<play_sound @l="181" buf="5" fadeout="0" type="1"/>
		<chgbg @l="183" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evkan08_02d" type="99"/>
		<msg @l="184" char="0" flags="0" index="118" line="184" name="花音"
		   text="「ごめんね、でもきしゅ……ちゅっ、すきすぎて……\n:ぺろっ、れろろっ」" timeout="" voices="kan18422"/>
		<play_sound @l="186" buf="5" fadeout="0" intro="0" src="kan80030" type="6"
		   volume="1."/>
		<msg @l="187" char="-1" flags="0" index="120" line="187" name=""
		   text="はむはむと下唇を甘噛みしながら、僕の口内に唾液を\n:流していく花音。" timeout="" voices=""/>
		<msg @l="188" char="-1" flags="0" index="122" line="188" name=""
		   text="ほのかに甘い唾液の味が口の中いっぱいに広がり、\n:頭がくらくらしてしまう。" timeout="" voices=""/>
		<play_sound @l="189" buf="5" fadeout="0" type="1"/>
		<chgbg @l="191" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evkan08_02a" type="99"/>
		<msg @l="192" char="0" flags="0" index="124" line="192" name="花音"
		   text="「ちゅっ、ちゅうぅぅ……ふぁぁ、ちゅーしながらだと、\n:もっと気持ちいい……はぁ、ちゅぷ、んんぅ」" timeout=""
		   voices="kan18423"/>
		<msg @l="194" char="0" flags="0" index="126" line="194" name="花音"
