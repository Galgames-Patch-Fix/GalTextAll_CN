		<face src="fsas25b"/>
		<msg @l="26" char="2" flags="0" index="2" line="26" name="紗々"
		   text="「さすがセンパイです。こんなときでも冷静に接客出来るなんて」" timeout="" voices="sas10500"/>
		<face src="f"/>
		<msg @l="28" char="11" flags="0" index="4" line="28" name="亮"
		   text="「……紗々ちゃん、今休憩中だよね？」" timeout="" voices=""/>
		<face src="fsas02b"/>
		<msg @l="32" char="2" flags="0" index="6" line="32" name="紗々"
		   text="「はい、休憩してますよ」" timeout="" voices="sas10501"/>
		<face src=""/>
		<msg @l="34" char="-1" flags="0" index="8" line="34" name=""
		   text="レジ周りには、僕と休憩中の紗々ちゃんのふたりしか居ない。" timeout="" voices=""/>
		<face src=""/>
		<msg @l="35" char="-1" flags="0" index="10" line="35" name=""
		   text="ごく稀にお客さんが少ない時間帯があり、今がまさに\n:それなのだが……。" timeout="" voices=""/>
		<face src=""/>
		<msg @l="36" char="-1" flags="0" index="12" line="36" name=""
		   text="問題はそこではなく――。" timeout="" voices=""/>
		<play_sound @l="40" buf="0" fadeout="2500" type="0"/>
		<showmsg @l="41" duration="500" show="0"/>
		<loadimage @l="47" bg="0" fit="0" screen="1" src="evsas08_01a"/>
		<scroll @l="48" async="0" center_x="45" center_y="45" duration="0" flags="0"
