		   y="0" zoom="1."/>
		<chgscrn @l="49" duration="2500" flags="4608" flip="-1" flip_flags="0"
		   param="0" screen="1" type="10"/>
		<play_sound @l="50" buf="0" fadeout="500" intro="-1" src="bgm18" type="0"
		   volume="1."/>
		<msg @l="56" char="2" flags="0" index="14" line="56" name="紗々"
		   text="「……ふあぁ、センパイのおち○ちん。またピクンって、\n:跳ねましたぁ\x;hearts:」" timeout=""
		   voices="sas10502"/>
		<msg @l="58" char="11" flags="0" index="16" line="58" name="亮"
		   text="「そりゃあこの状況だし、そうなるのは当然だろ？」" timeout="" voices=""/>
		<msg @l="60" char="-1" flags="0" index="18" line="60" name=""
		   text="レジ前に立っている僕のすぐ下、レジカウンター内に身を潜める\n:紗々ちゃんがしゃがみ込んでいた。" timeout=""
		   voices=""/>
		<msg @l="61" char="-1" flags="0" index="20" line="61" name=""
		   text="ちなみにズボンは既にズリ降ろされ、下腹部から露出したペニスが彼女の目の前にある。" timeout="" voices=""/>
		<msg @l="62" char="-1" flags="0" index="22" line="62" name=""
		   text="さっきから彼女が話す度に吐息が肉棒に当たって、\n:落ち着かないったらありゃしない。" timeout="" voices=""/>
		<chgbg @l="64" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evsas08_01c" type="99"/>
		<msg @l="66" char="2" flags="0" index="24" line="66" name="紗々"
		   text="「あぁ、たくましいです。さっき学園でいじったときとは\n:また違って……」" timeout="" voices="sas10503"/>
		<msg @l="68" char="11" flags="0" index="26" line="68" name="亮"
		   text="「あ、あの、紗々ちゃん？　もう少し、大人しく静かに\n:休憩してもいいと思うんだ僕は」" timeout="" voices=""/>
		<msg @l="70" char="-1" flags="0" index="28" line="70" name=""
		   text="お客ゼロの状況なせいか、ヒソヒソ声でも大声に感じてしまう。" timeout="" voices=""/>
		<chgbg @l="72" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evsas08_01b" type="99"/>
		<msg @l="74" char="2" flags="0" index="30" line="74" name="紗々"
		   text="「……んふっ、それもそうですね。おくちが塞がれば、小さい声になると思いますよ？」" timeout=""
		   voices="sas10504"/>
		<msg @l="76" char="11" flags="0" index="32" line="76" name="亮"
		   text="「うん、なるほど。……って、それはつまりアレだよね！？」" timeout="" voices=""/>
		<msg @l="78" char="11" flags="0" index="34" line="78" name="亮"
		   text="「というか、誰も見てないからってさすがに店の中でモロだしは\n:マズ過ぎるだろ」" timeout="" voices=""/>
		<msg @l="80" char="2" flags="0" index="36" line="80" name="紗々"
		   text="「はぁはぁ、私だって、こうしておっぱい丸出しですし……\n:これって、おあいこですよね？」" timeout=""
		   voices="sas10505"/>
		<msg @l="84" char="-1" flags="0" index="38" line="84" name=""
		   text="カウンターの下へチラリと目をやると、ほどよく膨らんだ胸元や、ピンク色の乳首が見えた。" timeout="" voices=""/>
		<msg @l="86" char="2" flags="0" index="40" line="86" name="紗々"
		   text="「あ、またピクンってしました！　おち○ちんに血管まで浮き\n:上がって……センパイ、もしかして期待しちゃってます？」"
		   timeout="" voices="sas10506"/>
		<msg @l="88" char="11" flags="0" index="42" line="88" name="亮"
		   text="「そりゃそうだ、こんなの見せられたら当然だろ」" timeout="" voices=""/>
		<msg @l="90" char="2" flags="0" index="44" line="90" name="紗々"
		   text="「ふふ、私も期待しちゃってます。こんな場所でえっちなこと\n:しちゃうなんて、いやらしすぎて……はぁっ\x;heart:」"
		   timeout="" voices="sas10507"/>
		<msg @l="92" char="-1" flags="0" index="46" line="92" name=""
		   text="紗々ちゃんの顔が近付き、柔らかなほっぺがふにふにと肉棒の\n:裏側を時々かすめていく。" timeout="" voices=""/>
		<msg @l="93" char="-1" flags="0" index="48" line="93" name=""
		   text="そのたびにビクビク跳ねるペニス。" timeout="" voices=""/>
		<chgbg @l="97" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evsas08_01a" type="99"/>
		<msg @l="99" char="2" flags="0" index="50" line="99" name="紗々"
		   text="「ハァハァ……匂いがすごく濃くなって。あぁ、センパイの\n:匂い\x;heart:　……すぅ。くんかくんか\x;hearts:」"
		   timeout="" voices="sas10508"/>
		<msg @l="101" char="11" flags="0" index="52" line="101" name="亮"
		   text="「……おぅッ！？」" timeout="" voices=""/>
		<msg @l="103" char="-1" flags="0" index="54" line="103" name=""
		   text="膨らむタマ袋に鼻先まで押し当ててきちゃったよ、この娘ってば！" timeout="" voices=""/>
		<chgbg @l="105" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evsas08_01d" type="99"/>
		<msg @l="107" char="2" flags="0" index="56" line="107" name="紗々"
		   text="「ぁぁ……センパイの匂い……はぁはぁ、どんどんエッチな気分になっていっちゃうますぅ……\x;heart:」" timeout=""
		   voices="sas10509"/>
		<msg @l="109" char="-1" flags="0" index="58" line="109" name=""
		   text="嬉しそうな様子で匂いを嗅ぐ紗々ちゃん。" timeout="" voices=""/>
		<play_sound @l="111" buf="1" fadeout="0" intro="-1" src="se511" type="1"
		   volume="1."/>
		<msg @l="112" char="-1" flags="0" index="60" line="112" name=""
		   text="ついにはグロい男根だというのに、いとおしそうな顔をピトリと\n:当てて頬ずりしだした。" timeout="" voices=""/>
		<play_sound @l="113" buf="1" fadeout="0" type="1"/>
		<chgbg @l="115" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evsas08_01a" type="99"/>
		<msg @l="117" char="2" flags="0" index="62" line="117" name="紗々"
		   text="「このえっちな匂い……あぁ、脳まで蕩けちゃいそう\x;heart:」" timeout="" voices="sas10510"/>
		<msg @l="119" char="11" flags="0" index="64" line="119" name="亮"
		   text="「……ど、どこまでも大胆だなぁ」" timeout="" voices=""/>
		<msg @l="121" char="-1" flags="0" index="66" line="121" name=""
		   text="お昼のＨが手コキで終わってしまったせいか、紗々ちゃんの\n:テンションは最初からクライマックスだ。" timeout=""
		   voices=""/>
		<msg @l="122" char="-1" flags="0" index="68" line="122" name=""
		   text="彼女の興奮に煽られる格好で、僕のペニスも完全に\n:勃起してしまう。" timeout="" voices=""/>
		<play_sound @l="123" buf="1" fadeout="0" intro="-1" src="se511" type="1"
		   volume="1."/>
		<chgbg @l="126" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evsas08_02b" type="99"/>
		<msg @l="128" char="2" flags="0" index="70" line="128" name="紗々"
		   text="「あはっ……センパイのおち○ちんこんなに大きくなって……\n:うれしいっ」" timeout="" voices="sas10511"/>
		<play_sound @l="129" buf="1" fadeout="0" type="1"/>
		<chgbg @l="131" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evsas08_02a" type="99"/>
		<msg @l="134" char="2" flags="0" index="72" line="134" name="紗々"
		   text="「くんくん……学園でしたときの匂い、まだ少し残ってますね」" timeout="" voices="sas10512"/>
		<msg @l="136" char="11" flags="0" index="74" line="136" name="亮"
		   text="「一応ティッシュでふいたつもりだったんだが……」" timeout="" voices=""/>
		<msg @l="138" char="-1" flags="0" index="76" line="138" name=""
		   text="うう、なんだかすごく恥ずかしい。" timeout="" voices=""/>
		<chgbg @l="140" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evsas08_02d" type="99"/>
		<msg @l="142" char="2" flags="0" index="78" line="142" name="紗々"
		   text="「えへっ、ラッキーです……だってセンパイのえっちな匂い\n:大好きですから……はぁはぁ\x;heart:」" timeout=""
		   voices="sas10513"/>
		<msg @l="144" char="11" flags="17" index="80" line="144" name="亮"
		   text="「むしろ喜んでたのか！？」" timeout="" voices=""/>
		<shakescrn @l="145" amp="20" cycle="20" duration="500" flags="2" screen="0"/>
		<pause @l="146" flags="0" index="82" timeout=""/>
		<chgbg @l="148" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evsas08_02b" type="99"/>
		<msg @l="150" char="2" flags="0" index="83" line="150" name="紗々" text="「？？」"
		   timeout="" voices="sas10514"/>
		<msg @l="152" char="-1" flags="0" index="85" line="152" name=""
		   text="ペニスに頬ずりしながらキョトンと首をかしげる。" timeout="" voices=""/>
		<msg @l="153" char="-1" flags="0" index="87" line="153" name=""
		   text="怒張した肉棒の前で、チャンスとばかりに大きく見開いた\n:まんまるお目々の紗々ちゃん。" timeout="" voices=""/>
		<msg @l="155" char="2" flags="0" index="89" line="155" name="紗々"
		   text="「もし、匂いが気になるようでしたら、私のお口で綺麗にして\n:あげますよ」" timeout="" voices="sas10515"/>
		<msg @l="157" char="-1" flags="0" index="91" line="157" name=""
		   text="潤んだ瞳で見上げてくる紗々ちゃん。" timeout="" voices=""/>
		<msg @l="158" char="-1" flags="0" index="93" line="158" name=""
		   text="ここに来て彼女の“匂いフェチ”と“舐めるの大好き”属性。その\n:ふたつの性癖に同時に火がついてしまったようだ。"
		   timeout="" voices=""/>
		<chgbg @l="162" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evsas08_03c" type="99"/>
		<msg @l="164" char="2" flags="0" index="95" line="164" name="紗々"
		   text="「それじゃ、ぺろぺろさせて頂きますね……あーん、んむっ」" timeout="" voices="sas10516"/>
		<play_sound @l="166" buf="5" fadeout="0" intro="0" src="sas80000" type="6"
		   volume="1."/>
		<msg @l="167" char="-1" flags="0" index="97" line="167" name=""
		   text="ぬるっとした柔らかい舌がペニスに触れた。" timeout="" voices=""/>
		<play_sound @l="168" buf="5" fadeout="0" type="1"/>
		<msg @l="170" char="2" flags="0" index="99" line="170" name="紗々"
		   text="「ぺろぺろ、ちゅぅ……ちゅぐぅ、ふむぁ……ん、むちゅ……\n:れろれろぉ\x;heart:」" timeout=""
		   voices="sas10517"/>
		<play_sound @l="172" buf="5" fadeout="0" intro="0" src="sas80001" type="6"
		   volume="1."/>
		<msg @l="173" char="-1" flags="0" index="101" line="173" name=""
		   text="クリームを舐めるように、肉棒の表面を這うようになめらかに\n:舐めていく。" timeout="" voices=""/>
		<play_sound @l="174" buf="5" fadeout="0" type="1"/>
		<chgbg @l="176" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evsas08_03a" type="99"/>
		<msg @l="178" char="2" flags="0" index="103" line="178" name="紗々"
		   text="「れろ……ちゅむ、ぴちゃ……はぁ、ん、ちゅっ……レロレロ\n:レロぉ……ちゅばっ、センパぁイ、どうですかぁ？」" timeout=""
		   voices="sas10518"/>
		<play_sound @l="180" buf="5" fadeout="0" intro="0" src="sas80000" type="6"
		   volume="1."/>
		<msg @l="181" char="11" flags="0" index="105" line="181" name="亮"
		   text="「……くッ。どうって？」" timeout="" voices=""/>
		<msg @l="183" char="-1" flags="0" index="107" line="183" name=""
		   text="店の奥にオーナーがいるので、ヘタに大きな声は出せない。" timeout="" voices=""/>
		<msg @l="184" char="-1" flags="0" index="109" line="184" name=""
		   text="紗々ちゃんの頭をポンポンと優しく撫でて、気持ちいいよという\n:主旨を伝える。" timeout="" voices=""/>
		<play_sound @l="185" buf="5" fadeout="0" type="1"/>
		<chgbg @l="187" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evsas08_03c" type="99"/>
		<msg @l="189" char="2" flags="0" index="111" line="189" name="紗々"
		   text="「んふっ。よかったぁ、気持ちいいみたいですねぇ。あぁ……ん、ちゅ、れろぉ\x;heart:」" timeout=""
		   voices="sas10519"/>
		<play_sound @l="191" buf="5" fadeout="0" intro="0" src="sas80001" type="6"
		   volume="1."/>
		<msg @l="192" char="-1" flags="0" index="113" line="192" name=""
		   text="舌先は裏筋から亀頭部、雁首周辺にチロチロと這わせられ、\n:指先は陰嚢や肉茎全体に這わされる。" timeout=""
		   voices=""/>
		<msg @l="193" char="-1" flags="0" index="115" line="193" name=""
		   text="ペロペロ好きの才能をいかんなく発揮したその滑らかな舌使いによって、先端部からは早くも先走り汁が溢れて出した。" timeout=""
		   voices=""/>
		<play_sound @l="194" buf="5" fadeout="0" type="1"/>
		<msg @l="196" char="2" flags="0" index="117" line="196" name="紗々"
		   text="「ちゅ……ちゅる、あはっ、えっちで透明なオツユがどんどん\n:溢れてきました……んふっ、いい匂ぉい\x;heart:」"
		   timeout="" voices="sas10520"/>
		<play_sound @l="198" buf="5" fadeout="0" intro="0" src="sas80000" type="6"
		   volume="1."/>
		<msg @l="199" char="11" flags="0" index="119" line="199" name="亮"
		   text="「い、いや、こんなこと続けてたらヤバイって。客が来たら、\n:どうすりゃいいんだッ！？」" timeout="" voices=""/>
		<play_sound @l="200" buf="5" fadeout="0" type="1"/>
		<chgbg @l="202" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evsas08_03a" type="99"/>
		<msg @l="204" char="2" flags="0" index="121" line="204" name="紗々"
		   text="「私はいま、絶賛休憩中ですからぁ。んっ……センパイが\n:頑張ってくださいね」" timeout=""
		   voices="sas10521"/>
		<play_sound @l="206" buf="5" fadeout="0" intro="0" src="sas80001" type="6"
		   volume="1."/>
		<msg @l="207" char="-1" flags="0" index="123" line="207" name=""
		   text="くぅっ！　まったく、この小悪魔ちゃんめ！" timeout="" voices=""/>
		<msg @l="208" char="-1" flags="0" index="125" line="208" name=""
		   text="普段はおどおどしているのに、一旦火がつくとどこまでも大胆で\n:エロい子になりおって。" timeout="" voices=""/>
		<play_sound @l="209" buf="5" fadeout="0" type="1"/>
		<msg @l="211" char="2" flags="0" index="127" line="211" name="紗々"
		   text="「センパイならやりきれますよぉ……ぺろぺろ、くちゅ……っ、私、信じてますから、んちゅっ、れろっ」" timeout=""
		   voices="sas10522"/>
		<chgbg @l="213" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evsas08_03d" type="99"/>
		<msg @l="215" char="2" flags="0" index="129" line="215" name="紗々"
		   text="「そしたら、後でご褒美あげちゃいますね…っ\x;heart:　私のおま○こ、\n:好きにしていいですからぁ……\x;hearts:」"
		   timeout="" voices="sas10523"/>
		<play_sound @l="217" buf="5" fadeout="0" intro="0" src="sas80000" type="6"
		   volume="1."/>
		<msg @l="218" char="11" flags="0" index="131" line="218" name="亮"
		   text="「わかったよ。こうなったら、やってやるとも！」" timeout="" voices=""/>
		<msg @l="220" char="-1" flags="0" index="133" line="220" name=""
		   text="執拗になめ回される肉棒のテンションそのままに、吹っ切る\n:ことにした。" timeout="" voices=""/>
		<msg @l="221" char="-1" flags="0" index="135" line="221" name=""
		   text="紗々ちゃんのご褒美ま○このために、この場を乗り切ってやる！" timeout="" voices=""/>
		<play_sound @l="222" buf="5" fadeout="0" type="1"/>
		<chgbg @l="224" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evsas08_03a" type="99"/>
		<msg @l="226" char="2" flags="0" index="137" line="226" name="紗々"
		   text="「ふふ。がんばれー、センパイ\x;heart:　れろぉ、ちゅ……んっ。私が、応援してますよぉ\x;heart:　ん、ちゅ、れろろ\x;hearts:」"
		   timeout="" voices="sas10524"/>
		<chgbg @l="228" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evsas08_03c" type="99"/>
		<msg @l="230" char="2" flags="0" index="139" line="230" name="紗々"
		   text="「がんばれがんばれー\x;heart:　ちゅっ、ちゅる……っ、じゅる、\n:ちゅちゅっ\x;hearts:」" timeout=""
		   voices="sas10525"/>
		<play_sound @l="232" buf="5" fadeout="0" intro="0" src="sas80001" type="6"
		   volume="1."/>
		<msg @l="233" char="-1" flags="0" index="141" line="233" name=""
