<?xml version="1.0" encoding="utf-8"?>
<xscript>
	<code>
		<label @l="4" id="scene_1_02"/>
		<play_sound @l="8" buf="0" fadeout="2500" type="0"/>
		<showmsg @l="9" duration="500" show="0"/>
		<loadimage @l="14" bg="0" fit="0" screen="1" src="evusi09_01a"/>
		<scroll @l="15" async="0" center_x="45" center_y="45" duration="0" flags="0"
		   rev="0." screen="1" transparency="0" v0="0." v1="0." wait_flags="0" x="0"
		   y="0" zoom="1."/>
		<chgscrn @l="16" duration="2500" flags="4608" flip="-1" flip_flags="0"
		   param="0" screen="1" type="10"/>
		<play_sound @l="17" buf="0" fadeout="500" intro="-1" src="bgm18" type="0"
		   volume="1."/>
		<msg @l="24" char="11" flags="0" index="0" line="24" name="亮" text="「おおっ……」"
		   timeout="" voices=""/>
		<msg @l="26" char="-1" flags="0" index="2" line="26" name=""
		   text="窓の外、地面がだんだんと遠ざかっていく。" timeout="" voices=""/>
		<msg @l="27" char="-1" flags="0" index="4" line="27" name=""
		   text="夜闇に包まれているせいか、この狭い空間だけが、ぽつんと宙に\n:浮いているように感じてしまう。" timeout=""
		   voices=""/>
		<msg @l="29" char="11" flags="0" index="6" line="29" name="亮"
		   text="「にしても、ずいぶんゆっくり動くんですね」" timeout="" voices=""/>
		<msg @l="31" char="1" flags="0" index="8" line="31" name="汐"
		   text="「週末のこの時間は、通常の倍以上時間をかけて一周するんだよ。カップル向けの特別企画なんだって」" timeout=""
		   voices="usi13400"/>
		<msg @l="33" char="-1" flags="0" index="10" line="33" name=""
		   text="そういえば僕たちの他に並んでたお客さんも、もれなく\n:恋人同士ぽかったし、大盛況の企画なんだろう。" timeout=""
		   voices=""/>
		<chgbg @l="36" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evusi09_01f" type="99"/>
		<msg @l="38" char="1" flags="0" index="12" line="38" name="汐"
		   text="「あっ、あそこに見えるの銭湯の煙突じゃない？　ってことは……お店はあっちのほうかな？」" timeout=""
		   voices="usi13401"/>
		<msg @l="40" char="11" flags="0" index="14" line="40" name="亮"
		   text="「たぶん、そうじゃないかと。さすがに場所までは\n:わかりませんけど」" timeout="" voices=""/>
		<chgbg @l="42" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evusi09_01a" type="99"/>
		<msg @l="44" char="1" flags="0" index="16" line="44" name="汐"
		   text="「ベタな感想だけど、街が小さくなっていくと、ちょっと\n:神様っぽい気分になっちゃうかも」" timeout=""
		   voices="usi13402"/>
		<msg @l="46" char="11" flags="0" index="18" line="46" name="亮"
		   text="「見ろッ、人がゴミのようだ！　みたいなやつですよね」" timeout="" voices=""/>
		<chgbg @l="49" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evusi09_01f" type="99"/>
		<msg @l="52" char="1" flags="0" index="20" line="52" name="汐"
		   text="「そういうのはもうちょっと上に行ってからでないと」" timeout="" voices="usi13403"/>
		<msg @l="54" char="11" flags="0" index="22" line="54" name="亮"
		   text="「でも、意外と他のゴンドラの中って見えないんですね」" timeout="" voices=""/>
		<msg @l="57" char="-1" flags="0" index="24" line="57" name=""
		   text="ゴンドラ同士がわりと離れているし、夜の薄暗さも手伝ってか、思っていた以上にお隣さんの様子はわからない。" timeout=""
		   voices=""/>
		<chgbg @l="59" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evusi09_01a" type="99"/>
		<msg @l="61" char="1" flags="0" index="26" line="61" name="汐"
		   text="「ここの観覧車は誰からも邪魔されず、二人きりの時間を\n:楽しめるっていうのも売りのひとつだからね」" timeout=""
		   voices="usi13404"/>
		<msg @l="63" char="1" flags="0" index="28" line="63" name="汐"
		   text="「だからこそ、ゆっくり時間をかけて回るわけだし……でさ、\n:ここまで言えば私がこれに乗りたかった理由もわかるでしょ？」"
		   timeout="" voices="usi13405"/>
		<msg @l="64" char="11" flags="0" index="30" line="64" name="亮"
		   text="「最後のお楽しみにとっといたワケがよくわかりました」" timeout="" voices=""/>
		<msg @l="66" char="1" flags="0" index="32" line="66" name="汐" text="「えへへ」"
		   timeout="" voices="usi13406"/>
		<play_sound @l="68" buf="1" fadeout="0" intro="-1" src="se511" type="1"
		   volume="0.6"/>
		<msg @l="70" char="-1" flags="0" index="34" line="70" name=""
		   text="はにかむように微笑んだ朝霞さんの手が、僕の膝に触れる。" timeout="" voices=""/>
		<chgbg @l="72" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evusi09_01b" type="99"/>
		<msg @l="74" char="1" flags="0" index="36" line="74" name="汐"
		   text="「なら……まずはキスして\x;heart:」" timeout="" voices="usi13407"/>
		<play_sound @l="76" buf="1" fadeout="0" type="1"/>
		<msg @l="78" char="-1" flags="0" index="38" line="78" name=""
		   text="言うが早いか、唇を捧げるように軽く突き出す。" timeout="" voices=""/>
		<msg @l="79" char="-1" flags="0" index="40" line="79" name=""
		   text="だから僕も、そっと腰を浮かせてすぐさまそれに応えた。" timeout="" voices=""/>
		<chgbg @l="81" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evusi09_01c" type="99"/>
		<msg @l="83" char="1" flags="0" index="42" line="83" name="汐"
		   text="「んぅ、ちゅっ……ふぅ、んっ……れろ、んむっ」" timeout="" voices="usi13408"/>
		<chgbg @l="85" duration="1000" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evusi09_02c" type="99"/>
		<play_sound @l="87" buf="5" fadeout="0" intro="0" src="usi80029" type="6"
		   volume="1."/>
		<msg @l="88" char="-1" flags="0" index="44" line="88" name=""
		   text="個室とはいえ外なわけだし軽く触れる程度かと思っていたら、\n:彼女は僕の服を掴み、しっかりと舌を絡めてきた。" timeout=""
		   voices=""/>
		<play_sound @l="89" buf="5" fadeout="0" type="1"/>
		<chgbg @l="91" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evusi09_02b" type="99"/>
		<msg @l="93" char="1" flags="0" index="46" line="93" name="汐"
		   text="「ふぅ、んぅ……ねぇ？　もっといっぱいキスして\x;hearts:」" timeout="" voices="usi13409"/>
		<msg @l="95" char="11" flags="0" index="48" line="95" name="亮"
		   text="「でも、こんなところで大丈夫ですかね？」" timeout="" voices=""/>
		<msg @l="97" char="1" flags="0" index="50" line="97" name="汐"
		   text="「人目なら大丈夫。ここの観覧車って、さっきも言ったように\n:ゴンドラ同士はもちろん外からも覗かれないから」" timeout=""
		   voices="usi13410"/>
		<msg @l="99" char="11" flags="0" index="52" line="99" name="亮"
		   text="「たしかに見づらい感じはしましたけど、そういうもの\n:なんですか？」" timeout="" voices=""/>
		<chgbg @l="101" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evusi09_02a" type="99"/>
		<msg @l="103" char="1" flags="0" index="54" line="103" name="汐"
		   text="「うん。例の本を貸してくれたお友達が教えてくれたんだ」" timeout="" voices="usi13411"/>
		<msg @l="105" char="1" flags="0" index="56" line="105" name="汐"
		   text="「だからキスはもちろん……それ以上のことも下に戻るまでは\n:したい放題だよ\x;heart:」" timeout=""
		   voices="usi13412"/>
		<play_sound @l="109" buf="1" fadeout="0" intro="-1" src="se560" type="1"
		   volume="0.6"/>
		<showmsg @l="110" duration="500" show="0"/>
		<scrlangle @l="111" duration="2000" view="0" y="-1300"/>
		<msg @l="113" char="-1" flags="0" index="58" line="113" name=""
		   text="小さな声で囁くと同時に、彼女がそっと両足を開く。\n:いまの話を聞いていなければ、大慌てで止めていたところだ。" timeout=""
		   voices=""/>
		<play_sound @l="115" buf="1" fadeout="0" type="1"/>
		<msg @l="117" char="11" flags="0" index="60" line="117" name="亮"
		   text="「うおおぉ……っ、大胆すぎる！！」" timeout="" voices=""/>
		<msg @l="119" char="1" flags="0" index="62" line="119" name="汐"
		   text="「昨日、寝るまで……ううん、寝ても私とひとつになったまま\n:だったキミが、そういうこと言うんだ？」" timeout=""
		   voices="usi13413"/>
		<msg @l="121" char="11" flags="0" index="64" line="121" name="亮"
		   text="「それを言われると、なにも言い返せませんね」" timeout="" voices=""/>
		<msg @l="123" char="-1" flags="0" index="66" line="123" name=""
		   text="現にあれだけ射精して今日は終日賢者タイムだなんて思って\n:いたのに、さっきムラムラしちゃったし。" timeout=""
		   voices=""/>
		<scrlangle @l="126" duration="2000" view="0" y="0"/>
		<msg @l="128" char="1" flags="0" index="68" line="128" name="汐"
		   text="「エッチがダメならキスだけでもいいから。初デートの記念に、いっぱいしよ？　……足は閉じないけどね」" timeout=""
		   voices="usi13414"/>
		<msg @l="130" char="-1" flags="0" index="70" line="130" name=""
		   text="悪戯っぽく微笑んで、開いた足の膝で僕の膝を\n:ちょんちょんとつつく。" timeout="" voices=""/>
		<chgbg @l="132" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evusi09_02b" type="99"/>
		<msg @l="134" char="1" flags="0" index="72" line="134" name="汐"
		   text="「それとも……こういう場所でエッチをしたいって\n:彼氏に求めちゃうような子はイヤ？」" timeout=""
		   voices="usi13415"/>
		<msg @l="136" char="11" flags="0" index="74" line="136" name="亮"
		   text="「まさか！　そこまで求めてもらえるなんて男冥利に尽きますよ」" timeout="" voices=""/>
		<msg @l="138" char="-1" flags="0" index="76" line="138" name=""
		   text="今日のデート、これまで二人きりの機会は何度もあった。" timeout="" voices=""/>
		<msg @l="139" char="-1" flags="0" index="78" line="139" name=""
		   text="そのとき朝霞さんがモーションをかけてこなかったのは、確実なチャンスが訪れることを知っていたからだろう。" timeout=""
		   voices=""/>
		<msg @l="140" char="-1" flags="0" index="80" line="140" name=""
		   text="そしてそのチャンスこそ、まさに今というわけだ。" timeout="" voices=""/>
		<msg @l="142" char="11" flags="0" index="82" line="142" name="亮"
		   text="「ただ、下に戻るまでに終わらなかったらどうしようって\n:思いまして」" timeout="" voices=""/>
		<msg @l="144" char="-1" flags="0" index="84" line="144" name=""
		   text="気づけば一晩中愛し合っちゃった僕たちにとって、いくらのんびりとはいえ、観覧車一周という時間は少し心許ない。" timeout=""
		   voices=""/>
		<chgbg @l="146" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evusi09_02a" type="99"/>
		<msg @l="148" char="1" flags="0" index="86" line="148" name="汐"
		   text="「そこは、お姉ちゃんと力をあわせて。ね？」" timeout="" voices="usi13416"/>
		<msg @l="150" char="11" flags="0" index="88" line="150" name="亮"
		   text="「一晩中の次は、ファストセックスですか」" timeout="" voices=""/>
		<msg @l="152" char="1" flags="0" index="90" line="152" name="汐"
		   text="「色々な体験ができてステキでしょ？」" timeout="" voices="usi13417"/>
		<msg @l="154" char="11" flags="0" index="92" line="154" name="亮"
		   text="「ははっ、たしかに……それじゃあ触りますね」" timeout="" voices=""/>
		<play_sound @l="156" buf="1" fadeout="0" intro="-1" src="se511" type="1"
		   volume="0.5"/>
		<scrlangle @l="157" duration="2000" view="0" y="-1300"/>
		<msg @l="159" char="1" flags="0" index="94" line="159" name="汐"
		   text="「はぁ、んんっ……」" timeout="" voices="usi13418"/>
		<play_sound @l="161" buf="1" fadeout="0" type="1"/>
		<play_sound @l="163" buf="5" fadeout="0" intro="0" src="usi80015" type="6"
		   volume="1."/>
		<msg @l="164" char="-1" flags="0" index="96" line="164" name=""
		   text="小さな吐息は妙に艶やかで、つい腰が浮きそうになる。" timeout="" voices=""/>
		<play_sound @l="165" buf="5" fadeout="0" type="1"/>
		<msg @l="167" char="1" flags="0" index="98" line="167" name="汐"
		   text="「もっと大胆にさわっても平気だよ？」" timeout="" voices="usi13419"/>
		<msg @l="171" char="11" flags="0" index="100" line="171" name="亮"
		   text="「わかってはいるんですけど、まだ状況に慣れてないというか」" timeout="" voices=""/>
		<msg @l="173" char="-1" flags="0" index="102" line="173" name=""
		   text="誰も見ていないとわかっていても、ドキドキしてしまう。" timeout="" voices=""/>
		<scrlangle @l="175" duration="2000" view="0" y="0"/>
		<msg @l="177" char="11" flags="0" index="104" line="177" name="亮"
		   text="「それになんかこの状況……痴漢してるみたいで緊張します」" timeout="" voices=""/>
		<msg @l="179" char="1" flags="0" index="106" line="179" name="汐"
		   text="「痴漢されたことはないけど……でも、キミにだったら、そういうことをされてみたいかも」" timeout=""
		   voices="usi13420"/>
		<msg @l="183" char="1" flags="0" index="108" line="183" name="汐"
		   text="「もちろん、痴漢したいぐらい魅力的ならだけど」" timeout="" voices="usi13421"/>
		<msg @l="185" char="11" flags="0" index="110" line="185" name="亮"
		   text="「そりゃ、言うまでもないですよ」" timeout="" voices=""/>
		<chgbg @l="187" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evusi09_02b" type="99"/>
		<msg @l="189" char="1" flags="0" index="112" line="189" name="汐"
		   text="「なら、もっと……ね？　スカートの中に手を入れて、\n:あそこさわってほしいな」" timeout=""
		   voices="usi13422"/>
		<showmsg @l="195" duration="500" show="0"/>
		<scrlangle @l="196" duration="2000" view="0" y="-1300"/>
		<msg @l="199" char="-1" flags="0" index="114" line="199" name=""
		   text="僕の手がちょうど無理なく入るぐらい開いている足の付け根には、熱気と湿り気、なにより女の子の匂いが満ちている。" timeout=""
		   voices=""/>
		<msg @l="201" char="11" flags="0" index="116" line="201" name="亮"
		   text="「……もしかして、もう濡れてます？」" timeout="" voices=""/>
		<msg @l="203" char="1" flags="0" index="118" line="203" name="汐"
		   text="「気になるなら、さわって確かめて」" timeout="" voices="usi13423"/>
		<play_sound @l="205" buf="1" fadeout="0" intro="-1" src="se560" type="1"
		   volume="1."/>
		<play_sound @l="206" buf="1" fadeout="2000" type="1"/>
		<msg @l="208" char="-1" flags="0" index="120" line="208" name=""
		   text="どうぞ、というふうに朝霞さんが足をさらに少し開く。" timeout="" voices=""/>
		<msg @l="210" char="11" flags="0" index="122" line="210" name="亮"
		   text="「そんなに開かれると、股下に潜り込みたくなっちゃいます」" timeout="" voices=""/>
		<chgbg @l="212" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evusi09_02a" type="99"/>
		<msg @l="214" char="1" flags="0" index="124" line="214" name="汐"
		   text="「くすっ。私はそれでも構わないけど……あっ、でも、\n:ちゅーできないのは寂しいかも」" timeout=""
		   voices="usi13424"/>
		<msg @l="216" char="1" flags="0" index="126" line="216" name="汐"
		   text="「だから、いざというときのために先払いでたっぷり――」" timeout="" voices="usi13425"/>
		<chgbg @l="218" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evusi09_02c" type="99"/>
		<msg @l="220" char="1" flags="0" index="128" line="220" name="汐"
		   text="「んまっ……はぁ、あむむ、ちゅっ……ふぅ、はぁ、れろれろっ、ちゅむむ、れーろ」" timeout=""
		   voices="usi13426"/>
		<chgbg @l="222" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evusi09_02a" type="99"/>
		<msg @l="224" char="1" flags="0" index="130" line="224" name="汐"
		   text="「はふっ、んふふっ……。私もキミも、初めての頃に比べて、\n:だいぶキスが上手になったよね」" timeout=""
		   voices="usi13427"/>
		<msg @l="226" char="11" flags="0" index="132" line="226" name="亮"
		   text="「昨日は夜通ししてたようなもんですからね、レベルアップ\n:してると思いますよ」" timeout="" voices=""/>
		<msg @l="228" char="1" flags="0" index="134" line="228" name="汐"
		   text="「すればするだけ上手になる、かぁ……。ケーキ作りと一緒だね。　くすっ……れろ、ちゅっ、ちゅむ」" timeout=""
		   voices="usi13428"/>
		<chgbg @l="230" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evusi09_02c" type="99"/>
		<msg @l="232" char="1" flags="0" index="136" line="232" name="汐"
		   text="「それなら、あそこももっと、いじって……はぐっ、はあっ、あ、そうっ……んむぅ、ちゅっ……はぁ、はあんっ」" timeout=""
		   voices="usi13429"/>
		<loadimage @l="234" bg="0" fit="0" screen="1" src="evusi09_02b"/>
		<scroll @l="235" async="0" center_x="0" center_y="1300" duration="0"
		   flags="0" rev="0." screen="1" transparency="0" v0="0." v1="0."
		   wait_flags="0" x="0" y="0" zoom="1."/>
		<chgscrn @l="236" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" type="99"/>
		<play_sound @l="238" buf="5" fadeout="0" intro="0" src="usi80028" type="6"
		   volume="1."/>
		<msg @l="239" char="-1" flags="0" index="138" line="239" name=""
		   text="狭い空間で見つめ合ってキスを繰り返しながら、下着の上から\n:しっかりと秘裂をなぞっていく。" timeout=""
		   voices=""/>
		<msg @l="241" char="11" flags="0" index="140" line="241" name="亮"
		   text="「えっと、そろそろ――」" timeout="" voices=""/>
		<play_sound @l="242" buf="5" fadeout="0" type="1"/>
		<chgbg @l="244" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evusi09_02b" type="99"/>
		<msg @l="246" char="1" flags="0" index="142" line="246" name="汐"
		   text="「うんっ、そろそろ……はぐっ、ふぅううんっ\x;hearts:」" timeout="" voices="usi13430"/>
		<loadimage @l="251" bg="0" fit="0" screen="1" src="evusi09_02b"/>
		<scroll @l="252" async="0" center_x="0" center_y="1300" duration="0"
		   flags="0" rev="0." screen="1" transparency="0" v0="0." v1="0."
		   wait_flags="0" x="0" y="0" zoom="1."/>
		<chgscrn @l="253" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" type="99"/>
		<play_sound @l="255" buf="1" fadeout="0" intro="-1" src="hse104" type="1"
		   volume="0.7"/>
		<loadimage @l="256" bg="0" fit="0" screen="1" src="evusi09_03b"/>
		<scroll @l="257" async="0" center_x="0" center_y="1300" duration="0"
		   flags="0" rev="0." screen="1" transparency="0" v0="0." v1="0."
