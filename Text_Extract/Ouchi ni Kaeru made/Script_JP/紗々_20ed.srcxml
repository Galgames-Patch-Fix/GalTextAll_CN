		   voices=""/>
		<msg @l="393" char="11" flags="0" index="175" line="393" name="亮"
		   text="「あははっ！　甘い……美味しい。こんな美味しかったんだな」" timeout="" voices=""/>
		<msg @l="395" char="2" flags="0" index="177" line="395" name="紗々"
