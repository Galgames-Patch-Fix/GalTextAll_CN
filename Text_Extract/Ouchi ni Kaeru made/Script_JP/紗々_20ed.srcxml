		   voices="sas15623"/>
		<msg @l="397" char="-1" flags="0" index="179" line="397" name=""
		   text="涙目で、にっこりと微笑む紗々ちゃん。\n:それは、まるで家族のようにあたたかくて。" timeout="" voices=""/>
		<msg @l="398" char="-1" flags="0" index="181" line="398" name=""
