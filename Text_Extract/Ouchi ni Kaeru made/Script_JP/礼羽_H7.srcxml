		<msg @l="123" char="-1" flags="0" index="78" line="123" name=""
		   text="スルリと滑るようなそれは、僕の安物なんかと全然違う\n:心地よい触り心地だった。" timeout="" voices=""/>
		<msg @l="125" char="11" flags="0" index="80" line="125" name="亮"
		   text="（さすがお嬢様！　パンツまで高級品とは……！！）" timeout="" voices=""/>
		<play_sound @l="126" buf="5" fadeout="0" type="1"/>
		<chgbg @l="128" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evrai18_05a" type="99"/>
		<msg @l="129" char="3" flags="0" index="82" line="129" name="礼羽"
		   text="「どうかしら？　そこまで染みこんでいるかしら？」" timeout="" voices="rai10611"/>
		<msg @l="131" char="11" flags="0" index="84" line="131" name="亮"
