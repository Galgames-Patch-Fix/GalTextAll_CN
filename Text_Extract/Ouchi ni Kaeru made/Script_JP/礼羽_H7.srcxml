		   param="16" screen="1" src="evrai18_01e" type="99"/>
		<msg @l="59" char="3" flags="0" index="24" line="59" name="礼羽"
		   text="「そうよ。なにか不満でも？」" timeout="" voices="rai10603"/>
		<msg @l="61" char="-1" flags="0" index="26" line="61" name=""
		   text="便座の上に陣取っている礼羽が、じっと僕を見下ろす。" timeout="" voices=""/>
		<msg @l="63" char="11" flags="0" index="28" line="63" name="亮"
		   text="（ヤベ、この見下ろし方。完璧すぎますお嬢様！）" timeout="" voices=""/>
		<msg @l="65" char="-1" flags="0" index="30" line="65" name=""
		   text="なんて高飛車な感じなんだ！　ハマリすぎてて怖いぐらいだぜ。" timeout="" voices=""/>
		<chgbg @l="66" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evrai18_01a" type="99"/>
		<msg @l="67" char="3" flags="0" index="32" line="67" name="礼羽"
		   text="「さあ、早く拭きなさい」" timeout="" voices="rai10604"/>
		<play_sound @l="69" buf="1" fadeout="0" intro="-1" src="se511" type="1"
		   volume="0.5"/>
		<msg @l="71" char="-1" flags="0" index="34" line="71" name=""
		   text="陶器の便座の上で、白く長い脚が、Ｍの字にすらりと広げられる。" timeout="" voices=""/>
		<msg @l="73" char="11" flags="0" index="36" line="73" name="亮"
