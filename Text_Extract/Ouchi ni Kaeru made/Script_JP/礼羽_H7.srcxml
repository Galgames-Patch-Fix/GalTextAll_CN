		<msg @l="174" char="11" flags="0" index="116" line="174" name="亮"
		   text="「う～ん」" timeout="" voices=""/>
		<msg @l="176" char="-1" flags="0" index="118" line="176" name=""
		   text="僕はあわてて、親指の腹を秘所に押し当てる。" timeout="" voices=""/>
		<msg @l="177" char="-1" flags="0" index="120" line="177" name=""
		   text="ぐにぐにと、中の肉が変形するのがわかった。" timeout="" voices=""/>
		<chgbg @l="179" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evrai18_05d" type="99"/>
		<msg @l="180" char="3" flags="0" index="122" line="180" name="礼羽"
		   text="「っ！　ちょっと、力の入れすぎではなくて？」" timeout="" voices="rai10618"/>
		<msg @l="182" char="11" flags="0" index="124" line="182" name="亮"
		   text="「あ、これは失礼しました。大丈夫です。ジュースは\n:こぼれていないようです」" timeout="" voices=""/>
		<chgbg @l="184" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evrai18_05a" type="99"/>
		<msg @l="185" char="3" flags="0" index="126" line="185" name="礼羽"
		   text="「そんなはずはないわ。なんだかそこ、湿っている気がするの。\n:よく見てちょうだい」" timeout=""
		   voices="rai10619"/>
		<play_sound @l="187" buf="1" fadeout="0" intro="-1" src="hse101" type="1"
		   volume="0.4"/>
		<msg @l="189" char="-1" flags="0" index="128" line="189" name=""
		   text="言葉通りに指先にじんわりと熱い湿り気を感じる。" timeout="" voices=""/>
		<play_sound @l="191" buf="1" fadeout="0" type="1"/>
		<msg @l="192" char="3" flags="0" index="130" line="192" name="礼羽"
		   text="「っ、ど、どう……？　濡れて、いるでしょう？」" timeout="" voices="rai10620"/>
		<play_sound @l="194" buf="5" fadeout="0" intro="0" src="rai80016" type="6"
		   volume="1."/>
		<msg @l="195" char="11" flags="0" index="132" line="195" name="亮"
		   text="「は、はい。たしかに……でもこれ、\n:ジュースじゃないような――」" timeout="" voices=""/>
		<play_sound @l="196" buf="5" fadeout="0" type="1"/>
		<chgbg @l="198" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evrai18_05e" type="99"/>
		<msg @l="199" char="3" flags="0" index="134" line="199" name="礼羽"
		   text="「ジュースよ！　他になにがあるの？」" timeout="" voices="rai10621"/>
		<msg @l="201" char="11" flags="0" index="136" line="201" name="亮"
		   text="「失礼しました、たしかにこれはジュースです。\n:すぐにお拭きしませんと」" timeout="" voices=""/>
		<msg @l="203" char="3" flags="0" index="138" line="203" name="礼羽"
		   text="「そうよ。早くなさい……っ、んぁ……\x;hearts:」" timeout="" voices="rai10622"/>
		<msg @l="205" char="11" flags="0" index="140" line="205" name="亮"
		   text="「では失礼して」" timeout="" voices=""/>
		<play_sound @l="207" buf="5" fadeout="0" intro="0" src="rai80015" type="6"
		   volume="1."/>
		<msg @l="208" char="-1" flags="0" index="142" line="208" name=""
		   text="押し込んだ親指の周囲をじっと見る。" timeout="" voices=""/>
		<msg @l="209" char="-1" flags="0" index="144" line="209" name=""
		   text="指先の周囲のシルクに、ゆっくりとシミが広がっていく。" timeout="" voices=""/>
		<chgbg @l="211" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evrai18_06a" type="99"/>
		<msg @l="213" char="11" flags="0" index="146" line="213" name="亮"
		   text="「お嬢様、さらに濡れてきましたよ」" timeout="" voices=""/>
		<play_sound @l="214" buf="5" fadeout="0" type="1"/>
		<chgbg @l="216" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evrai18_06e" type="99"/>
		<msg @l="217" char="3" flags="0" index="148" line="217" name="礼羽"
		   text="「濡れてきたんじゃなくって、元々濡れているの！\n:そこは間違えないで……」" timeout=""
		   voices="rai10623"/>
		<msg @l="219" char="11" flags="0" index="150" line="219" name="亮"
		   text="「う～ん、でも中からしみ出てきてるような？」" timeout="" voices=""/>
		<play_sound @l="221" buf="5" fadeout="0" intro="0" src="rai80016" type="6"
		   volume="1."/>
		<msg @l="222" char="-1" flags="0" index="152" line="222" name=""
		   text="指先を巧みに操って、下着の上から秘所を刺激する。" timeout="" voices=""/>
		<msg @l="223" char="-1" flags="0" index="154" line="223" name=""
		   text="それに応えるようにシミはどんどんと広がり、下着の色を\n:濃く塗り替えていく。" timeout="" voices=""/>
		<play_sound @l="224" buf="5" fadeout="0" type="1"/>
		<chgbg @l="226" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evrai18_06d" type="99"/>
		<msg @l="227" char="3" flags="0" index="156" line="227" name="礼羽"
		   text="「んんっ、あ、はぁ……なんだかシミが広がってるみたいよ。\n:ちゃんと拭いているの？」" timeout=""
		   voices="rai10624"/>
		<play_sound @l="229" buf="5" fadeout="0" intro="0" src="rai80015" type="6"
		   volume="1."/>
