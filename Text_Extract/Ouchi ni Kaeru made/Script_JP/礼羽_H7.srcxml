		   volume="0.5"/>
		<msg @l="97" char="-1" flags="0" index="56" line="97" name=""
		   text="言われるがままに手を伸ばし下着に触れる。" timeout="" voices=""/>
		<play_sound @l="99" buf="1" fadeout="0" type="1"/>
		<chgbg @l="100" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evrai18_05d" type="99"/>
		<msg @l="101" char="3" flags="0" index="58" line="101" name="礼羽"
		   text="「んっ……」" timeout="" voices="rai10607"/>
		<msg @l="103" char="11" flags="0" index="60" line="103" name="亮"
		   text="「あ、ごめ……申し訳ありません」" timeout="" voices=""/>
		<chgbg @l="105" duration="300" flags="256" flip="-1" flip_flags="0"
