		<play_sound @l="140" buf="1" fadeout="0" intro="-1" src="se511" type="1"
		   volume="0.4"/>
		<msg @l="142" char="-1" flags="0" index="92" line="142" name=""
		   text="布の向こうの秘所の形を確認するかのように、指の腹でなぞった。" timeout="" voices=""/>
