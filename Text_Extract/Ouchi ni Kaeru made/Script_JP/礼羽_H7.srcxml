		<play_sound @l="159" buf="5" fadeout="0" intro="0" src="rai80016" type="6"
		   volume="1."/>
		<msg @l="160" char="-1" flags="0" index="104" line="160" name=""
		   text="下腹部のなだらかな丘の下には、慎ましく\n:閉ざされた陰唇があった。" timeout="" voices=""/>
		<msg @l="161" char="-1" flags="0" index="106" line="161" name=""
		   text="下着越しに指で軽く押すと、ぷにぷにと押し返してくる。" timeout="" voices=""/>
		<msg @l="163" char="11" flags="0" index="108" line="163" name="亮"
		   text="（すごい。あらためてさわると、ココって柔らかいんだな）" timeout="" voices=""/>
		<play_sound @l="164" buf="5" fadeout="0" type="1"/>
		<chgbg @l="166" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evrai18_05a" type="99"/>
		<msg @l="167" char="3" flags="0" index="110" line="167" name="礼羽"
		   text="「……いつまでそうしているの？　その、どうなの？」" timeout="" voices="rai10616"/>
		<msg @l="169" char="11" flags="0" index="112" line="169" name="亮"
		   text="「？？　とおっしゃいますと？」" timeout="" voices=""/>
		<chgbg @l="171" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evrai18_05e" type="99"/>
		<msg @l="172" char="3" flags="0" index="114" line="172" name="礼羽"
