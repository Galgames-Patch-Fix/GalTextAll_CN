		<msg @l="106" char="3" flags="0" index="62" line="106" name="礼羽"
		   text="「いいのよ、続けて。ほら……」" timeout="" voices="rai10608"/>
		<msg @l="108" char="-1" flags="0" index="64" line="108" name=""
		   text="まるで見せつけるように、礼羽が股間を突き出してくる。" timeout="" voices=""/>
		<chgbg @l="110" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evrai18_05e" type="99"/>
		<msg @l="111" char="3" flags="0" index="66" line="111" name="礼羽"
		   text="「こほん！　私に見とれるのはわかるけど、\n:手を動かしてもらえるかしら？」" timeout=""
		   voices="rai10609"/>
		<msg @l="113" char="11" flags="0" index="68" line="113" name="亮"
		   text="「あ、うん。いや、はい！」" timeout="" voices=""/>
		<msg @l="115" char="-1" flags="0" index="70" line="115" name=""
		   text="今の僕は執事。とにかくジュースを拭き取るのが先決だ。" timeout="" voices=""/>
		<msg @l="116" char="-1" flags="0" index="72" line="116" name=""
