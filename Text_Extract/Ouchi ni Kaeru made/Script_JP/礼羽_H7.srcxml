		<play_sound @l="75" buf="1" fadeout="0" type="1"/>
		<msg @l="77" char="-1" flags="0" index="38" line="77" name=""
		   text="思わず生唾を呑み込む。" timeout="" voices=""/>
		<msg @l="78" char="-1" flags="0" index="40" line="78" name=""
		   text="スカートは大きくはだけられ、下着は丸見えだ。" timeout="" voices=""/>
		<chgbg @l="80" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evrai18_02a" type="99"/>
		<msg @l="81" char="3" flags="0" index="42" line="81" name="礼羽"
		   text="「……どうしたの？　シミになったら困るのだけど」" timeout="" voices="rai10605"/>
		<msg @l="83" char="-1" flags="0" index="44" line="83" name=""
		   text="ぶっきらぼうだが、語尾は熱っぽい。まるで僕を試すかのような\n:口調だった。" timeout="" voices=""/>
		<msg @l="85" char="11" flags="0" index="46" line="85" name="亮"
		   text="「……はい。それでは失礼して」" timeout="" voices=""/>
		<msg @l="87" char="-1" flags="0" index="48" line="87" name=""
		   text="ミントグリーンの下着は、デザインこそシンプルだが、\n:素材はいかにも高級そうだった。" timeout="" voices=""/>
		<msg @l="88" char="-1" flags="0" index="50" line="88" name=""
		   text="Ｍ字開脚という姿勢のせいか、秘所を覆う部分が\n:こんもりと膨らんでいるのが妙にエロい。" timeout="" voices=""/>
		<chgbg @l="90" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evrai18_02e" type="99"/>
		<msg @l="91" char="3" flags="0" index="52" line="91" name="礼羽"
