<?xml version="1.0" encoding="utf-8"?>
<xscript>
	<code>
		<face src=""/>
		<msg @l="7" char="-1" flags="0" index="0" line="7" name=""
		   text="スイッチが入ってしまったらしい紗々ちゃんに連れられて、\n:近くの茂みへと押し倒された。" timeout="" voices=""/>
		<label @l="15" id="scene_2_01"/>
		<play_sound @l="20" buf="0" fadeout="2500" type="0"/>
		<showmsg @l="21" duration="500" show="0"/>
		<loadimage @l="26" bg="0" fit="0" screen="1" src="evsas07_01a"/>
		<scroll @l="27" async="0" center_x="45" center_y="45" duration="0" flags="0"
		   rev="0." screen="1" transparency="0" v0="0." v1="0." wait_flags="0" x="0"
		   y="0" zoom="1."/>
		<chgscrn @l="28" duration="2500" flags="4608" flip="-1" flip_flags="0"
		   param="0" screen="1" type="10"/>
		<play_sound @l="29" buf="0" fadeout="500" intro="-1" src="bgm18" type="0"
		   volume="1."/>
		<play_sound @l="39" buf="5" fadeout="0" intro="0" src="sas80015" type="6"
		   volume="1."/>
		<msg @l="40" char="-1" flags="0" index="2" line="40" name=""
		   text="周りを茂みに囲まれた芝生の上に倒れ込むように\n:重なり合う僕たち。" timeout="" voices=""/>
		<msg @l="41" char="-1" flags="0" index="4" line="41" name=""
		   text="紗々ちゃんが僕の上に乗り、ふくらみかけたペニスを膝裏で挟み、ちょんちょんとさわってくる。" timeout="" voices=""/>
		<play_sound @l="42" buf="5" fadeout="0" type="1"/>
		<msg @l="44" char="2" flags="0" index="6" line="44" name="紗々"
		   text="「はぁはぁ……センパイのおち○ちん、だんだんおおきくなって\n:きてますね」" timeout=""
		   voices="sas16000"/>
		<play_sound @l="46" buf="5" fadeout="0" intro="0" src="sas80016" type="6"
		   volume="1."/>
		<msg @l="47" char="-1" flags="0" index="8" line="47" name=""
		   text="いつのまに胸元をはだけさせたのか、ミントグリーンの\n:ブラが姿をあらわしている。" timeout="" voices=""/>
		<msg @l="48" char="-1" flags="0" index="10" line="48" name=""
		   text="瞳をらんらんと輝かせ、吐息もすでに荒くなってきている。" timeout="" voices=""/>
		<play_sound @l="49" buf="5" fadeout="0" type="1"/>
		<chgbg @l="51" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evsas07_01e" type="99"/>
		<msg @l="53" char="2" flags="0" index="12" line="53" name="紗々"
		   text="「まかせてください、私がすぐにスッキリさせちゃいます\n:からねっ」" timeout="" voices="sas16001"/>
		<play_sound @l="55" buf="5" fadeout="0" intro="0" src="sas80017" type="6"
		   volume="1."/>
		<msg @l="56" char="11" flags="0" index="14" line="56" name="亮"
		   text="「気持ちは嬉しいし僕もしたいのはやまやまなんだが、\n:さすがに校内、しかも屋外でっていうのは……」" timeout=""
		   voices=""/>
		<play_sound @l="57" buf="5" fadeout="0" type="1"/>
		<msg @l="59" char="2" flags="0" index="16" line="59" name="紗々"
		   text="「大丈夫です。ここは植え込みに隠れてるし、\n:この体勢ならまず見つかりませんから」" timeout=""
		   voices="sas16002"/>
		<play_sound @l="61" buf="5" fadeout="0" intro="0" src="sas80016" type="6"
		   volume="1."/>
		<msg @l="62" char="11" flags="0" index="18" line="62" name="亮"
		   text="「す、すごい自信だな」" timeout="" voices=""/>
		<play_sound @l="63" buf="5" fadeout="0" type="1"/>
		<msg @l="65" char="2" flags="0" index="20" line="65" name="紗々"
		   text="「いつセンパイに求められてもいいように、校内の人目に付かないスポットを事前に全部調べておきましたぁ」" timeout=""
		   voices="sas16003"/>
		<play_sound @l="67" buf="5" fadeout="0" intro="0" src="sas80015" type="6"
		   volume="1."/>
		<msg @l="68" char="11" flags="0" index="22" line="68" name="亮" text="「全部！？」"
		   timeout="" voices=""/>
		<msg @l="70" char="-1" flags="0" index="24" line="70" name=""
		   text="どんだけやる気満々なのこの子！？" timeout="" voices=""/>
		<play_sound @l="71" buf="5" fadeout="0" type="1"/>
		<chgbg @l="73" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evsas07_01a" type="99"/>
		<msg @l="75" char="2" flags="0" index="26" line="75" name="紗々"
		   text="「センパイは私とエッチなこと、したくないですか……？」" timeout="" voices="sas16004"/>
		<play_sound @l="77" buf="5" fadeout="0" intro="0" src="sas80016" type="6"
		   volume="1."/>
		<msg @l="78" char="-1" flags="0" index="28" line="78" name=""
		   text="少し自信なさげなような顔で、僕の様子を伺ってくる。" timeout="" voices=""/>
		<msg @l="80" char="11" flags="0" index="30" line="80" name="亮"
		   text="「もちろんしたいに決まってるぞ。今したいすぐしたい」" timeout="" voices=""/>
		<play_sound @l="81" buf="5" fadeout="0" type="1"/>
		<chgbg @l="83" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evsas07_01e" type="99"/>
		<msg @l="85" char="2" flags="0" index="32" line="85" name="紗々"
		   text="「……あはっ、嬉しいっ！　センパイと同じ気持ちだなんて\n:それだけで胸の奥がキュンッてなっちゃいます\x;heart:」"
		   timeout="" voices="sas16005"/>
		<play_sound @l="87" buf="5" fadeout="0" intro="0" src="sas80015" type="6"
		   volume="1."/>
		<msg @l="88" char="-1" flags="0" index="34" line="88" name=""
		   text="僕の首に腕を回して、ギュッと抱きついてくる紗々ちゃん。" timeout="" voices=""/>
		<msg @l="89" char="-1" flags="0" index="36" line="89" name=""
		   text="ふわりと柔らかな長髪がなびき、ほのかなシャンプーの香りが\n:僕の鼻孔をくすぐる。" timeout="" voices=""/>
		<play_sound @l="90" buf="5" fadeout="0" type="1"/>
		<msg @l="92" char="2" flags="0" index="38" line="92" name="紗々"
		   text="「ここはぜーんぶ私に任せてくださいね。センパイはそのまま\n:寝そべっていてくれれば大丈夫ですから」" timeout=""
		   voices="sas16006"/>
		<play_sound @l="94" buf="5" fadeout="0" intro="0" src="sas80017" type="6"
		   volume="1."/>
		<msg @l="95" char="11" flags="0" index="40" line="95" name="亮"
		   text="「寝そべってれば……って聞いてる！？」" timeout="" voices=""/>
		<play_sound @l="96" buf="5" fadeout="0" type="1"/>
		<chgbg @l="98" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evsas07_01i" type="99"/>
		<msg @l="100" char="2" flags="0" index="42" line="100" name="紗々"
		   text="「ふんふふ～ん\x;note:　まずは、センパイの上着を\n:はだけさせてっと……」" timeout=""
		   voices="sas16007"/>
		<play_sound @l="102" buf="5" fadeout="0" intro="0" src="sas80016" type="6"
		   volume="1."/>
		<msg @l="103" char="-1" flags="0" index="44" line="103" name=""
		   text="問いかけが耳に入っていないのか、鼻歌交じりに\n:僕のシャツのボタンを外すと胸を露わにさせる。" timeout=""
		   voices=""/>
		<play_sound @l="104" buf="5" fadeout="0" type="1"/>
		<chgbg @l="106" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evsas07_01e" type="99"/>
		<msg @l="108" char="2" flags="0" index="46" line="108" name="紗々"
		   text="「えへっ、センパイのおっぱい出しちゃいました」" timeout="" voices="sas16008"/>
		<play_sound @l="110" buf="5" fadeout="0" intro="0" src="sas80016" type="6"
		   volume="1."/>
		<msg @l="111" char="11" flags="0" index="48" line="111" name="亮"
		   text="「な、なんていうか、男でも胸だけ露出するって\n:恥ずかしいもんだな」" timeout="" voices=""/>
		<play_sound @l="112" buf="5" fadeout="0" type="1"/>
		<msg @l="114" char="2" flags="0" index="50" line="114" name="紗々"
		   text="「恥ずかしがらなくてもいいじゃないですか、私としては\n:センパイのおっぱいが一番ですっ。紗々的ベストおっぱい\n:ですからっ」"
		   timeout="" voices="sas16009"/>
		<play_sound @l="116" buf="5" fadeout="0" intro="0" src="sas80017" type="6"
		   volume="1."/>
		<msg @l="117" char="11" flags="0" index="52" line="117" name="亮"
		   text="「意味わかんないんだけど！？」" timeout="" voices=""/>
		<play_sound @l="118" buf="5" fadeout="0" type="1"/>
		<chgbg @l="120" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evsas07_01b" type="99"/>
		<msg @l="122" char="2" flags="0" index="54" line="122" name="紗々"
		   text="「というわけで、今からペロペロしちゃいますねぇ……ぺろっ\x;heart:」" timeout=""
		   voices="sas16010"/>
		<play_sound @l="124" buf="5" fadeout="0" intro="0" src="sas80015" type="6"
		   volume="1."/>
		<msg @l="125" char="11" flags="0" index="56" line="125" name="亮"
		   text="「……んんっ！？」" timeout="" voices=""/>
		<msg @l="127" char="-1" flags="0" index="58" line="127" name=""
		   text="胸にぬとりと生温かく滑った感触に、思わず変な声が出てしまう。" timeout="" voices=""/>
		<msg @l="128" char="-1" flags="0" index="60" line="128" name=""
		   text="紗々ちゃんは露わになった僕の胸に唇をくっつけ、\n:舌を出してちろちろと舐めてくる。" timeout="" voices=""/>
		<play_sound @l="129" buf="5" fadeout="0" type="1"/>
		<chgbg @l="131" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evsas07_01j" type="99"/>
		<msg @l="133" char="2" flags="0" index="62" line="133" name="紗々"
		   text="「はうぅぅんっ、女の子みたいな声出して、んちゅぅぅ、\n:かわいいです……れろれりゅ、んちゅっ、ふぁうっ！」" timeout=""
		   voices="sas16011"/>
		<msg @l="135" char="2" flags="0" index="64" line="135" name="紗々"
		   text="「ちゅ、ちゅうぅっ、センパイのおっぱい、男の人だから\n:ゴツゴツしてて、でも……れろれろっ、お肌はすべすべで\n:舐めやすいです……れりゅうぅっ」"
		   timeout="" voices="sas16012"/>
		<msg @l="137" char="2" flags="0" index="66" line="137" name="紗々"
		   text="「はふぅ、ちょっと汗ばんできて、ちゅぷ……っ、少ししょっぱいです、れろっ、れろぉ……これがセンパイの味……\n:ちゅっ、ちゅぷうぅ\x;hearts:」"
		   timeout="" voices="sas16013"/>
		<play_sound @l="139" buf="5" fadeout="0" intro="0" src="sas80000" type="6"
		   volume="1."/>
		<msg @l="140" char="-1" flags="0" index="68" line="140" name=""
		   text="上目遣いに僕の反応を見ながら、乳輪をなぞるように舌を\n:這わしていく。" timeout="" voices=""/>
		<msg @l="141" char="-1" flags="0" index="70" line="141" name=""
		   text="舐められる度に身体がビクリと反応して\n:徐々に汗ばんでいくのがわかる。" timeout="" voices=""/>
		<play_sound @l="142" buf="5" fadeout="0" type="1"/>
		<chgbg @l="144" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evsas07_01d" type="99"/>
		<msg @l="146" char="2" flags="0" index="72" line="146" name="紗々"
		   text="「すぅー、はぁ……センパイの匂いがどんどん濃くなって\n:きました……すぅはぁっ、あはっ、私の大好きな匂いです……ちゅぅ、ちゅぅ\x;heart:」"
		   timeout="" voices="sas16014"/>
		<play_sound @l="148" buf="5" fadeout="0" intro="0" src="sas80002" type="6"
		   volume="1."/>
		<msg @l="149" char="11" flags="0" index="74" line="149" name="亮"
		   text="「んっ、くうぅ……っ」" timeout="" voices=""/>
		<play_sound @l="150" buf="5" fadeout="0" type="1"/>
		<chgbg @l="152" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evsas07_01j" type="99"/>
		<msg @l="154" char="2" flags="0" index="76" line="154" name="紗々"
		   text="「気持ちいいですか？　ちゅ、ちゅるっ、もっとこうして欲しい\n:とかあったら、れりゅ、れろぉ……遠慮なく言ってくださいね」"
		   timeout="" voices="sas16015"/>
		<play_sound @l="156" buf="5" fadeout="0" intro="0" src="sas80001" type="6"
		   volume="1."/>
		<msg @l="157" char="-1" flags="0" index="78" line="157" name=""
		   text="ぺろぺろと熱心に舌を動かしながら尋ねてくる紗々ちゃん。" timeout="" voices=""/>
		<msg @l="158" char="-1" flags="0" index="80" line="158" name=""
		   text="気持ちいいというよりはくすぐったい。" timeout="" voices=""/>
		<msg @l="160" char="11" flags="0" index="82" line="160" name="亮"
		   text="「出来れば……んっ、んんぅ、乳首をもっと責めて欲しい、かな」" timeout="" voices=""/>
		<play_sound @l="161" buf="5" fadeout="0" type="1"/>
		<chgbg @l="163" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evsas07_01k" type="99"/>
		<msg @l="165" char="2" flags="0" index="84" line="165" name="紗々"
		   text="「乳首ですねわかりました！　はむっ、ちゅぅ、ちゅぅ……\n:こんな風に吸うと気持ちいいんですね？」" timeout=""
		   voices="sas16016"/>
		<msg @l="167" char="2" flags="0" index="86" line="167" name="紗々"
		   text="「んちゅ、ちゅぷっ……！　こうしてると、センパイの赤ちゃんになっちゃったみたいです……ちゅぅ、ちゅずぅ」" timeout=""
		   voices="sas16017"/>
		<play_sound @l="169" buf="5" fadeout="0" intro="0" src="sas80003" type="6"
		   volume="1."/>
		<msg @l="170" char="-1" flags="0" index="88" line="170" name=""
		   text="甘えるように僕の胸板に頬ずりしながら、ちゅうちゅうと乳首を吸ってくる紗々ちゃん。" timeout="" voices=""/>
		<play_sound @l="171" buf="5" fadeout="0" type="1"/>
		<chgbg @l="173" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evsas07_01g" type="99"/>
		<msg @l="175" char="2" flags="0" index="90" line="175" name="紗々"
		   text="「こんな感じでちゅうちゅう吸えば……いいですか？\n:　ちゅぅ、ちゅるるっ！」" timeout=""
		   voices="sas16018"/>
		<play_sound @l="177" buf="5" fadeout="0" intro="0" src="sas80001" type="6"
		   volume="1."/>
		<msg @l="178" char="11" flags="0" index="92" line="178" name="亮"
		   text="「ああ、気持ちいいよ……はぁはぁ、あと甘噛みとかもして\n:くれたら、もっといいかも」" timeout="" voices=""/>
		<play_sound @l="179" buf="5" fadeout="0" type="1"/>
		<chgbg @l="181" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evsas07_01o" type="99"/>
		<msg @l="183" char="2" flags="0" index="94" line="183" name="紗々"
		   text="「甘噛みですね……はーむっ、もぐもご……歯を立てすぎない\n:ようにやさしーく、こりゅ、こりゅっ！」" timeout=""
		   voices="sas16019"/>
		<play_sound @l="185" buf="5" fadeout="0" intro="0" src="sas80003" type="6"
		   volume="1."/>
		<msg @l="186" char="11" flags="0" index="96" line="186" name="亮"
		   text="「うっ、うおぉっ！？　今の、や、ヤバいって」" timeout="" voices=""/>
		<play_sound @l="187" buf="5" fadeout="0" type="1"/>
		<chgbg @l="189" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evsas07_01h" type="99"/>
		<msg @l="191" char="2" flags="0" index="98" line="191" name="紗々"
		   text="「カミカミしたの気持ちよかったんですね？　はぁはぁ、\n:センパイの感じるポイント……分かってきたかもです」" timeout=""
		   voices="sas16020"/>
		<chgbg @l="193" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evsas07_01k" type="99"/>
		<msg @l="195" char="2" flags="0" index="100" line="195" name="紗々"
		   text="「こうやっれぇ、舌先で乳首をぺろぺろしながら……ちゅりゅ、\n:れりゅぅっ、強めにちゅーって吸ってぇ、ちゅずぅぅぅ！！」"
		   timeout="" voices="sas16021"/>
		<msg @l="197" char="2" flags="0" index="102" line="197" name="紗々"
		   text="「乳首がプクってなったらカリって優しく噛んでぇ……\n:はむっ、かみかみ……\x;hearts:」" timeout=""
		   voices="sas16022"/>
		<play_sound @l="199" buf="5" fadeout="0" intro="0" src="sas80000" type="6"
		   volume="1."/>
		<msg @l="200" char="11" flags="0" index="104" line="200" name="亮"
		   text="「くうぅっ！？」" timeout="" voices=""/>
		<msg @l="202" char="-1" flags="0" index="106" line="202" name=""
		   text="早くもコツを掴んだ紗々ちゃんが、的確にポイントを\n:刺激してくる。" timeout="" voices=""/>
		<msg @l="203" char="-1" flags="0" index="108" line="203" name=""
		   text="木漏れ日が僕らを照らし、木々のシルエットが\n:僕らの上でゆらゆらと揺らめいている。" timeout="" voices=""/>
		<msg @l="204" char="-1" flags="0" index="110" line="204" name=""
		   text="こんな場所だというのに、すでに僕は力が抜けて\n:彼女のなすがままに身を任せてしまっていた。" timeout=""
		   voices=""/>
		<play_sound @l="205" buf="5" fadeout="0" type="1"/>
		<chgbg @l="207" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evsas07_01g" type="99"/>
		<msg @l="209" char="2" flags="0" index="112" line="209" name="紗々"
		   text="「センパイのちくびぃ……れりゅ、ぢゅずっ、ビンビンになって\n:きましたよ？　ちゅりゅ、じゅりゅっ、かりっ！」" timeout=""
		   voices="sas16023"/>
		<play_sound @l="211" buf="5" fadeout="0" intro="0" src="sas80001" type="6"
		   volume="1."/>
		<msg @l="212" char="11" flags="0" index="114" line="212" name="亮"
		   text="「はぁはぁ……紗々ちゃんが一生懸命舐めてくれたおかげだ。\n:こんなに乳首が立つなんて思いもしなかった」" timeout=""
		   voices=""/>
		<play_sound @l="213" buf="5" fadeout="0" type="1"/>
		<chgbg @l="215" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evsas07_01f" type="99"/>
		<msg @l="217" char="2" flags="0" index="116" line="217" name="紗々"
		   text="「私もビックリしました、男の人の乳首もこんなに\n:大きくなるんですね……ちゅっ、れりゅぅ、\n:何だかすっごくエッチれす……ちゅぶ、ちゅぅっ！」"
		   timeout="" voices="sas16024"/>
		<play_sound @l="219" buf="5" fadeout="0" intro="0" src="sas80003" type="6"
		   volume="1."/>
		<msg @l="220" char="-1" flags="0" index="118" line="220" name=""
		   text="僕の乳首を舐めて興奮を高めてきているのか、\n:挟んでいる足の間が熱っぽく汗ばんできている。" timeout=""
		   voices=""/>
