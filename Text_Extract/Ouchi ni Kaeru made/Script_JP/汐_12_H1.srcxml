<?xml version="1.0" encoding="utf-8"?>
<xscript>
	<code>
		<label @l="3" id="scene_1_00"/>
		<play_sound @l="5" buf="0" fadeout="2500" type="0"/>
		<showmsg @l="6" duration="500" show="0"/>
		<loadimage @l="7" bg="0" fit="0" screen="1" src="evusi04_01b"/>
		<scroll @l="8" async="0" center_x="45" center_y="45" duration="0" flags="0"
		   rev="0." screen="1" transparency="0" v0="0." v1="0." wait_flags="0" x="0"
		   y="0" zoom="1."/>
		<chgscrn @l="9" duration="2500" flags="4608" flip="-1" flip_flags="0"
		   param="0" screen="1" type="10"/>
		<play_sound @l="10" buf="0" fadeout="500" intro="-1" src="bgm18" type="0"
		   volume="1."/>
		<msg @l="14" char="11" flags="0" index="0" line="14" name="亮" text="「あっ……」"
		   timeout="" voices=""/>
		<msg @l="16" char="-1" flags="0" index="2" line="16" name=""
		   text="小さくて柔らかい手が、僕の股間をおずおずと撫でる。" timeout="" voices=""/>
		<chgbg @l="18" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evusi04_01c" type="99"/>
		<msg @l="20" char="1" flags="0" index="4" line="20" name="汐"
		   text="「わっ……こ、こんなに熱いんだ。\n:私、男の子のあそこにさわったの、初めて……」" timeout=""
		   voices="usi10808"/>
		<msg @l="22" char="11" flags="0" index="6" line="22" name="亮"
		   text="「僕もさわられるのは初めてです」" timeout="" voices=""/>
		<msg @l="24" char="-1" flags="0" index="8" line="24" name=""
		   text="自分の手では何度も触れてきたけど、\n:誰かの手で触れられると、こんなにも感じ方が違うなんて。" timeout=""
		   voices=""/>
		<msg @l="26" char="11" flags="0" index="10" line="26" name="亮"
		   text="「朝霞さんの手、ふにっとしてて気持ちいい……」" timeout="" voices=""/>
		<chgbg @l="28" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evusi04_01a" type="99"/>
		<msg @l="30" char="1" flags="0" index="12" line="30" name="汐"
		   text="「そ、そう？　まだ、軽くさわっただけなんだけど……\n:それだけで気持ちよくなっちゃった？」" timeout=""
		   voices="usi10809"/>
		<msg @l="32" char="11" flags="0" index="14" line="32" name="亮"
		   text="「はい。さすがはお姉ちゃんですね」" timeout="" voices=""/>
		<chgbg @l="34" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evusi04_01b" type="99"/>
		<msg @l="36" char="1" flags="0" index="16" line="36" name="汐"
		   text="「ふふん。こんなふうにエッチに迫ることぐらい、\n:お姉ちゃんには朝飯前なんだから」" timeout=""
		   voices="usi10810"/>
		<msg @l="38" char="-1" flags="0" index="18" line="38" name=""
		   text="僕の一言で勢いづき、小さな指はさらに強くペニスを握ってくる。" timeout="" voices=""/>
		<msg @l="40" char="11" flags="0" index="20" line="40" name="亮"
		   text="「うわっ、指だけでこんな……はぁ、ああっ」" timeout="" voices=""/>
		<msg @l="42" char="-1" flags="0" index="22" line="42" name=""
		   text="その感触だけで、思わず腰が少し浮いてしまう。" timeout="" voices=""/>
		<msg @l="43" char="-1" flags="0" index="24" line="43" name=""
		   text="血管が浮いているグロテスクなペニスと、\n:ぷにぷにですべすべのロリっ子の指先。" timeout="" voices=""/>
		<msg @l="44" char="-1" flags="0" index="26" line="44" name=""
		   text="そのギャップが、\n:なんだかイケナイことをしているような錯覚に陥らせる。" timeout="" voices=""/>
		<chgbg @l="46" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evusi04_01d" type="99"/>
		<msg @l="48" char="1" flags="0" index="28" line="48" name="汐"
		   text="「やっ、あわわっ……？！　ちょっ、\n:なんだかさっきよりおっきくなってる気がするんだけどっ……？」" timeout=""
		   voices="usi10811"/>
		<msg @l="50" char="11" flags="0" index="30" line="50" name="亮"
		   text="「な、なんか、そうみたいですね……？」" timeout="" voices=""/>
		<msg @l="52" char="1" flags="0" index="32" line="52" name="汐"
		   text="「みたいって……え、ええっ？　キミのあそこだよ？　\n:自分のことなのにわからないの？」" timeout=""
		   voices="usi10812"/>
		<msg @l="54" char="11" flags="0" index="34" line="54" name="亮"
		   text="「それが、好きな人にさわってもらってるせいか、\n:興奮に歯止めがきかないっていうかっ……」" timeout=""
		   voices=""/>
		<msg @l="56" char="-1" flags="0" index="36" line="56" name=""
		   text="小さくて軽い身体、すらっとした四肢、あったかい肌、\n:すべすべの指、ふわふわの髪。そして、女の子特有の甘い匂い。"
		   timeout="" voices=""/>
		<msg @l="57" char="-1" flags="0" index="38" line="57" name=""
		   text="それらがすべて同時に、僕の理性に襲いかかってくる。" timeout="" voices=""/>
		<chgbg @l="61" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evusi04_01e" type="99"/>
		<msg @l="63" char="1" flags="0" index="40" line="63" name="汐"
		   text="「あ、ああっ……ほ、ほんと、こんなに……って、ええっ？　\n:ま、待って待ってっ。おっきすぎないこれっ……！？」"
		   timeout="" voices="usi10813"/>
		<msg @l="65" char="11" flags="0" index="42" line="65" name="亮"
		   text="「それがですね、まだ大きくなる可能性も――」" timeout="" voices=""/>
		<chgbg @l="67" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evusi04_01c" type="99"/>
		<msg @l="69" char="1" flags="0" index="44" line="69" name="汐"
		   text="「う、うそっ？　まだおっきくなっちゃうのっ……？！」" timeout="" voices="usi10814"/>
		<msg @l="71" char="-1" flags="0" index="46" line="71" name=""
		   text="貧血になりそうな勢いで、全身の血が股間に集まっていく。" timeout="" voices=""/>
		<chgbg @l="75" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evusi04_01e" type="99"/>
		<msg @l="77" char="1" flags="0" index="48" line="77" name="汐"
		   text="「わっ、わわっ……手の中で、\n:キミのあそこがどんどん大きくなって……\n:はぁ、はぁ……んくっ\x;hearts:」"
		   timeout="" voices="usi10815"/>
		<msg @l="79" char="-1" flags="0" index="50" line="79" name=""
		   text="恥じらいつつも目は逸らさず、股間をじっと見つめてくる。" timeout="" voices=""/>
		<msg @l="80" char="-1" flags="0" index="52" line="80" name=""
		   text="朝霞さんの乱れた呼吸が亀頭にかかり、実にくすぐったい。" timeout="" voices=""/>
		<msg @l="82" char="11" flags="0" index="54" line="82" name="亮"
		   text="「あ、あの、朝霞さん。そんなにまじまじ見られると、\n:すっごいドキドキするんですがっ……」" timeout=""
		   voices=""/>
		<chgbg @l="84" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evusi04_01a" type="99"/>
		<msg @l="86" char="1" flags="0" index="56" line="86" name="汐"
		   text="「それって、私の視線だけで興奮しちゃうってこと？」" timeout="" voices="usi10816"/>
		<msg @l="88" char="11" flags="0" index="58" line="88" name="亮"
		   text="「ま、まあ、そんな感じです。はい」" timeout="" voices=""/>
		<msg @l="90" char="-1" flags="0" index="60" line="90" name=""
		   text="だって僕の股間を握ってるだけでも、\n:犯罪臭がすさまじいんだもん。" timeout="" voices=""/>
		<msg @l="91" char="-1" flags="0" index="62" line="91" name=""
		   text="いま朝霞さんに黒い目線が入ったら、完全アウトだろう。\n:だがそれがいい！！" timeout="" voices=""/>
		<chgbg @l="94" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evusi04_01c" type="99"/>
		<msg @l="96" char="1" flags="0" index="64" line="96" name="汐"
		   text="\h;60:\s;40:「ついさっき、私は性的じゃないとか言ってたのに……\n:それでも、こんなに興奮しちゃうんだ……？」\s:\h:"
		   timeout="" voices="usi10817"/>
		<msg @l="98" char="11" flags="0" index="66" line="98" name="亮"
		   text="「だって、他でもない朝霞さんですしっ……」" timeout="" voices=""/>
		<chgbg @l="100" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evusi04_01b" type="99"/>
		<msg @l="102" char="1" flags="0" index="68" line="102" name="汐"
		   text="「んッ……えへへっ。よっ、よぉしっ。\n:それならお姉ちゃんが、もっとドキドキさせてあげるっ\x;heart:」"
		   timeout="" voices="usi10818"/>
		<msg @l="104" char="1" flags="0" index="70" line="104" name="汐"
		   text="「いい？　よーく見てなさい？　これからキミのあそこを、\n:もっと気持ち良くしてあげちゃうんだから」" timeout=""
		   voices="usi10819"/>
		<msg @l="106" char="11" flags="0" index="72" line="106" name="亮"
		   text="「けど、いまでも充分ですし、これ以上は――」" timeout="" voices=""/>
		<chgbg @l="108" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evusi04_01a" type="99"/>
		<msg @l="110" char="1" flags="0" index="74" line="110" name="汐"
		   text="「遠慮しないの……\n:あ、それとも私にはそんなこと出来ないとか思ってたりする？」" timeout=""
		   voices="usi10820"/>
		<msg @l="112" char="11" flags="0" index="76" line="112" name="亮"
		   text="「いっ、いえ、そんなことは」" timeout="" voices=""/>
		<chgbg @l="114" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evusi04_01b" type="99"/>
		<msg @l="117" char="1" flags="0" index="78" line="117" name="汐"
		   text="「なら、キミはそこでお姉ちゃんのテクニシャンっぷりを、\n:じっくりと眺めてなさい」" timeout=""
		   voices="usi10821"/>
		<msg @l="119" char="-1" flags="0" index="80" line="119" name=""
		   text="強い口調で言い切られると、反射的に頷いてしまう。" timeout="" voices=""/>
		<chgbg @l="123" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evusi04_01c" type="99"/>
		<msg @l="125" char="1" flags="0" index="82" line="125" name="汐"
		   text="「えっと……やっぱり、\n:こういうのって舐めてあげたほうがいいんだよね……？」" timeout=""
		   voices="usi10822"/>
		<msg @l="127" char="1" flags="0" index="84" line="127" name="汐"
		   text="「ともかく、さっきしたみたいなキスを、\n:ここにも……ん、ちゅっ」" timeout="" voices="usi10823"/>
		<play_sound @l="129" buf="5" fadeout="0" intro="0" src="usi80000" type="6"
		   volume="1."/>
		<msg @l="130" char="11" flags="0" index="86" line="130" name="亮"
		   text="「あっ……」" timeout="" voices=""/>
		<msg @l="132" char="-1" flags="0" index="88" line="132" name=""
		   text="ちろっと伸びてきた舌の先端が、亀頭に触れる。" timeout="" voices=""/>
		<play_sound @l="133" buf="5" fadeout="0" type="1"/>
		<chgbg @l="137" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evusi04_01d" type="99"/>
		<msg @l="139" char="1" flags="0" index="90" line="139" name="汐"
		   text="「ん、れろっ……ちゅ、んん、れろれろ、れろ、ちゅっ、\n:ちゅるる、ちゅっ、ちゅちゅっ……はぁ、はぁ」" timeout=""
		   voices="usi10824"/>
		<chgbg @l="141" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evusi04_01a" type="99"/>
		<msg @l="142" char="1" flags="0" index="92" line="142" name="汐"
		   text="「……どう？　気持ちいい？」" timeout="" voices="usi10825"/>
		<play_sound @l="144" buf="5" fadeout="0" intro="0" src="usi80001" type="6"
		   volume="1."/>
		<msg @l="145" char="11" flags="0" index="94" line="145" name="亮"
		   text="「は、はいっ……」" timeout="" voices=""/>
		<msg @l="147" char="-1" flags="0" index="96" line="147" name=""
		   text="正直、舐めてくる舌は遠慮し過ぎてて、刺激としては微妙だ。" timeout="" voices=""/>
		<msg @l="148" char="-1" flags="0" index="98" line="148" name=""
		   text="でも、朝霞さんが僕の亀頭にキスしてるんだって思うと、\n:信じられないぐらい息子が熱くなっていく。" timeout=""
		   voices=""/>
		<play_sound @l="149" buf="5" fadeout="0" type="1"/>
		<chgbg @l="153" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evusi04_01c" type="99"/>
		<msg @l="155" char="1" flags="0" index="100" line="155" name="汐"
		   text="「あっ……すごい、手の中でびくびくして、れろ、ちゅっ……\n:はぁはぁ、ほんとに、まだおっきくなるんだ……」" timeout=""
		   voices="usi10826"/>
		<msg @l="157" char="1" flags="0" index="102" line="157" name="汐"
		   text="「これ、もっとなでなでしながら、さきっぽだけでなく、\n:全体もしっかり――ちゅっ、あむ、れるれる、れろろっ」" timeout=""
		   voices="usi10827"/>
		<msg @l="159" char="1" flags="0" index="104" line="159" name="汐"
		   text="「れるれる、んっ……舐めながら、握って、\n:しこしこも、してあげて……れろ、んんっ」" timeout=""
		   voices="usi10828"/>
		<play_sound @l="161" buf="5" fadeout="0" intro="0" src="usi80002" type="6"
		   volume="1."/>
		<msg @l="162" char="-1" flags="0" index="106" line="162" name=""
		   text="気持ちいいと言われたことで自信がついたのか、\n:動きが少しずつ大胆になっていく。" timeout="" voices=""/>
		<msg @l="163" char="-1" flags="0" index="108" line="163" name=""
		   text="そうなると背徳的な見た目だけでなく、\n:愛撫による刺激でもだんだんと気持ち良くなってきた。" timeout=""
		   voices=""/>
		<play_sound @l="164" buf="5" fadeout="0" type="1"/>
		<chgbg @l="170" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evusi04_02e" type="99"/>
		<msg @l="173" char="1" flags="0" index="110" line="173" name="汐"
		   text="「はぁ、はぁ……あっ、なんだかさきっぽの割れ目から、\n:透明なのが、ちょろって垂れてきた……」" timeout=""
		   voices="usi10829"/>
		<msg @l="177" char="1" flags="0" index="112" line="177" name="汐"
		   text="「れろ、ちゅっ……ん、れろろ、れろ、ちゅるるるっ。\n:ん、ん……？　あれ？　甘くない？？」" timeout=""
		   voices="usi10830"/>
		<play_sound @l="179" buf="5" fadeout="0" intro="0" src="usi80003" type="6"
		   volume="1."/>
		<msg @l="180" char="11" flags="0" index="114" line="180" name="亮"
		   text="「あっ、それはえと……いわゆる我慢汁ってやつで」" timeout="" voices=""/>
		<play_sound @l="181" buf="5" fadeout="0" type="1"/>
		<chgbg @l="183" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evusi04_02b" type="99"/>
		<msg @l="185" char="1" flags="0" index="116" line="185" name="汐"
		   text="「えっ？　どういうこと？　いま我慢してるってこと？」" timeout="" voices="usi10831"/>
		<msg @l="187" char="11" flags="0" index="118" line="187" name="亮"
		   text="「じゃなくて、男も女の子と同じで、\n:気持ち良くなってくると濡れてきちゃうというか――」" timeout="" voices=""/>
		<msg @l="189" char="1" flags="0" index="120" line="189" name="汐"
		   text="「あ……」" timeout="" voices="usi10832"/>
		<msg @l="191" char="-1" flags="0" index="122" line="191" name=""
		   text="瞬間、朝霞さんの顔が耳までぼぼっと赤くなった。" timeout="" voices=""/>
		<chgbg @l="195" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evusi04_02a" type="99"/>
		<msg @l="197" char="1" flags="0" index="124" line="197" name="汐"
		   text="「わっ、わわわわかってたんだからそのぐらいっ。\n:いまのは、ちょっと言ってみただけだからっ」" timeout=""
		   voices="usi10833"/>
		<msg @l="199" char="11" flags="0" index="126" line="199" name="亮"
		   text="「あたたっ」" timeout="" voices=""/>
		<chgbg @l="201" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evusi04_02c" type="99"/>
		<msg @l="203" char="1" flags="0" index="128" line="203" name="汐"
		   text="「あっ、ご、ごめんっ」" timeout="" voices="usi10834"/>
		<msg @l="205" char="-1" flags="0" index="130" line="205" name=""
		   text="慌てた朝霞さんが竿をぎゅっと握ったので思わず声が出た。" timeout="" voices=""/>
		<msg @l="207" char="11" flags="0" index="132" line="207" name="亮"
		   text="「い、いえ……。でも、どうして甘いと思ったんです？」" timeout="" voices=""/>
		<chgbg @l="209" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evusi04_02b" type="99"/>
		<msg @l="211" char="1" flags="0" index="134" line="211" name="汐"
		   text="「それは……だって、なんかガムシロップぽくない？」" timeout="" voices="usi10835"/>
		<msg @l="213" char="11" flags="0" index="136" line="213" name="亮"
		   text="「ああ――」" timeout="" voices=""/>
		<msg @l="215" char="-1" flags="0" index="138" line="215" name=""
		   text="初めてカウパーを見て連想したのが甘味料ってあたりが、\n:朝霞さんらしくて実に可愛い。" timeout="" voices=""/>
		<chgbg @l="218" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evusi04_02c" type="99"/>
		<msg @l="219" char="1" flags="0" index="140" line="219" name="汐"
		   text="「んっ……でも、こうして濡れてきたってことは、\n:本当に感じてくれてるんだね……」" timeout=""
		   voices="usi10836"/>
		<msg @l="221" char="11" flags="0" index="142" line="221" name="亮"
		   text="「信じてなかったんですか？」" timeout="" voices=""/>
		<chgbg @l="224" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evusi04_02b" type="99"/>
		<msg @l="225" char="1" flags="0" index="144" line="225" name="汐"
		   text="「だって……キミ、あんまり声とか出さないし」" timeout="" voices="usi10837"/>
		<msg @l="227" char="11" flags="0" index="146" line="227" name="亮"
		   text="「恥ずかしいから我慢してるだけで――」" timeout="" voices=""/>
		<msg @l="229" char="-1" flags="0" index="148" line="229" name=""
		   text="……って、待てよ？" timeout="" voices=""/>
		<msg @l="231" char="11" flags="0" index="150" line="231" name="亮"
		   text="「つまり、朝霞さんは感じると声が出ちゃうほうだったり？」" timeout="" voices=""/>
		<chgbg @l="235" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evusi04_02e" type="99"/>
		<msg @l="237" char="1" flags="0" index="152" line="237" name="汐"
		   text="「ふえっ！？　ど、どうしてそういうところだけ鋭いのっ、\n:もうっ。この、えいっ、このっ」" timeout=""
		   voices="usi10838"/>
		<msg @l="239" char="11" flags="0" index="154" line="239" name="亮"
		   text="「あわわわわっ？！」" timeout="" voices=""/>
		<msg @l="241" char="-1" flags="0" index="156" line="241" name=""
		   text="操縦桿でも操るみたいに、息子が右に左に倒される。" timeout="" voices=""/>
		<chgbg @l="243" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evusi04_02a" type="99"/>
		<msg @l="245" char="1" flags="0" index="158" line="245" name="汐"
