		<play_sound @l="118" buf="5" fadeout="0" intro="0" src="kan80021" type="6"
		   volume="1."/>
		<msg @l="119" char="-1" flags="0" index="68" line="119" name=""
		   text="僕の唇からこぼれる水音と共に、花音の吐息も激しさを\n:増していく。" timeout="" voices=""/>
		<play_sound @l="120" buf="5" fadeout="0" type="1"/>
		<chgbg @l="122" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evkan15_01b" type="99"/>
		<msg @l="124" char="0" flags="0" index="70" line="124" name="花音"
		   text="「はぁっ、はぁっ……！　本当にっ、らめっ、らめだよぉ……\x;heart:」" timeout=""
		   voices="kan17614"/>
		<msg @l="126" char="0" flags="0" index="72" line="126" name="花音"
		   text="「ああぁっ！　あきゅん！　ふぁっ、あっ、あっ！　あぁっん！」" timeout="" voices="kan17615"/>
		<play_sound @l="128" buf="5" fadeout="0" intro="0" src="kan80022" type="6"
		   volume="1."/>
		<msg @l="129" char="-1" flags="0" index="74" line="129" name=""
		   text="絶え間なく続く快楽の波に、彼女のしなやかな体が強張る。" timeout="" voices=""/>
		<play_sound @l="130" buf="5" fadeout="0" type="1"/>
		<msg @l="132" char="0" flags="0" index="76" line="132" name="花音"
		   text="「イクぅっ！　イッちゃうのぉっ！　おっぱい吸われて\n:イッちゃうよぉ！」" timeout="" voices="kan17616"/>
		<play_sound @l="134" buf="5" fadeout="0" intro="0" src="kan80023" type="6"
		   volume="1."/>
		<msg @l="135" char="-1" flags="0" index="78" line="135" name=""
