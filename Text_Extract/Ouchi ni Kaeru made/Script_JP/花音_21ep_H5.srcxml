		<msg @l="153" char="0" flags="0" index="82" line="153" name="花音"
		   text="「ああァァぁぁぁぁぁっ！！！！」" timeout="" voices="kan17618"/>
		<play_sound @l="155" buf="5" fadeout="0" intro="0" src="kan80021" type="6"
		   volume="1."/>
		<msg @l="156" char="-1" flags="0" index="84" line="156" name=""
		   text="花音は僕の頭をぎゅっと強く抱いたまま、押し寄せた絶頂に全身を震わせる。" timeout="" voices=""/>
		<play_sound @l="157" buf="5" fadeout="0" type="1"/>
		<msg @l="159" char="0" flags="0" index="86" line="159" name="花音"
		   text="「イッてる！　イッちゃってるよぉ！」" timeout="" voices="kan17619"/>
		<msg @l="161" char="0" flags="0" index="88" line="161" name="花音"
		   text="「おっぱい吸われるの気持ちよすぎて、おっぱいだけで\n:イッちゃってるのぉっ！！！」" timeout=""
		   voices="kan17620"/>
		<play_sound @l="163" buf="5" fadeout="0" intro="0" src="kan80022" type="6"
		   volume="1."/>
		<msg @l="164" char="-1" flags="0" index="90" line="164" name=""
		   text="耳の奥まで響く嬌声を楽しみながら、構わず花音の胸を\n:吸い続ける。" timeout="" voices=""/>
		<play_sound @l="165" buf="5" fadeout="0" type="1"/>
		<chgbg @l="167" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evkan15_01c" type="99"/>
		<msg @l="169" char="0" flags="0" index="92" line="169" name="花音"
		   text="「ひゃぁあうっ！？　まだイッてる\x;heart:　イッてるからぁ\x;hearts:」" timeout=""
		   voices="kan17621"/>
		<msg @l="171" char="0" flags="0" index="94" line="171" name="花音"
		   text="「んああぁぁっ！　ひゃっ、はうっ、あっ、あんっ！　イクの\n:とまりゃない……気持ちいいのとまりゃないのぉ\x;heart:\x;hearts:」"
		   timeout="" voices="kan17622"/>
		<play_sound @l="173" buf="5" fadeout="0" intro="0" src="kan80021" type="6"
		   volume="1."/>
		<msg @l="174" char="-1" flags="0" index="96" line="174" name=""
		   text="ミルク絞りの要領で根本からひねり出すように乳房を\n:揉みくちゃにしていく。" timeout="" voices=""/>
		<msg @l="175" char="-1" flags="0" index="98" line="175" name=""
		   text="出ないとはわかっていても、花音の母乳を求めてやまない。" timeout="" voices=""/>
		<play_sound @l="176" buf="5" fadeout="0" type="1"/>
		<chgbg @l="178" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evkan15_01d" type="99"/>
		<msg @l="180" char="0" flags="0" index="100" line="180" name="花音"
		   text="「きゃうぅぅんっ！？　そんなにおっぱい吸っても出ないよぉ、おっぱい出ないからぁ！」" timeout=""
		   voices="kan17623"/>
		<msg @l="182" char="0" flags="0" index="102" line="182" name="花音"
		   text="「あなたに出してあげたいけどまだ出ないよぉっ……！　\n:あっ、あぁぁっんん！」" timeout=""
		   voices="kan17624"/>
		<msg @l="184" char="0" flags="0" index="104" line="184" name="花音"
		   text="「くふぅぅっ、またイクッ！？　おっぱいちゅーちゅーされて、イッちゃうぅぅーーーっ！！！！」" timeout=""
		   voices="kan17625"/>
		<showmsg @l="188" duration="300" show="0"/>
		<effect @l="189" cycle="0" flags="512" param="0" screen="0" type="2"/>
		<fade @l="190" async="0" degree="256" duration="200" flags="0" screen="0"
		   wait_flags="0"/>
		<fade @l="191" async="0" degree="0" duration="200" flags="0" screen="0"
		   wait_flags="0"/>
		<effect @l="192" cycle="0" flags="512" param="0" screen="0" type="2"/>
		<fade @l="193" async="0" degree="256" duration="200" flags="0" screen="0"
		   wait_flags="0"/>
		<fade @l="194" async="0" degree="0" duration="200" flags="0" screen="0"
		   wait_flags="0"/>
		<play_sound @l="196" buf="5" fadeout="0" intro="0" src="kan80022" type="6"
		   volume="1."/>
		<msg @l="197" char="-1" flags="0" index="106" line="197" name=""
		   text="再び花音の体が大きく跳ねるように震える。" timeout="" voices=""/>
		<msg @l="198" char="-1" flags="0" index="108" line="198" name=""
		   text="二度目の絶頂がおさまるまで、僕は乳首を咥えたまま\n:離さなかった。" timeout="" voices=""/>
		<play_sound @l="202" buf="5" fadeout="0" intro="0" src="kan80025" type="6"
		   volume="1."/>
		<msg @l="203" char="11" flags="0" index="110" line="203" name="亮"
		   text="「ぷはぁ……ごちそうさま」" timeout="" voices=""/>
		<msg @l="205" char="-1" flags="0" index="112" line="205" name=""
		   text="ねぶりにねぶられた花音の胸元はすっかり赤くなっていた。" timeout="" voices=""/>
		<msg @l="207" char="11" flags="0" index="114" line="207" name="亮"
		   text="（よし！　２回もイかせたぞ。満足だ）" timeout="" voices=""/>
		<msg @l="209" char="-1" flags="0" index="116" line="209" name=""
		   text="勝ち誇ったように胸を反らす僕。" timeout="" voices=""/>
		<msg @l="210" char="-1" flags="0" index="118" line="210" name=""
		   text="ちなみに花音の胸は唾液にまみれ、表面はテカテカといやらしく\n:輝いて、突き刺すような刺激を視覚から与えてくる。"
		   timeout="" voices=""/>
		<play_sound @l="211" buf="5" fadeout="0" type="1"/>
		<chgbg @l="213" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evkan15_01a" type="99"/>
		<msg @l="215" char="0" flags="0" index="120" line="215" name="花音"
		   text="「はぁ、はぁ……えへへ\x;heart:　私のおっぱい、あなたの唾液で\n:べとべとになっちゃったね」" timeout=""
		   voices="kan17626"/>
		<play_sound @l="217" buf="5" fadeout="0" intro="0" src="kan80026" type="6"
		   volume="1."/>
		<msg @l="218" char="11" flags="0" index="122" line="218" name="亮"
		   text="「花音のおっぱいなら、どれだけ舐めても飽きないからな」" timeout="" voices=""/>
		<play_sound @l="219" buf="5" fadeout="0" type="1"/>
		<msg @l="221" char="0" flags="0" index="124" line="221" name="花音"
		   text="「夢中になっておっぱい吸ってる姿、赤ちゃんみたいで\n:可愛かったよ」" timeout="" voices="kan17627"/>
		<msg @l="223" char="0" flags="0" index="126" line="223" name="花音"
		   text="「今は母乳は出ないけど……出るようになったらまたしてね\x;heart:」" timeout="" voices="kan17628"/>
		<play_sound @l="225" buf="5" fadeout="0" intro="0" src="kan80015" type="6"
		   volume="1."/>
		<msg @l="226" char="-1" flags="0" index="128" line="226" name=""
		   text="花音は悪戯っぽく微笑むと僕のチャックを下ろし、もうパンパンに膨れあがっているペニスを取り出した。" timeout=""
