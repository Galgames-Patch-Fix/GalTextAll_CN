		<play_sound @l="136" buf="5" fadeout="0" type="1"/>
		<chgbg @l="138" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evkan15_01d" type="99"/>
		<msg @l="140" char="0" flags="0" index="80" line="140" name="花音"
		   text="「あっ\x;heart:　あっ\x;heart:　あっ\x;heart:　あぁっ\x;heart:　イクッイクイクイクイクうぅ～っ\x;heart:\x;hearts:」"
