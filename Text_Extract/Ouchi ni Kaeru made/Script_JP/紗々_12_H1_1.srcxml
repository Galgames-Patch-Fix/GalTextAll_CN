		<play_sound @l="1155" buf="5" fadeout="0" intro="0" src="sas80017" type="6"
		   volume="1."/>
		<msg @l="1156" char="-1" flags="0" index="774" line="1156" name=""
		   text="そっか、紗々ちゃんも初めてだもんな不安になるのも無理はない。" timeout="" voices=""/>
		<msg @l="1157" char="-1" flags="0" index="776" line="1157" name=""
		   text="とはいえ、部屋の電気を消さないでよかったぜ。マジ僕ＧＪ！" timeout="" voices=""/>
		<play_sound @l="1158" buf="5" fadeout="0" type="1"/>
		<chgbg @l="1160" duration="300" flags="256" flip="-1" flip_flags="0"
