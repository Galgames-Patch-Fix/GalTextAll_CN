		   voices="sas17129"/>
		<play_sound @l="1124" buf="5" fadeout="0" intro="0" src="sas80016" type="6"
		   volume="1."/>
		<msg @l="1125" char="11" flags="0" index="754" line="1125" name="亮"
		   text="「また液体がとろとろって出てきた」" timeout="" voices=""/>
		<msg @l="1127" char="-1" flags="0" index="756" line="1127" name=""
		   text="紗々ちゃんが腰をもじもじ動かすだけで割れ目がひくっと震え、\n:蜜がとろとろ垂れてくる。" timeout="" voices=""/>
		<msg @l="1128" char="-1" flags="0" index="758" line="1128" name=""
		   text="こんなにもエロいものが世の中にあったなんて。" timeout="" voices=""/>
