		<play_sound @l="1046" buf="5" fadeout="0" intro="0" src="sas80018" type="6"
		   volume="1."/>
		<msg @l="1047" char="11" flags="0" index="706" line="1047" name="亮"
		   text="「じゃあ、こういうのはどうだ？」" timeout="" voices=""/>
		<msg @l="1049" char="-1" flags="0" index="708" line="1049" name=""
		   text="親指と人差し指でクリトリスを挟むようにしながら、\n:くにくにと軽く動かしていく。" timeout="" voices=""/>
		<play_sound @l="1050" buf="5" fadeout="0" type="1"/>
		<chgbg @l="1052" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evsas04_07c" type="99"/>
		<msg @l="1054" char="2" flags="0" index="710" line="1054" name="紗々"
		   text="「にゃはんっ\x;heart:　センパイの指がぁっ、気持ちいいとこ優しく\n:擦ってきてぇ……はあっ、はあっ……\x;hearts:」"
		   timeout="" voices="sas17122"/>
		<msg @l="1056" char="2" flags="0" index="712" line="1056" name="紗々"
		   text="「ひゃうぅっ、クリちゃんをくりくりってされてるぅ……\n:はあっ、ああっ、す、すごいぃ……やっ、ひぐぐっ！！」" timeout=""
		   voices="sas17123"/>
		<play_sound @l="1058" buf="5" fadeout="0" intro="0" src="sas80019" type="6"
		   volume="1."/>
		<msg @l="1059" char="11" flags="0" index="714" line="1059" name="亮"
		   text="「あっ、ご、ごめんっ。痛かったか？」" timeout="" voices=""/>
		<play_sound @l="1060" buf="5" fadeout="0" type="1"/>
		<chgbg @l="1062" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evsas04_07a" type="99"/>
		<msg @l="1064" char="2" flags="0" index="716" line="1064" name="紗々"
		   text="「いえ、もっとしてほしいと言ったのは私ですから、\n:そこは大丈夫というか……」" timeout=""
		   voices="sas17124"/>
		<chgbg @l="1066" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evsas04_07b" type="99"/>
		<msg @l="1068" char="2" flags="0" index="718" line="1068" name="紗々"
		   text="「そうじゃなくて、えと……下着越しだともどかしくて、\n:物足りないというか……」" timeout=""
		   voices="sas17125"/>
		<play_sound @l="1070" buf="5" fadeout="0" intro="0" src="sas80015" type="6"
		   volume="1."/>
		<msg @l="1071" char="11" flags="0" index="720" line="1071" name="亮"
		   text="「ああ……なるほど」" timeout="" voices=""/>
		<msg @l="1073" char="-1" flags="0" index="722" line="1073" name=""
		   text="消え入りそうな声だったけど、つまり。" timeout="" voices=""/>
		<msg @l="1075" char="11" flags="0" index="724" line="1075" name="亮"
		   text="「直接さわるぞ」" timeout="" voices=""/>
		<play_sound @l="1076" buf="5" fadeout="0" type="1"/>
		<chgbg @l="1078" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evsas04_07a" type="99"/>
		<msg @l="1080" char="2" flags="0" index="726" line="1080" name="紗々"
		   text="「あっ……は、はい。よろしくお願いしますっ」" timeout="" voices="sas17126"/>
		<play_sound @l="1082" buf="5" fadeout="0" intro="0" src="sas80016" type="6"
		   volume="1."/>
		<msg @l="1083" char="-1" flags="0" index="728" line="1083" name=""
		   text="ちょっとだけ居住まいを正すように頷いた紗々ちゃんが、\n:スカートを脱がせた時と同じように腰を浮かせてくれる。" timeout=""
		   voices=""/>
		<play_sound @l="1085" buf="1" fadeout="0" intro="-1" src="se511" type="1"
		   volume="1."/>
		<chgbg @l="1089" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evsas04_08a" type="99"/>
		<msg @l="1091" char="11" flags="0" index="730" line="1091" name="亮"
		   text="「ふおおおっ……」" timeout="" voices=""/>
		<play_sound @l="1092" buf="1" fadeout="0" type="1"/>
		<play_sound @l="1094" buf="1" fadeout="0" intro="-1" src="hse101" type="1"
		   volume="1."/>
		<msg @l="1095" char="-1" flags="0" index="732" line="1095" name=""
		   text="指をかけて下げた瞬間、食い込むように割れ目に貼りついていた\n:パンツが、くちゅっと粘液の糸を引く。" timeout=""
		   voices=""/>
		<msg @l="1096" char="-1" flags="0" index="734" line="1096" name=""
		   text="けど、それよりなにより、女の子の濃い匂いに脳がくらっときた。" timeout="" voices=""/>
		<play_sound @l="1097" buf="5" fadeout="0" type="1"/>
		<play_sound @l="1098" buf="1" fadeout="0" type="1"/>
		<chgbg @l="1099" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evsas04_08b" type="99"/>
		<msg @l="1101" char="2" flags="0" index="736" line="1101" name="紗々"
		   text="「あぁ、は、恥ずかしいぃ……センパイに大事なところ、全部\n:見られちゃってる」" timeout=""
		   voices="sas17127"/>
		<play_sound @l="1103" buf="5" fadeout="0" intro="0" src="sas80017" type="6"
		   volume="1."/>
		<msg @l="1104" char="-1" flags="0" index="738" line="1104" name=""
		   text="これまで嗅いだことがない強烈な女の子の匂いは、\n:一瞬で鼻から脳に到達して、心拍数を跳ね上げる。" timeout=""
		   voices=""/>
		<msg @l="1105" char="-1" flags="0" index="740" line="1105" name=""
		   text="蒸したてのほかほか肉まんを二つに割ったときの蒸気みたいに、\n:一瞬で部屋いっぱいに紗々ちゃんの匂いが満ちた。" timeout=""
		   voices=""/>
		<play_sound @l="1106" buf="5" fadeout="0" type="1"/>
		<chgbg @l="1108" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evsas04_08a" type="99"/>
		<msg @l="1110" char="2" flags="0" index="742" line="1110" name="紗々"
		   text="「はあぅ、はあっ……あの、センパイ？」" timeout="" voices="sas17128"/>
		<play_sound @l="1112" buf="5" fadeout="0" intro="0" src="sas80015" type="6"
		   volume="1."/>
		<msg @l="1113" char="11" flags="0" index="744" line="1113" name="亮"
		   text="「すぅ、はぁぁぁ……」" timeout="" voices=""/>
		<msg @l="1115" char="-1" flags="0" index="746" line="1115" name=""
		   text="どことなく甘酸っぱい匂いを嗅ぎながら、紗々ちゃんの女の子の\n:部分をじっと見つめてしまう。" timeout=""
		   voices=""/>
		<msg @l="1116" char="-1" flags="0" index="748" line="1116" name=""
		   text="閉じ合わさった割れ目はちょっとだけ開いていて、\n:そこから透明な蜜が溢れている。" timeout="" voices=""/>
		<msg @l="1117" char="-1" flags="0" index="750" line="1117" name=""
		   text="上の方には皮のフードかぶった小さな突起があって、\n:それがクリトリスなんだと一目でわかった。" timeout=""
		   voices=""/>
