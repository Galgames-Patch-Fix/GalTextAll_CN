<?xml version="1.0" encoding="utf-8"?>
<xscript>
	<code>
		<msg @l="592" char="2" flags="0" index="404" line="592" name="紗々"
		   text="「ひくっ、あっ……はぁ、ああっ\x;hearts:」" timeout="" voices="sas17067"/>
		<play_sound @l="594" buf="5" fadeout="0" intro="0" src="sas80016" type="6"
		   volume="1."/>
		<msg @l="595" char="-1" flags="0" index="406" line="595" name=""
		   text="いきなり乳首をしゃぶることは出来なくて、その近く、\n:まずは乳輪のまわりをぺろっと舐める。" timeout=""
		   voices=""/>
		<msg @l="597" char="11" flags="0" index="408" line="597" name="亮"
		   text="「へえ、ちょっとしょっぱいんだな」" timeout="" voices=""/>
		<play_sound @l="598" buf="5" fadeout="0" type="1"/>
		<chgbg @l="600" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evsas04_04b" type="99"/>
		<msg @l="602" char="2" flags="0" index="410" line="602" name="紗々"
		   text="「あうっ、センパイが来る前にシャワー浴びたのに……\n:ドキドキして汗をかいちゃってるからでしょうか？」" timeout=""
		   voices="sas17068"/>
		<play_sound @l="604" buf="5" fadeout="0" intro="0" src="sas80017" type="6"
		   volume="1."/>
		<msg @l="605" char="11" flags="0" index="412" line="605" name="亮"
		   text="「僕が来る前にシャワーって！？」" timeout="" voices=""/>
		<play_sound @l="606" buf="5" fadeout="0" type="1"/>
		<chgbg @l="608" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evsas04_04c" type="99"/>
		<msg @l="610" char="2" flags="0" index="414" line="610" name="紗々"
		   text="「や、あの！　セックスするかもなんて思って浴びたわけじゃ……そりゃ、ちょっとは期待しましたけどっ」" timeout=""
		   voices="sas17069"/>
		<msg @l="612" char="2" flags="0" index="416" line="612" name="紗々"
		   text="「でも、そうじゃなくて少しでもいい匂いがした方がいいかな？　とか礼儀としてというか……」" timeout=""
		   voices="sas17070"/>
		<play_sound @l="614" buf="5" fadeout="0" intro="0" src="sas80015" type="6"
		   volume="1."/>
		<msg @l="615" char="-1" flags="0" index="418" line="615" name=""
		   text="しどろもどろになりながら、\n:あれこれ言い訳を繰り出してくる紗々ちゃん。" timeout="" voices=""/>
		<msg @l="616" char="-1" flags="0" index="420" line="616" name=""
		   text="その様子がおかしくて、ついつい頬が緩んでしまう。" timeout="" voices=""/>
		<play_sound @l="617" buf="5" fadeout="0" type="1"/>
		<chgbg @l="619" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evsas04_04b" type="99"/>
		<msg @l="621" char="2" flags="0" index="422" line="621" name="紗々"
		   text="「あー、センパイにエッチな子だって思われてます、\n:うぅっ、恥ずかしいぃ」" timeout=""
		   voices="sas17071"/>
		<play_sound @l="623" buf="5" fadeout="0" intro="0" src="sas80016" type="6"
		   volume="1."/>
		<msg @l="624" char="11" flags="0" index="424" line="624" name="亮"
		   text="「いいじゃないかエッチだって。ぼくだって頭の中はもうエロい\n:ことしか考えてないぞ」" timeout="" voices=""/>
		<msg @l="626" char="-1" flags="0" index="426" line="626" name=""
		   text="さっきまでめちゃくちゃテンパってたのに、紗々ちゃんのおかげでリラックスすることが出来た。" timeout="" voices=""/>
		<msg @l="627" char="-1" flags="0" index="428" line="627" name=""
		   text="好きな人同士でする行為―なるほど。普段のコミュニケーションの延長がセックスなのかも知れない。" timeout=""
		   voices=""/>
		<msg @l="629" char="11" flags="0" index="430" line="629" name="亮"
		   text="（僕の知らない紗々ちゃんが、どんどんいろんな顔を見せて\n:くれる……それが嬉しい）" timeout="" voices=""/>
		<msg @l="631" char="-1" flags="0" index="432" line="631" name=""
		   text="もっと知りたい好きな人のことを――。" timeout="" voices=""/>
		<msg @l="632" char="-1" flags="0" index="434" line="632" name=""
		   text="気がつくと、僕は彼女のおっぱいにむしゃぶりついていた。" timeout="" voices=""/>
		<msg @l="634" char="11" flags="0" index="436" line="634" name="亮"
		   text="「じゅるる、ぺろっ、ぺろぺろ……っ」" timeout="" voices=""/>
		<play_sound @l="635" buf="5" fadeout="0" type="1"/>
		<chgbg @l="637" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evsas04_04c" type="99"/>
		<msg @l="639" char="2" flags="0" index="438" line="639" name="紗々"
		   text="「ひゃふぅぅっ！？　センパイの舌がおっぱいに触れて\n:きてますぅ……っ」" timeout="" voices="sas17072"/>
		<play_sound @l="641" buf="5" fadeout="0" intro="0" src="sas80017" type="6"
		   volume="1."/>
		<msg @l="642" char="-1" flags="0" index="440" line="642" name=""
		   text="指で触れただけではわからない、紗々ちゃんのすべすべした肌の\n:感触を舌でしっかりと味わう。" timeout=""
		   voices=""/>
		<msg @l="643" char="-1" flags="0" index="442" line="643" name=""
		   text="彼女がどきどきするたび、おっぱいがびくっと跳ねるように\n:浮いて、それがちょっと楽しい。" timeout="" voices=""/>
		<msg @l="645" char="11" flags="0" index="444" line="645" name="亮"
		   text="「んむっ、ちゅっ……乳首も、いい？」" timeout="" voices=""/>
		<play_sound @l="646" buf="5" fadeout="0" type="1"/>
		<chgbg @l="648" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evsas04_04a" type="99"/>
		<msg @l="650" char="2" flags="0" index="446" line="650" name="紗々"
		   text="「はいっ。むしろそこを舐めるのが、たぶん正解じゃない\n:かなって……\x;heart:」" timeout=""
		   voices="sas17073"/>
		<play_sound @l="652" buf="5" fadeout="0" intro="0" src="sas80015" type="6"
		   volume="1."/>
		<msg @l="653" char="11" flags="0" index="448" line="653" name="亮"
		   text="「じゃあ、いただきます」" timeout="" voices=""/>
		<play_sound @l="654" buf="5" fadeout="0" type="1"/>
		<chgbg @l="656" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evsas04_04e" type="99"/>
		<msg @l="658" char="2" flags="0" index="450" line="658" name="紗々"
		   text="「んんっ！？　はあっ……\x;heart:」" timeout="" voices="sas17074"/>
		<play_sound @l="660" buf="5" fadeout="0" intro="0" src="sas80018" type="6"
		   volume="1."/>
		<msg @l="661" char="-1" flags="0" index="452" line="661" name=""
		   text="乳首に吸い付いた瞬間、これまでで一番可愛い声を漏らして\n:紗々ちゃんがのけぞった。" timeout="" voices=""/>
		<msg @l="662" char="-1" flags="0" index="454" line="662" name=""
		   text="その動きに後押しされ、口内が乳首でいっぱいになる。" timeout="" voices=""/>
		<msg @l="664" char="11" flags="0" index="456" line="664" name="亮"
		   text="「ふむっ、はあっ……れろれろ、れるっ、ちゅっ」" timeout="" voices=""/>
		<play_sound @l="665" buf="5" fadeout="0" type="1"/>
		<chgbg @l="667" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evsas04_04b" type="99"/>
		<msg @l="669" char="2" flags="0" index="458" line="669" name="紗々"
		   text="「わ、わわっ……センパイが私の乳首、ちゅーちゅー\n:してますっ……はあっ、ああっ」" timeout=""
		   voices="sas17075"/>
		<chgbg @l="671" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evsas04_04c" type="99"/>
		<msg @l="673" char="2" flags="0" index="460" line="673" name="紗々"
		   text="「センパイが、あのセンパイが私の乳首をぉっ……\n:はふぅ、ふぅ、はあああっ\x;heart:」" timeout=""
		   voices="sas17076"/>
		<play_sound @l="675" buf="5" fadeout="0" intro="0" src="sas80019" type="6"
		   volume="1."/>
		<msg @l="676" char="-1" flags="0" index="462" line="676" name=""
		   text="舌の動きにあわせ、紗々ちゃんが甘い声を漏らす。" timeout="" voices=""/>
		<msg @l="677" char="-1" flags="0" index="464" line="677" name=""
		   text="びくんびくんと電流が走ったように震える身体から力が抜け、\n:僕に身を委ねてくれる。" timeout="" voices=""/>
		<msg @l="679" char="11" flags="0" index="466" line="679" name="亮"
		   text="「さっきより、乳首がかたくなってきたかも……」" timeout="" voices=""/>
		<msg @l="681" char="-1" flags="0" index="468" line="681" name=""
		   text="最初は舌で押し潰せそうなぐらい柔らかかったのに、\n:いつのまにかグミみたいな硬さになっている。" timeout=""
		   voices=""/>
		<msg @l="682" char="-1" flags="0" index="470" line="682" name=""
		   text="舌先で押しても反発されて、それがまたクセになるほど魅力的で、ついつい舌先でつついてしまう。" timeout="" voices=""/>
		<play_sound @l="683" buf="5" fadeout="0" type="1"/>
		<chgbg @l="685" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evsas04_04b" type="99"/>
		<msg @l="687" char="2" flags="0" index="472" line="687" name="紗々"
		   text="「こんなに気持ちいいのは初めて……あっ、やあっ、\n:センパイそんなに舌でくりくりしちゃっ……」" timeout=""
		   voices="sas17077"/>
		<chgbg @l="689" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evsas04_04c" type="99"/>
		<msg @l="691" char="2" flags="0" index="474" line="691" name="紗々"
		   text="「ひぅ、はあっ……あ、舌すごい、れろれろって……\n:はふぅ、んふぅ、ふぅっ……ああっ」" timeout=""
		   voices="sas17078"/>
		<play_sound @l="693" buf="5" fadeout="0" intro="0" src="sas80020" type="6"
		   volume="1."/>
		<msg @l="694" char="11" flags="0" index="476" line="694" name="亮"
		   text="「んちゅっ、れろっ……はぁはぁ、はあっ……」" timeout="" voices=""/>
		<msg @l="696" char="-1" flags="0" index="478" line="696" name=""
		   text="充血してきたのか、ぷっくりと乳輪と乳首が膨らんできた。" timeout="" voices=""/>
		<msg @l="697" char="-1" flags="0" index="480" line="697" name=""
		   text="舌先をすぼめ、より強くその感触を味わう。" timeout="" voices=""/>
		<msg @l="699" char="11" flags="0" index="482" line="699" name="亮"
		   text="「はむ、んっ……あぁ、おっぱい、おいしい……」" timeout="" voices=""/>
		<msg @l="701" char="-1" flags="0" index="484" line="701" name=""
		   text="舐めれば舐めるほど、私をしゃぶってほしいと乳首自体が\n:せがんでくるみたいだ。" timeout="" voices=""/>
		<play_sound @l="702" buf="5" fadeout="0" type="1"/>
		<chgbg @l="704" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evsas04_04a" type="99"/>
		<msg @l="706" char="2" flags="0" index="486" line="706" name="紗々"
		   text="「はぁ、はあっ……んぅ、センパイの舌、気持ちよすぎます……あぁんっ」" timeout="" voices="sas17079"/>
		<msg @l="708" char="2" flags="0" index="488" line="708" name="紗々"
		   text="「あのっ、このまま乳首ちゅーちゅーされ続けたら、\n:私、おかしくなっちゃうかもなのでっ……」" timeout=""
		   voices="sas17080"/>
		<chgbg @l="710" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evsas04_04b" type="99"/>
		<msg @l="712" char="2" flags="0" index="490" line="712" name="紗々"
		   text="「そうなる前に、その、えとっ……ちがうところも\n:ですねっ……？」" timeout="" voices="sas17081"/>
		<play_sound @l="714" buf="5" fadeout="0" intro="0" src="sas80015" type="6"
		   volume="1."/>
		<msg @l="715" char="11" flags="0" index="492" line="715" name="亮"
		   text="「ぷはっ……違うところって言うと――」" timeout="" voices=""/>
		<play_sound @l="716" buf="5" fadeout="0" type="1"/>
		<msg @l="718" char="2" flags="0" index="494" line="718" name="紗々"
		   text="「ですから、下の方というか、なんというか」" timeout="" voices="sas17082"/>
		<play_sound @l="720" buf="5" fadeout="0" intro="0" src="sas80016" type="6"
		   volume="1."/>
		<msg @l="721" char="-1" flags="0" index="496" line="721" name=""
		   text="呟いた紗々ちゃんが、もじもじと内股をすりあわせると、\n:スカートがめくれてパンツがちらっと見えた。" timeout=""
		   voices=""/>
		<msg @l="723" char="11" flags="0" index="498" line="723" name="亮"
		   text="「おおぉ……っ」" timeout="" voices=""/>
		<msg @l="725" char="-1" flags="0" index="500" line="725" name=""
		   text="そうだ。エッチをするなら、おっぱいばっかりに夢中になってちゃダメなんだ。" timeout="" voices=""/>
		<msg @l="726" char="-1" flags="0" index="502" line="726" name=""
		   text="ひとつになるためには、下半身こそが本丸なんだから。" timeout="" voices=""/>
		<msg @l="728" char="11" flags="0" index="504" line="728" name="亮"
		   text="「じゃあ、スカートも脱がせていいか？」" timeout="" voices=""/>
		<play_sound @l="729" buf="5" fadeout="0" type="1"/>
		<chgbg @l="731" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evsas04_04d" type="99"/>
		<msg @l="733" char="2" flags="0" index="506" line="733" name="紗々"
		   text="「はい、もちろんっ……」" timeout="" voices="sas17083"/>
		<play_sound @l="735" buf="5" fadeout="0" intro="0" src="sas80017" type="6"
		   volume="1."/>
		<msg @l="736" char="-1" flags="0" index="508" line="736" name=""
		   text="ちょっとはリードしなくちゃと、今度は自分から提案して\n:紗々ちゃんの腰に手を添える。" timeout="" voices=""/>
		<play_sound @l="737" buf="5" fadeout="0" type="1"/>
		<chgbg @l="739" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evsas04_04b" type="99"/>
		<msg @l="741" char="2" flags="0" index="510" line="741" name="紗々"
		   text="「お尻を浮かせるので、せーので――」" timeout="" voices="sas17084"/>
		<play_sound @l="743" buf="5" fadeout="0" intro="0" src="sas80015" type="6"
		   volume="1."/>
		<msg @l="744" char="11" flags="0" index="512" line="744" name="亮"
		   text="「せーのっ……」" timeout="" voices=""/>
		<play_sound @l="745" buf="5" fadeout="0" type="1"/>
		<play_sound @l="746" buf="1" fadeout="0" intro="-1" src="se511" type="1"
		   volume="1."/>
		<chgbg @l="750" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evsas04_05b" type="99"/>
		<msg @l="752" char="2" flags="0" index="514" line="752" name="紗々"
		   text="「あっ、はあっ、ああっ……」" timeout="" voices="sas17085"/>
		<play_sound @l="753" buf="1" fadeout="0" type="1"/>
		<play_sound @l="754" buf="5" fadeout="0" intro="0" src="sas80016" type="6"
		   volume="1."/>
		<msg @l="755" char="11" flags="0" index="516" line="755" name="亮"
		   text="「わ、こっちも可愛い……お揃いだ」" timeout="" voices=""/>
		<msg @l="757" char="-1" flags="0" index="518" line="757" name=""
		   text="スカートを脱いで丸見えになったパンツは、ブラと同じ色合い、\n:デザインでお揃いなんだと一目でわかる。" timeout=""
		   voices=""/>
		<play_sound @l="758" buf="5" fadeout="0" type="1"/>
		<msg @l="760" char="2" flags="0" index="520" line="760" name="紗々"
		   text="「これ、リボンがすっごく可愛いのでお気に入りで……」" timeout="" voices="sas17086"/>
		<play_sound @l="762" buf="5" fadeout="0" intro="0" src="sas80017" type="6"
		   volume="1."/>
		<msg @l="763" char="11" flags="0" index="522" line="763" name="亮"
		   text="「リボンって……これのことか？」" timeout="" voices=""/>
		<msg @l="765" char="-1" flags="0" index="524" line="765" name=""
		   text="言われてみれば、中央にリボンがちょこんとついている。" timeout="" voices=""/>
		<msg @l="767" char="11" flags="0" index="526" line="767" name="亮"
		   text="「……なんだか、紗々ちゃんみたいだ」" timeout="" voices=""/>
		<play_sound @l="768" buf="5" fadeout="0" type="1"/>
		<chgbg @l="770" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evsas04_05a" type="99"/>
		<msg @l="772" char="2" flags="0" index="528" line="772" name="紗々"
		   text="「えっ？　それって、どういう――」" timeout="" voices="sas17087"/>
		<play_sound @l="774" buf="5" fadeout="0" intro="0" src="sas80015" type="6"
		   volume="1."/>
		<msg @l="775" char="11" flags="0" index="530" line="775" name="亮"
		   text="「控えめだけど、すっごく可愛いってこと」" timeout="" voices=""/>
		<play_sound @l="776" buf="5" fadeout="0" type="1"/>
		<chgbg @l="778" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evsas04_05b" type="99"/>
		<msg @l="781" char="2" flags="0" index="532" line="781" name="紗々"
		   text="「……センパイって、たまにしれっとそういう恥ずかしいこと、\n:平気で言うことありますよね？」" timeout=""
		   voices="sas17088"/>
		<play_sound @l="783" buf="5" fadeout="0" intro="0" src="sas80016" type="6"
		   volume="1."/>
		<msg @l="784" char="11" flags="0" index="534" line="784" name="亮"
		   text="「思ったことを素直に言っただけなんだが……はて？」" timeout="" voices=""/>
		<play_sound @l="785" buf="5" fadeout="0" type="1"/>
		<chgbg @l="787" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evsas04_05e" type="99"/>
		<msg @l="789" char="2" flags="0" index="536" line="789" name="紗々"
		   text="「可愛いって言われると、その言葉だけで……はふぅ、\n:んふぅっ\x;hearts:」" timeout=""
		   voices="sas17089"/>
		<play_sound @l="791" buf="5" fadeout="0" intro="0" src="sas80017" type="6"
		   volume="1."/>
		<msg @l="792" char="-1" flags="0" index="538" line="792" name=""
		   text="火照った息を漏らしながら、紗々ちゃんが太ももをもじもじと\n:こすり合わせる。" timeout="" voices=""/>
		<msg @l="794" char="11" flags="0" index="540" line="794" name="亮"
		   text="「あっ。下着に、ちっちゃな染みが出来てる」" timeout="" voices=""/>
		<play_sound @l="795" buf="5" fadeout="0" type="1"/>
		<chgbg @l="797" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evsas04_05c" type="99"/>
		<msg @l="799" char="2" flags="0" index="542" line="799" name="紗々"
		   text="「ひゃいいっ？！　やっ、あっ、そっ、そそそそそれはちがっ、やっ、あああうっ……？！」" timeout=""
		   voices="sas17090"/>
		<play_sound @l="801" buf="5" fadeout="0" intro="0" src="sas80015" type="6"
		   volume="1."/>
		<msg @l="802" char="11" flags="0" index="544" line="802" name="亮"
		   text="「さ、紗々ちゃん？！　落ちついてっ？！」" timeout="" voices=""/>
		<play_sound @l="803" buf="5" fadeout="0" type="1"/>
