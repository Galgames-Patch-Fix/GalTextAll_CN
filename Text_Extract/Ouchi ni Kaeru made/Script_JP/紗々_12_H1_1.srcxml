		   param="16" screen="1" src="evsas04_08b" type="99"/>
		<msg @l="1183" char="2" flags="0" index="792" line="1183" name="紗々"
		   text="「答えになってませんよぉ……」" timeout="" voices="sas17136"/>
		<play_sound @l="1185" buf="5" fadeout="0" intro="0" src="sas80015" type="6"
		   volume="1."/>
		<msg @l="1186" char="-1" flags="0" index="794" line="1186" name=""
		   text="脱ぎかけ、というのもそれはそれで趣があるけど、これから\n:初エッチなんだし生まれたままの姿もみたい。" timeout=""
		   voices=""/>
		<msg @l="1187" char="-1" flags="0" index="796" line="1187" name=""
		   text="更に言えば、自分の手で脱がせていくというシチュエーションだとなおのことよし！" timeout="" voices=""/>
		<msg @l="1189" char="11" flags="0" index="798" line="1189" name="亮"
		   text="「全部、脱がせていいか？」" timeout="" voices=""/>
		<play_sound @l="1190" buf="5" fadeout="0" type="1"/>
		<chgbg @l="1192" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evsas04_08d" type="99"/>
		<msg @l="1194" char="2" flags="0" index="800" line="1194" name="紗々"
		   text="「はい、よろしくお願いします」" timeout="" voices="sas17137"/>
		<play_sound @l="1196" buf="5" fadeout="0" intro="0" src="sas80016" type="6"
		   volume="1."/>
		<msg @l="1197" char="-1" flags="0" index="802" line="1197" name=""
		   text="紗々ちゃんの衣服に手をかけ、しゅるしゅると音を立てながら\n:脱がせていく。" timeout="" voices=""/>
		<msg @l="1199" char="11" flags="0" index="804" line="1199" name="亮"
		   text="（この脱がせていく瞬間ってすげえ興奮する。\n:これだけで射精しそうだ……）" timeout="" voices=""/>
		<msg @l="1201" char="-1" flags="0" index="806" line="1201" name=""
		   text="下腹部に力を入れてなんとか踏ん張り、パンツを掴んでスルリと\n:つま先までおろしていった。" timeout="" voices=""/>
		<play_sound @l="1202" buf="1" fadeout="0" intro="-1" src="se511" type="1"
		   volume="1."/>
		<chgbg @l="1206" duration="300" flags="256" flip="-1" flip_flags="0"
