		<msg @l="1142" char="11" flags="0" index="766" line="1142" name="亮"
		   text="「えぇっ、そんなにか？」" timeout="" voices=""/>
		<play_sound @l="1143" buf="5" fadeout="0" type="1"/>
		<msg @l="1145" char="2" flags="0" index="768" line="1145" name="紗々"
		   text="「はい。パンツを脱がしたとたん急に黙り込んじゃって、そのまま私のあそこをじっと見つめたまま動かなくなっちゃいました」"
		   timeout="" voices="sas17132"/>
		<play_sound @l="1147" buf="5" fadeout="0" intro="0" src="sas80016" type="6"
		   volume="1."/>
		<msg @l="1148" char="11" flags="0" index="770" line="1148" name="亮"
		   text="「す、すまん。つい見入ってたみたいだ」" timeout="" voices=""/>
