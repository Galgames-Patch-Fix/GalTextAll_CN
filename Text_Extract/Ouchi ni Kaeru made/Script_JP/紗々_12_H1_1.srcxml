		<msg @l="1136" char="11" flags="0" index="762" line="1136" name="亮"
		   text="「えっ？　な、なんだ？」" timeout="" voices=""/>
		<play_sound @l="1137" buf="5" fadeout="0" type="1"/>
		<msg @l="1139" char="2" flags="0" index="764" line="1139" name="紗々"
		   text="「大丈夫ですか？　さっきからずっと呼びかけてたんですけど\n:返事がなくて」" timeout=""
