<?xml version="1.0" encoding="utf-8"?>
<xscript>
	<code>
		<label @l="1" id="scene_1_03"/>
		<play_sound @l="13" buf="0" fadeout="2500" type="0"/>
		<showmsg @l="14" duration="500" show="0"/>
		<loadimage @l="19" bg="0" fit="0" screen="1" src="evusi11_01a"/>
		<scroll @l="20" async="0" center_x="45" center_y="45" duration="0" flags="0"
		   rev="0." screen="1" transparency="0" v0="0." v1="0." wait_flags="0" x="0"
		   y="0" zoom="1."/>
		<chgscrn @l="21" duration="2500" flags="4608" flip="-1" flip_flags="0"
		   param="0" screen="1" type="10"/>
		<play_sound @l="22" buf="0" fadeout="500" intro="-1" src="bgm18" type="0"
		   volume="1."/>
		<msg @l="27" char="1" flags="0" index="0" line="27" name="汐"
		   text="「んっと……こうして、こんな感じで……」" timeout="" voices="usi11100"/>
		<msg @l="30" char="11" flags="0" index="2" line="30" name="亮" text="「うわわ……」"
		   timeout="" voices=""/>
		<msg @l="32" char="-1" flags="0" index="4" line="32" name=""
		   text="いつもの慣れた手つき――ではなく、ちょっと\n:たどたどしい感じで、生クリームがおっぱいに\n:デコレーションされていく。"
		   timeout="" voices=""/>
		<msg @l="34" char="1" flags="0" index="6" line="34" name="汐"
		   text="「……どう、かな？　美味しそうに見える……？」" timeout="" voices="usi11101"/>
		<msg @l="36" char="11" flags="0" index="8" line="36" name="亮"
		   text="「もちろん……朝霞さんの乳首、すごく可愛い」" timeout="" voices=""/>
		<chgbg @l="39" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evusi11_01b" type="99"/>
		<msg @l="41" char="1" flags="0" index="10" line="41" name="汐"
		   text="「んッ……き、キミも随分とはっきり言うよね？」" timeout="" voices="usi11102"/>
		<msg @l="43" char="11" flags="0" index="12" line="43" name="亮"
		   text="「だって、実際可愛いですし……ん、ごくっ」" timeout="" voices=""/>
		<msg @l="45" char="-1" flags="0" index="14" line="45" name=""
		   text="クリームに飾られた乳首は不思議と卑猥で、見てるだけでも\n:生唾がどんどん溢れてくる。" timeout="" voices=""/>
		<msg @l="47" char="11" flags="0" index="16" line="47" name="亮"
		   text="「あの、早く舐めたいので、もっとこっちに……」" timeout="" voices=""/>
		<chgbg @l="49" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evusi11_01a" type="99"/>
		<msg @l="51" char="1" flags="0" index="18" line="51" name="汐"
		   text="「あっ、うん。ちょっと待ってね、いま――」" timeout="" voices="usi11103"/>
		<msg @l="53" char="11" flags="0" index="20" line="53" name="亮" text="「あむっ」"
		   timeout="" voices=""/>
		<chgbg @l="55" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evusi11_01d" type="99"/>
		<msg @l="57" char="1" flags="0" index="22" line="57" name="汐"
		   text="「ひゃんっ？！　ちょっ……んくッ、はあっ」" timeout="" voices="usi11104"/>
		<play_sound @l="61" buf="5" fadeout="0" intro="0" src="usi80017" type="6"
		   volume="1."/>
		<msg @l="62" char="-1" flags="0" index="24" line="62" name=""
		   text="前屈みになり、接近してきたちっぱいからこぼれ落ちそうになる\n:生クリームごと、つんと勃つ乳首にしゃぶりつく。" timeout=""
		   voices=""/>
		<play_sound @l="63" buf="5" fadeout="0" type="1"/>
		<chgbg @l="65" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evusi11_01b" type="99"/>
		<msg @l="67" char="1" flags="0" index="26" line="67" name="汐"
		   text="「ちょっ、だ、だめだってばっ……\n:まだ、いいって言ってないのにっ……ふぅ、んんんっ」" timeout=""
		   voices="usi11105"/>
		<play_sound @l="71" buf="5" fadeout="0" intro="0" src="usi80016" type="6"
		   volume="1."/>
		<msg @l="72" char="11" flags="0" index="28" line="72" name="亮"
		   text="「だって、あむっ……生クリームが、落ちちゃいそうでっ……\n:れろ、ちゅっ」" timeout="" voices=""/>
		<play_sound @l="73" buf="5" fadeout="0" type="1"/>
		<chgbg @l="75" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evusi11_01d" type="99"/>
		<msg @l="77" char="1" flags="0" index="30" line="77" name="汐"
		   text="「はうんっ、だ、だからってそんなにいきなりっ……ふぅ、\n:はあっ、んくくっ……はぁ、はぁ\x;hearts:」"
		   timeout="" voices="usi11106"/>
		<play_sound @l="79" buf="5" fadeout="0" intro="0" src="usi80015" type="6"
		   volume="1."/>
		<msg @l="80" char="-1" flags="0" index="32" line="80" name=""
		   text="不意打ちを食らった朝霞さんは、それでも僕の口から\n:乳首が離れないように、身を捩るのを堪えた。" timeout=""
		   voices=""/>
		<msg @l="81" char="-1" flags="0" index="34" line="81" name=""
		   text="そのいじらしさが可愛くて、さらにしゃぶりついてしまう。" timeout="" voices=""/>
		<play_sound @l="82" buf="5" fadeout="0" type="1"/>
		<chgbg @l="84" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evusi11_01c" type="99"/>
		<msg @l="86" char="1" flags="0" index="36" line="86" name="汐"
		   text="「んっ……そ、それで？　生クリーム、美味しい？」" timeout="" voices="usi11107"/>
		<play_sound @l="88" buf="5" fadeout="0" intro="0" src="usi80016" type="6"
		   volume="1."/>
		<msg @l="89" char="11" flags="0" index="38" line="89" name="亮"
		   text="「はい、とってもっ……あむ、れろっ」" timeout="" voices=""/>
		<msg @l="91" char="-1" flags="0" index="40" line="91" name=""
		   text="普段ならケーキを食べるってなっただけでも身構えてしまうのに、\n:今はこのおっぱいケーキを貪るようになめ回している。"
		   timeout="" voices=""/>
		<msg @l="92" char="-1" flags="0" index="42" line="92" name=""
		   text="口の中に広がる甘さが心地いい。" timeout="" voices=""/>
		<play_sound @l="93" buf="5" fadeout="0" type="1"/>
		<chgbg @l="95" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evusi11_02b" type="99"/>
		<msg @l="97" char="1" flags="0" index="44" line="97" name="汐"
		   text="「わっ、わわっ……あんなにたっぷり塗ったのに、\n:あっという間になくなってくよぅっ……はあっ」" timeout=""
		   voices="usi11108"/>
		<play_sound @l="99" buf="5" fadeout="0" intro="0" src="usi80017" type="6"
		   volume="1."/>
		<msg @l="100" char="11" flags="0" index="46" line="100" name="亮"
		   text="「あむあむ、ぺろっ……生クリームって、\n:こんなに美味しいものだったんですね……」" timeout="" voices=""/>
		<msg @l="102" char="-1" flags="0" index="48" line="102" name=""
		   text="悶える朝霞さんのおっぱいに、夢中でしゃぶりつく。" timeout="" voices=""/>
		<play_sound @l="103" buf="5" fadeout="0" type="1"/>
		<chgbg @l="107" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evusi11_02a" type="99"/>
		<msg @l="109" char="1" flags="0" index="50" line="109" name="汐"
		   text="「んぅんっ、ふぅ……でも、本当に食べられてるみたいだし……\n:やっぱりこの方法、試してよかった……」" timeout=""
		   voices="usi11109"/>
		<play_sound @l="111" buf="5" fadeout="0" intro="0" src="usi80015" type="6"
		   volume="1."/>
		<msg @l="112" char="11" flags="0" index="52" line="112" name="亮"
		   text="「ほんと、すっごい美味しいです……。\n:とくに、この先っぽのところについたのが……れろろっ」" timeout=""
		   voices=""/>
		<play_sound @l="113" buf="5" fadeout="0" type="1"/>
		<chgbg @l="115" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evusi11_02d" type="99"/>
		<msg @l="117" char="1" flags="0" index="54" line="117" name="汐"
		   text="「やんっ、そこは……はぁはぁ、だっ、だめだよ、\n:そんなに舌を使ったらぁっ……ふぅ、はあっ\x;hearts:」"
		   timeout="" voices="usi11110"/>
		<play_sound @l="119" buf="5" fadeout="0" intro="0" src="usi80016" type="6"
		   volume="1."/>
		<msg @l="120" char="-1" flags="0" index="56" line="120" name=""
		   text="小さな膨らみに舌を這わせているかと思うと、\n:背徳感による興奮で、自然と息が荒くなる。" timeout="" voices=""/>
		<play_sound @l="121" buf="5" fadeout="0" type="1"/>
		<chgbg @l="125" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evusi11_02a" type="99"/>
		<msg @l="127" char="1" flags="0" index="58" line="127" name="汐"
		   text="「もう、ダメでしょ？　キミが味わうべきは、\n:私のおっぱいじゃなくて生クリームなのにぃ……ふぅ、はあっ」" timeout=""
		   voices="usi11111"/>
		<msg @l="129" char="1" flags="0" index="60" line="129" name="汐"
		   text="「でも、どう？　これなら、おかわりも食べられそう？」" timeout="" voices="usi11112"/>
		<play_sound @l="131" buf="5" fadeout="0" intro="0" src="usi80015" type="6"
		   volume="1."/>
		<msg @l="132" char="11" flags="0" index="62" line="132" name="亮"
		   text="「はい。朝霞さんのもっと舐めたいです」" timeout="" voices=""/>
		<play_sound @l="133" buf="5" fadeout="0" type="1"/>
		<chgbg @l="135" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evusi11_02d" type="99"/>
		<msg @l="136" char="1" flags="0" index="64" line="136" name="汐"
		   text="「んッ……わ、私のっていうと、なんか変な感じがしない？」" timeout="" voices="usi11113"/>
		<play_sound @l="138" buf="5" fadeout="0" intro="0" src="usi80016" type="6"
		   volume="1."/>
		<msg @l="139" char="11" flags="0" index="66" line="139" name="亮"
		   text="「でも、いま僕が舐めてるのは、朝霞さんお手製の\n:生クリームですよね？」" timeout="" voices=""/>
		<play_sound @l="140" buf="5" fadeout="0" type="1"/>
		<msg @l="142" char="1" flags="0" index="68" line="142" name="汐"
		   text="「う、うん。確かにキミのことを想って作った、\n:甘さ控えめのお手製生クリームだけど」" timeout=""
		   voices="usi11114"/>
		<play_sound @l="144" buf="5" fadeout="0" intro="0" src="usi80016" type="6"
		   volume="1."/>
		<msg @l="145" char="11" flags="0" index="70" line="145" name="亮"
		   text="「朝霞さんの生クリーム――それってもう、朝霞さんの乳製品……\n:生ミルクってことになりません？」" timeout=""
		   voices=""/>
		<play_sound @l="146" buf="5" fadeout="0" type="1"/>
		<chgbg @l="150" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evusi11_02b" type="99"/>
		<msg @l="152" char="1" flags="0" index="72" line="152" name="汐"
		   text="「なりませんっ！　って、はぁ、やっ……\n:なっ、なんでクリームなくなっても乳首ぺろぺろしてるのっ」" timeout=""
		   voices="usi11115"/>
		<play_sound @l="154" buf="5" fadeout="0" intro="0" src="usi80017" type="6"
		   volume="1."/>
		<msg @l="155" char="11" flags="0" index="74" line="155" name="亮"
		   text="「おかわり、なかなかくれないので……れろ、ちゅっ」" timeout="" voices=""/>
		<showmsg @l="157" duration="500" show="0"/>
		<scroll @l="158" async="-1" center_x="540" center_y="260" duration="1000"
		   flags="0" rev="0." screen="0" transparency="0" v0="0." v1="0."
		   wait_flags="0" x="0" y="0" zoom="1.5"/>
		<msg @l="160" char="-1" flags="0" index="76" line="160" name=""
		   text="かすかに残る生クリームの甘さを味わうようにして、それはもう、\n:丹念に彼女の乳首に舌を絡める。" timeout=""
		   voices=""/>
		<play_sound @l="161" buf="5" fadeout="0" type="1"/>
		<msg @l="165" char="1" flags="0" index="78" line="165" name="汐"
		   text="「ふぅ、はぁ……くっ、はあっ、ああっ……んんッ\x;hearts:」" timeout="" voices="usi11116"/>
		<msg @l="167" char="1" flags="0" index="80" line="167" name="汐"
		   text="「最初はお試しのつもりで、ちょっと量を控えめにしたとはいえ、\n:それを一瞬でぺろっと食べちゃうなんてっ……」" timeout=""
		   voices="usi11117"/>
		<play_sound @l="169" buf="5" fadeout="0" intro="0" src="usi80016" type="6"
		   volume="1."/>
		<msg @l="170" char="11" flags="0" index="82" line="170" name="亮"
		   text="「次はもっといっぱいでも平気です……ちゅ、あむ」" timeout="" voices=""/>
		<play_sound @l="171" buf="5" fadeout="0" type="1"/>
		<msg @l="173" char="1" flags="0" index="84" line="173" name="汐"
		   text="「う、うんっ……。じゃあ、いまおかわりを塗ってあげるから、\n:ちょっとだけ待ってくれる？」" timeout=""
		   voices="usi11118"/>
		<showmsg @l="176" duration="500" show="0"/>
		<loadimage @l="177" bg="0" fit="0" screen="1" src="evusi11_02c"/>
		<scroll @l="178" async="0" center_x="45" center_y="45" duration="0"
		   flags="0" rev="0." screen="1" transparency="0" v0="0." v1="0."
		   wait_flags="0" x="0" y="0" zoom="1."/>
		<chgscrn @l="179" duration="1000" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" type="99"/>
		<play_sound @l="183" buf="5" fadeout="0" intro="0" src="usi80015" type="6"
		   volume="1."/>
		<msg @l="184" char="11" flags="0" index="86" line="184" name="亮"
		   text="「あ、はいっ……はぁ、はぁ」" timeout="" voices=""/>
		<msg @l="186" char="-1" flags="0" index="88" line="186" name=""
		   text="お預けをされる犬の気持ちで、遠ざかるちっぱいを見送る。" timeout="" voices=""/>
		<play_sound @l="187" buf="5" fadeout="0" type="1"/>
		<chgbg @l="189" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evusi11_02b" type="99"/>
		<msg @l="191" char="1" flags="0" index="90" line="191" name="汐"
		   text="「はぁ、んぅ……うわ、すごいよぅ、キミのよだれで、\n:おっぱい、こんなにとろとろに……」" timeout=""
		   voices="usi11119"/>
		<msg @l="194" char="11" flags="0" index="92" line="194" name="亮"
		   text="「美味しくて、つい……」" timeout="" voices=""/>
		<msg @l="196" char="-1" flags="0" index="94" line="196" name=""
		   text="夢心地とは、まさにこのこと。" timeout="" voices=""/>
		<msg @l="197" char="-1" flags="0" index="96" line="197" name=""
		   text="口の中で広がる甘さと舌で感じる突起のハーモニーは絶妙で、\n:許されることなら、ずっとこれだけを舐めていたいぐらいだ。"
		   timeout="" voices=""/>
		<msg @l="199" char="11" flags="0" index="98" line="199" name="亮"
		   text="「あの、はやくおかわりを……」" timeout="" voices=""/>
		<chgbg @l="201" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evusi11_01c" type="99"/>
		<msg @l="203" char="1" flags="0" index="100" line="203" name="汐"
		   text="「んぅ、んっと……なら、少し量を多めに――ふぅ、んんっ……\n:っと、これでどう？」" timeout=""
		   voices="usi11120"/>
		<msg @l="205" char="11" flags="0" index="102" line="205" name="亮"
		   text="「おおっ……」" timeout="" voices=""/>
		<msg @l="207" char="-1" flags="0" index="104" line="207" name=""
		   text="僕の食べっぷりを考慮してか、おかわりはかなり増量だった。" timeout="" voices=""/>
		<msg @l="209" char="11" flags="0" index="106" line="209" name="亮"
		   text="「こ、こんなにいっぱい舐めていいんですか？」" timeout="" voices=""/>
		<chgbg @l="211" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evusi11_01a" type="99"/>
		<msg @l="213" char="1" flags="0" index="108" line="213" name="汐"
		   text="「もちろんだよ。この特訓は、キミがケーキを食べられるように\n:するためのものなんだから」" timeout=""
		   voices="usi11121"/>
		<msg @l="217" char="1" flags="0" index="110" line="217" name="汐"
		   text="「……それに、どんな形であれ、自分の生クリームを美味しそうに\n:食べてもらえるのは、やっぱり嬉しいもん」" timeout=""
		   voices="usi11122"/>
		<msg @l="219" char="11" flags="0" index="112" line="219" name="亮"
		   text="「朝霞さん……」" timeout="" voices=""/>
		<chgbg @l="221" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evusi11_01e" type="99"/>
		<msg @l="223" char="1" flags="0" index="114" line="223" name="汐"
		   text="「あっ、でも、さっきのはちょっとお行儀が悪かったよ？」" timeout="" voices="usi11123"/>
		<msg @l="225" char="1" flags="0" index="116" line="225" name="汐"
		   text="「いただきますもせずに、いきなりしゃぶりつくなんて……\n:特訓とはいえ、これも立派なお食事なんだからね」" timeout=""
		   voices="usi11124"/>
		<msg @l="227" char="1" flags="0" index="118" line="227" name="汐"
		   text="「いーい？　今度は、ちゃんといただきますをすること。\n:それが約束できるなら、おかわりを食べさせてあげる」" timeout=""
		   voices="usi11125"/>
		<msg @l="229" char="-1" flags="0" index="120" line="229" name=""
		   text="見上げた朝霞さんの表情はちょっと悪戯っぽい笑顔で、\n:それがまた、ぞくぞくっと背徳感を刺激する。" timeout=""
		   voices=""/>
		<msg @l="231" char="1" flags="0" index="122" line="231" name="汐"
		   text="「ほら、いただきますは？　うちのお店にくる\n:ちっちゃな子だって、ケーキを食べる前は\n:ちゃんと言えてるよ？」"
		   timeout="" voices="usi11126"/>
		<msg @l="233" char="11" flags="0" index="124" line="233" name="亮"
		   text="「いっ、いただきますっ……」" timeout="" voices=""/>
		<chgbg @l="237" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evusi11_01a" type="99"/>
		<msg @l="239" char="1" flags="0" index="126" line="239" name="汐"
		   text="「んふふっ、よくできました。それじゃあ、お口をおっきく、\n:あーんして\x;heart:」" timeout=""
		   voices="usi11127"/>
		<msg @l="241" char="11" flags="0" index="128" line="241" name="亮"
		   text="「あーん――んむっ、ちゅぱっ」" timeout="" voices=""/>
		<chgbg @l="245" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evusi11_01b" type="99"/>
		<msg @l="247" char="1" flags="0" index="130" line="247" name="汐"
		   text="「んっ……ふはっ、はぁ、んんっ……ふぅ、んんっ……」" timeout="" voices="usi11128"/>
		<play_sound @l="249" buf="5" fadeout="0" intro="0" src="usi80017" type="6"
		   volume="1."/>
		<msg @l="250" char="-1" flags="0" index="132" line="250" name=""
		   text="再び生クリームたっぷりになった乳首にしゃぶりつくと、\n:朝霞さんの身体がぷるっと震えた。" timeout="" voices=""/>
		<msg @l="251" char="-1" flags="0" index="134" line="251" name=""
		   text="火照った吐息と、口に広がる甘さと、朝霞さん自身の良い匂いが\n:合わさって、脳がどんどん溶けていく。" timeout=""
		   voices=""/>
