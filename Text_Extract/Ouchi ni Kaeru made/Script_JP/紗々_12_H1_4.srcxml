<?xml version="1.0" encoding="utf-8"?>
<xscript>
	<code>
		<play_sound @l="2764" buf="0" fadeout="500" intro="-1" src="bgm19" type="0"
		   volume="1."/>
		<msg @l="2769" char="-1" flags="0" index="1792" line="2769" name=""
		   text="――目の前に、可愛らしいお尻とおま○こ。" timeout="" voices=""/>
		<msg @l="2770" char="-1" flags="0" index="1794" line="2770" name=""
		   text="さっき膣内に出した精液がトロトロとゆっくり流れ出している。" timeout="" voices=""/>
		<play_sound @l="2771" buf="5" fadeout="0" type="1"/>
		<msg @l="2775" char="2" flags="0" index="1796" line="2775" name="紗々"
		   text="「あっ……さっきまでに比べると、おち○ちん、\n:ちょっとちっちゃくなっちゃってます」" timeout=""
		   voices="sas17342"/>
		<msg @l="2778" char="11" flags="0" index="1798" line="2778" name="亮"
		   text="「あれだけ出した後だと、さすがに元気もなくなるかな」" timeout="" voices=""/>
		<msg @l="2780" char="-1" flags="0" index="1800" line="2780" name=""
		   text="紗々ちゃんが背中を向けた体勢のまま身体を擦りつけてくる。" timeout="" voices=""/>
		<msg @l="2781" char="-1" flags="0" index="1802" line="2781" name=""
		   text="すべすべの肌触りが、太もも全体を柔らかくなでてきて\n:気持ちいい。" timeout="" voices=""/>
		<play_sound @l="2782" buf="5" fadeout="0" type="1"/>
		<chgbg @l="2784" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evsas06_01a" type="99"/>
		<msg @l="2786" char="2" flags="0" index="1804" line="2786" name="紗々"
		   text="「私が気持ちよくしてあげれば、きっとまたおっきく\n:なりますよね？」" timeout="" voices="sas17343"/>
		<msg @l="2789" char="11" flags="0" index="1806" line="2789" name="亮"
		   text="「気持ちよくって？」" timeout="" voices=""/>
		<play_sound @l="2790" buf="5" fadeout="0" type="1"/>
		<msg @l="2792" char="2" flags="0" index="1808" line="2792" name="紗々"
		   text="「私、頑張ってセンパイのおち○ちんを勃起させちゃいますから、そのまま待っててください」" timeout=""
		   voices="sas17344"/>
		<msg @l="2795" char="11" flags="0" index="1810" line="2795" name="亮"
		   text="「あっ……」" timeout="" voices=""/>
		<msg @l="2797" char="-1" flags="0" index="1812" line="2797" name=""
		   text="紗々ちゃんの柔らかい手が、ペニスを優しく包み込む。" timeout="" voices=""/>
		<msg @l="2798" char="-1" flags="0" index="1814" line="2798" name=""
		   text="ペニスに添えた手が、ただしごくのではなく自分の股間へ軽く\n:押しつけ、挟むように圧をかけてくる。" timeout=""
		   voices=""/>
		<play_sound @l="2799" buf="5" fadeout="0" type="1"/>
		<chgbg @l="2802" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evsas06_01b" type="99"/>
		<msg @l="2804" char="2" flags="0" index="1816" line="2804" name="紗々"
		   text="「んふぅ、ふぅ……こうすれば、おち○ちんをもっと\n:ヌルヌルにできちゃいますから……んんっ」" timeout=""
		   voices="sas17345"/>
		<msg @l="2806" char="2" flags="0" index="1818" line="2806" name="紗々"
		   text="「おち○ちんが元気になるようにぃ……\n:んしょ、んっ、んくくっ……」" timeout="" voices="sas17346"/>
		<play_sound @l="2808" buf="5" fadeout="0" intro="0" src="sas80017" type="6"
		   volume="1."/>
		<msg @l="2809" char="-1" flags="0" index="1820" line="2809" name=""
		   text="手だけでなく腰も一緒に動かす紗々ちゃんのお尻が、\n:目の前でぷるんぷるんと小刻みに揺れる。" timeout=""
		   voices=""/>
		<msg @l="2810" char="-1" flags="0" index="1822" line="2810" name=""
		   text="ぶっちゃけ手コキ自体はたどたどしいけど、それが\n:かえって健気で懸命で甲斐甲斐しい。" timeout="" voices=""/>
		<play_sound @l="2811" buf="5" fadeout="0" type="1"/>
		<msg @l="2813" char="2" flags="0" index="1824" line="2813" name="紗々"
		   text="「おち○ちん、どんどんヌルヌルになって……これなら、\n:あとは勃起するだけですよね」" timeout=""
		   voices="sas17347"/>
		<msg @l="2816" char="2" flags="0" index="1826" line="2816" name="紗々"
		   text="「んぅ、はぁ……おっきくなったら、トロトロのおま○こに\n:入れますよー？　なんて、えへへ……」" timeout=""
		   voices="sas17348"/>
		<chgbg @l="2818" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evsas06_01a" type="99"/>
		<msg @l="2820" char="2" flags="0" index="1828" line="2820" name="紗々"
		   text="「あっ、いま、おち○ちんがぴくぴくって……\x;heart:」" timeout="" voices="sas17349"/>
		<msg @l="2822" char="2" flags="0" index="1830" line="2822" name="紗々"
		   text="「だんだんガチガチになって……ふぅ、はぁ……\n:おち○ちん、おっきくなぁれ、おっきくなぁれ\x;heart:」"
		   timeout="" voices="sas17350"/>
		<play_sound @l="2824" buf="5" fadeout="0" intro="0" src="sas80015" type="6"
		   volume="1."/>
		<msg @l="2825" char="-1" flags="0" index="1832" line="2825" name=""
		   text="まるで子供をあやすかのように、ち○こに向かって話しかける\n:紗々ちゃん。" timeout="" voices=""/>
		<msg @l="2826" char="-1" flags="0" index="1834" line="2826" name=""
		   text="それが心地よすぎてペニスがひくひくと反応してしまう。" timeout="" voices=""/>
		<msg @l="2828" char="11" flags="0" index="1836" line="2828" name="亮"
		   text="「もう、そろそろ……」" timeout="" voices=""/>
		<play_sound @l="2829" buf="5" fadeout="0" type="1"/>
		<chgbg @l="2831" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evsas06_01h" type="99"/>
		<msg @l="2833" char="2" flags="0" index="1838" line="2833" name="紗々"
		   text="「えっ？　あっ……ごめんなさい。私ってばいつのまにか夢中に\n:なっちゃってました」" timeout=""
		   voices="sas17351"/>
		<play_sound @l="2835" buf="5" fadeout="0" intro="0" src="sas80016" type="6"
		   volume="1."/>
		<msg @l="2836" char="-1" flags="0" index="1840" line="2836" name=""
		   text="はにかんだ紗々ちゃんの献身的な愛撫のおかげで、\n:ペニスはすっかり大きさを取り戻した。" timeout="" voices=""/>
		<msg @l="2837" char="-1" flags="0" index="1842" line="2837" name=""
		   text="いや、もしかすると、さっきより大きいかもしれない。" timeout="" voices=""/>
		<play_sound @l="2838" buf="5" fadeout="0" type="1"/>
		<chgbg @l="2840" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evsas06_01a" type="99"/>
		<msg @l="2842" char="2" flags="0" index="1844" line="2842" name="紗々"
		   text="「じゃあ、今度は私が動くので、センパイはくつろいでいて\n:くださいね」" timeout="" voices="sas17352"/>
		<play_sound @l="2844" buf="5" fadeout="0" intro="0" src="sas80017" type="6"
		   volume="1."/>
		<msg @l="2845" char="-1" flags="0" index="1846" line="2845" name=""
		   text="ペニスに添えた手を器用に動かしながら腰の位置を調整していく\n:紗々ちゃん。" timeout="" voices=""/>
		<play_sound @l="2846" buf="5" fadeout="0" type="1"/>
		<chgbg @l="2848" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evsas06_01b" type="99"/>
		<msg @l="2850" char="2" flags="0" index="1848" line="2850" name="紗々"
		   text="「ああっ……先っぽ入りましたね」" timeout="" voices="sas17353"/>
		<play_sound @l="2852" buf="1" fadeout="0" intro="-1" src="hse101" type="1"
		   volume="1."/>
		<play_sound @l="2853" buf="5" fadeout="0" intro="0" src="sas80015" type="6"
		   volume="1."/>
		<msg @l="2854" char="-1" flags="0" index="1850" line="2854" name=""
		   text="亀頭が、熱々の秘唇にくちゅっと埋まる。" timeout="" voices=""/>
		<play_sound @l="2855" buf="5" fadeout="0" type="1"/>
		<play_sound @l="2856" buf="1" fadeout="0" type="1"/>
		<msg @l="2858" char="2" flags="0" index="1852" line="2858" name="紗々"
		   text="「このまま、おま○こに挿入をっ……ふぅ、んふぅ……\n:ふッ、くくくっ」" timeout="" voices="sas17354"/>
		<chgbg @l="2863" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evsas06_02b" type="99"/>
		<play_sound @l="2865" buf="1" fadeout="0" intro="-1" src="hse107" type="1"
		   volume="1."/>
		<play_sound @l="2866" buf="5" fadeout="0" intro="0" src="sas80016" type="6"
		   volume="1."/>
		<msg @l="2867" char="-1" flags="0" index="1854" line="2867" name=""
		   text="わずかに力んだ紗々ちゃんがお尻をすり寄せると、\n:ペニスが膣内へとゆっくり飲み込まれていく。" timeout=""
		   voices=""/>
		<play_sound @l="2868" buf="1" fadeout="0" type="1"/>
		<msg @l="2870" char="11" flags="0" index="1856" line="2870" name="亮"
		   text="「ふはっ、くうぅううっ……」" timeout="" voices=""/>
		<msg @l="2872" char="-1" flags="0" index="1858" line="2872" name=""
		   text="挿入直後、全身を強い快感が駆け抜ける。" timeout="" voices=""/>
		<msg @l="2873" char="-1" flags="0" index="1860" line="2873" name=""
		   text="紗々ちゃんに言われて力を抜いていた分、息子に流れ込んできた\n:快感をまともに受けてしまった。" timeout=""
		   voices=""/>
		<play_sound @l="2874" buf="5" fadeout="0" type="1"/>
		<msg @l="2876" char="2" flags="0" index="1862" line="2876" name="紗々"
		   text="「入ってきたおち○ちん、すごい元気にびくびくってっ……\n:ぐっ、ふみゃあっ\x;heart:」" timeout=""
		   voices="sas17355"/>
		<play_sound @l="2878" buf="5" fadeout="0" intro="0" src="sas80017" type="6"
		   volume="1."/>
		<msg @l="2879" char="11" flags="0" index="1864" line="2879" name="亮"
		   text="「すまん、力は抜いててもそこは勝手に動いちゃうというか」" timeout="" voices=""/>
		<play_sound @l="2880" buf="5" fadeout="0" type="1"/>
		<chgbg @l="2882" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evsas06_02g" type="99"/>
		<msg @l="2884" char="2" flags="0" index="1866" line="2884" name="紗々"
		   text="「大丈夫ですっ。ちょっと驚いちゃっただけで、むしろ擦れて\n:気持ちいいというか、さっきと当たる場所が違って新鮮というか」"
		   timeout="" voices="sas17356"/>
		<msg @l="2886" char="2" flags="0" index="1868" line="2886" name="紗々"
		   text="「おち○ちん、すっごいぬるぬるですし、おま○こだって、\n:おなじぐらいぐちゅぐちゅで……」" timeout=""
		   voices="sas17357"/>
		<msg @l="2888" char="2" flags="0" index="1870" line="2888" name="紗々"
		   text="「さっき射精してもらったセンパイの精子のおかげで、\n:さらにぬるぬるでさっきと感触が違うのもすごい……」" timeout=""
		   voices="sas17358"/>
		<play_sound @l="2890" buf="5" fadeout="0" intro="0" src="sas80015" type="6"
		   volume="1."/>
		<msg @l="2891" char="-1" flags="0" index="1872" line="2891" name=""
		   text="彼女の胎内はさっき中出しした精液がたぷたぷと波打ち、\n:肉棒への刺激を変化させていた。" timeout="" voices=""/>
		<msg @l="2892" char="-1" flags="0" index="1874" line="2892" name=""
		   text="挿抜自体がなめらかだし、膣内もさっきよりさらに熱い。" timeout="" voices=""/>
		<play_sound @l="2893" buf="5" fadeout="0" type="1"/>
		<chgbg @l="2895" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evsas06_02b" type="99"/>
		<msg @l="2897" char="2" flags="0" index="1876" line="2897" name="紗々"
		   text="「はぁ、んはっ……おま○ことろとろで、おち○ちん、\n:さっきより強くこすれているようなっ……？」" timeout=""
		   voices="sas17359"/>
		<play_sound @l="2899" buf="5" fadeout="0" intro="0" src="sas80016" type="6"
		   volume="1."/>
		<msg @l="2900" char="11" flags="0" index="1878" line="2900" name="亮"
		   text="「紗々ちゃんもそう思うか？」" timeout="" voices=""/>
		<play_sound @l="2901" buf="5" fadeout="0" type="1"/>
		<msg @l="2903" char="2" flags="0" index="1880" line="2903" name="紗々"
		   text="「ということは、センパイも？」" timeout="" voices="sas17360"/>
		<play_sound @l="2905" buf="5" fadeout="0" intro="0" src="sas80017" type="6"
		   volume="1."/>
		<msg @l="2906" char="11" flags="0" index="1882" line="2906" name="亮"
		   text="「なんだか、さっきよりさらに気持ちよくてっ……」" timeout="" voices=""/>
		<msg @l="2908" char="-1" flags="0" index="1884" line="2908" name=""
		   text="中出し精子の影響もあるだろうが、どちらかというと、\n:よりフィットしてきたというか。" timeout="" voices=""/>
		<msg @l="2909" char="-1" flags="0" index="1886" line="2909" name=""
		   text="亀頭とヒダのこすれ具合からして違う感じだ。" timeout="" voices=""/>
		<msg @l="2911" char="11" flags="0" index="1888" line="2911" name="亮"
		   text="「さっきと体勢が変わったのもあると思うけど、紗々ちゃんのが\n:僕のに馴染んできたからだと思う」" timeout=""
		   voices=""/>
		<play_sound @l="2912" buf="5" fadeout="0" type="1"/>
		<chgbg @l="2914" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evsas06_02a" type="99"/>
		<msg @l="2916" char="2" flags="0" index="1890" line="2916" name="紗々"
		   text="「あはっ！　嬉しいです、それってセンパイ専用のおま○こに\n:なってきてるってことですよね？」" timeout=""
		   voices="sas17361"/>
		<msg @l="2918" char="2" flags="0" index="1892" line="2918" name="紗々"
		   text="「センパイの形を覚えてエッチに搾りとろうとするなんて、\n:私のおま○こは偉いです、ゴッドま○こです\x;heart:」"
		   timeout="" voices="sas17362"/>
		<play_sound @l="2920" buf="5" fadeout="0" intro="0" src="sas80015" type="6"
		   volume="1."/>
		<msg @l="2921" char="-1" flags="0" index="1894" line="2921" name=""
		   text="膣内の動きが活発になり、ペニスへの刺激がより\n:本格的になってきた。" timeout="" voices=""/>
		<msg @l="2922" char="-1" flags="0" index="1896" line="2922" name=""
		   text="根元はキュッと締まり奥は包み込むように、ぽよんぽよんと肉厚な\n:子宮口へと肉棒を導いていく。" timeout=""
		   voices=""/>
		<msg @l="2924" char="11" flags="0" index="1898" line="2924" name="亮"
		   text="「やば……これやばいぞ……」" timeout="" voices=""/>
		<play_sound @l="2925" buf="5" fadeout="0" type="1"/>
		<chgbg @l="2927" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evsas06_02c" type="99"/>
		<msg @l="2929" char="2" flags="0" index="1900" line="2929" name="紗々"
		   text="「んはっ！　気持ちいい……センパイのおち○ちんが、\n:じゅぼじゅぼって奥まで入ってきますぅ！」" timeout=""
		   voices="sas17363"/>
		<play_sound @l="2931" buf="5" fadeout="0" intro="0" src="sas80016" type="6"
		   volume="1."/>
		<msg @l="2932" char="11" flags="0" index="1902" line="2932" name="亮"
		   text="「体勢がいいのか、さっきまで痛いくらいだった締め付けが、\n:ちょうどいい強さでち○こを刺激してくる……っ！」" timeout=""
		   voices=""/>
		<play_sound @l="2933" buf="5" fadeout="0" type="1"/>
		<chgbg @l="2935" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evsas06_02h" type="99"/>
		<msg @l="2937" char="2" flags="0" index="1904" line="2937" name="紗々"
		   text="「ということは、さらに体勢を変えれば、もっともっと、\n:気持ち良くなる可能性もっ……」" timeout=""
		   voices="sas17364"/>
		<play_sound @l="2939" buf="5" fadeout="0" intro="0" src="sas80017" type="6"
		   volume="1."/>
		<msg @l="2940" char="11" flags="0" index="1906" line="2940" name="亮"
		   text="「って……あ、ああっ？！」" timeout="" voices=""/>
		<msg @l="2942" char="-1" flags="0" index="1908" line="2942" name=""
		   text="背伸びする猫のようにのけぞった紗々ちゃんが、\n:そのまま獲物を狙う感じで僕の足をぎゅっと掴む。" timeout=""
		   voices=""/>
		<play_sound @l="2943" buf="5" fadeout="0" type="1"/>
		<chgbg @l="2945" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evsas06_02b" type="99"/>
		<msg @l="2947" char="2" flags="0" index="1910" line="2947" name="紗々"
		   text="「あぁ、ここもセンパイの汗でいっぱいです……」" timeout="" voices="sas17365"/>
		<msg @l="2950" char="2" flags="0" index="1912" line="2950" name="紗々"
		   text="「汗って、場所によって味が違ったりするんでしょうか？」" timeout="" voices="sas17366"/>
		<play_sound @l="2952" buf="5" fadeout="0" intro="0" src="sas80015" type="6"
		   volume="1."/>
		<msg @l="2953" char="11" flags="0" index="1914" line="2953" name="亮"
		   text="「さ、さあ……？」" timeout="" voices=""/>
		<play_sound @l="2954" buf="5" fadeout="0" type="1"/>
		<chgbg @l="2956" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evsas06_02d" type="99"/>
		<msg @l="2958" char="2" flags="0" index="1916" line="2958" name="紗々"
		   text="「じゃあ、確かめちゃいますね。ちゅっ、れろろっ……\n:れるれる、れーろっ\x;hearts:」" timeout=""
		   voices="sas17367"/>
		<play_sound @l="2960" buf="5" fadeout="0" intro="0" src="sas80028" type="6"
		   volume="1."/>
		<msg @l="2961" char="-1" flags="0" index="1918" line="2961" name=""
		   text="僕の足に顔をすり寄せた紗々ちゃんが迷いなく舌を這わせると、\n:ぺろぺろと舐め始める。" timeout="" voices=""/>
		<play_sound @l="2962" buf="5" fadeout="0" type="1"/>
		<chgbg @l="2964" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evsas06_02f" type="99"/>
		<msg @l="2967" char="2" flags="0" index="1920" line="2967" name="紗々"
		   text="「れろ、ちゅっ……ぺーろ、ちゅむむぅっ……ちゅっ、\n:れりゅれりゅ……んふぅ、はあっ……れろれろ\x;heart:」"
		   timeout="" voices="sas17368"/>
		<play_sound @l="2969" buf="5" fadeout="0" intro="0" src="sas80029" type="6"
		   volume="1."/>
		<msg @l="2970" char="11" flags="0" index="1922" line="2970" name="亮"
		   text="「さ、紗々ちゃん？　あの、足はさすがに……」" timeout="" voices=""/>
		<msg @l="2972" char="-1" flags="0" index="1924" line="2972" name=""
