		<play_sound @l="1101" buf="0" fadeout="500" intro="-1" src="bgm03" type="0"
		   volume="1."/>
		<msg @l="1105" char="0" flags="0" index="476" line="1105" name="花音"
		   text="「汐ちゃんとは昔、何度も銭湯に行ったことあるけど、\n:こうして広々としたお風呂に一緒に来たのは初めてだね」" timeout=""
		   voices="kan15610"/>
		<msg @l="1107" char="1" flags="0" index="478" line="1107" name="汐"
		   text="「あー、アヒルちゃん浮かべて遊んでたね」" timeout="" voices="usi16103"/>
		<msg @l="1109" char="4" flags="0" index="480" line="1109" name="ＪＣ"
		   text="「パイセンがアヒルちゃんとか似合いすぎるんですけどーぉ！」" timeout="" voices="jei13111"/>
		<msg @l="1111" char="4" flags="0" index="482" line="1111" name="ＪＣ"
		   text="「ほら、紗々っちも早く来いよ。おもしれ―話がきけっぞー」" timeout="" voices="jei13112"/>
		<msg @l="1113" char="-1" flags="0" index="484" line="1113" name=""
		   text="ＪＣさんに呼ばれ、恥ずかしいけど私も湯船へ。" timeout="" voices=""/>
		<msg @l="1115" char="3" flags="0" index="486" line="1115" name="礼羽"
		   text="「むー、社長め。\n:サウナを作っておくように言っといたのに、出来てないじゃない」" timeout=""
		   voices="rai15806"/>
		<msg @l="1117" char="1" flags="0" index="488" line="1117" name="汐"
		   text="「私たちが合宿に参加決まったの２日前だよ！？　\n:さすがにムチャ言い過ぎでしょ」" timeout=""
		   voices="usi16104"/>
		<msg @l="1119" char="-1" flags="0" index="490" line="1119" name=""
		   text="そっか。礼羽さんはサウナの国フィンランド出身だから、\n:こういう裸の付き合いにも慣れてるんだ。" timeout=""
		   voices=""/>
		<msg @l="1121" char="2" flags="0" index="492" line="1121" name="紗々"
		   text="「しずかちゃんは、\n:みんなとお風呂に入るの恥ずかしかったりしない？」" timeout="" voices="sas14028"/>
		<msg @l="1123" char="8" flags="0" index="494" line="1123" name="しずか"
		   text="「わたしはお肌を磨くのに精いっぱいで、\n:恥ずかしいとか言ってる暇もありませんから」" timeout=""
		   voices="siz04801"/>
		<msg @l="1125" char="8" flags="0" index="496" line="1125" name="しずか"
		   text="「おにーさんにいつ見られてもいいように、\n:つるつるにしておかないと」" timeout="" voices="siz04802"/>
		<msg @l="1127" char="0" flags="0" index="498" line="1127" name="花音"
		   text="「たしかこのお湯って、近くの温泉から直接引いてるんだよね？」" timeout="" voices="kan15611"/>
		<msg @l="1129" char="3" flags="0" index="500" line="1129" name="礼羽"
		   text="「そうそう。皮膚に溜まった老廃物をきれいにする効果があって、美肌の湯って言われてるらしいよ」" timeout=""
		   voices="rai15807"/>
		<msg @l="1131" char="4" flags="0" index="502" line="1131" name="ＪＣ"
		   text="「マジか！？　タダで研修受けられて、\n:しかも天然温泉でお肌つるつるって、めっちゃお得じゃね？」" timeout=""
		   voices="jei13113"/>
		<msg @l="1133" char="-1" flags="0" index="504" line="1133" name=""
		   text="そういうことなら、私も恥ずかしいなんて思ってられない。\n:呼吸が出来るギリギリまで湯船に浸かる。" timeout=""
		   voices=""/>
		<msg @l="1135" char="4" flags="0" index="506" line="1135" name="ＪＣ"
		   text="「いやー、それにしても合宿来てよかったすね」" timeout="" voices="jei13114"/>
		<msg @l="1137" char="-1" flags="0" index="508" line="1137" name=""
		   text="大きく伸びをしながらリラックスしているＪＣさんを見ると、\n:おっぱいが湯船の上に浮かんでいた。" timeout=""
		   voices=""/>
		<msg @l="1139" char="2" flags="0" index="510" line="1139" name="紗々"
		   text="（な、なんてサイズ！　まさに規格外）" timeout="" voices="sas14029"/>
		<msg @l="1141" char="-1" flags="0" index="512" line="1141" name=""
		   text="肌も健康的な小麦色で艶がある。" timeout="" voices=""/>
		<msg @l="1143" char="2" flags="0" index="514" line="1143" name="紗々"
		   text="（すごいなあ。うらやましいなあ）" timeout="" voices="sas14030"/>
		<msg @l="1145" char="0" flags="0" index="516" line="1145" name="花音"
		   text="「遊んでばっかりじゃダメだよ。\n:ちゃんと授業の方もがんばろうね」" timeout="" voices="kan15612"/>
		<msg @l="1147" char="2" flags="0" index="518" line="1147" name="紗々"
		   text="（オーナーもかなりの大きさ……\n:しかもお肌ももちもちしてそうなうるおいボディ）" timeout=""
		   voices="sas14031"/>
		<msg @l="1149" char="-1" flags="0" index="520" line="1149" name=""
		   text="髪の毛もさらさらでキューティクルもバッチリだ。" timeout="" voices=""/>
		<msg @l="1151" char="3" flags="0" index="522" line="1151" name="礼羽"
		   text="「花音がそれを言っちゃう！？　\n:荷物に遊び道具がいっぱい入ってたでしょ」" timeout=""
		   voices="rai15808"/>
		<msg @l="1153" char="2" flags="0" index="524" line="1153" name="紗々"
		   text="（礼羽さんなんてもう、女性の私から見ても\n:パーフェクトなボディで思わず見とれちゃうよ）" timeout=""
		   voices="sas14032"/>
		<msg @l="1155" char="-1" flags="0" index="526" line="1155" name=""
		   text="北欧からきた妖精？　絵本から飛び出してきちゃったとか？？" timeout="" voices=""/>
		<msg @l="1157" char="4" flags="0" index="528" line="1157" name="ＪＣ"
		   text="「それより礼羽っちの授業の方が心配なんだが？　\n:いつもみたいに変なボケかましてンじゃねーの？」" timeout=""
		   voices="jei13115"/>
		<msg @l="1159" char="3" flags="0" index="530" line="1159" name="礼羽"
		   text="「むぅ、失礼な。　ちゃんとやってたよねー？　汐」" timeout="" voices="rai15809"/>
		<msg @l="1161" char="1" flags="0" index="532" line="1161" name="汐"
		   text="「礼羽さんはその……マイペースだから。\n:ほら、天才ってそういうとこあるでしょ？」" timeout=""
		   voices="usi16105"/>
		<msg @l="1163" char="8" flags="0" index="534" line="1163" name="しずか"
		   text="「朝霞お姉さんがフォローしきれないなんて……」" timeout="" voices="siz04803"/>
		<msg @l="1165" char="2" flags="0" index="536" line="1165" name="紗々"
		   text="（このふたりは胸のサイズは小さいけど、お肌のハリがプルンってすごい。思わずぷにぷにしたくなっちゃう）" timeout=""
		   voices="sas14033"/>
		<msg @l="1167" char="-1" flags="0" index="538" line="1167" name=""
		   text="みんなすごくステキでこの輪の中にいるのが場違いみたい。" timeout="" voices=""/>
		<msg @l="1169" char="3" flags="0" index="540" line="1169" name="礼羽"
		   text="「マイペースとか言われちゃったらさ。\n:自由気ままにやっていいんだよね」" timeout="" voices="rai15810"/>
		<msg @l="1171" char="3" flags="0" index="542" line="1171" name="礼羽"
		   text="「これは明日、生まれてきたことを後悔するくらい\n:真面目にしごいちゃおうっかなー？」" timeout=""
		   voices="rai15811"/>
		<msg @l="1173" char="1" flags="0" index="544" line="1173" name="汐"
		   text="「ひぃ！？　楽しくってためになる、素晴らしい授業でした！」" timeout="" voices="usi16106"/>
		<msg @l="1175" char="0" flags="0" index="546" line="1175" name="花音"
		   text="「ほどほどにお願いね？　明日は川で遊ぶ予定なんだー」" timeout="" voices="kan15613"/>
		<msg @l="1177" char="4" flags="0" index="548" line="1177" name="ＪＣ"
		   text="「やっぱり遊ぶ気マンマンだった！？」" timeout="" voices="jei13116"/>
		<msg @l="1179" char="8" flags="0" index="550" line="1179" name="しずか"
		   text="「プールじゃなくって川なんですか？　溺れたふりして\n:おにーさんに助けてもらう予定が本当に溺れそう！？」" timeout=""
		   voices="siz04804"/>
		<msg @l="1181" char="1" flags="0" index="552" line="1181" name="汐"
		   text="「しずかちゃんはぶれないね、ほんと……あはは」" timeout="" voices="usi16107"/>
		<msg @l="1183" char="3" flags="0" index="554" line="1183" name="礼羽"
		   text="「川をせき止めた天然のプールだから危なくはないけど、\n:亮については危険そう……」" timeout=""
		   voices="rai15812"/>
		<msg @l="1185" char="4" flags="0" index="556" line="1185" name="ＪＣ"
		   text="「こうなったら紗々っちも負けてらんねーなっ！　\n:明日はビシッと水着姿で悩殺してやれっ！」" timeout=""
		   voices="jei13117"/>
		<msg @l="1187" char="2" flags="0" index="558" line="1187" name="紗々"
		   text="「悩殺ですかっ！？」" timeout="" voices="sas14034"/>
		<msg @l="1189" char="3" flags="0" index="560" line="1189" name="礼羽"
		   text="「それだったら私の出番じゃない？　\n:紗々には悪いけど彼の視線はひとり占め。つってね」" timeout=""
		   voices="rai15813"/>
		<msg @l="1191" char="-1" flags="0" index="562" line="1191" name=""
		   text="手を頭の後ろで組んでポーズを決めて挑発してくる礼羽さん。" timeout="" voices=""/>
		<msg @l="1193" char="2" flags="0" index="564" line="1193" name="紗々"
		   text="「おっぱいの大きさは負けてますけど、\n:気持ちだけなら負けません」" timeout="" voices="sas14035"/>
		<msg @l="1195" char="3" flags="0" index="566" line="1195" name="礼羽"
		   text="「くすっ、いい返事。それでこそ張り合いがあるよ\x;note:」" timeout="" voices="rai15814"/>
		<msg @l="1197" char="-1" flags="0" index="568" line="1197" name=""
		   text="なんだか乗せられた気がするけど、\n:礼羽さんなりの優しさに胸があたたかくなる。" timeout="" voices=""/>
		<msg @l="1199" char="8" flags="0" index="570" line="1199" name="しずか"
		   text="「もうひとり忘れてませんか！　\n:最後に笑うのは１０年お得なこのわたしですよっ！」" timeout=""
		   voices="siz04805"/>
		<msg @l="1201" char="1" flags="0" index="572" line="1201" name="汐"
		   text="「じゃ１０年たったら参加しようね。\n:ここは大人の戦いみたいだから」" timeout="" voices="usi16108"/>
		<msg @l="1203" char="8" flags="0" index="574" line="1203" name="しずか"
		   text="「まさか若さがディスアドバンテージになるなんてっ！　\n:戦いの場にすら立たせてもらえないとはっ！」" timeout=""
		   voices="siz04806"/>
		<msg @l="1205" char="-1" flags="0" index="576" line="1205" name=""
		   text="朝霞さんがしずかちゃんを連れて隅に行った。" timeout="" voices=""/>
		<msg @l="1207" char="2" flags="0" index="578" line="1207" name="紗々"
		   text="「でも相手は（主におっぱいが）強大です。\n:どうしたらいいんでしょうか？」" timeout=""
		   voices="sas14036"/>
		<msg @l="1211" char="0" flags="0" index="580" line="1211" name="花音"
		   text="「ハプニングを装って抱きついちゃうとかどうかな？　\n:男の人って不意に見せる弱々しい姿にグッとくるって\n:聞いたことあるよ」"
		   timeout="" voices="kan15614"/>
		<msg @l="1213" char="2" flags="0" index="582" line="1213" name="紗々"
		   text="「保護欲を刺激するわけですね。参考にします！」" timeout="" voices="sas14037"/>
		<msg @l="1215" char="4" flags="0" index="584" line="1215" name="ＪＣ"
		   text="「ついでにおっぱいくっつけちゃえば？　\n:そーすりゃギンッギンってなもんよ？　マジで」" timeout=""
		   voices="jei13118"/>
		<msg @l="1217" char="2" flags="0" index="586" line="1217" name="紗々"
		   text="「ギンギン！？」" timeout="" voices="sas14038"/>
		<msg @l="1219" char="1" flags="0" index="588" line="1219" name="汐"
		   text="「もうＪＣったら。小さい子もいるんだから、\n:もう少し言葉を選んでよ」" timeout="" voices="usi16109"/>
		<msg @l="1221" char="4" flags="0" index="590" line="1221" name="ＪＣ"
		   text="「サーセンした！」" timeout="" voices="jei13119"/>
		<msg @l="1223" char="8" flags="0" index="592" line="1223" name="しずか"
		   text="「お気遣いなく。そういうのが全然ないよりも、ちょっとくらいあった方が男子として健全だしかわいいと思いますから」" timeout=""
		   voices="siz04807"/>
		<msg @l="1225" char="1" flags="0" index="594" line="1225" name="汐"
		   text="「この場で一番大人な感想きちゃった！？」" timeout="" voices="usi16110"/>
		<msg @l="1227" char="-1" flags="0" index="596" line="1227" name=""
		   text="みなさんの気遣いに、心の奥がぽかぽかしてくる。" timeout="" voices=""/>
		<msg @l="1228" char="-1" flags="0" index="598" line="1228" name=""
		   text="それは温泉よりもはるかに効果的に、\n:私の身も心も元気づけてくれた。" timeout="" voices=""/>
		<msg @l="1230" char="2" flags="0" index="600" line="1230" name="紗々"
		   text="「ありがとうございます！　皇鈴紗々、\n:みなさんの後押しを受けて女になってきます！」" timeout=""
		   voices="sas14039"/>
		<msg @l="1233" char="10" flags="0" index="602" line="1233" name="全員"
		   text="「それ、気が早すぎるから！！」" timeout="" voices="kan15615k"/>
