<?xml version="1.0" encoding="utf-8"?>
<xscript>
	<code>
		<msg @l="1208" char="11" flags="0" index="808" line="1208" name="亮"
		   text="「おおっ……」" timeout="" voices=""/>
		<play_sound @l="1209" buf="1" fadeout="0" type="1"/>
		<msg @l="1210" char="-1" flags="0" index="810" line="1210" name=""
		   text="今日何度目になるかわからない、感嘆の吐息。" timeout="" voices=""/>
		<msg @l="1211" char="-1" flags="0" index="812" line="1211" name=""
		   text="生まれたての姿――――と呼ぶには、まだニーソが残ってる。" timeout="" voices=""/>
		<play_sound @l="1212" buf="5" fadeout="0" type="1"/>
		<msg @l="1214" char="2" flags="0" index="814" line="1214" name="紗々"
		   text="「ニーソックスも脱がせて頂ければ……」" timeout="" voices="sas17138"/>
		<play_sound @l="1216" buf="5" fadeout="0" intro="0" src="sas80017" type="6"
		   volume="1."/>
		<msg @l="1217" char="11" flags="0" index="816" line="1217" name="亮"
		   text="「それはよしておこう！　これはこれでアリだっていうか、\n:こっちの方がアリだ！」" timeout="" voices=""/>
		<play_sound @l="1218" buf="5" fadeout="0" type="1"/>
		<chgbg @l="1220" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evsas04_09a" type="99"/>
		<msg @l="1222" char="2" flags="0" index="818" line="1222" name="紗々"
		   text="「そうなんですか！？」" timeout="" voices="sas17139"/>
		<play_sound @l="1224" buf="5" fadeout="0" intro="0" src="sas80015" type="6"
		   volume="1."/>
		<msg @l="1225" char="-1" flags="0" index="820" line="1225" name=""
		   text="スカートがなくても、絶対領域って成立するんだな。" timeout="" voices=""/>
		<msg @l="1226" char="-1" flags="0" index="822" line="1226" name=""
		   text="まる裸より、一部残ってたほうがなぜか興奮度が高い。\n:黒いニーソと白い肌のコントラストがそう思わせるのだろうか。"
		   timeout="" voices=""/>
		<play_sound @l="1227" buf="5" fadeout="0" type="1"/>
		<chgbg @l="1229" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evsas04_09b" type="99"/>
		<msg @l="1231" char="2" flags="0" index="824" line="1231" name="紗々"
		   text="「はぁ、はぁ……私たち、これからいよいよセックスしちゃうん\n:ですよね？」" timeout=""
		   voices="sas17140"/>
		<play_sound @l="1233" buf="5" fadeout="0" intro="0" src="sas80016" type="6"
		   volume="1."/>
		<msg @l="1234" char="11" flags="0" index="826" line="1234" name="亮"
		   text="「お、おう」" timeout="" voices=""/>
		<msg @l="1236" char="-1" flags="0" index="828" line="1236" name=""
		   text="紗々ちゃんみたいな子の口からセックスって単語が出てくると、\n:なんだか妙に生々しい。" timeout="" voices=""/>
		<msg @l="1237" char="-1" flags="0" index="830" line="1237" name=""
		   text="普段、絶対に口にしないような単語なだけに心のレコーダーに\n:永久保存することにした。" timeout="" voices=""/>
		<msg @l="1239" char="11" flags="0" index="832" line="1239" name="亮"
		   text="「じゃあ、直接さわるぞ？」" timeout="" voices=""/>
		<play_sound @l="1240" buf="5" fadeout="0" type="1"/>
		<chgbg @l="1242" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evsas04_09a" type="99"/>
		<msg @l="1244" char="2" flags="0" index="834" line="1244" name="紗々"
		   text="「あっ、はいっ……」" timeout="" voices="sas17141"/>
		<play_sound @l="1246" buf="5" fadeout="0" intro="0" src="sas80017" type="6"
		   volume="1."/>
		<msg @l="1247" char="-1" flags="0" index="836" line="1247" name=""
		   text="パンツを脱がしたあと秘所をガン見しちゃって中断していたけど、脱がした本来の目的はそれだ。" timeout="" voices=""/>
		<msg @l="1248" char="-1" flags="0" index="838" line="1248" name=""
		   text="よーく観察していた紗々ちゃんの割れ目に、あらためて手を――。" timeout="" voices=""/>
		<play_sound @l="1249" buf="5" fadeout="0" type="1"/>
		<chgbg @l="1254" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evsas04_10c" type="99"/>
		<msg @l="1256" char="2" flags="0" index="840" line="1256" name="紗々"
		   text="「はぁんっ、はっ……あ、あああああッ\x;heart:」" timeout="" voices="sas17142"/>
		<play_sound @l="1258" buf="1" fadeout="0" intro="-1" src="hse101" type="1"
		   volume="1."/>
		<play_sound @l="1259" buf="5" fadeout="0" intro="0" src="sas80015" type="6"
		   volume="1."/>
		<msg @l="1260" char="-1" flags="0" index="842" line="1260" name=""
		   text="割れ目に指を乗せた瞬間、部屋中に響き渡るぐらいの\n:ボリュームで、くちゅっと水音が鳴った。" timeout=""
		   voices=""/>
		<play_sound @l="1261" buf="1" fadeout="0" type="1"/>
		<msg @l="1263" char="11" flags="0" index="844" line="1263" name="亮"
		   text="「うわ、うわわっ……」" timeout="" voices=""/>
		<msg @l="1265" char="-1" flags="0" index="846" line="1265" name=""
		   text="一瞬で愛液にまみれた指が、割れ目に沈んでいく。" timeout="" voices=""/>
		<msg @l="1266" char="-1" flags="0" index="848" line="1266" name=""
		   text="正確には割れ目の表面を触れているだけなんだが、土手全体が\n:ぷにぷにと柔らかいせいか、指が飲み込まれるような\n:錯覚すらおぼえる。"
		   timeout="" voices=""/>
		<play_sound @l="1267" buf="5" fadeout="0" type="1"/>
		<msg @l="1269" char="2" flags="0" index="850" line="1269" name="紗々"
		   text="「私のあそこ、くちゅくちゅって……んふぅ、センパイに\n:いじられてエッチな音がいっぱいっ……」" timeout=""
		   voices="sas17143"/>
		<chgbg @l="1271" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evsas04_10b" type="99"/>
		<msg @l="1273" char="2" flags="0" index="852" line="1273" name="紗々"
		   text="「はふぅ、はあっ……ああ、これがセンパイの指っ……\n:はふぅ、はあっ\x;hearts:」" timeout=""
		   voices="sas17144"/>
		<play_sound @l="1275" buf="5" fadeout="0" intro="0" src="sas80018" type="6"
		   volume="1."/>
		<msg @l="1276" char="11" flags="0" index="854" line="1276" name="亮"
		   text="「紗々ちゃんのあそこ、すごい……」" timeout="" voices=""/>
		<msg @l="1278" char="-1" flags="0" index="856" line="1278" name=""
		   text="おっぱいも柔らかかったけど、秘所はそれ以上にふわとろで、\n:よく熟れた果物みたいだった。" timeout="" voices=""/>
		<msg @l="1279" char="-1" flags="0" index="858" line="1279" name=""
		   text="指を押し込むと、果汁のような蜜がじゅわっと染み出る。" timeout="" voices=""/>
		<play_sound @l="1280" buf="5" fadeout="0" type="1"/>
		<msg @l="1282" char="2" flags="0" index="860" line="1282" name="紗々"
		   text="「んふぅ、ふぅっ……ああっ、そこをもっとこすってもらって\n:いいですかっ……？」" timeout=""
		   voices="sas17145"/>
		<play_sound @l="1284" buf="5" fadeout="0" intro="0" src="sas80019" type="6"
		   volume="1."/>
		<msg @l="1285" char="11" flags="0" index="862" line="1285" name="亮"
		   text="「そこって……えっと、このピンク色になってるところか？」" timeout="" voices=""/>
		<play_sound @l="1286" buf="5" fadeout="0" type="1"/>
		<chgbg @l="1288" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evsas04_10e" type="99"/>
		<msg @l="1290" char="2" flags="0" index="864" line="1290" name="紗々"
		   text="「は、はいっ……ピンクです\x;heart:　はふぅ、はあっ、やんっ、\n:指だけでこんなに気持ちいいなんてっ……あぁっ」"
		   timeout="" voices="sas17146"/>
		<play_sound @l="1292" buf="5" fadeout="0" intro="0" src="sas80020" type="6"
		   volume="1."/>
		<msg @l="1293" char="-1" flags="0" index="866" line="1293" name=""
		   text="割れ目が徐々に広がり、少ししか見えなかったピンク色の秘裂が\n:徐々に姿をあらわしてきた。" timeout="" voices=""/>
		<msg @l="1295" char="11" flags="0" index="868" line="1295" name="亮"
		   text="「おぉっ、これが女の子の……」" timeout="" voices=""/>
		<msg @l="1297" char="-1" flags="0" index="870" line="1297" name=""
		   text="このあと、この割れ目に自分の息子を挿入する――\n:そう考えただけで思考が甘く痺れてくる。" timeout="" voices=""/>
		<msg @l="1298" char="-1" flags="0" index="872" line="1298" name=""
		   text="こんなトロトロな場所にペニスを挿れたら、すぐに射精して\n:しまうんじゃないだろうか。" timeout="" voices=""/>
		<play_sound @l="1299" buf="5" fadeout="0" type="1"/>
		<chgbg @l="1301" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evsas04_10b" type="99"/>
		<msg @l="1303" char="2" flags="0" index="874" line="1303" name="紗々"
		   text="「んぐっ、はあっ……！　あっ、はあっ、センパイ、もっとっ……んぐっ、いじってもらっていいですかっ？」" timeout=""
		   voices="sas17147"/>
		<play_sound @l="1305" buf="5" fadeout="0" intro="0" src="sas80018" type="6"
		   volume="1."/>
		<msg @l="1306" char="11" flags="0" index="876" line="1306" name="亮"
		   text="「わかった。なら、もっとっ……」" timeout="" voices=""/>
		<msg @l="1308" char="-1" flags="0" index="878" line="1308" name=""
		   text="本番に備えて少しでも感触に慣れておこうと、紗々ちゃんの\n:割れ目を丁寧にいじっていく。" timeout="" voices=""/>
		<play_sound @l="1309" buf="5" fadeout="0" type="1"/>
		<chgbg @l="1311" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evsas04_10e" type="99"/>
		<msg @l="1313" char="2" flags="0" index="880" line="1313" name="紗々"
		   text="「はうぅんっ、センパイの指が私の入り口……\n:んふっ、うぅ、広げてきてますぅ\x;hearts:」" timeout=""
		   voices="sas17148"/>
		<msg @l="1315" char="2" flags="0" index="882" line="1315" name="紗々"
		   text="「見られて恥ずかしいのに、んふぅ……っ、あそこがぱくぱくって勝手に開いちゃって、んああぁぁ……」" timeout=""
		   voices="sas17149"/>
		<play_sound @l="1317" buf="5" fadeout="0" intro="0" src="sas80019" type="6"
		   volume="1."/>
		<msg @l="1318" char="-1" flags="0" index="884" line="1318" name=""
		   text="ひくつく膣口がなんともなまめかしく、思わず指をちょっとだけ\n:差し込んでしまう。" timeout="" voices=""/>
		<play_sound @l="1319" buf="5" fadeout="0" type="1"/>
		<chgbg @l="1321" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evsas04_10c" type="99"/>
		<msg @l="1323" char="2" flags="0" index="886" line="1323" name="紗々"
		   text="「ひゃあぁぁっ！？　ゆ、指を入れられたら……\n:はぁっ、はぁっ、こ、こわいです」" timeout=""
		   voices="sas17150"/>
		<msg @l="1325" char="2" flags="0" index="888" line="1325" name="紗々"
		   text="「だって初めては、センパイのおち○ちんに捧げたいから……\n:指だとその……」" timeout=""
		   voices="sas17151"/>
		<play_sound @l="1327" buf="5" fadeout="0" intro="0" src="sas80020" type="6"
		   volume="1."/>
		<msg @l="1328" char="-1" flags="0" index="890" line="1328" name=""
		   text="おそらく処女膜のことを言ってるんだと察して、\n:指を浅くキープしたまま止める。" timeout="" voices=""/>
		<msg @l="1330" char="11" flags="0" index="892" line="1330" name="亮"
		   text="「先っちょだけだから心配しないでくれ」" timeout="" voices=""/>
		<play_sound @l="1331" buf="5" fadeout="0" type="1"/>
		<chgbg @l="1333" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evsas04_10b" type="99"/>
		<msg @l="1335" char="2" flags="0" index="894" line="1335" name="紗々"
		   text="「はぁはぁ、それでしたら……続けてください」" timeout="" voices="sas17152"/>
		<play_sound @l="1337" buf="5" fadeout="0" intro="0" src="sas80018" type="6"
		   volume="1."/>
		<msg @l="1338" char="-1" flags="0" index="896" line="1338" name=""
		   text="安心したのか強ばった膣口が緩み、第１関節まで指が飲み\n:込まれていく。" timeout="" voices=""/>
		<msg @l="1339" char="-1" flags="0" index="898" line="1339" name=""
		   text="そこは熱く潤っていて、あっという間に指がふやけてしまう。" timeout="" voices=""/>
		<play_sound @l="1340" buf="5" fadeout="0" type="1"/>
		<chgbg @l="1342" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evsas04_10e" type="99"/>
		<msg @l="1344" char="2" flags="0" index="900" line="1344" name="紗々"
		   text="「んはぁっ、センパイの指……太くてあったかくて、んくぅぅっ、あそこがヒクヒクしちゃってます」" timeout=""
		   voices="sas17153"/>
		<play_sound @l="1346" buf="5" fadeout="0" intro="0" src="sas80019" type="6"
		   volume="1."/>
		<msg @l="1347" char="11" flags="0" index="902" line="1347" name="亮"
		   text="「ああ、クリトリスもピンッてなってふるふる震えてかわいい……こっちも触るぞ」" timeout="" voices=""/>
		<play_sound @l="1348" buf="5" fadeout="0" type="1"/>
		<msg @l="1350" char="2" flags="0" index="904" line="1350" name="紗々"
		   text="「やっぱりクリちゃんがいちばんっ……ふぅ、ふぅっ……\n:はぐっ、はっ、ああっ……\x;heart:」" timeout=""
		   voices="sas17154"/>
		<play_sound @l="1352" buf="5" fadeout="0" intro="0" src="sas80021" type="6"
		   volume="1."/>
		<msg @l="1353" char="11" flags="0" index="906" line="1353" name="亮"
		   text="「クリトリスが気持ちいいのか？」" timeout="" voices=""/>
		<play_sound @l="1354" buf="5" fadeout="0" type="1"/>
		<chgbg @l="1356" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evsas04_10b" type="99"/>
		<msg @l="1358" char="2" flags="0" index="908" line="1358" name="紗々"
		   text="「はいっ……いじられると、びくってなって……いまもまたっ……はんっ、はあんっ！」" timeout=""
		   voices="sas17155"/>
		<play_sound @l="1360" buf="5" fadeout="0" intro="0" src="sas80022" type="6"
		   volume="1."/>
		<msg @l="1361" char="-1" flags="0" index="910" line="1361" name=""
		   text="腰を震わせ、紗々ちゃんが内股をぎゅっと閉じる。" timeout="" voices=""/>
		<msg @l="1362" char="-1" flags="0" index="912" line="1362" name=""
		   text="浅く挿れた指でもわかるぐらい、割れ目がキツく狭くなった。" timeout="" voices=""/>
		<msg @l="1364" char="11" flags="0" index="914" line="1364" name="亮"
		   text="「気持ちいいなら、もっと……」" timeout="" voices=""/>
		<play_sound @l="1365" buf="5" fadeout="0" type="1"/>
		<chgbg @l="1367" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evsas04_10c" type="99"/>
		<msg @l="1369" char="2" flags="0" index="916" line="1369" name="紗々"
		   text="「ふえっ？！　やっ、これ以上はっ……くあっ、はあっ……\n:指だめっ、だめだめっ、だめえっ」" timeout=""
		   voices="sas17156"/>
		<play_sound @l="1371" buf="5" fadeout="0" intro="0" src="sas80021" type="6"
		   volume="1."/>
		<msg @l="1372" char="11" flags="0" index="918" line="1372" name="亮"
		   text="（だめってことは、もっと触ってという意味だったよな……？）" timeout="" voices=""/>
		<msg @l="1374" char="-1" flags="0" index="920" line="1374" name=""
		   text="さっきの言葉を思い出し、とにかくクリトリスを重点的に攻めて\n:攻めて攻めまくる。" timeout="" voices=""/>
		<msg @l="1375" char="-1" flags="0" index="922" line="1375" name=""
		   text="根元を擦ったり、皮ごと先端を撫でたり……とにかく、思いつく\n:限りの方法で充血した陰豆を刺激していく。" timeout=""
		   voices=""/>
		<play_sound @l="1376" buf="5" fadeout="0" type="1"/>
		<msg @l="1378" char="2" flags="0" index="924" line="1378" name="紗々"
		   text="「ああんっ！？　やあっ、センパイのゆび、しゅごっ……\n:ああっ、あああっ……\x;heart:」" timeout=""
		   voices="sas17157"/>
		<msg @l="1380" char="2" flags="0" index="926" line="1380" name="紗々"
		   text="「センパイごめんなさいっ、私、わたしっ……\n:んぐぅっ、はあっ、ああんっ！！」" timeout=""
		   voices="sas17158"/>
		<play_sound @l="1382" buf="5" fadeout="0" intro="0" src="sas80022" type="6"
		   volume="1."/>
		<msg @l="1383" char="-1" flags="0" index="928" line="1383" name=""
		   text="ビクビクと小刻みに身体を痙攣させながら\n:紗々ちゃんが仰け反っていく。" timeout="" voices=""/>
		<msg @l="1384" char="-1" flags="0" index="930" line="1384" name=""
		   text="膣口がぱくぱくと開き、とめどなく愛液が奥からあふれてくる。" timeout="" voices=""/>
		<play_sound @l="1385" buf="5" fadeout="0" type="1"/>
		<chgbg @l="1387" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evsas04_10b" type="99"/>
		<msg @l="1389" char="2" flags="0" index="932" line="1389" name="紗々"
		   text="「だめっ、ダメですっ……！　もう我慢できなっ……\n:やっ、いくっ、いぐっ……イッちゃいますっ！！」" timeout=""
		   voices="sas17159"/>
		<play_sound @l="1391" buf="5" fadeout="0" intro="0" src="sas80023" type="6"
		   volume="1."/>
		<msg @l="1392" char="-1" flags="0" index="934" line="1392" name=""
		   text="思いがけない言葉に驚くより早く、紗々ちゃんの全身が、\n:腰を中心にがくがくっと大きく震えた。" timeout=""
		   voices=""/>
		<msg @l="1393" char="-1" flags="0" index="936" line="1393" name=""
		   text="だらしなく開いた口からよだれが垂れ、あごの方へつぅーっと\n:唾液の筋を作っていく。" timeout="" voices=""/>
		<play_sound @l="1394" buf="5" fadeout="0" type="1"/>
		<chgbg @l="1396" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evsas04_10c" type="99"/>
		<msg @l="1398" char="2" flags="0" index="938" line="1398" name="紗々"
		   text="「やっあああっ……\x;heart:　だめ、気持ちいいっ……いいっ、\n:出るっ、でちゃううぅっ……\x;heart:\x;hearts:」"
		   timeout="" voices="sas17160"/>
		<msg @l="1400" char="2" flags="0" index="940" line="1400" name="紗々"
		   text="「センパイ離れてっ、でないとセンパイにかかっちゃっ……\n:ひあっ、ああ、漏れるっ、漏れちゃいましゅっ……！」" timeout=""
		   voices="sas17161"/>
		<play_sound @l="1402" buf="5" fadeout="0" intro="0" src="sas80024" type="6"
		   volume="1."/>
		<msg @l="1403" char="11" flags="0" index="942" line="1403" name="亮"
