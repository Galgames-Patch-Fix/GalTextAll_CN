		<waitbs @l="21" flags="0" screen="0"/>
		<face src="fsas03b"/>
		<msg @l="22" char="2" flags="0" index="4" line="22" name="紗々"
		   text="「そうですね。打ち合わせが思ったより長引いちゃい\n:ましたからね」" timeout="" voices="sas16500"/>
		<face src=""/>
		<msg @l="24" char="-1" flags="0" index="6" line="24" name=""
		   text="まあ途中、何度もいちゃついては編集さんが突っ込むという\n:進行だったので、打ち合わせに時間がかかったのも\n:無理はないけど。"
		   timeout="" voices=""/>
		<face src=""/>
		<msg @l="25" char="-1" flags="0" index="8" line="25" name=""
		   text="とはいえ、時間がかかってみんなに迷惑をかけたのも事実なので、こうして店じまいと掃除を申し出たわけである。" timeout=""
		   voices=""/>
		<chgbs async="0" duration="200" priority="0" screen="0" src="sas08b"
		   transparency="0" x="2147483648"/>
		<waitbs @l="27" flags="0" screen="0"/>
		<face src="fsas08b"/>
		<msg @l="28" char="2" flags="0" index="10" line="28" name="紗々"
		   text="「でも、こうして二人きりだと、前にお店でエッチしたときの\n:ことを思い出しちゃいますよね」" timeout=""
		   voices="sas16501"/>
		<face src="f"/>
		<msg @l="30" char="11" flags="0" index="12" line="30" name="亮"
		   text="「そ、そうだな」" timeout="" voices=""/>
		<face src=""/>
		<msg @l="32" char="-1" flags="0" index="14" line="32" name=""
		   text="カウンターの下に潜り込んだ紗々ちゃんの濃厚なフェラ、\n:そしてお店の裏での行為を思い出し、ゴクリと唾を飲む。" timeout=""
		   voices=""/>
		<chgbs async="0" duration="200" priority="0" screen="0" src="sas15b"
		   transparency="0" x="2147483648"/>
		<waitbs @l="34" flags="0" screen="0"/>
		<face src="fsas15b"/>
		<msg @l="35" char="2" flags="0" index="16" line="35" name="紗々"
		   text="「えっと……もうみなさんも帰りましたし、さっき戸締まりも\n:確認したので大丈夫ですよ？」" timeout=""
		   voices="sas16502"/>
		<face src="f"/>
		<msg @l="37" char="11" flags="0" index="18" line="37" name="亮"
		   text="「そ、そうか、なら安心だな！」" timeout="" voices=""/>
		<face src=""/>
		<msg @l="39" char="-1" flags="0" index="20" line="39" name=""
		   text="なにが安心なのかわからないが、とにかく紗々ちゃんのお誘いに\n:早くも股間が熱くなってくる。" timeout=""
		   voices=""/>
		<chgbs async="0" duration="200" priority="0" screen="0" src="sas08b"
		   transparency="0" x="2147483648"/>
		<waitbs @l="41" flags="0" screen="0"/>
		<face src="fsas08b"/>
		<msg @l="42" char="2" flags="0" index="22" line="42" name="紗々"
		   text="「お片付けもありますから、先にいちゃいちゃした方が\n:いいですよね？」" timeout="" voices="sas16503"/>
		<face src="f"/>
		<msg @l="44" char="11" flags="0" index="24" line="44" name="亮"
		   text="「そ、そうだな！　匂いとかあるもんな！！」" timeout="" voices=""/>
		<chgbs async="0" duration="200" priority="0" screen="0" src="sas26b"
		   transparency="0" x="2147483648"/>
		<waitbs @l="46" flags="0" screen="0"/>
		<face src="fsas26b"/>
		<msg @l="47" char="2" flags="17" index="26" line="47" name="紗々"
