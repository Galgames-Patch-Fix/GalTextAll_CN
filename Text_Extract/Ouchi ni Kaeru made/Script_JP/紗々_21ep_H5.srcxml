		<play_sound @l="157" buf="5" fadeout="0" intro="0" src="sas80018" type="6"
		   volume="1."/>
		<msg @l="158" char="-1" flags="0" index="85" line="158" name=""
		   text="肉芽に触れた途端、紗々ちゃんの喘ぎ声が裏返る。" timeout="" voices=""/>
		<play_sound @l="159" buf="5" fadeout="0" type="1"/>
		<msg @l="161" char="2" flags="0" index="87" line="161" name="紗々"
		   text="「そこ敏感なんです……はふっ、あふぅぅ、ぐいって\n:押されると……っ、んひゃああぁぁっ！？」" timeout=""
		   voices="sas16517"/>
		<msg @l="163" char="2" flags="0" index="89" line="163" name="紗々"
		   text="「だめです、クリちゃんツンツンってされるとぉっ……んはっ、あぁっ！　んああんっ！？　気持ちよすぎて頭の中が真っ白に\n:なっちゃいますぅ！」"
