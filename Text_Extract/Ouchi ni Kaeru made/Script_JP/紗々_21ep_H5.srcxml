		<chgbg @l="115" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evsas14_01c" type="99"/>
		<msg @l="117" char="2" flags="0" index="63" line="117" name="紗々"
		   text="「指がくにくにって……行ったり来たりして、気持ちいいです、あぁん……っ、くすぐったいような、ムズムズしてくるような、はぁうぅぅっ！」"
		   timeout="" voices="sas16512"/>
		<play_sound @l="119" buf="5" fadeout="0" intro="0" src="sas80015" type="6"
		   volume="1."/>
		<msg @l="120" char="-1" flags="0" index="65" line="120" name=""
		   text="指先をスジに沿って上下に這わせていくと、内側に隠れた陰唇が\n:待ちかねたかのように、ひくひくと動いた。" timeout=""
		   voices=""/>
		<play_sound @l="121" buf="5" fadeout="0" type="1"/>
