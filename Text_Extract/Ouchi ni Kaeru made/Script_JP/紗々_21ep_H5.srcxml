		<play_sound @l="207" buf="5" fadeout="0" intro="0" src="sas80018" type="6"
		   volume="1."/>
		<msg @l="208" char="-1" flags="0" index="115" line="208" name=""
		   text="まんぐり返しの体勢であそこが目の前に来た紗々ちゃんが、\n:恥ずかしそうに自分の性器を観察する。" timeout=""
