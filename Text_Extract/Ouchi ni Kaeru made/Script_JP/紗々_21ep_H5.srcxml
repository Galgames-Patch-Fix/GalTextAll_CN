		<play_sound @l="130" buf="5" fadeout="0" intro="0" src="sas80017" type="6"
		   volume="1."/>
		<msg @l="131" char="-1" flags="0" index="69" line="131" name=""
		   text="中指を陰唇の上で滑らせていると、徐々ににゅるりという\n:液体の感触が広がってくる。" timeout="" voices=""/>
		<msg @l="133" char="11" flags="0" index="71" line="133" name="亮"
		   text="「だんだん指を動かしやすくなってきたよ」" timeout="" voices=""/>
		<play_sound @l="134" buf="5" fadeout="0" type="1"/>
		<chgbg @l="136" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evsas14_02a" type="99"/>
		<msg @l="138" char="2" flags="0" index="73" line="138" name="紗々"
		   text="「わかります、はぁはぁ……大好きな人に触れられて、いっぱいえっちなお汁があふれてきちゃってますぅ……\n:ふぁあん、やっ、はやく……続きをしてくださぁい」"
		   timeout="" voices="sas16514"/>
		<play_sound @l="140" buf="5" fadeout="0" intro="0" src="sas80015" type="6"
		   volume="1."/>
		<msg @l="141" char="11" flags="0" index="75" line="141" name="亮"
		   text="「それじゃ、今度は一気に……」" timeout="" voices=""/>
		<play_sound @l="142" buf="5" fadeout="0" type="1"/>
		<chgbg @l="144" duration="300" flags="256" flip="-1" flip_flags="0"
