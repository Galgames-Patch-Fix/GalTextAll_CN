		<play_sound @l="209" buf="5" fadeout="0" type="1"/>
		<chgbg @l="211" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evsas14_03b" type="99"/>
		<msg @l="213" char="2" flags="0" index="117" line="213" name="紗々"
		   text="「ああ、センパイにおま○こじっと見られてるらけで……\n:はぁはぁ、どんどん熱くなってきましゅ\x;heart:」"
		   timeout="" voices="sas16523"/>
		<msg @l="215" char="2" flags="0" index="119" line="215" name="紗々"
		   text="「早くまた触ってくださぁい……はぁんっ、はぁはぁっ、\n:このままじゃ、おかしくなっちゃいましゅうぅ！」" timeout=""
		   voices="sas16524"/>
