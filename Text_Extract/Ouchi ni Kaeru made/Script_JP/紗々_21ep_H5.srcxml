		<play_sound @l="165" buf="5" fadeout="0" intro="0" src="sas80019" type="6"
		   volume="1."/>
		<msg @l="166" char="-1" flags="0" index="91" line="166" name=""
		   text="紗々ちゃんをもっと喜ばせようと指先で肉芽をキュッとつまむ。" timeout="" voices=""/>
		<play_sound @l="167" buf="5" fadeout="0" type="1"/>
		<chgbg @l="169" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evsas14_02d" type="99"/>
		<msg @l="171" char="2" flags="0" index="93" line="171" name="紗々"
		   text="「ふああんっ！？　クリトリス、引っ張られちゃってます……\n:はうっ、はぅぅぅっ、痺れるぅぅっ\x;heart:」"
		   timeout="" voices="sas16519"/>
		<play_sound @l="173" buf="5" fadeout="0" intro="0" src="sas80020" type="6"
		   volume="1."/>
		<msg @l="174" char="11" flags="0" index="95" line="174" name="亮"
		   text="「これが好きだもんな？」" timeout="" voices=""/>
		<play_sound @l="175" buf="5" fadeout="0" type="1"/>
		<msg @l="177" char="2" flags="0" index="97" line="177" name="紗々"
		   text="「はいっ！　もっとビンビンってしてくださいぃっ！　\n:いつもみたいにいじめてくださいっ！！」" timeout=""
		   voices="sas16520"/>
		<play_sound @l="179" buf="5" fadeout="0" intro="0" src="sas80018" type="6"
		   volume="1."/>
		<msg @l="180" char="-1" flags="0" index="99" line="180" name=""
		   text="何度も身体を重ね合わせてきたおかげで、お互いの弱点は\n:知り尽くしている。" timeout="" voices=""/>
		<msg @l="181" char="-1" flags="0" index="101" line="181" name=""
		   text="紗々ちゃんはクリトリスを指先でビンッと弾くと、途端にあへってくるのだ。" timeout="" voices=""/>
		<play_sound @l="182" buf="5" fadeout="0" type="1"/>
		<chgbg @l="184" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evsas14_02c" type="99"/>
		<msg @l="186" char="2" flags="0" index="103" line="186" name="紗々"
		   text="「はぁはぁ……しゅごぃ\x;heart:　センパイの指、気持ちいいですぅ\x;hearts:」" timeout=""
		   voices="sas16521"/>
		<play_sound @l="188" buf="5" fadeout="0" intro="0" src="sas80016" type="6"
		   volume="1."/>
		<msg @l="189" char="-1" flags="0" index="105" line="189" name=""
		   text="だらしなく口が開き、少しづつろれつが怪しくなってくるのも\n:慣れたもの。" timeout="" voices=""/>
		<msg @l="191" char="11" flags="0" index="107" line="191" name="亮"
		   text="（そろそろかな？）" timeout="" voices=""/>
		<play_sound @l="196" buf="1" fadeout="0" intro="-1" src="hse104" type="1"
		   volume="1."/>
		<chgbg @l="197" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evsas14_03c" type="99"/>
		<msg @l="199" char="-1" flags="0" index="109" line="199" name=""
		   text="愛撫を進めていくと、ほぐされたスジが徐々に緩んできて\n:中からピンク色の陰唇が姿を現した。" timeout=""
		   voices=""/>
		<play_sound @l="200" buf="1" fadeout="0" type="1"/>
		<msg @l="202" char="11" flags="0" index="111" line="202" name="亮"
		   text="「ほら、紗々ちゃんのピンク色がだんだん外に出てきたよ」" timeout="" voices=""/>
		<play_sound @l="203" buf="5" fadeout="0" type="1"/>
		<msg @l="205" char="2" flags="0" index="113" line="205" name="紗々"
		   text="「もう、しょんなこと……あぁんっ、言われなくても\n:わかりますよぉ……っ\x;hearts:」" timeout=""
