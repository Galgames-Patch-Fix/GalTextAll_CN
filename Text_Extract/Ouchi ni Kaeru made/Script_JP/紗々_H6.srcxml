		   voices="sas16773"/>
		<msg @l="670" char="2" flags="0" index="339" line="670" name="紗々"
		   text="「くわえてるおち○ちんを挿れられるところ想像しながら、\n:おま○こをこすこすしちゃってごめんなさぁいっ」" timeout=""
		   voices="sas16774"/>
		<chgbg @l="672" duration="300" flags="256" flip="-1" flip_flags="0"
