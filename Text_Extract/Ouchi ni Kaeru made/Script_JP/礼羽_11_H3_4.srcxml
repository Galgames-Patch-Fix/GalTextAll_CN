		<msg @l="3091" char="-1" flags="0" index="1721" line="3091" name=""
		   text="びゅるびゅるびゅるるるっ！！！" timeout="" voices=""/>
		<play_sound @l="3093" buf="5" fadeout="0" type="1"/>
		<msg @l="3098" char="3" flags="0" index="1723" line="3098" name="礼羽"
		   text="「んああアアっッッ！！！　熱いのがまた奥まで\n:出てるぅーーーーっ\x;heart:\x;hearts:」" timeout=""
		   voices="rai01103"/>
		<play_sound @l="3100" buf="5" fadeout="0" intro="0" src="rai80022" type="6"
		   volume="1."/>
		<msg @l="3101" char="-1" flags="0" index="1725" line="3101" name=""
		   text="しっかりとペニスを礼羽の子宮口に突き立て、ドクドクと\n:精液を胎内に流し込む。" timeout="" voices=""/>
		<msg @l="3102" char="-1" flags="0" index="1727" line="3102" name=""
		   text="ペニスがビクンと跳ねる度に、ぴゅっと精液が鈴口から\n:発射される。" timeout="" voices=""/>
		<play_sound @l="3104" buf="5" fadeout="0" type="1"/>
		<msg @l="3106" char="3" flags="0" index="1729" line="3106" name="礼羽"
		   text="「ふあっ、あああぁ……！！　イッてるおま○こに、\n:精子ダメぇぇ……っ\x;heart:　ふあっ、ああぁ……またイクッ……\n:イクゥゥ……ッ！！　あっ、あっ、あああぁぁ……\x;heart:\x;hearts:」"
		   timeout="" voices="rai01104"/>
		<chgbg @l="3108" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evrai10_09b" type="99"/>
		<msg @l="3110" char="3" flags="0" index="1731" line="3110" name="礼羽"
		   text="「はぁあぁぁ、きちゃう！　濃厚精子が奥に当たるだけで、\n:何度でも……っ、ふわぁあぁぁっ、またきちゃうぅぅぅっ！！」"
		   timeout="" voices="rai01105"/>
		<showmsg @l="3112" duration="300" show="0"/>
		<effect @l="3113" cycle="0" flags="512" param="0" screen="0" type="2"/>
