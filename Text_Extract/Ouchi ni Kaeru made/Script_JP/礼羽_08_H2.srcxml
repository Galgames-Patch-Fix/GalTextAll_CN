<?xml version="1.0" encoding="utf-8"?>
<xscript>
	<code>
		<label @l="1" id="scene_3_01"/>
		<play_sound @l="10" buf="0" fadeout="2500" type="0"/>
		<loadimage @l="16" bg="0" fit="0" screen="1" src="evrai05_01a"/>
		<scroll @l="17" async="0" center_x="45" center_y="45" duration="0" flags="0"
		   rev="0." screen="1" transparency="0" v0="0." v1="0." wait_flags="0" x="0"
		   y="0" zoom="1."/>
		<chgscrn @l="18" duration="2500" flags="4608" flip="-1" flip_flags="0"
		   param="0" screen="1" type="10"/>
		<play_sound @l="19" buf="0" fadeout="500" intro="-1" src="bgm19" type="0"
		   volume="1."/>
		<msg @l="26" char="3" flags="0" index="0" line="26" name="礼羽"
		   text="「ふふっ。パンパンに膨らんで苦しそうだね、おち○ちん\x;heart:」" timeout="" voices="rai13700"/>
		<msg @l="28" char="-1" flags="0" index="2" line="28" name=""
		   text="ズボンから解放され勢いよく飛び出した僕のペニスを見て、\n:頬を緩めるライライ。" timeout="" voices=""/>
		<msg @l="29" char="-1" flags="0" index="4" line="29" name=""
		   text="そんな表情ひとつ取っても、今の僕にはいやらしく感じられ、\n:ペニスがピクリと反応してしまう。" timeout=""
		   voices=""/>
		<msg @l="31" char="3" flags="0" index="6" line="31" name="礼羽"
		   text="「早くしゃぶって欲しそうに、上を向いてピクピクしてる……」" timeout="" voices="rai13701"/>
		<msg @l="33" char="-1" flags="0" index="8" line="33" name=""
		   text="眼前で脈動するペニスにうっとりと呟き、僕に見せつけるように\n:ゆっくりと口を近づけていく。" timeout=""
		   voices=""/>
		<msg @l="35" char="11" flags="0" index="10" line="35" name="亮"
		   text="「ちょ、ちょっと待ってくれ……！　本当にするのか……？」" timeout="" voices=""/>
		<msg @l="37" char="3" flags="0" index="12" line="37" name="礼羽"
		   text="「そのつもりだけど。もしかしてイヤだった？」" timeout="" voices="rai13702"/>
		<msg @l="39" char="11" flags="0" index="14" line="39" name="亮"
		   text="「嫌か嫌じゃないかって問題じゃなくってだな……その、\n:こういうことをする前にお互いの気持ちをハッキリさせるべき\n:というか……」"
		   timeout="" voices=""/>
		<chgbg @l="41" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evrai05_01c" type="99"/>
		<msg @l="43" char="3" flags="0" index="16" line="43" name="礼羽"
		   text="「おち○ちんを、こんなにギンギンに勃起させてる人が言っても\n:説得力ないよ」" timeout=""
		   voices="rai13703"/>
		<msg @l="45" char="11" flags="0" index="18" line="45" name="亮"
		   text="「まあ、そうなんだけどな！」" timeout="" voices=""/>
		<msg @l="47" char="-1" flags="0" index="20" line="47" name=""
		   text="痛いところを突いてくるなあ。" timeout="" voices=""/>
		<msg @l="49" char="3" flags="0" index="22" line="49" name="礼羽"
		   text="「それに気持ちなんて分かりきってるでしょ」" timeout="" voices="rai13704"/>
		<msg @l="51" char="3" flags="0" index="24" line="51" name="礼羽"
		   text="「貴方はスッキリしたくて私は貴方を気持ち良くしてあげたい。\n:それだけだよ」" timeout=""
		   voices="rai13705"/>
		<msg @l="53" char="-1" flags="0" index="26" line="53" name=""
		   text="だからその気持ちが何で湧いてくるのか、根本の部分を\n:知りたいんだが。" timeout="" voices=""/>
		<chgbg @l="55" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evrai05_01b" type="99"/>
		<msg @l="57" char="3" flags="0" index="28" line="57" name="礼羽"
		   text="「それに一回、おち○ちんの味を知っちゃったんだもの……\n:こんな立派なのを見せつけられて我慢なんて出来るわけ\n:ないじゃない」"
		   timeout="" voices="rai13706"/>
		<msg @l="59" char="-1" flags="0" index="30" line="59" name=""
		   text="目を細め、隆起し脈動を繰り返しているペニスを\n:ジッと見つめるライライ。" timeout="" voices=""/>
		<msg @l="60" char="-1" flags="0" index="32" line="60" name=""
		   text="そんな顔をされたら拒めるわけないじゃないか……。" timeout="" voices=""/>
		<msg @l="62" char="3" flags="0" index="34" line="62" name="礼羽"
		   text="「それじゃ、ぺろぺろって舐めちゃうからね」" timeout="" voices="rai13707"/>
		<msg @l="64" char="3" flags="0" index="36" line="64" name="礼羽"
		   text="「いただきまーす。あむっ\x;heart:」" timeout="" voices="rai13708"/>
		<chgbg @l="68" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evrai05_02a" type="99"/>
		<msg @l="70" char="3" flags="0" index="38" line="70" name="礼羽"
		   text="「あむ……んちゅ、ちゅ、ちゅる……」" timeout="" voices="rai13709"/>
		<play_sound @l="72" buf="5" fadeout="0" intro="0" src="rai80000" type="6"
		   volume="1."/>
		<msg @l="73" char="-1" flags="0" index="40" line="73" name=""
		   text="結局、何も言えず彼女のフェラチオを受け入れてしまった。" timeout="" voices=""/>
		<play_sound @l="74" buf="5" fadeout="0" type="1"/>
		<chgbg @l="76" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evrai05_02e" type="99"/>
		<msg @l="78" char="3" flags="0" index="42" line="78" name="礼羽"
		   text="「ちゅず、ちゅるる……！　んふぅ……あの時と同じ味、\n:同じ熱さ、同じ硬さ……ちゅりゅ、れりゅ……\n:ぢゅぶ、ぢゅぶぅ……！」"
		   timeout="" voices="rai13710"/>
		<play_sound @l="80" buf="5" fadeout="0" intro="0" src="rai80001" type="6"
		   volume="1."/>
		<msg @l="81" char="-1" flags="0" index="44" line="81" name=""
		   text="先日の情事を思い出しているのか、ペニスを咥えたまま\n:うわごとのように零す。" timeout="" voices=""/>
		<msg @l="82" char="-1" flags="0" index="46" line="82" name=""
		   text="ねっとりとした舌の感触に唾液で濡れた温かな口内。\n:下腹部に当たるライライの鼻息。" timeout="" voices=""/>
		<msg @l="84" char="11" flags="0" index="48" line="84" name="亮"
		   text="「うっ……ふっ、くぅ……っ！　んっ、んんっ……！」" timeout="" voices=""/>
		<play_sound @l="85" buf="5" fadeout="0" type="1"/>
		<chgbg @l="87" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evrai05_02a" type="99"/>
		<msg @l="90" char="3" flags="0" index="50" line="90" name="礼羽"
		   text="「今日は声、思いっ切り出してもいいんらよ……\n:ぢゅぶ、ぢゅずず……！　ここには私と貴方しかいないんだから」" timeout=""
		   voices="rai13711"/>
		<play_sound @l="92" buf="5" fadeout="0" intro="0" src="rai80002" type="6"
		   volume="1."/>
		<msg @l="93" char="-1" flags="0" index="52" line="93" name=""
		   text="僕の腰を抱きかかえるように引き寄せながら、\n:頭を前後させてきた。" timeout="" voices=""/>
		<play_sound @l="94" buf="5" fadeout="0" type="1"/>
		<chgbg @l="96" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evrai05_02d" type="99"/>
		<msg @l="99" char="3" flags="0" index="54" line="99" name="礼羽"
		   text="「んんぅ……っ、ぐぶぅっ……！　これ、すごい……っ、\n:奥までおち○ちんが入ってきて、ぢゅぶ、\n:ぢゅりゅぅぅぅ……っ！！」"
		   timeout="" voices="rai13712"/>
		<msg @l="101" char="3" flags="0" index="56" line="101" name="礼羽"
		   text="「んぼぉぉ……っ、お口の中、いっぱいに広がってるよぉ、\n:じゅぼっ、じゅぼ……っ、んじゅる、じゅぶぶぅっ……！」"
		   timeout="" voices="rai13713"/>
		<play_sound @l="103" buf="5" fadeout="0" intro="0" src="rai80003" type="6"
		   volume="1."/>
		<msg @l="104" char="-1" flags="0" index="58" line="104" name=""
		   text="ペニスの先端が喉奥に届き、粘膜と粘膜がぴとっと触れ合う。" timeout="" voices=""/>
		<play_sound @l="105" buf="5" fadeout="0" type="1"/>
		<chgbg @l="107" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evrai05_02e" type="99"/>
		<msg @l="109" char="3" flags="0" index="60" line="109" name="礼羽"
		   text="「んうぅぅ……っ、先っぽがのどに当たって、んじゅっ、\n:じゅぶっ、じゅぶぶぶっ！！」" timeout=""
		   voices="rai13714"/>
		<play_sound @l="111" buf="5" fadeout="0" intro="0" src="rai80001" type="6"
		   volume="1."/>
		<msg @l="112" char="-1" flags="0" index="62" line="112" name=""
		   text="思いっ切り脚を開き、がに股の体勢でペニスにしゃぶりつく。" timeout="" voices=""/>
		<msg @l="113" char="-1" flags="0" index="64" line="113" name=""
		   text="下品なのに、それでも可愛さを失わないのは、\n:ライライのスタイルの良さや気品ゆえなんだろうか。" timeout=""
		   voices=""/>
		<play_sound @l="114" buf="5" fadeout="0" type="1"/>
		<msg @l="116" char="3" flags="0" index="66" line="116" name="礼羽"
		   text="「ぢゅず、ぢゅるるっ……！　んぶっ、ぢゅっ……！\n:ぢゅず、ずぅぅ……！」" timeout=""
		   voices="rai13715"/>
		<chgbg @l="118" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evrai05_02b" type="99"/>
		<msg @l="120" char="3" flags="0" index="68" line="120" name="礼羽"
		   text="「んふぅ……おち○ちんの味が変わってきた……ぢゅる、\n:ぢゅぶぅ……もうカウパー出てきひゃったの……？」" timeout=""
		   voices="rai13716"/>
		<play_sound @l="122" buf="5" fadeout="0" intro="0" src="rai80000" type="6"
		   volume="1."/>
		<msg @l="123" char="11" flags="0" index="70" line="123" name="亮"
		   text="「はぁ、はぁ……最初からこんなに思いっ切りされたら\n:そりゃあな」" timeout="" voices=""/>
		<msg @l="125" char="11" flags="0" index="72" line="125" name="亮"
		   text="「それに喫茶店の時点で、かなりムラムラしてたから」" timeout="" voices=""/>
		<play_sound @l="126" buf="5" fadeout="0" type="1"/>
		<chgbg @l="128" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evrai05_02a" type="99"/>
		<msg @l="130" char="3" flags="0" index="74" line="130" name="礼羽"
		   text="「私に発情してたってことらね……？　んふふ～、\n:目論見通り……\x;hearts:」" timeout=""
		   voices="rai13717"/>
		<play_sound @l="132" buf="5" fadeout="0" intro="0" src="rai80001" type="6"
		   volume="1."/>
		<msg @l="133" char="11" flags="0" index="76" line="133" name="亮"
		   text="「目論見通りって……まさか最初っからこうするつもり\n:だったのか！？」" timeout="" voices=""/>
		<play_sound @l="134" buf="5" fadeout="0" type="1"/>
		<chgbg @l="136" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evrai05_02e" type="99"/>
		<msg @l="138" char="3" flags="0" index="78" line="138" name="礼羽"
		   text="「……ふふ、内緒\x;heart:　んんぅ……ぢゅず、じゅぼっ……！　\n:ぢゅるっ、ぢゅずぅぅ……！　ぢゅぶっ、ずずずぅっ……！！」"
		   timeout="" voices="rai13718"/>
		<play_sound @l="140" buf="5" fadeout="0" intro="0" src="rai80002" type="6"
		   volume="1."/>
		<msg @l="141" char="11" flags="0" index="80" line="141" name="亮"
		   text="「またそうやって誤魔化して……！　あっ、くうぅ……！\n:う、あぁ……！」" timeout="" voices=""/>
		<msg @l="143" char="-1" flags="0" index="82" line="143" name=""
		   text="僕の言葉を遮るように、ペニスに思いっ切り吸い付いてくる。" timeout="" voices=""/>
		<play_sound @l="144" buf="5" fadeout="0" type="1"/>
		<msg @l="146" char="3" flags="0" index="84" line="146" name="礼羽"
		   text="「ぢゅぶ、ぢゅぅ……！　舌で先っぽを弄られるのが\n:好きなんらよね……？　れりゅ、ぢゅりゅ、れろぉ……！」" timeout=""
		   voices="rai13719"/>
		<play_sound @l="148" buf="5" fadeout="0" intro="0" src="rai80003" type="6"
		   volume="1."/>
		<msg @l="149" char="-1" flags="0" index="86" line="149" name=""
		   text="既にフェラチオを経験したからか、それとも\n:元々才能があったのか、根本まで咥えても苦しがる様子もない。" timeout=""
		   voices=""/>
		<msg @l="150" char="-1" flags="0" index="88" line="150" name=""
		   text="テクニック自体も向上していて、的確に僕が感じるポイントに\n:舌先を巧みに動かして刺激してきた。" timeout=""
		   voices=""/>
		<msg @l="152" char="11" flags="0" index="90" line="152" name="亮"
		   text="「あ、あぁぁ……！！　それダメだ……！」" timeout="" voices=""/>
		<play_sound @l="153" buf="5" fadeout="0" type="1"/>
		<chgbg @l="155" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evrai05_02c" type="99"/>
		<msg @l="157" char="3" flags="0" index="92" line="157" name="礼羽"
		   text="「気持ちいいのにダメってことはないれしょ……？\n:れぢゅ、ちゅりゅぅ……！　私のお口で感じて、\n:もっろおち○ちん大きくして……\x;hearts:」"
		   timeout="" voices="rai13720"/>
		<play_sound @l="159" buf="5" fadeout="0" intro="0" src="rai80001" type="6"
		   volume="1."/>
		<msg @l="160" char="-1" flags="0" index="94" line="160" name=""
		   text="強い快感に腰や背筋が勝手に震え、思わず膝から\n:崩れ落ちてしまいそうになる。" timeout="" voices=""/>
		<msg @l="161" char="-1" flags="0" index="96" line="161" name=""
		   text="ペニスは熱く疼き、ライライの口の中で刺激に反応して\n:窮屈そうに跳ねている。" timeout="" voices=""/>
		<play_sound @l="162" buf="5" fadeout="0" type="1"/>
		<chgbg @l="164" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evrai05_02a" type="99"/>
		<msg @l="167" char="3" flags="0" index="98" line="167" name="礼羽"
		   text="「ぢゅりゅ、ぢゅぶぅ……！　んっ、んふぅ……\n:貴方の感じてる顔と声……たまらないよ……\n:ぢゅる、ぢゅぶっ……！　ぢゅず、ずずずっ……！」"
		   timeout="" voices="rai13721"/>
		<play_sound @l="169" buf="5" fadeout="0" intro="0" src="rai80002" type="6"
		   volume="1."/>
		<msg @l="170" char="-1" flags="0" index="100" line="170" name=""
		   text="ライライも僕のペニスを舐めしゃぶることで\n:興奮を高めているようで、鼻孔から熱い息を零しながら、\n:情熱的なフェラチオを披露してくる。"
		   timeout="" voices=""/>
		<msg @l="171" char="-1" flags="0" index="102" line="171" name=""
		   text="そのたびに女の子の匂いが濃くなってきて、露わになっている\n:下着のクロッチ部分がうっすらと湿っていた。" timeout=""
		   voices=""/>
		<play_sound @l="172" buf="5" fadeout="0" type="1"/>
		<play_sound @l="177" buf="5" fadeout="0" intro="0" src="rai80004" type="6"
		   volume="1."/>
		<msg @l="178" char="11" flags="0" index="104" line="178" name="亮"
		   text="「はぁ、はぁ……！　ライライも興奮してくれてるんだな……」" timeout="" voices=""/>
		<play_sound @l="179" buf="5" fadeout="0" type="1"/>
		<msg @l="181" char="3" flags="0" index="106" line="181" name="礼羽"
		   text="「女の子らって男の人と同じらよ……んぢゅ、ぢゅぶ……\n:エッチなことすれば興奮するし……興奮すれば濡れるの……\n:ぢゅるっ、ぢゅずずぅ……！」"
		   timeout="" voices="rai13722"/>
		<play_sound @l="183" buf="5" fadeout="0" intro="0" src="rai80005" type="6"
		   volume="1."/>
		<msg @l="184" char="-1" flags="0" index="108" line="184" name=""
		   text="これまで僕をからかうためだけに、こういったことを\n:しているのかと思っていた。" timeout="" voices=""/>
		<msg @l="185" char="-1" flags="0" index="110" line="185" name=""
		   text="けど違ったんだ。ライライも興奮し身体を反応させている。" timeout="" voices=""/>
		<msg @l="187" char="11" flags="0" index="112" line="187" name="亮"
		   text="（何より、興奮して濡れてくれてるという事実が嬉しい）" timeout="" voices=""/>
		<msg @l="189" char="-1" flags="0" index="114" line="189" name=""
		   text="僕の気持ちに反応したのかペニスがビクンと一際大きく跳ね、\n:硬度が増していく。" timeout="" voices=""/>
		<play_sound @l="190" buf="5" fadeout="0" type="1"/>
		<chgbg @l="192" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evrai05_02b" type="99"/>
		<msg @l="194" char="3" flags="0" index="116" line="194" name="礼羽"
		   text="「んんぅ……！？　おち○ちん、まら大きくなっれ……\n:んぢゅ、んむ……前にした時よりも大きくなってる……\n:ぢゅるる、ぢゅぶ、ぢゅずぅ……！」"
		   timeout="" voices="rai13723"/>
		<play_sound @l="196" buf="5" fadeout="0" intro="0" src="rai80006" type="6"
		   volume="1."/>
		<msg @l="197" char="-1" flags="0" index="118" line="197" name=""
		   text="更に肥大化したペニスを懸命に奉仕するライライ。" timeout="" voices=""/>
		<msg @l="198" char="-1" flags="0" index="120" line="198" name=""
		   text="口を窄めて肉棒に思いっ切り吸い付き、口内で忙しなく\n:舌を動かしていく。" timeout="" voices=""/>
		<play_sound @l="199" buf="5" fadeout="0" type="1"/>
		<chgbg @l="201" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evrai05_02d" type="99"/>
		<msg @l="203" char="3" flags="0" index="122" line="203" name="礼羽"
		   text="「ぢゅるるっ！　ぢゅぶっ、ぢゅずっ！　すごい、\n:お口の中れビクビクってなっれぇ、すっごい舐めやすい\x;heart:\n:れぢゅ、れろぉっ……\x;hearts:」"
		   timeout="" voices="rai13724"/>
		<msg @l="205" char="3" flags="0" index="124" line="205" name="礼羽"
		   text="「ふぅ、ふぅ……血管が浮き出てやらしんだぁ、なめちゃお……\n:ちゅぱっ、ちゅりゅっ、はむっ、ちゅぷぷ……っ\x;heart:」"
		   timeout="" voices="rai13725"/>
		<play_sound @l="207" buf="5" fadeout="0" intro="0" src="rai80005" type="6"
		   volume="1."/>
		<msg @l="208" char="11" flags="0" index="126" line="208" name="亮"
		   text="「あっ、くうぅ……！　舌でカリや裏筋を\n:舐めてくれないか……？」" timeout="" voices=""/>
		<play_sound @l="209" buf="5" fadeout="0" type="1"/>
		<chgbg @l="212" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evrai05_02b" type="99"/>
		<msg @l="214" char="3" flags="0" index="128" line="214" name="礼羽"
