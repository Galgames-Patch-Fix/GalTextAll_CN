		   volume="1."/>
		<msg @l="1960" char="4" flags="0" index="907" line="1960" name="ＪＣ"
		   text="「はぁはぁ……マジやばたんした……」" timeout="" voices="jei10923"/>
		<msg @l="1962" char="0" flags="0" index="909" line="1962" name="花音"
		   text="「それにしても、まさかＪＣさんが\n:あんなにビンカンだったとはね」" timeout="" voices="kan12521"/>
		<msg @l="1964" char="3" flags="0" index="911" line="1964" name="礼羽"
		   text="「ちょっと触っただけでビクンってなっちゃって、かわいい\x;note:」" timeout="" voices="rai12073"/>
		<msg @l="1966" char="4" flags="0" index="913" line="1966" name="ＪＣ"
		   text="「もうお嫁にいけない……シクシク」" timeout="" voices="jei10924"/>
		<msg @l="1968" char="2" flags="0" index="915" line="1968" name="紗々"
		   text="「なにげに乙女！！？」" timeout="" voices="sas11620"/>
		<msg @l="1970" char="8" flags="0" index="917" line="1970" name="しずか"
		   text="「こういう時、混ざれない寂しさってありますよね。\n:ええわかります」" timeout="" voices="siz03719"/>
		<msg @l="1972" char="1" flags="0" index="919" line="1972" name="汐"
		   text="「だから憐れみの目で私を見ないでーーっ！！」" timeout="" voices="usi13617"/>
		<msg @l="1974" char="8" flags="0" index="921" line="1974" name="しずか"
		   text="「大丈夫ですよ、マニアックな人はきっといますから。\n:世界のどこかに」" timeout="" voices="siz03720"/>
		<msg @l="1976" char="1" flags="0" index="923" line="1976" name="汐"
		   text="「しずかちゃんだって似たようなものでしょ？　っていうか、\n:私の方がまだおっぱいあるし」" timeout=""
		   voices="usi13618"/>
		<msg @l="1979" char="8" flags="0" index="925" line="1979" name="しずか"
		   text="「ふっ……わたしはいずれ成長しますから」" timeout="" voices="siz03721"/>
		<msg @l="1981" char="1" flags="0" index="927" line="1981" name="汐"
		   text="「勝ち誇ってる！？　誰しもがあんなにおっきく\n:なるわけじゃないんだよ！？　私だって昔は勝手に\n:大きくなるもんだって思ってたんだからっ！！」"
		   timeout="" voices="usi13619"/>
		<msg @l="1983" char="8" flags="0" index="929" line="1983" name="しずか"
		   text="「な、なんですって……！？　言われてみればそうですね」" timeout="" voices="siz03722"/>
		<msg @l="1985" char="8" flags="0" index="931" line="1985" name="しずか"
		   text="「礼羽お姉さん、どうやったらそんな大きく育つんですか？」" timeout="" voices="siz03723"/>
		<msg @l="1987" char="1" flags="0" index="933" line="1987" name="汐"
		   text="「そこ！　それ詳しく！！　ついでに\n:今から間に合う方法も教えてっ！！」" timeout="" voices="usi13620"/>
		<msg @l="1989" char="3" flags="0" index="935" line="1989" name="礼羽"
		   text="「や、別に何もしてないけど……」" timeout="" voices="rai12074"/>
		<msg @l="1991" char="8" flags="0" index="937" line="1991" name="しずか"
		   text="「じゃあＪＣさんっ！！」" timeout="" voices="siz03724"/>
		<msg @l="1993" char="4" flags="0" index="939" line="1993" name="ＪＣ"
		   text="「よく食って、よく寝る！」" timeout="" voices="jei10925"/>
		<msg @l="1996" char="2" flags="0" index="941" line="1996" name="紗々"
		   text="「それは、横におっきくなるだけじゃないですかね？」" timeout="" voices="sas11621"/>
		<msg @l="1998" char="8" flags="0" index="943" line="1998" name="しずか"
		   text="「じゃあ紗々おね……。花音お姉さん、どうしたら\n:おっぱい大きくなりますか？」" timeout=""
		   voices="siz03725"/>
		<msg @l="2000" char="2" flags="0" index="945" line="2000" name="紗々"
		   text="「私のサイズはスルーされたぁっ！！？」" timeout="" voices="sas11622"/>
		<msg @l="2002" char="-1" flags="0" index="947" line="2002" name=""
		   text="――結局、みんなで騒ぎ過ぎて全員のぼせてしまった。" timeout="" voices=""/>
		<msg @l="2003" char="-1" flags="0" index="949" line="2003" name=""
		   text="次に温泉に入る時は気をつけようっと。つってね。" timeout="" voices=""/>
