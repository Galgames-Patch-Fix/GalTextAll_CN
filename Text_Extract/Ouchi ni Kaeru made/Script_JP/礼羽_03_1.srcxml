		<msg @l="1829" char="8" flags="0" index="802" line="1829" name="しずか"
		   text="「わぁー。こんなに広いお風呂なんて初めてです～」" timeout="" voices="siz03714"/>
		<msg @l="1831" char="0" flags="0" index="804" line="1831" name="花音"
		   text="「はぁ～生き返る～」" timeout="" voices="kan12511"/>
		<msg @l="1833" char="1" flags="0" index="806" line="1833" name="汐"
		   text="「はぁ～若返る～」" timeout="" voices="usi13609"/>
		<msg @l="1835" char="4" flags="0" index="808" line="1835" name="ＪＣ"
		   text="「パイセンはそれ以上、若返ったらヤバくないっスか？\n:こう、倫理的に色々と」" timeout=""
		   voices="jei10911"/>
		<msg @l="1837" char="1" flags="0" index="810" line="1837" name="汐"
		   text="「ちょっ！？　どういう意味？」" timeout="" voices="usi13610"/>
		<msg @l="1839" char="2" flags="0" index="812" line="1839" name="紗々"
		   text="「はぁ……やっぱり疲れてる時は温泉ですね。生き返ります」" timeout="" voices="sas11614"/>
		<msg @l="1841" char="3" flags="0" index="814" line="1841" name="礼羽"
		   text="「ふふっ。今日１日、ガンバった甲斐があったでしょ？」" timeout="" voices="rai12064"/>
		<msg @l="1843" char="4" flags="0" index="816" line="1843" name="ＪＣ"
		   text="「あー、頑張った頑張った。しっかしこの講習、\n:思ったよりハードだよな」" timeout="" voices="jei10912"/>
		<msg @l="1845" char="0" flags="0" index="818" line="1845" name="花音"
		   text="「スヴニールグループの正規研修だからね。この後にある\n:資格試験に受からないとお給料があがらないんだってさ」" timeout=""
		   voices="kan12512"/>
		<msg @l="1847" char="2" flags="0" index="820" line="1847" name="紗々"
		   text="「なるほど。それで、まわりのみなさんが\n:ものすごい熱気で取り組んでたんですね」" timeout=""
		   voices="sas11615"/>
		<msg @l="1849" char="3" flags="0" index="822" line="1849" name="礼羽"
		   text="「それもひとつの理由かもね。でもみんなと同じ、\n:自分のお店をよりよくするためにガンバってるのもあると思うよ」"
		   timeout="" voices="rai12065"/>
		<msg @l="1851" char="2" flags="0" index="824" line="1851" name="紗々"
		   text="「自分の働いてるお店って、愛着がわいて\n:頑張りたくなりますもんね」" timeout="" voices="sas11616"/>
		<msg @l="1853" char="1" flags="0" index="826" line="1853" name="汐"
		   text="「わかる、わかるよ紗々ちゃん。でも、今日は\n:がんばりすぎた～。ぶくぶく……」" timeout=""
		   voices="usi13611"/>
		<msg @l="1855" char="0" flags="0" index="828" line="1855" name="花音"
		   text="「沈んでる、沈んでるぅ！！」" timeout="" voices="kan12513"/>
		<msg @l="1857" char="-1" flags="0" index="830" line="1857" name=""
		   text="花音が汐を慌てて引き上げる。" timeout="" voices=""/>
		<msg @l="1859" char="4" flags="0" index="832" line="1859" name="ＪＣ"
		   text="「こういうのって全身まるごと浸かった方が、\n:疲労回復がはやいんスかね？」" timeout=""
		   voices="jei10913"/>
		<msg @l="1861" char="2" flags="0" index="834" line="1861" name="紗々"
		   text="「その前に、疲労も痛みも感じない体になっちゃいますよ！」" timeout="" voices="sas11617"/>
		<msg @l="1863" char="8" flags="0" index="836" line="1863" name="しずか"
		   text="「あはっ、こうやってみんなでお風呂に入るのって楽しいですね」" timeout="" voices="siz03715"/>
		<msg @l="1865" char="3" flags="0" index="838" line="1865" name="礼羽"
		   text="「そうね。フィンランドではよくサウナに入ったけど、\n:日本の温泉も広々してていいね」" timeout=""
		   voices="rai12066"/>
		<msg @l="1867" char="0" flags="0" index="840" line="1867" name="花音"
		   text="「気にいったんなら今度、別の温泉にも行ってみる？\n:温泉めぐりをしていろんな効能を味わおうよ」" timeout=""
		   voices="kan12514"/>
		<msg @l="1869" char="1" flags="0" index="842" line="1869" name="汐"
		   text="「いいねえ。ちなみにここはどんな効能があるの？」" timeout="" voices="usi13612"/>
		<msg @l="1871" char="0" flags="0" index="844" line="1871" name="花音"
		   text="「ジャジャーン！　この温泉の効能は、消化器系の病気と\n:美肌効果でーす。お肌がツルツル、スベスベになるらしいよ」"
		   timeout="" voices="kan12515"/>
		<msg @l="1873" char="2" flags="0" index="846" line="1873" name="紗々"
		   text="「美肌効果！　それならこのまま入ってれば、\n:礼羽さんみたいな綺麗なお肌が手に入るんですか？」" timeout=""
		   voices="sas11618"/>
		<msg @l="1875" char="1" flags="0" index="848" line="1875" name="汐"
		   text="「でも礼羽さんも入ってるんだから、\n:もっと綺麗になっちゃうんじゃないの？」" timeout=""
		   voices="usi13613"/>
		<msg @l="1877" char="2" flags="0" index="850" line="1877" name="紗々"
		   text="「これだけ綺麗なのにまだカンストしてないんですか！？\n:ズルいですよっ！」" timeout=""
		   voices="sas11619"/>
		<msg @l="1879" char="8" flags="0" index="852" line="1879" name="しずか"
		   text="「スタイルもいいですよね。私もそんなふうになれるのかなぁ」" timeout="" voices="siz03716"/>
		<msg @l="1881" char="0" flags="0" index="854" line="1881" name="花音"
		   text="「しかも美人さんだよ」" timeout="" voices="kan12516"/>
		<msg @l="1883" char="1" flags="0" index="856" line="1883" name="汐"
		   text="「そして天才パティシエ……」" timeout="" voices="usi13614"/>
		<msg @l="1885" char="4" flags="0" index="858" line="1885" name="ＪＣ"
		   text="「さらに巨乳と来たもんだ……これってよく考えたら\n:チートじゃねー？」" timeout="" voices="jei10914"/>
		<msg @l="1887" char="8" flags="0" index="860" line="1887" name="しずか"
		   text="「もしかして、わたしたちから吸収してたり？」" timeout="" voices="siz03717"/>
		<play_sound @l="1889" buf="0" fadeout="2500" type="0"/>
		<msg @l="1891" char="1" flags="0" index="862" line="1891" name="汐"
		   text="「“たち”って、ひょっとして私もふくまれているのかな？」" timeout="" voices="usi13615"/>
		<msg @l="1894" char="8" flags="0" index="864" line="1894" name="しずか"
		   text="「……………」" timeout="" voices="siz03718"/>
		<play_sound @l="1896" buf="0" fadeout="500" intro="-1" src="bgm08" type="0"
		   volume="1."/>
		<msg @l="1898" char="1" flags="0" index="866" line="1898" name="汐"
		   text="「憐れみの目で、ぽんって肩を叩かれた！？」" timeout="" voices="usi13616"/>
		<msg @l="1900" char="4" flags="0" index="868" line="1900" name="ＪＣ"
		   text="「ってわけで、これはチート行為の確認をせねばなりませんなあ。\n:まずは礼羽っちご自慢のオッパイからだーー！！」"
		   timeout="" voices="jei10915"/>
		<msg @l="1902" char="-1" flags="0" index="870" line="1902" name=""
		   text="ＪＣが両手をウネウネとイヤらしく動かしながら近づいてくる。" timeout="" voices=""/>
		<msg @l="1904" char="3" flags="0" index="872" line="1904" name="礼羽"
		   text="「ちょっ！！？　なんで私なの？　花音だってかわいいし\n:胸も大きいでしょ」" timeout=""
		   voices="rai12067"/>
		<msg @l="1906" char="3" flags="0" index="874" line="1906" name="礼羽"
		   text="「そもそもＪＣの方が私より大き――あんっ！？\n:触っちゃダメだってばー！」" timeout=""
		   voices="rai12068"/>
		<msg @l="1908" char="4" flags="0" index="876" line="1908" name="ＪＣ"
		   text="「よいではないか、よいではないか～。この\n:きめ細やかな肌触りっ！　舶来品は違いますなぁ」" timeout=""
		   voices="jei10916"/>
		<msg @l="1910" char="3" flags="0" index="878" line="1910" name="礼羽"
		   text="「ＪＣがおじさん化してる！！？」" timeout="" voices="rai12069"/>
		<msg @l="1912" char="4" flags="0" index="880" line="1912" name="ＪＣ"
		   text="「うりゃ！　たぷたぷたぷ……」" timeout="" voices="jei10917"/>
		<msg @l="1914" char="3" flags="0" index="882" line="1914" name="礼羽"
		   text="「ひゃぁぁぁあぁん！！？」" timeout="" voices="rai12070"/>
		<msg @l="1916" char="4" flags="0" index="884" line="1916" name="ＪＣ"
		   text="「ふぅ、堪能、堪能。ってわけで舶来品の次は\n:日本製をイッときますか！」" timeout="" voices="jei10918"/>
		<msg @l="1918" char="0" flags="0" index="886" line="1918" name="花音"
		   text="「え！？　なんで私の方を見て手をわきわきさせているのかな？」" timeout="" voices="kan12517"/>
		<msg @l="1920" char="4" flags="0" index="888" line="1920" name="ＪＣ"
		   text="「うおりゃーーっ！　こっちもたぷたぷだーっ！！\n:もちもちで吸い付くような感触だぜーっ！！」" timeout=""
		   voices="jei10919"/>
		<msg @l="1922" char="0" flags="0" index="890" line="1922" name="花音"
		   text="「きゃぁぁぁぁっ！！！？」" timeout="" voices="kan12518"/>
		<msg @l="1924" char="4" flags="0" index="892" line="1924" name="ＪＣ"
		   text="「いやー満足満足……って、ふたりともなんで\n:そんな目であたしを見てるんスか？」" timeout=""
		   voices="jei10920"/>
		<msg @l="1926" char="3" flags="0" index="894" line="1926" name="礼羽"
		   text="「いいわね花音。私がバックをとって羽交い締めにするから\n:思いっきりやっちゃって！」" timeout=""
		   voices="rai12071"/>
		<msg @l="1928" char="0" flags="0" index="896" line="1928" name="花音"
		   text="「オッケーだよ礼羽さん。さあＪＣさんの巨乳を\n:揉みまくっちゃうよー」" timeout="" voices="kan12519"/>
		<msg @l="1930" char="4" flags="0" index="898" line="1930" name="ＪＣ"
		   text="「ちょっ！？　あたしのはいいんスよ！　そこに尺を取っても\n:誰得ってヤツで」" timeout=""
		   voices="jei10921"/>
		<msg @l="1932" char="3" flags="0" index="900" line="1932" name="礼羽"
		   text="「ふっふっふっ。もう遅い」" timeout="" voices="rai12072"/>
		<msg @l="1934" char="0" flags="0" index="902" line="1934" name="花音"
		   text="「ふふふのふ。さあ覚悟してね」" timeout="" voices="kan12520"/>
