		<chgbg @l="1323" duration="2500" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="bg17b" type="99"/>
		<chgbg @l="1324" duration="4000" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="bg26b" type="99"/>
		<play_sound @l="1326" buf="0" fadeout="500" intro="-1" src="bgm03" type="0"
		   volume="1."/>
		<face src=""/>
		<msg @l="1328" char="-1" flags="0" index="594" line="1328" name=""
		   text="――結局、ライライには会えずに放課後になってしまった。" timeout="" voices=""/>
		<face src="f"/>
		<msg @l="1334" char="11" flags="0" index="596" line="1334" name="亮"
		   text="（朝霞さんたちになんて報告すればいいんだろう……はぁ）" timeout="" voices=""/>
		<face src=""/>
