<xscript>
	<code>
		<face src="fkyu01c"/>
		<msg @l="1121" char="9" flags="0" index="475" line="1121" name="久太郎"
		   text="「宮原、会いたかったぞ。やっと会えたな」" timeout="" voices="mome00304"/>
		<face src=""/>
		<msg @l="1123" char="-1" flags="0" index="477" line="1123" name=""
		   text="僕の肩を掴んだまま、モロキュウがいきなり男泣きし始める。" timeout="" voices=""/>
		<face src="fkyu01c"/>
		<msg @l="1126" char="9" flags="0" index="479" line="1126" name="久太郎"
		   text="「よかったぁ～！　復学できて本当に良かったなぁ～っ！\n:俺ずっとお前と勝負したくてさぁ」" timeout=""
		   voices="mome00305"/>
		<face src="fkyu01c"/>
		<msg @l="1129" char="9" flags="0" index="481" line="1129" name="久太郎"
		   text="「お前がいないと張り合いなくて、ううっ……寂しかったぜぇ」" timeout="" voices="mome00306"/>
		<face src=""/>
		<msg @l="1131" char="-1" flags="0" index="483" line="1131" name=""
		   text="相変わらず暑苦しいが、基本的にいいやつなのだ。" timeout="" voices=""/>
		<face src="f"/>
		<msg @l="1133" char="11" flags="0" index="485" line="1133" name="亮"
		   text="「僕も寂しかったぞ。ってわけで、聞きたいんだけど。\n:ライコネンさんってお前のクラスだったよな？」" timeout=""
		   voices=""/>
		<face src="fkyu01c"/>
		<msg @l="1136" char="9" flags="0" index="487" line="1136" name="久太郎"
		   text="「えぇ！？　そんなアッサリ話題変えるのか？　ここは男同士の\n:友情的に握手なんかして……」" timeout=""
		   voices="mome00307"/>
		<face src="f"/>
		<msg @l="1138" char="11" flags="17" index="489" line="1138" name="亮"
		   text="「はい握手。それでライコネンさんについてなんだけど」" timeout="" voices=""/>
		<scrlangle @l="1139" duration="200" view="0" y="10"/>
		<scrlangle @l="1140" duration="200" view="0" y="0"/>
		<scrlangle @l="1141" duration="200" view="0" y="10"/>
		<scrlangle @l="1142" duration="200" view="0" y="0"/>
		<pause @l="1143" flags="0" index="491" timeout=""/>
		<face src="fkyu01c"/>
		<msg @l="1146" char="9" flags="0" index="492" line="1146" name="久太郎"
		   text="「すっげーおざなりなんだけどぉ！？　ちょっと待って、\n:なに感動の再会より女の話なわけ？」" timeout=""
		   voices="mome00308"/>
		<play_sound @l="1148" buf="1" fadeout="0" intro="-1" src="se601" type="1"
		   volume="1."/>
		<face src="f"/>
		<msg @l="1149" char="11" flags="17" index="494" line="1149" name="亮"
		   text="\h;70:\s;70:「あたり前だこのやろう！」\s:\h:" timeout="" voices=""/>
		<shakescrn @l="1150" amp="20" cycle="100" duration="300" flags="2"
		   screen="0"/>
		<pause @l="1151" flags="0" index="496" timeout=""/>
		<play_sound @l="1152" buf="1" fadeout="0" type="1"/>
		<face src="fkyu01c"/>
		<msg @l="1155" char="9" flags="0" index="497" line="1155" name="久太郎"
		   text="「えなに！？　そこキレるとこ？　あっれー？　\n:俺が悪いのかなー？　どうなのかなー？」" timeout=""
		   voices="mome00309"/>
		<face src="f"/>
		<msg @l="1157" char="11" flags="0" index="499" line="1157" name="亮"
		   text="「お前の持ってる情報ぜんぶだして。そのためのポジション\n:なんだからサクサク進めよ？」" timeout="" voices=""/>
		<face src="fkyu01c"/>
		<msg @l="1160" char="9" flags="0" index="501" line="1160" name="久太郎"
		   text="「なんか、いまいち納得がいかないんだが……」" timeout="" voices="mome00310"/>
		<face src="f"/>
		<msg @l="1162" char="11" flags="0" index="503" line="1162" name="亮"
		   text="「はい死んだー。便利な友人ポジションとしてのお前死んだー」" timeout="" voices=""/>
		<face src="fkyu01c"/>
		<msg @l="1165" char="9" flags="17" index="505" line="1165" name="久太郎"
		   text="「ま、まま待って！説明するから、\n:なんでも話すからーーっ！！」" timeout="" voices="mome00311"/>
		<shakescrn @l="1166" amp="20" cycle="100" duration="1000" flags="2"
		   screen="0"/>
		<pause @l="1167" flags="0" index="507" timeout=""/>
		<play_sound @l="1170" buf="0" fadeout="2500" type="0"/>
		<showmsg @l="1171" duration="500" show="0"/>
		<chgbg @l="1172" duration="1000" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="black" type="99"/>
		<chgbg @l="1173" duration="1000" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="bg26a" type="99"/>
		<play_sound @l="1175" buf="1" fadeout="0" intro="0" src="se405" type="1"
		   volume="1."/>
		<face src=""/>
		<msg @l="1177" char="-1" flags="0" index="508" line="1177" name=""
		   text="モロキュウに事情を説明した。" timeout="" voices=""/>
		<play_sound @l="1182" buf="1" fadeout="1500" type="1"/>
		<play_sound @l="1184" buf="0" fadeout="2500" type="0"/>
		<showmsg @l="1185" duration="500" show="0"/>
		<chgbg @l="1187" duration="1000" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="bg27a" type="99"/>
		<play_sound @l="1188" buf="0" fadeout="500" intro="-1" src="bgm02" type="0"
		   volume="1."/>
		<face src="fkyu01c"/>
		<msg @l="1191" char="9" flags="0" index="510" line="1191" name="久太郎"
		   text="「なるほど、ライコネンくんを春日部くんの店に引き抜くと」" timeout="" voices="mome00312"/>
		<face src="f"/>
		<msg @l="1193" char="11" flags="0" index="512" line="1193" name="亮"
		   text="「引き抜きっていうのはちょっと語弊があるな。もうすでに彼女は\n:店を辞めている、だからウチが雇っても問題にはならないはずだ」"
		   timeout="" voices=""/>
		<face src="fkyu01c"/>
		<msg @l="1196" char="9" flags="0" index="514" line="1196" name="久太郎"
		   text="「確かにライコネンくんは、もうスヴニールグループを\n:辞めているな。いやー、あの時は衝撃が走ったぜ」" timeout=""
		   voices="mome00313"/>
		<face src="f"/>
		<msg @l="1198" char="11" flags="0" index="516" line="1198" name="亮"
		   text="「ん？　どういうこと？」" timeout="" voices=""/>
		<face src="fkyu01c"/>
		<msg @l="1201" char="9" flags="0" index="518" line="1201" name="久太郎"
		   text="「あ、今俺がバイトしてるのがスヴニールグループの\n:アイスクリーム店なんだけどな。そこまで届いてくるくらいの\n:ビッグニュースだったんだ」"
		   timeout="" voices="mome00314"/>
		<face src="fkyu01c"/>
		<msg @l="1204" char="9" flags="0" index="520" line="1204" name="久太郎"
		   text="「なにせグループの看板が電撃的に辞めたんだぜ。\n:ジェイク社長なんてショックでしばらく普通の\n:日本語だけで喋ってたらしい\n:からな」"
		   timeout="" voices="mome00315"/>
		<face src="f"/>
		<msg @l="1206" char="11" flags="0" index="522" line="1206" name="亮"
		   text="「いいよいいよ、そういうのだよ。そういう説明が便利な\n:友人ポジションなんだよ」" timeout="" voices=""/>
		<face src="fkyu01c"/>
		<msg @l="1209" char="9" flags="0" index="524" line="1209" name="久太郎"
		   text="「そ、そうか？」" timeout="" voices="mome00316"/>
		<face src="f"/>
		<msg @l="1211" char="11" flags="0" index="526" line="1211" name="亮"
		   text="「あ、照れたりとかそういうのはいらないから。\n:情報だけ言えばいいから」" timeout="" voices=""/>
		<play_sound @l="1213" buf="0" fadeout="500" intro="-1" src="bgm08" type="0"
		   volume="1."/>
		<face src="fkyu01c"/>
		<msg @l="1216" char="9" flags="17" index="528" line="1216" name="久太郎"
		   text="「扱いひどすぎるでしょちょっとぉぉっ！？」" timeout="" voices="mome00317"/>
		<shakescrn @l="1217" amp="20" cycle="100" duration="300" flags="2"
		   screen="0"/>
		<pause @l="1218" flags="0" index="530" timeout=""/>
		<face src=""/>
		<msg @l="1220" char="-1" flags="0" index="531" line="1220" name=""
		   text="こいつには悪いがこっちは使命があるからな。" timeout="" voices=""/>
		<face src="f"/>
		<msg @l="1222" char="11" flags="0" index="533" line="1222" name="亮"
		   text="「で、学園での様子はどうなんだ？」" timeout="" voices=""/>
		<face src="fkyu01c"/>
		<msg @l="1225" char="9" flags="0" index="535" line="1225" name="久太郎"
		   text="「最近サボりがちではあるな。元々、優秀な生徒で必要な単位は\n:ほぼ取り終わってるから、誰も問題視してないが」" timeout=""
		   voices="mome00318"/>
		<face src="f"/>
		<msg @l="1227" char="11" flags="0" index="537" line="1227" name="亮"
		   text="「病気……ではないよな？」" timeout="" voices=""/>
		<face src="fkyu01c"/>
		<msg @l="1230" char="9" flags="0" index="539" line="1230" name="久太郎"
		   text="「そんな話は聞かないな。まあ、精神的な方っていうか、\n:ぶっちゃけ無気力っていうか。おそらくはそっちだな」" timeout=""
		   voices="mome00319"/>
		<face src="fkyu01c"/>
		<msg @l="1233" char="9" flags="0" index="541" line="1233" name="久太郎"
		   text="「退社した時期と学園をサボりがちになったタイミングが\n:ほぼ一致しているし、間違いない」" timeout=""
		   voices="mome00320"/>
		<face src="fkyu01c"/>
		<msg @l="1236" char="9" flags="0" index="543" line="1236" name="久太郎"
		   text="「まあ、学園に来たとしてもどこか上の空でボーッとしてるか、\n:机に突っ伏しているのが彼女の日常だから、出席日数以外、\n:来ても来なくても大差ないとは思うが……」"
		   timeout="" voices="mome00321"/>
		<face src=""/>
		<msg @l="1238" char="-1" flags="0" index="545" line="1238" name=""
		   text="ふむ。かなり飄々としてるタイプだとは思ってたが、\n:そんなことになっていたとは。" timeout="" voices=""/>
		<face src="fkyu01c"/>
		<msg @l="1241" char="9" flags="0" index="547" line="1241" name="久太郎"
		   text="「そういうところがミステリアスでいい！　みたいな男子も多いし\n:人気があるのは確かだ」" timeout=""
		   voices="mome00322"/>
		<face src="fkyu01c"/>
		<msg @l="1244" char="9" flags="0" index="549" line="1244" name="久太郎"
		   text="「明るい春日部くんとミステリアスなライコネンくん。\n:このふたりが今、我が学園のツートップだろう」" timeout=""
		   voices="mome00323"/>
		<face src="f"/>
		<msg @l="1246" char="11" flags="0" index="551" line="1246" name="亮"
		   text="「いいよいいよ。そういう情報どんどんちょうだい。お前、\n:だんだん自分の役割わかってきたじゃん」" timeout=""
		   voices=""/>
		<face src="fkyu01c"/>
		<msg @l="1249" char="9" flags="0" index="553" line="1249" name="久太郎"
		   text="「そ、そうか？」" timeout="" voices="mome00324"/>
		<face src="f"/>
		<msg @l="1251" char="11" flags="0" index="555" line="1251" name="亮"
		   text="「だから照れたりとかは一切いらないから」" timeout="" voices=""/>
		<face src="fkyu01c"/>
		<msg @l="1254" char="9" flags="17" index="557" line="1254" name="久太郎"
		   text="「扱いひどすぎるでしょちょっとぉぉっ！？」" timeout="" voices="mome00317"/>
		<shakescrn @l="1255" amp="20" cycle="100" duration="300" flags="2"
		   screen="0"/>
		<pause @l="1256" flags="0" index="559" timeout=""/>
		<play_sound @l="1258" buf="0" fadeout="500" intro="-1" src="bgm20" type="0"
		   volume="1."/>
		<face src="fkyu01c"/>
		<msg @l="1261" char="9" flags="0" index="560" line="1261" name="久太郎"
		   text="「それにしても、しばらく見ない間に宮原がこんなに\n:変わっているとは……前は女に興味なしって感じだったのに」" timeout=""
		   voices="mome00325"/>
		<face src="f"/>
		<msg @l="1263" char="11" flags="0" index="562" line="1263" name="亮"
		   text="「僕にも色々と事情ってもんがあってなあ」" timeout="" voices=""/>
		<face src="fkyu01c"/>
		<msg @l="1266" char="9" flags="0" index="564" line="1266" name="久太郎"
		   text="「あっ、ちなみに俺はミステリアスなライコネンくんや明るくて\n:気さくな春日部くんよりも、ミエちゃんの方が好きだから！」"
		   timeout="" voices="mome00326"/>
		<face src="f"/>
		<msg @l="1268" char="11" flags="17" index="566" line="1268" name="亮"
		   text="「ふーん。って、誰だよミエちゃんって！？」" timeout="" voices=""/>
		<wait @l="1271" flags="5" time="1400"/>
		<play_sound @l="1272" buf="1" fadeout="0" intro="-1" src="se601" type="1"
		   volume="1."/>
		<shakescrn @l="1273" amp="20" cycle="100" duration="300" flags="2"
		   screen="0"/>
		<pause @l="1274" flags="-2147483648" index="568" timeout=""/>
		<play_sound @l="1277" buf="1" fadeout="0" type="1"/>
		<face src="fkyu01c"/>
		<msg @l="1280" char="9" flags="0" index="569" line="1280" name="久太郎"
		   text="「あれ、言ってなかったか？　バイト先で一緒のシフトになった\n:ミエちゃん」" timeout=""
		   voices="mome00327"/>
		<face src="fkyu01c"/>
		<msg @l="1283" char="9" flags="0" index="571" line="1283" name="久太郎"
		   text="「これがまたかわいいんだよ。あ、歳は一個下なんだけどな、\n:すっげー素直なの」" timeout=""
		   voices="mome00328"/>
		<face src="fkyu01c"/>
		<msg @l="1286" char="9" flags="0" index="573" line="1286" name="久太郎"
		   text="「“センパイわからないから教えて？”　って、すぐに聞いて\n:くるような子でさあ。庇護欲をかきたてられるわけよこれ」"
		   timeout="" voices="mome00329"/>
		<face src="f"/>
		<msg @l="1288" char="11" flags="0" index="575" line="1288" name="亮"
		   text="「うん、それはどうでもいい情報だな。それよりも\n:ライコネンさんのこと“わからないから教えて？”」" timeout=""
		   voices=""/>
		<face src="fkyu01c"/>
		<msg @l="1291" char="9" flags="0" index="577" line="1291" name="久太郎"
		   text="「お前には庇護欲感じねーよ！　それより語ろうぜ。\n:やっと女に興味わいてきたんだろ？　これを機会に\n:修学旅行の夜ごっこしよーぜ」"
		   timeout="" voices="mome00330"/>
		<face src="fkyu01c"/>
		<msg @l="1294" char="9" flags="0" index="579" line="1294" name="久太郎"
		   text="「俺、友人と女の子について喋るの夢だったんだ」" timeout="" voices="mome00331"/>
		<face src="f"/>
		<msg @l="1296" char="11" flags="0" index="581" line="1296" name="亮"
		   text="「そのうちな。今日はちょっと忙しいからまた今度」" timeout="" voices=""/>
		<face src="fkyu01c"/>
		<msg @l="1299" char="9" flags="0" index="583" line="1299" name="久太郎"
		   text="「また今度っていつだよ？」" timeout="" voices="mome00332"/>
		<play_sound @l="1301" buf="0" fadeout="2500" type="0"/>
		<face src="f"/>
		<msg @l="1303" char="11" flags="0" index="585" line="1303" name="亮"
		   text="「ん？　土用の丑の日とか、適当にそんな感じの日」" timeout="" voices=""/>
		<play_sound @l="1305" buf="0" fadeout="500" intro="-1" src="bgm09" type="0"
		   volume="1."/>
		<face src="fkyu01c"/>
		<msg @l="1308" char="9" flags="17" index="587" line="1308" name="久太郎"
		   text="「曖昧すぎるでしょちょっとぉぉっ！？」" timeout="" voices="mome00333"/>
		<shakescrn @l="1309" amp="20" cycle="100" duration="1000" flags="2"
		   screen="0"/>
		<pause @l="1310" flags="0" index="589" timeout=""/>
		<face src="fkyu01c"/>
		<msg @l="1313" char="9" flags="0" index="590" line="1313" name="久太郎"
		   text="「それ聞く気ないよね！？　語る気も一切ないよねぇぇっ！！？」" timeout="" voices="mome00334"/>
		<face src=""/>
		<msg @l="1315" char="-1" flags="0" index="592" line="1315" name=""
		   text="そんな感じで、学園でのライライの様子などを聞いた。" timeout="" voices=""/>
		<play_sound @l="1319" buf="1" fadeout="0" type="1"/>
		<play_sound @l="1320" buf="0" fadeout="5000" type="0"/>
		<showmsg @l="1321" duration="500" show="0"/>
		<chgbg @l="1322" duration="1000" flags="256" flip="-1" flip_flags="0"
