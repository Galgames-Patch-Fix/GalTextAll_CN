		<play_sound @l="2181" buf="5" fadeout="0" intro="0" src="usi80023" type="6"
		   volume="1."/>
		<msg @l="2182" char="-1" flags="0" index="1346" line="2182" name=""
		   text="言われるまでもなく、僕もイキそうだった。" timeout="" voices=""/>
