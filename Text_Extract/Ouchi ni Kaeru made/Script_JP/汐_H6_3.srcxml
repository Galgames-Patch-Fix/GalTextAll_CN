		   volume="1."/>
		<msg @l="2165" char="-1" flags="0" index="1334" line="2165" name=""
		   text="まるで熱病にうなされたみたいに隠語を連発する朝霞さん。" timeout="" voices=""/>
		<msg @l="2166" char="-1" flags="0" index="1336" line="2166" name=""
