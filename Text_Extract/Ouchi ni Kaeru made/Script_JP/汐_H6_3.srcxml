		   volume="1."/>
		<msg @l="2059" char="11" flags="0" index="1264" line="2059" name="亮"
		   text="「なら、そのぶん僕がっ！」" timeout="" voices=""/>
		<play_sound @l="2060" buf="5" fadeout="0" type="1"/>
		<chgbg @l="2062" duration="300" flags="256" flip="-1" flip_flags="0"
