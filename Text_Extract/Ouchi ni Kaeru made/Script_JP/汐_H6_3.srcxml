		   volume="1."/>
		<msg @l="2136" char="-1" flags="0" index="1314" line="2136" name=""
		   text="瞳の焦点がだんだん怪しくなってきたけれど、\n:膣だけはしっかりと僕を捕まえて離さない。" timeout="" voices=""/>
		<play_sound @l="2138" buf="1" fadeout="0" intro="-1" src="hse110" type="1"
		   volume="1."/>
		<msg @l="2139" char="-1" flags="0" index="1316" line="2139" name=""
		   text="割れ目のキツさが愛の深さに思えて、\n:僕もそれに応えようとピストンをさらに早く、深くしていく。" timeout=""
		   voices=""/>
		<msg @l="2141" char="11" flags="0" index="1318" line="2141" name="亮"
