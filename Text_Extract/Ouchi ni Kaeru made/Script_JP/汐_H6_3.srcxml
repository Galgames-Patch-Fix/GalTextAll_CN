		   volume="1."/>
		<msg @l="2108" char="11" flags="0" index="1292" line="2108" name="亮"
		   text="「まっ、まさか、この体勢で――」" timeout="" voices=""/>
		<msg @l="2110" char="-1" flags="0" index="1294" line="2110" name=""
		   text="汗で滑ってうまくキまらないけれど、\n:いわゆる大好きホールドを狙っているらしい。" timeout="" voices=""/>
		<play_sound @l="2111" buf="5" fadeout="0" type="1"/>
		<msg @l="2113" char="1" flags="0" index="1296" line="2113" name="汐"
		   text="「んぐっ……ううぅ、もうちょっと足が長かったら、\n:うまくひっかけられるのにっ……んぅ、んくっ、んんッ」" timeout=""
		   voices="usi17672"/>
		<play_sound @l="2115" buf="5" fadeout="0" intro="0" src="usi80022" type="6"
		   volume="1."/>
		<msg @l="2116" char="-1" flags="0" index="1298" line="2116" name=""
		   text="あとほんの少し足が長ければ、\n:膝裏がうまいこと僕の太ももに引っかかっていたに違いない。" timeout="" voices=""/>
		<msg @l="2117" char="-1" flags="0" index="1300" line="2117" name=""
		   text="けど、ちっちゃな身体では、その少しがすごく遠かった。" timeout="" voices=""/>
		<msg @l="2119" char="11" flags="0" index="1302" line="2119" name="亮"
		   text="「そんなに無理しなくても、\n:僕がちゃーんと離さずぎゅってしてますからっ……」" timeout="" voices=""/>
