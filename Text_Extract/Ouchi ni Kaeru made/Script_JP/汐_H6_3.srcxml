		<msg @l="2042" char="1" flags="0" index="1252" line="2042" name="汐"
		   text="「やはっ\x;heart:　うんっ、ちゅーすりゅ……\n:んむっ、はあっ、れろろっ、んちゅっ」" timeout=""
		   voices="usi17661"/>
		<msg @l="2044" char="1" flags="0" index="1254" line="2044" name="汐"
		   text="「はあっ、ああっ、こんなにベロだして、れろれろして……\n:どんどんにゃんこになってくよぉ\x;heart:」" timeout=""
		   voices="usi17662"/>
		<play_sound @l="2046" buf="5" fadeout="0" intro="0" src="usi80030" type="6"
		   volume="1."/>
		<msg @l="2047" char="-1" flags="0" index="1256" line="2047" name=""
		   text="見つめ合ったままお互いに伸ばした舌を動かすと、\n:まるで本当にキスをしてるみたいに脳が甘く痺れた。" timeout=""
		   voices=""/>
		<msg @l="2048" char="-1" flags="0" index="1258" line="2048" name=""
		   text="味も、感触も、温度も、匂いも。全部よーく知ってるからこそ\n:出来る、バーチャルなディープキスだ。" timeout=""
		   voices=""/>
		<play_sound @l="2049" buf="5" fadeout="0" type="1"/>
		<chgbg @l="2053" duration="300" flags="256" flip="-1" flip_flags="0"
