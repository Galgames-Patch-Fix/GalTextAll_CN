		<chgbg @l="2178" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evusi17_02a" type="99"/>
		<msg @l="2179" char="1" flags="0" index="1344" line="2179" name="汐"
