		<msg @l="2122" char="1" flags="0" index="1304" line="2122" name="汐"
		   text="「でも、されるばっかじゃなくて、私もキミのこと、\n:もっと気持ちよくしてあげたいんだもんっ」" timeout=""
		   voices="usi17673"/>
		<play_sound @l="2124" buf="5" fadeout="0" intro="0" src="usi80021" type="6"
		   volume="1."/>
		<msg @l="2125" char="11" flags="0" index="1306" line="2125" name="亮"
		   text="「襲われてるんだから、\n:そんなことまで気にしなくてもいいんですっ」" timeout="" voices=""/>
		<msg @l="2127" char="11" flags="0" index="1308" line="2127" name="亮"
		   text="「だから安心して、めいっぱい感じてくださいっ……！」" timeout="" voices=""/>
		<play_sound @l="2128" buf="5" fadeout="0" type="1"/>
		<chgbg @l="2130" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evusi17_02c" type="99"/>
		<msg @l="2131" char="1" flags="0" index="1310" line="2131" name="汐"
		   text="「んぅっ\x;heart:　んもぅ、\n:キミってばドキッとさせる天才なんだからっ……」" timeout=""
		   voices="usi17674"/>
		<msg @l="2133" char="1" flags="0" index="1312" line="2133" name="汐"
		   text="「そんなこと言われたら、安心しちゃってっ……\n:はあっ、ほんとにおま○こごと、おかしくなっちゃうよ？」" timeout=""
