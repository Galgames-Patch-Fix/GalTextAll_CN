<?xml version="1.0" encoding="utf-8"?>
<xscript>
	<code>
		<msg @l="2007" char="1" flags="0" index="1228" line="2007" name="汐"
		   text="「カリがぐにぐにこしゅれるっ……ふあっ、はあっ、ああっっ！」" timeout="" voices="usi17656"/>
		<msg @l="2009" char="1" flags="0" index="1230" line="2009" name="汐"
		   text="「にゃんこみたいなかっこで、後ろから激しく襲われて、なのに、しゅごくきもちーよおっ」" timeout=""
		   voices="usi17657"/>
		<msg @l="2011" char="1" flags="0" index="1232" line="2011" name="汐"
		   text="「んぐっ、はあっ、お願い、もっとキスしてっ？　\n:亀頭でちゅっちゅって、いっぱい子宮にちゅっちゅっしてっ？」" timeout=""
		   voices="usi17658"/>
		<chgbg @l="2015" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evusi17_02b" type="99"/>
		<msg @l="2016" char="1" flags="0" index="1234" line="2016" name="汐"
		   text="「はああぁ……んむぅ、んっ、ちゅーしたいっ、ちゅーっ、\n:いっぱいちゅーうっ……ふはっ、はぁむっ」" timeout=""
		   voices="usi17659"/>
		<play_sound @l="2018" buf="5" fadeout="0" intro="0" src="usi80021" type="6"
		   volume="1."/>
		<msg @l="2019" char="-1" flags="0" index="1236" line="2019" name=""
		   text="言葉にしたことでキスをしたくなったみたいに、\n:口をあけて舌だけ伸ばしている朝霞さん。" timeout="" voices=""/>
		<msg @l="2020" char="-1" flags="0" index="1238" line="2020" name=""
		   text="しかしこの体勢ではどうやっても届かない。\n:代わりにご希望どおり亀頭で子宮口にキスの嵐を浴びせていく。" timeout=""
		   voices=""/>
		<msg @l="2022" char="11" flags="0" index="1240" line="2022" name="亮"
		   text="「そんな、ベロをしまい忘れた猫みたいに……ぐぐっ」" timeout="" voices=""/>
		<msg @l="2024" char="-1" flags="0" index="1242" line="2024" name=""
		   text="乱れる呼吸を逃がすように伸ばした舌が、\n:れろれろと可愛く動き回る。" timeout="" voices=""/>
		<msg @l="2025" char="-1" flags="0" index="1244" line="2025" name=""
		   text="それを見ただけで、自然と喉が鳴ってしまう。" timeout="" voices=""/>
		<msg @l="2027" char="11" flags="0" index="1246" line="2027" name="亮"
		   text="「こっち向いて、もっと舌をだしてくださいっ」" timeout="" voices=""/>
		<play_sound @l="2028" buf="5" fadeout="0" type="1"/>
		<chgbg @l="2032" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evusi17_02a" type="99"/>
		<msg @l="2033" char="1" flags="0" index="1248" line="2033" name="汐"
		   text="「んぅっ？　こーお？」" timeout="" voices="usi17660"/>
		<play_sound @l="2035" buf="5" fadeout="0" intro="0" src="usi80031" type="6"
		   volume="1."/>
		<msg @l="2036" char="11" flags="0" index="1250" line="2036" name="亮"
		   text="「そのままキスする感じで、れろれろって……はぁ、んむっ」" timeout="" voices=""/>
		<play_sound @l="2037" buf="5" fadeout="0" type="1"/>
		<chgbg @l="2041" duration="300" flags="256" flip="-1" flip_flags="0"
