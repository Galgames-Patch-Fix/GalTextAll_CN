		<waitbs @l="42" flags="0" screen="0"/>
		<face src="fusi02b"/>
		<msg @l="43" char="1" flags="0" index="18" line="43" name="汐"
		   text="「ううん。明日もお休みだし、このまま洗濯しちゃうよ」" timeout="" voices="usi12903"/>
		<chgbs async="0" duration="200" priority="0" screen="0" src="usi08b"
		   transparency="0" x="2147483648"/>
		<waitbs @l="45" flags="0" screen="0"/>
		<face src="fusi08b"/>
		<msg @l="46" char="1" flags="0" index="20" line="46" name="汐"
		   text="「ってことで、キミのも一緒に洗ってあげる……\n:だから、いいよ？　汚しても」" timeout=""
		   voices="usi12904"/>
		<face src="f"/>
		<msg @l="48" char="11" flags="0" index="22" line="48" name="亮"
		   text="「よ、汚す？　そそ、それってつまり……」" timeout="" voices=""/>
		<chgbs async="0" duration="200" priority="0" screen="0" src="usi05b"
		   transparency="0" x="2147483648"/>
		<waitbs @l="50" flags="0" screen="0"/>
		<face src="fusi05b"/>
		<msg @l="51" char="1" flags="0" index="24" line="51" name="汐"
		   text="「どうせ洗うならせっかくだし……このまま汚してもいいよ」" timeout="" voices="usi12905"/>
		<face src="f"/>
		<msg @l="53" char="11" flags="0" index="26" line="53" name="亮"
		   text="「それってつまり、制服でプレイしてもいいってことですよね！　やったーっ！」" timeout="" voices=""/>
		<chgbs async="0" duration="200" priority="0" screen="0" src="usi08b"
		   transparency="0" x="2147483648"/>
		<waitbs @l="55" flags="0" screen="0"/>
		<face src="fusi08b"/>
		<msg @l="56" char="1" flags="0" index="28" line="56" name="汐"
		   text="「プレイとか言っちゃダメ、恥ずかしいじゃない」" timeout="" voices="usi12906"/>
		<face src=""/>
		<msg @l="58" char="-1" flags="0" index="30" line="58" name=""
		   text="顔を赤らめながら、少し身をよじる朝霞さん。" timeout="" voices=""/>
		<chgbs async="0" duration="200" priority="0" screen="0" src="usi26b"
		   transparency="0" x="2147483648"/>
		<waitbs @l="60" flags="0" screen="0"/>
		<face src="fusi26b"/>
		<msg @l="61" char="1" flags="0" index="32" line="61" name="汐"
		   text="「でも、もうガマンできないの。\n:ほんとはお昼ご飯を一緒に食べた時から\n:ずっとエッチな事ばっかり考えちゃってて」"
		   timeout="" voices="usi12907"/>
		<face src="f"/>
		<msg @l="63" char="11" flags="0" index="34" line="63" name="亮"
		   text="「僕もです！　もうどうやってムラムラを抑えていいのか、\n:そっちの方に必死でしたから」" timeout="" voices=""/>
		<chgbs async="0" duration="200" priority="0" screen="0" src="usi10b"
		   transparency="0" x="2147483648"/>
		<waitbs @l="65" flags="0" screen="0"/>
		<face src="fusi10b"/>
		<msg @l="66" char="1" flags="17" index="36" line="66" name="汐"
		   text="「あはっ、嬉しいこと言ってくれちゃって。\n:じゃあ、いっぱいエッチしよ？」" timeout=""
		   voices="usi12908"/>
		<emotion @l="67" id="usi" loop="1" screen="0" src="emotion_06" time="-1"
		   type="usi_1" x="0" y="0"/>
		<pause @l="68" flags="0" index="38" timeout=""/>
		<clremt @l="69" id="usi" screen="0"/>
		<face src="f"/>
		<msg @l="71" char="11" flags="0" index="39" line="71" name="亮"
		   text="「望むところです。では早速――」" timeout="" voices=""/>
		<chgbs async="0" duration="200" priority="0" screen="0" src="usi05b"
		   transparency="0" x="2147483648"/>
		<waitbs @l="73" flags="0" screen="0"/>
		<face src="fusi05b"/>
		<msg @l="74" char="1" flags="0" index="41" line="74" name="汐"
		   text="「あっ、ちょっと待って。その前に、えっと……あ、あった。\n:これを見ながらしてもいい？」" timeout=""
		   voices="usi12909"/>
		<face src=""/>
		<msg @l="76" char="-1" flags="0" index="43" line="76" name=""
		   text="そう言った彼女の手には、見慣れない一冊の本。" timeout="" voices=""/>
		<chgbs async="0" duration="200" priority="0" screen="0" src="usi02b"
		   transparency="0" x="2147483648"/>
		<waitbs @l="78" flags="0" screen="0"/>
		<face src="fusi02b"/>
		<msg @l="79" char="1" flags="0" index="45" line="79" name="汐"
		   text="「クラスのお友達が、こういう勉強も必要になるだろうからって、貸してくれて……」" timeout=""
		   voices="usi12910"/>
		<chgbs async="0" duration="200" priority="0" screen="0" src="usi05b"
		   transparency="0" x="2147483648"/>
		<waitbs @l="81" flags="0" screen="0"/>
		<face src="fusi05b"/>
		<msg @l="82" char="1" flags="0" index="47" line="82" name="汐"
		   text="「本当はもっと覚えてから試すつもりだったんだけど……\n:せっかくだし、見ながら実践してもいいかな？」" timeout=""
		   voices="usi12911"/>
		<face src="f"/>
		<msg @l="84" char="11" flags="0" index="49" line="84" name="亮"
		   text="「よくわかりませんが、全部お任せします」" timeout="" voices=""/>
		<play_sound @l="89" buf="0" fadeout="2500" type="0"/>
		<showmsg @l="90" duration="500" show="0"/>
		<loadimage @l="95" bg="0" fit="0" screen="1" src="evusi06_01c"/>
		<scroll @l="96" async="0" center_x="45" center_y="45" duration="0" flags="0"
		   rev="0." screen="1" transparency="0" v0="0." v1="0." wait_flags="0" x="0"
		   y="0" zoom="1."/>
		<chgscrn @l="97" duration="2500" flags="4608" flip="-1" flip_flags="0"
		   param="0" screen="1" type="10"/>
		<play_sound @l="98" buf="0" fadeout="500" intro="-1" src="bgm18" type="0"
		   volume="1."/>
		<msg @l="102" char="-1" flags="0" index="51" line="102" name=""
		   text="――なんの本だろう、という疑問はすぐに解決した。" timeout="" voices=""/>
		<msg @l="106" char="1" flags="0" index="53" line="106" name="汐"
		   text="「ふむふむ。ここまでが亀頭でこの段差がカリ、\n:あとは裏筋にそれから、この膨らみが玉袋……」" timeout=""
		   voices="usi12912"/>
		<msg @l="108" char="1" flags="0" index="55" line="108" name="汐"
		   text="「どう？　あってる？」" timeout="" voices="usi12913"/>
		<msg @l="110" char="11" flags="0" index="57" line="110" name="亮"
		   text="「あってますけど……」" timeout="" voices=""/>
		<msg @l="112" char="1" flags="0" index="59" line="112" name="汐"
		   text="「よかった。なら、次は――」" timeout="" voices="usi12914"/>
		<msg @l="114" char="11" flags="0" index="61" line="114" name="亮"
		   text="「えっと、その本ってもしかして」" timeout="" voices=""/>
		<chgbg @l="117" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evusi06_01a" type="99"/>
		<msg @l="119" char="1" flags="0" index="63" line="119" name="汐"
		   text="「うん。いわゆる、エッチのハウツー本。\n:……あっ、もちろん女の子向けだからね？」" timeout=""
		   voices="usi12915"/>
		<msg @l="121" char="11" flags="0" index="65" line="121" name="亮"
		   text="「なるほど、そういうのもあるんですね」" timeout="" voices=""/>
		<msg @l="123" char="-1" flags="0" index="67" line="123" name=""
		   text="まあ男性向けにそういう本があるんだし、\n:女性向けがあるのも当然ちゃ当然か。" timeout="" voices=""/>
		<msg @l="125" char="1" flags="0" index="69" line="125" name="汐"
		   text="「この前はふたりとも初めてで、うまくできなかったでしょ？　\n:だから、次はお姉ちゃんがリードできればと思って」"
		   timeout="" voices="usi12916"/>
		<msg @l="127" char="11" flags="0" index="71" line="127" name="亮"
		   text="「そうだったんですね。でも最初はみんな不慣れですし、\n:そこまで熱心にならなくてもいいのに」" timeout=""
		   voices=""/>
		<msg @l="129" char="1" flags="0" index="73" line="129" name="汐"
		   text="「不慣れだったのは事実だし……だから、\n:ちゃんとお勉強すれば、キミをもっと気持ちよくできるでしょ？」" timeout=""
		   voices="usi12917"/>
		<msg @l="131" char="1" flags="0" index="75" line="131" name="汐"
		   text="「だって、好きな人には\n:いっぱい気持ちよくなってもらいたいもん\x;heart:」" timeout=""
		   voices="usi12918"/>
		<msg @l="133" char="-1" flags="0" index="77" line="133" name=""
		   text="なんて、なんて出来た彼女なんだ！" timeout="" voices=""/>
		<msg @l="134" char="-1" flags="0" index="79" line="134" name=""
		   text="胸がじんわりと熱くなっていくのがわかる。" timeout="" voices=""/>
		<msg @l="136" char="11" flags="0" index="81" line="136" name="亮"
		   text="「そこまで考えてくれてたなんて、感激っス！」" timeout="" voices=""/>
		<chgbg @l="138" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evusi06_01c" type="99"/>
		<msg @l="140" char="1" flags="0" index="83" line="140" name="汐"
		   text="「んもう、おおげさなんだから……。\n:それなら、さっそく本の内容試してもいい？」" timeout=""
		   voices="usi12919"/>
		<msg @l="142" char="11" flags="0" index="85" line="142" name="亮"
		   text="「僕にできることなら、何でも協力しますよ。\n:それがお互いのためにもなりますからね」" timeout="" voices=""/>
		<msg @l="144" char="1" flags="0" index="87" line="144" name="汐"
		   text="「じゃあ……気持ちよかったら、いっぱい声を出したり、\n:気持ちいいって顔してね」" timeout=""
		   voices="usi12920"/>
		<msg @l="146" char="1" flags="0" index="89" line="146" name="汐"
		   text="「恥ずかしがって我慢されたら、\n:うまくできてるのかどうかわからないから」" timeout=""
		   voices="usi12921"/>
		<msg @l="148" char="11" flags="0" index="91" line="148" name="亮"
		   text="「なるほど。たしかにそれは一理ありますね」" timeout="" voices=""/>
		<msg @l="150" char="1" flags="0" index="93" line="150" name="汐"
		   text="「あとは……たくさん勃起してくれると嬉しいかな。\n:そのほうが、私のおっぱいでも挟みやすくなるはずだし」" timeout=""
		   voices="usi12922"/>
		<msg @l="152" char="1" flags="0" index="95" line="152" name="汐"
		   text="「ということで……ん、くくっ……」" timeout="" voices="usi12923"/>
		<msg @l="154" char="-1" flags="0" index="97" line="154" name=""
		   text="朝霞さんが本を手にしたまま、おっぱいを寄せる。" timeout="" voices=""/>
		<cutbs async="0" duration="0" priority="0" screen="0" src="hevusi06_cut02a"
		   transparency="256" x="2147483648"/>
		<waitbs @l="165" flags="0" screen="0"/>
		<movebs duration="0" flags="0" id="hevusi06_cut02a" rev="0." screen="0"
		   transparency="256" v0="0." v1="0." x="2147483648" y="-100"/>
		<waitbs @l="166" flags="0" screen="0"/>
		<movebs duration="800" flags="0" id="hevusi06_cut02a" rev="0." screen="0"
		   transparency="0" v0="0." v1="0." x="2147483648" y="0"/>
		<waitbs @l="168" flags="0" screen="0"/>
		<set_cg_flag @l="170" cg="hevusi06_cut02a.eri"/>
		<msg @l="173" char="1" flags="0" index="99" line="173" name="汐"
		   text="「ふぅ……どっ、どう？」" timeout="" voices="usi12924"/>
		<msg @l="175" char="11" flags="0" index="101" line="175" name="亮"
		   text="「おお……確かに、この前より包まれてる感じがします」" timeout="" voices=""/>
		<msg @l="177" char="-1" flags="0" index="103" line="177" name=""
		   text="入浴に例えるなら肩までどっぷりには遠く、半身浴。" timeout="" voices=""/>
		<msg @l="180" char="-1" flags="0" index="105" line="180" name=""
		   text="いやいや１／５浴ってところだけど、\n:前回よりも触れあう面積は大きくなった気がする。" timeout="" voices=""/>
		<msg @l="182" char="1" flags="0" index="107" line="182" name="汐"
		   text="「よかった。でね、えっと……\n:こんなふうな体勢の時は男の子に動いてもらったほうが\n:いいみたい」" timeout=""
		   voices="usi12925"/>
		<msg @l="184" char="11" flags="0" index="109" line="184" name="亮"
		   text="「わかりました。なら、最初はゆっくりで――」" timeout="" voices=""/>
		<msg @l="186" char="-1" flags="0" index="111" line="186" name=""
		   text="今回は自分のペースで腰を前後に揺すっていく。" timeout="" voices=""/>
		<loadimage @l="189" bg="0" fit="0" screen="1" src="evusi06_01b"/>
		<cutbs async="0" duration="0" priority="0" screen="1" src="hevusi06_cut02a"
		   transparency="0" x="2147483648"/>
		<waitbs @l="190" flags="0" screen="1"/>
		<chgscrn @l="191" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" type="99"/>
		<msg @l="193" char="1" flags="0" index="113" line="193" name="汐"
		   text="「はぁ、んっ……はぁ、あぁ……すごい！　\n:おち○ちんが目の前をいったりきたりっ……はぁ、はぁっ」" timeout=""
		   voices="usi12926"/>
		<msg @l="195" char="1" flags="0" index="115" line="195" name="汐"
		   text="「ふぅ……どうかな？　おっぱい、気持ちいい？」" timeout="" voices="usi12927"/>
		<play_sound @l="197" buf="5" fadeout="0" intro="0" src="usi80015" type="6"
		   volume="1."/>
		<msg @l="198" char="11" flags="0" index="117" line="198" name="亮"
		   text="「気持ちいいんですけど……ちょっと物足りないというか」" timeout="" voices=""/>
		<play_sound @l="199" buf="5" fadeout="0" type="1"/>
		<loadimage @l="202" bg="0" fit="0" screen="1" src="evusi06_01d"/>
		<cutbs async="0" duration="0" priority="0" screen="1" src="hevusi06_cut02a"
		   transparency="0" x="2147483648"/>
		<waitbs @l="203" flags="0" screen="1"/>
		<chgscrn @l="204" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" type="99"/>
		<msg @l="206" char="1" flags="0" index="119" line="206" name="汐"
		   text="「物足りない……？　んっと、本によると――」" timeout="" voices="usi12928"/>
		<msg @l="208" char="1" flags="0" index="121" line="208" name="汐"
		   text="「……あっ、わかった。おっぱいで挟むときは、\n:濡らさないと痛いこともあるって書いてある」" timeout=""
		   voices="usi12929"/>
		<msg @l="211" char="11" flags="0" index="123" line="211" name="亮"
		   text="「たしかにそれかもしれません、さすがハウツー本」" timeout="" voices=""/>
		<msg @l="213" char="-1" flags="0" index="125" line="213" name=""
		   text="前回パイズリしてもらったときは、自分のカウパーや\n:彼女の唾液で、竿全体がぬるぬるだったもんな。" timeout=""
		   voices=""/>
		<msg @l="215" char="11" flags="0" index="127" line="215" name="亮"
		   text="（しかし、これはパイズリと言っていいのだろうか？）" timeout="" voices=""/>
		<msg @l="217" char="-1" flags="0" index="129" line="217" name=""
		   text="ちっぱいに擦り付けてるだけなのだが……。" timeout="" voices=""/>
		<msg @l="218" char="-1" flags="0" index="131" line="218" name=""
		   text="まあ！　彼女が僕のためにやってくれてると思えば\n:それはそれでアリというもの。" timeout="" voices=""/>
		<msg @l="219" char="-1" flags="0" index="133" line="219" name=""
		   text="パイズリということにしておこう！　うんうんそれがいい。" timeout="" voices=""/>
		<loadimage @l="222" bg="0" fit="0" screen="1" src="evusi06_01c"/>
		<cutbs async="0" duration="0" priority="0" screen="1" src="hevusi06_cut02a"
		   transparency="0" x="2147483648"/>
		<waitbs @l="223" flags="0" screen="1"/>
		<chgscrn @l="224" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" type="99"/>
		<msg @l="227" char="1" flags="0" index="135" line="227" name="汐"
		   text="「カウパーは……さすがにまだだろうから、私が舐めてあげるね」" timeout="" voices="usi12930"/>
		<msg @l="229" char="1" flags="0" index="137" line="229" name="汐"
		   text="「もうちょっと腰をこっちに寄せて、\n:さきっぽを私のお口に近づけてもらってもいい？」" timeout=""
		   voices="usi12931"/>
		<msg @l="232" char="11" flags="0" index="139" line="232" name="亮"
		   text="「そ、そんな大胆なことしちゃっていいんですか？」" timeout="" voices=""/>
