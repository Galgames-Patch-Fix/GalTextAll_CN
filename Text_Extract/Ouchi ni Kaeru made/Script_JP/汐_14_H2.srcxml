		<chgbs async="0" duration="200" priority="0" screen="0" src="usi04b"
		   transparency="0" x="2147483648"/>
		<waitbs @l="31" flags="0" screen="0"/>
		<face src="fusi04b"/>
		<msg @l="32" char="1" flags="17" index="11" line="32" name="汐"
		   text="「うぅ、お水ひっかけちゃった。\n:ボウルを洗うとき気をつけないとこうなっちゃうんだよねぇ……」" timeout=""
