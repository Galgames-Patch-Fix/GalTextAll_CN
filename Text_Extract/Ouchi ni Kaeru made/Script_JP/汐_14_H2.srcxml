		<msg @l="38" char="1" flags="0" index="14" line="38" name="汐"
		   text="「私としたことが……この後のことばっかり考えてて、つい」" timeout="" voices="usi12902"/>
		<face src="f"/>
		<msg @l="40" char="11" flags="0" index="16" line="40" name="亮"
