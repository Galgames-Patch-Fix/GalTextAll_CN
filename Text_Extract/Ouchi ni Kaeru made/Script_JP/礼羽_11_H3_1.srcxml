		<msg @l="793" char="3" flags="0" index="461" line="793" name="礼羽"
		   text="「ダーメッ\x;heart:　私が聞きたいのはそういうセリフじゃ\n:なーいのっ！」" timeout=""
		   voices="rai00578"/>
		<msg @l="795" char="11" flags="17" index="463" line="795" name="亮"
		   text="「うひぃっ！！？　爪立てちゃメッ！　お婿にいけなく\n:なっちゃうぅっ！」" timeout="" voices=""/>
