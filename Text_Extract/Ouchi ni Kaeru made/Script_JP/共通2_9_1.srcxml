		<play_sound @l="1373" buf="0" fadeout="500" intro="-1" src="bgm01" type="0"
		   volume="1."/>
		<face src="fjei01a"/>
		<msg @l="1376" char="9" flags="0" index="609" line="1376" name="ジェイク"
		   text="「その口ぶり、自分でもよくわかってないのか。ふむ……\n:おもしろいな」" timeout="" voices="momd02223"/>
		<face src="f"/>
		<msg @l="1378" char="11" flags="0" index="611" line="1378" name="亮"
		   text="「おもしろい、ですか？」" timeout="" voices=""/>
		<face src="fjei01a"/>
		<msg @l="1381" char="9" flags="0" index="613" line="1381" name="ジェイク"
		   text="「先程も言ったように礼羽ライコネンはスペシャルなんだ。\n:だから、彼女のためにいかなることもキャパし、\n:コミットを許した」"
		   timeout="" voices="momd02224"/>
		<face src="fjei01a"/>
		<msg @l="1384" char="9" flags="0" index="615" line="1384" name="ジェイク"
		   text="「だが彼女はクビを縦には振らなかった。しかし考えてみれば、\n:キミのようなエビデンスで接したことはなかったよ」"
		   timeout="" voices="momd02225"/>
		<face src="fjei01a"/>
		<msg @l="1387" char="9" flags="0" index="617" line="1387" name="ジェイク"
		   text="「この話まではするつもりはなかったが……\n:まあ聞いてくれたまえ」" timeout="" voices="momd02226"/>
		<play_sound @l="1392" buf="0" fadeout="2500" type="0"/>
