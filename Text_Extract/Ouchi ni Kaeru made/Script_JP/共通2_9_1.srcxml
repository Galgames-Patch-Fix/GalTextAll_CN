		<chgbg @l="1418" duration="1000" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="bg42a" type="99"/>
		<face src="fjei01a"/>
		<msg @l="1421" char="9" flags="0" index="631" line="1421" name="ジェイク"
		   text="「社交界に政敵がいるような家だったからな。これみよがしに\n:謀略を張り巡らせ、父娘は直ぐに駆けつけられなかったのだよ」"
		   timeout="" voices="momd02227"/>
		<face src="fjei01a"/>
		<msg @l="1423" char="9" flags="0" index="633" line="1423" name="ジェイク"
		   text="「そうこうしている間に、養生してた母親が\n:消息を絶ってしまった」" timeout="" voices="momd02228"/>
		<face src="f"/>
		<msg @l="1425" char="11" flags="0" index="635" line="1425" name="亮"
		   text="「では、礼羽さんが来日した目的は、消息不明の母親を\n:探すためだったんですね」" timeout="" voices=""/>
		<face src="fjei01a"/>
		<msg @l="1428" char="9" flags="0" index="637" line="1428" name="ジェイク"
		   text="「母親を探すこと自体は私のネットワークを使えば難しいことでは\n:なかった。だが、ここで別のイシューが発生してしまったのだ」"
		   timeout="" voices="momd02229"/>
		<face src=""/>
		<msg @l="1430" char="-1" flags="0" index="639" line="1430" name=""
		   text="ジェイクさんが苦虫を噛み潰したような表情を見せる。" timeout="" voices=""/>
		<face src="fjei01a"/>
		<msg @l="1433" char="9" flags="0" index="641" line="1433" name="ジェイク"
		   text="「発見したとき、すでに再婚して小学生のお子さんまで\n:いたのだよ」" timeout="" voices="momd02230"/>
		<face src="f"/>
		<msg @l="1435" char="11" flags="0" index="643" line="1435" name="亮"
		   text="「それじゃ、いつも見ていた家って――」" timeout="" voices=""/>
		<face src="fjei01a"/>
		<msg @l="1438" char="9" flags="0" index="645" line="1438" name="ジェイク"
		   text="「母親の再婚先だ」" timeout="" voices="momd02231"/>
		<loadimage @l="1440" bg="1" fit="0" screen="1" src="evrai20_01a"/>
		<scrnfilter @l="1441" duration="1" file="sepia.tct" screen="0"/>
		<chgscrn @l="1442" duration="1000" flags="0" flip="-1" flip_flags="0"
		   param="0" screen="1" type="0"/>
		<face src=""/>
		<msg @l="1444" char="-1" flags="0" index="647" line="1444" name=""
		   text="家を見ていた時のなんとも表現しがたい表情の正体が分かり、\n:今までのことが一気に繋がり始める。" timeout=""
		   voices=""/>
		<chgbg @l="1446" duration="1000" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="bg42a" type="99"/>
		<face src="fjei01a"/>
		<msg @l="1449" char="9" flags="0" index="649" line="1449" name="ジェイク"
		   text="「新しい家庭を築き幸せに暮らしている母親の姿を\n:見てしまっては、今さら娘だとはノーティスできない」" timeout=""
		   voices="momd02232"/>
		<face src="fjei01a"/>
		<msg @l="1452" char="9" flags="0" index="651" line="1452" name="ジェイク"
		   text="「今が幸せなのだとしたら、記憶がない過去の自分が娘と\n:名乗り出ることでハレーションしてしまうかもしれない。\n:そう考え苦悩した結果、このまま黙っていようと決めたのだ」"
		   timeout="" voices="momd02233"/>
		<face src="f"/>
		<msg @l="1454" char="11" flags="0" index="653" line="1454" name="亮"
		   text="「……………」" timeout="" voices=""/>
		<face src="fjei01a"/>
		<msg @l="1457" char="9" flags="0" index="655" line="1457" name="ジェイク"
		   text="「再婚自体を父親が咎めることができなかったのは、\n:それだけ差別や政敵からの嫌がらせがあって、\n:そのことで後ろめたさもあったからなのさ」"
		   timeout="" voices="momd02234"/>
		<fade_volume @l="1459" buf="0" time="1000" volume="0.3"/>
