		<face src="f"/>
		<msg @l="1196" char="11" flags="0" index="497" line="1196" name="亮"
		   text="「今からジェイク本庄社長とお会いする約束をしております、\n:宮原ですが」" timeout="" voices=""/>
		<face src="f"/>
		<msg @l="1198" char="10" flags="0" index="499" line="1198" name="受付嬢"
