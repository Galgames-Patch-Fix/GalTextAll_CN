		<play_sound @l="1203" buf="0" fadeout="2500" type="0"/>
		<showmsg @l="1204" duration="500" show="0"/>
		<chgbg @l="1205" duration="1000" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="black" type="201"/>
		<chgbg @l="1206" duration="1000" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="bg42a" type="201"/>
		<play_sound @l="1208" buf="0" fadeout="500" intro="-1" src="bgm02" type="0"
