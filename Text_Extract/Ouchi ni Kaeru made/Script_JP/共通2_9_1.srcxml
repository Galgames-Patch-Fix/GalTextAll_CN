		<face src=""/>
		<msg @l="1366" char="-1" flags="0" index="605" line="1366" name=""
		   text="家を見てたときの今にも泣き出しそうな横顔と、その話題を\n:振った瞬間、表情が消えたこと。" timeout="" voices=""/>
		<face src=""/>
		<msg @l="1367" char="-1" flags="0" index="607" line="1367" name=""
		   text="それらが印象深く心に刺さって、気がつけば行動していた。" timeout="" voices=""/>
		<play_sound @l="1369" buf="0" fadeout="2500" type="0"/>
		<showmsg @l="1370" duration="500" show="0"/>
		<chgbg @l="1371" duration="1500" flags="4608" flip="-1" flip_flags="0"
