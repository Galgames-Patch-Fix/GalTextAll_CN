		<face src=""/>
		<msg @l="1312" char="-1" flags="0" index="571" line="1312" name=""
		   text="店の前で会ったときのことやスーパーでのやり取り、\n:学園での様子など。僕の知ってることは全て。" timeout=""
		   voices=""/>
		<face src=""/>
		<msg @l="1313" char="-1" flags="0" index="573" line="1313" name=""
		   text="そして今日初めて見せた、氷のような冷たい表情に話が及ぶ。" timeout="" voices=""/>
		<play_sound @l="1318" buf="0" fadeout="2500" type="0"/>
