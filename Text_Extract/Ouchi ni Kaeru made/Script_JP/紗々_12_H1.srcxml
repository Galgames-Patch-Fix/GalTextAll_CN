<?xml version="1.0" encoding="utf-8"?>
<xscript>
	<code>
		<label @l="7" id="scene_2_00"/>
		<play_sound @l="11" buf="0" fadeout="2500" type="0"/>
		<showmsg @l="12" duration="500" show="0"/>
		<loadimage @l="16" bg="0" fit="0" screen="1" src="evsas04_01e"/>
		<scroll @l="17" async="0" center_x="45" center_y="45" duration="0" flags="0"
		   rev="0." screen="1" transparency="0" v0="0." v1="0." wait_flags="0" x="0"
		   y="0" zoom="1."/>
		<chgscrn @l="18" duration="2500" flags="4608" flip="-1" flip_flags="0"
		   param="0" screen="1" type="10"/>
		<play_sound @l="19" buf="0" fadeout="500" intro="-1" src="bgm18" type="0"
		   volume="1."/>
		<msg @l="25" char="2" flags="0" index="0" line="25" name="紗々"
		   text="「はい……続けてください」" timeout="" voices="sas17000"/>
		<msg @l="27" char="-1" flags="0" index="2" line="27" name=""
		   text="そう言ってそっと目を閉じる紗々ちゃん。" timeout="" voices=""/>
		<msg @l="29" char="11" flags="0" index="4" line="29" name="亮"
		   text="（続けると意気込んだものの、\n:ここからどうすればいいんだろう？）" timeout="" voices=""/>
		<msg @l="31" char="-1" flags="0" index="6" line="31" name=""
		   text="おっぱいからいくのか？　それとも下？　\n:キスはしたけど、もう一回する？？" timeout="" voices=""/>
		<msg @l="32" char="-1" flags="0" index="8" line="32" name=""
		   text="いざそういう行為をしましょうと言われても、\n:こっちはあいにくバリバリの初心者だ。" timeout="" voices=""/>
		<msg @l="33" char="-1" flags="0" index="10" line="33" name=""
		   text="気持ちばかりが先走り、焦りがつのっていく。" timeout="" voices=""/>
		<msg @l="35" char="11" flags="0" index="12" line="35" name="亮"
		   text="（こっから、どうすればいいんだーーっ！！）" timeout="" voices=""/>
		<chgbg @l="37" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evsas04_01a" type="99"/>
		<msg @l="39" char="2" flags="0" index="14" line="39" name="紗々"
		   text="「あの……センパイ？　どうして黙りこんじゃってるんですか？」" timeout="" voices="sas17001"/>
		<msg @l="41" char="11" flags="0" index="16" line="41" name="亮"
		   text="「あ、や……えっと……なんていうか」" timeout="" voices=""/>
		<msg @l="43" char="2" flags="0" index="18" line="43" name="紗々"
		   text="「もしかして私になにか至らないところがあったとか？　\n:だとしたら申し訳ありません」" timeout=""
		   voices="sas17002"/>
		<msg @l="45" char="11" flags="0" index="20" line="45" name="亮"
		   text="「そういうわけじゃないんだ、うん。こっちの問題というか……」" timeout="" voices=""/>
		<msg @l="47" char="2" flags="0" index="22" line="47" name="紗々"
		   text="「はっ！？　まさか急に萎えてしまったとか？」" timeout="" voices="sas17003"/>
		<msg @l="49" char="11" flags="0" index="24" line="49" name="亮"
		   text="「や、それは大丈夫っていうか、ほんとイ○ポじゃないから！　\n:それは完全に忘れてくれ！！」" timeout=""
		   voices=""/>
		<msg @l="51" char="-1" flags="0" index="26" line="51" name=""
		   text="お互い目的は一緒の筈なんだが、なんともチグハグだ。" timeout="" voices=""/>
		<msg @l="53" char="11" flags="0" index="28" line="53" name="亮"
		   text="「ちなみに紗々ちゃん、こういう経験は――」" timeout="" voices=""/>
		<chgbg @l="55" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evsas04_01b" type="99"/>
		<msg @l="57" char="2" flags="0" index="30" line="57" name="紗々"
		   text="「ないに決まってるじゃないですかぁっ」" timeout="" voices="sas17004"/>
		<msg @l="59" char="11" flags="0" index="32" line="59" name="亮"
		   text="「ですよねー」" timeout="" voices=""/>
		<msg @l="61" char="-1" flags="0" index="34" line="61" name=""
		   text="ああもうっ、テンパってるとはいえ、\n:なにを言い出してるんだ僕はっ。" timeout="" voices=""/>
		<msg @l="62" char="-1" flags="0" index="36" line="62" name=""
		   text="もうこうなったら年上とかセンパイとか関係なく、\n:素直に話すしかない。" timeout="" voices=""/>
		<msg @l="64" char="11" flags="0" index="38" line="64" name="亮"
		   text="「僕もこういうことをするのが初めてだから……\n:どうしていいかよくわからなくてさ」" timeout="" voices=""/>
		<msg @l="66" char="11" flags="0" index="40" line="66" name="亮"
		   text="「格好悪いんだけど、紗々ちゃんにも協力して貰えれば嬉しい」" timeout="" voices=""/>
		<chgbg @l="68" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evsas04_01e" type="99"/>
		<msg @l="70" char="2" flags="0" index="42" line="70" name="紗々"
		   text="「はぁぁ、センパイ……好き！　大好きですっ！！」" timeout="" voices="sas17005"/>
		<msg @l="72" char="-1" flags="0" index="44" line="72" name=""
		   text="紗々ちゃんが唇を尖らせながら頭を持ち上げようともがいている。" timeout="" voices=""/>
		<msg @l="73" char="-1" flags="0" index="46" line="73" name=""
		   text="キスしろってことか？　あってるよな？？" timeout="" voices=""/>
		<msg @l="74" char="-1" flags="0" index="48" line="74" name=""
		   text="しかし、今のなにがツボに入ったのか、さっぱりわからん。" timeout="" voices=""/>
		<msg @l="76" char="2" flags="0" index="50" line="76" name="紗々"
		   text="「はんっ……ちゅむ、ちゅっ、ちゅむむ……」" timeout="" voices="sas17006"/>
		<chgbg @l="78" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evsas04_01a" type="99"/>
		<msg @l="80" char="2" flags="0" index="52" line="80" name="紗々"
		   text="「んぅ……ふっ……えへへ、またキスしちゃいましたね。\n:こうやって大好き同士ですることをすればきっと最後まで\n:なんとかなりますよ」"
		   timeout="" voices="sas17007"/>
		<msg @l="82" char="2" flags="0" index="54" line="82" name="紗々"
		   text="「センパイの初めてを貰えるなんて光栄で嬉しくて、\n:涙が出てきちゃいそうです」" timeout=""
		   voices="sas17008"/>
		<msg @l="84" char="11" flags="0" index="56" line="84" name="亮"
		   text="「それ、普通男のセリフだよな！？」" timeout="" voices=""/>
		<chgbg @l="86" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evsas04_01d" type="99"/>
		<msg @l="88" char="2" flags="0" index="58" line="88" name="紗々"
		   text="「くすくすっ。そうかも知れませんね、でも今のは本心なので」" timeout="" voices="sas17009"/>
		<msg @l="90" char="-1" flags="0" index="60" line="90" name=""
		   text="紗々ちゃんが僕を見ながら柔らかく微笑む。" timeout="" voices=""/>
		<msg @l="91" char="-1" flags="0" index="62" line="91" name=""
		   text="なるほど、それがツボだったのか。" timeout="" voices=""/>
		<msg @l="93" char="11" flags="0" index="64" line="93" name="亮"
		   text="（でも、そうだよな……）" timeout="" voices=""/>
		<msg @l="95" char="-1" flags="0" index="66" line="95" name=""
		   text="どっちがリードするとかそういうのは関係ない。\n:そんなちっぽけなプライドより大切なのは\n:相手になにをしてあげたいかなんだ。"
		   timeout="" voices=""/>
		<msg @l="96" char="-1" flags="0" index="68" line="96" name=""
		   text="この気持ちをそのまま態度にすればいい。" timeout="" voices=""/>
		<msg @l="98" char="11" flags="0" index="70" line="98" name="亮"
		   text="「大好きだよ、紗々ちゃん」" timeout="" voices=""/>
		<msg @l="100" char="2" flags="0" index="72" line="100" name="紗々"
		   text="「はい。私も、センパイが大好きです\x;heart:」" timeout="" voices="sas17010"/>
		<msg @l="102" char="-1" flags="0" index="74" line="102" name=""
		   text="唇をくっつけあう――ただそれだけの事なのに、\n:顔は火照るし心臓はバクバクするしで大騒ぎだ。" timeout=""
		   voices=""/>
		<msg @l="103" char="-1" flags="0" index="76" line="103" name=""
		   text="でも、いまはそんな鼓動の上昇さえ心地良い。" timeout="" voices=""/>
		<chgbg @l="105" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evsas04_01a" type="99"/>
		<msg @l="107" char="2" flags="0" index="78" line="107" name="紗々"
		   text="「センパイ、私もっとキスしたいです」" timeout="" voices="sas17011"/>
		<msg @l="109" char="11" flags="0" index="80" line="109" name="亮"
		   text="「うん、僕もだ」" timeout="" voices=""/>
		<chgbg @l="111" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evsas04_01e" type="99"/>
		<msg @l="113" char="2" flags="0" index="82" line="113" name="紗々"
		   text="「ふぁむ、ちゅるるっ、ちゅっ……はぁはぁ、ふむぅ、むむっ……ちゅっ、れろろろっ」" timeout=""
		   voices="sas17012"/>
		<play_sound @l="115" buf="5" fadeout="0" intro="0" src="sas80028" type="6"
		   volume="1."/>
		<msg @l="116" char="-1" flags="0" index="84" line="116" name=""
		   text="今度は少し強く重ねると、舌が探るように絡み合った。" timeout="" voices=""/>
		<play_sound @l="117" buf="5" fadeout="0" type="1"/>
		<msg @l="119" char="2" flags="0" index="86" line="119" name="紗々"
		   text="「はふぅ、はふぅ……あぁ、センパイの唾液が……\n:れろ、ちゅむるるっ……はぅ、んふぅ」" timeout=""
		   voices="sas17013"/>
		<play_sound @l="121" buf="5" fadeout="0" intro="0" src="sas80029" type="6"
		   volume="1."/>
		<msg @l="122" char="-1" flags="0" index="88" line="122" name=""
		   text="重力に逆らえない唾液が、\n:舌を伝って彼女の愛らしい口の中へと垂れていく。" timeout="" voices=""/>
		<msg @l="123" char="-1" flags="0" index="90" line="123" name=""
		   text="紗々ちゃんはそれを嫌がらず迎え入れ、飲み込んだ。" timeout="" voices=""/>
		<play_sound @l="124" buf="5" fadeout="0" type="1"/>
		<chgbg @l="126" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evsas04_01b" type="99"/>
		<msg @l="128" char="2" flags="0" index="92" line="128" name="紗々"
		   text="「んくっ。こくっ、はぁ、はぁ……キスだけで、\n:身体がサウナに入ったみたく熱くなってきちゃいましたっ……」" timeout=""
		   voices="sas17014"/>
		<play_sound @l="130" buf="5" fadeout="0" intro="0" src="sas80015" type="6"
		   volume="1."/>
		<msg @l="131" char="11" flags="0" index="94" line="131" name="亮"
		   text="「うん。ふぅ、はぁ……キスってすごいな」" timeout="" voices=""/>
		<play_sound @l="132" buf="5" fadeout="0" type="1"/>
		<msg @l="134" char="2" flags="0" index="96" line="134" name="紗々"
		   text="「はい。キスだけでこんなに気持ち良くなっちゃうなんて……\n:この先、どうなっちゃうんでしょう……？」" timeout=""
		   voices="sas17015"/>
		<play_sound @l="136" buf="5" fadeout="0" intro="0" src="sas80016" type="6"
		   volume="1."/>
		<msg @l="137" char="11" flags="0" index="98" line="137" name="亮"
		   text="「本当、どうなっちゃうんだろ……はぁはぁ」" timeout="" voices=""/>
		<msg @l="139" char="-1" flags="0" index="100" line="139" name=""
		   text="まったく予想がつかないだけに、少し怖いけど……\n:でも、その不安を軽々凌駕するぐらい楽しみでもある。" timeout=""
		   voices=""/>
		<play_sound @l="140" buf="5" fadeout="0" type="1"/>
		<chgbg @l="142" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evsas04_01d" type="99"/>
		<msg @l="144" char="2" flags="0" index="102" line="144" name="紗々"
		   text="「えへへ、電気を消さないでよかったです\x;heart:　\n:だってセンパイのお顔がハッキリ見えますから……」"
		   timeout="" voices="sas17016"/>
		<play_sound @l="146" buf="5" fadeout="0" intro="0" src="sas80017" type="6"
		   volume="1."/>
		<msg @l="147" char="-1" flags="0" index="104" line="147" name=""
		   text="顔と言ってるのに視線は僕の股間の方に何度も移動しては\n:戻ってくる。" timeout="" voices=""/>
		<msg @l="149" char="11" flags="0" index="106" line="149" name="亮"
		   text="「見たいのは顔だけじゃないくせに」" timeout="" voices=""/>
		<play_sound @l="150" buf="5" fadeout="0" type="1"/>
		<chgbg @l="152" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evsas04_01a" type="99"/>
		<msg @l="154" char="2" flags="0" index="108" line="154" name="紗々"
		   text="「そ、それはそうかもしれないですけど……\n:もう、センパイのいじわるぅ……」" timeout=""
		   voices="sas17017"/>
		<play_sound @l="156" buf="5" fadeout="0" intro="0" src="sas80015" type="6"
		   volume="1."/>
		<msg @l="157" char="-1" flags="0" index="110" line="157" name=""
		   text="拗ねた紗々ちゃんの声はもちろん、その表情も愛らしくて、\n:いじめるのもアリかも？　なんて思ってしまう。" timeout=""
		   voices=""/>
		<msg @l="159" char="11" flags="0" index="112" line="159" name="亮"
		   text="「じゃあ、電気もついててくっきりハッキリ見える中で、\n:じっくりと鑑賞しよっかな」" timeout="" voices=""/>
		<play_sound @l="160" buf="5" fadeout="0" type="1"/>
		<msg @l="162" char="2" flags="0" index="114" line="162" name="紗々"
		   text="「せ、センパイってば、なんだか言い方がエッチっぽいですよぉ」" timeout="" voices="sas17018"/>
		<play_sound @l="164" buf="5" fadeout="0" intro="0" src="sas80016" type="6"
		   volume="1."/>
		<msg @l="165" char="11" flags="0" index="116" line="165" name="亮"
		   text="「でもほら、こんなに……ごくんっ」" timeout="" voices=""/>
		<msg @l="167" char="-1" flags="0" index="118" line="167" name=""
		   text="改めて見下ろすと、組み伏せた紗々ちゃんの身体に思わず生ツバを飲んでしまう。" timeout="" voices=""/>
		<msg @l="169" char="11" flags="0" index="120" line="169" name="亮"
		   text="「おへそって、こんなにエッチだったっけ……？」" timeout="" voices=""/>
		<play_sound @l="170" buf="5" fadeout="0" type="1"/>
		<chgbg @l="172" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evsas04_01b" type="99"/>
		<msg @l="174" char="2" flags="0" index="122" line="174" name="紗々"
		   text="「あの、そんなに見つめられると、\n:なんだかむずむずしますっ……」" timeout="" voices="sas17019"/>
		<play_sound @l="176" buf="5" fadeout="0" intro="0" src="sas80017" type="6"
		   volume="1."/>
		<msg @l="177" char="11" flags="0" index="124" line="177" name="亮"
		   text="「お腹も……ほら、さわるとすべすべだし」" timeout="" voices=""/>
		<play_sound @l="178" buf="5" fadeout="0" type="1"/>
		<chgbg @l="180" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evsas04_01e" type="99"/>
		<msg @l="182" char="2" flags="0" index="126" line="182" name="紗々"
		   text="「はっ、はあっ……んぅ、んんっ」" timeout="" voices="sas17020"/>
		<play_sound @l="184" buf="5" fadeout="0" intro="0" src="sas80015" type="6"
		   volume="1."/>
		<msg @l="185" char="-1" flags="0" index="128" line="185" name=""
		   text="お腹をそっと撫でると、紗々ちゃんが身じろぐ。" timeout="" voices=""/>
		<msg @l="186" char="-1" flags="0" index="130" line="186" name=""
		   text="もじもじと足を動かすから、スカートが少しめくれて、\n:可愛い太ももがよりあらわになった。" timeout="" voices=""/>
		<msg @l="188" char="11" flags="0" index="132" line="188" name="亮"
		   text="「あぁ……太ももも、すっごくエッチな感じだ」" timeout="" voices=""/>
		<play_sound @l="189" buf="5" fadeout="0" type="1"/>
		<chgbg @l="191" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evsas04_01b" type="99"/>
		<msg @l="193" char="2" flags="0" index="134" line="193" name="紗々"
		   text="「あっ、やんっ。スカートがまくれて、それ以上はっ……はぁ、ふぅんんっ\x;hearts:」" timeout=""
		   voices="sas17021"/>
		<play_sound @l="195" buf="5" fadeout="0" intro="0" src="sas80016" type="6"
		   volume="1."/>
		<msg @l="196" char="-1" flags="0" index="136" line="196" name=""
		   text="スカートを戻そうとさらに足を動かす紗々ちゃんだけど、\n:めくれた布がそう簡単に戻るはずもない。" timeout=""
		   voices=""/>
		<msg @l="198" char="11" flags="0" index="138" line="198" name="亮"
		   text="「かえってエロくなっちゃったかもな」" timeout="" voices=""/>
		<play_sound @l="199" buf="5" fadeout="0" type="1"/>
		<chgbg @l="201" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evsas04_01a" type="99"/>
		<msg @l="204" char="2" flags="0" index="140" line="204" name="紗々"
		   text="「私、エロいですかっ……？」" timeout="" voices="sas17022"/>
		<play_sound @l="206" buf="5" fadeout="0" intro="0" src="sas80017" type="6"
		   volume="1."/>
		<msg @l="207" char="11" flags="0" index="142" line="207" name="亮"
		   text="「ああ。チラリズムってこんなに興奮するんだ……」" timeout="" voices=""/>
		<msg @l="209" char="-1" flags="0" index="144" line="209" name=""
		   text="見えそうで見えないからこそ、あれこれ脳内で補完することに\n:よって生み出される興奮。" timeout="" voices=""/>
		<msg @l="210" char="-1" flags="0" index="146" line="210" name=""
		   text="それはときに、丸見えすら上回る破壊力をもつと思う。" timeout="" voices=""/>
		<msg @l="212" char="11" flags="0" index="148" line="212" name="亮"
		   text="「少し着崩した感じもエッチだし……それに、おっぱいだって、\n:こんなに……」" timeout="" voices=""/>
		<msg @l="214" char="-1" flags="0" index="150" line="214" name=""
		   text="形を保つように支える衣服と、だぼっと余った袖の裾。" timeout="" voices=""/>
		<msg @l="215" char="-1" flags="0" index="152" line="215" name=""
		   text="着衣状態ならではの絶妙なバランスに、カーッと熱いなにかが\n:食道を通ってこみ上げてくる。" timeout="" voices=""/>
		<play_sound @l="216" buf="5" fadeout="0" type="1"/>
		<chgbg @l="218" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evsas04_01b" type="99"/>
		<msg @l="220" char="2" flags="0" index="154" line="220" name="紗々"
		   text="「センパイ、だんだん息が荒くなって……はぁはぁって、息が肌にかかってくすぐったいですっ……」" timeout=""
		   voices="sas17023"/>
		<msg @l="222" char="2" flags="0" index="156" line="222" name="紗々"
		   text="「いっそ、もっと息が感じられるぐらい近くにっ……\n:顔、もっとこっちに……あ、んんっ」" timeout=""
		   voices="sas17024"/>
		<play_sound @l="224" buf="5" fadeout="0" intro="0" src="sas80015" type="6"
		   volume="1."/>
		<msg @l="225" char="11" flags="0" index="158" line="225" name="亮"
		   text="「あぁ、紗々ちゃんっ……」" timeout="" voices=""/>
		<play_sound @l="226" buf="5" fadeout="0" type="1"/>
