		<play_sound @l="507" buf="5" fadeout="0" intro="0" src="sas80017" type="6"
		   volume="1."/>
		<msg @l="508" char="-1" flags="0" index="346" line="508" name=""
		   text="ブラを上にずらすと、ワイヤーによって持ち上げられた膨らみが、ぷるんと揺れながら現れる。" timeout="" voices=""/>
		<msg @l="509" char="-1" flags="0" index="348" line="509" name=""
		   text="ちょこんと屹立したピンク色の乳首があまりにも可愛くて、\n:赤ちゃん用のおしゃぶりを連想してしまった。" timeout=""
		   voices=""/>
		<msg @l="511" char="11" flags="0" index="350" line="511" name="亮"
		   text="「これが、紗々ちゃんのおっぱい……」" timeout="" voices=""/>
		<play_sound @l="512" buf="5" fadeout="0" type="1"/>
		<chgbg @l="514" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evsas04_04e" type="99"/>
		<msg @l="516" char="2" flags="0" index="352" line="516" name="紗々"
		   text="「はいっ。私のおっぱいですっ……はぁぁ、恥ずかしい……っ」" timeout="" voices="sas17057"/>
		<play_sound @l="518" buf="5" fadeout="0" intro="0" src="sas80015" type="6"
		   volume="1."/>
		<msg @l="519" char="-1" flags="0" index="354" line="519" name=""
		   text="目をキュッと閉じながらまつげをフルフルと震わせるが、\n:でも決して身体を隠そうとはしない。" timeout=""
		   voices=""/>
		<msg @l="520" char="-1" flags="0" index="356" line="520" name=""
		   text="はぁはぁと火照った吐息が漏れるたび、胸がわずかに上下する。" timeout="" voices=""/>
		<msg @l="521" char="-1" flags="0" index="358" line="521" name=""
		   text="膨らみや曲線、肌自体の白さはもちろんだけど、やっぱり目を\n:引くのは、なんといっても綺麗なピンク色の乳首だった。"
		   timeout="" voices=""/>
		<msg @l="523" char="11" flags="0" index="360" line="523" name="亮"
		   text="「女の子の乳首って、こんなふうにずっと立ってるものなのか？」" timeout="" voices=""/>
		<play_sound @l="524" buf="5" fadeout="0" type="1"/>
		<chgbg @l="526" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evsas04_04b" type="99"/>
		<msg @l="528" char="2" flags="0" index="362" line="528" name="紗々"
		   text="「ちがいますっ。これは寒かったりドキドキすると、\n:こんなふうにっ……」" timeout="" voices="sas17058"/>
		<msg @l="530" char="2" flags="0" index="364" line="530" name="紗々"
		   text="「いまはすごくドキドキしてるので、それで勝手にっ……\n:はぅ、はぁっ……」" timeout=""
