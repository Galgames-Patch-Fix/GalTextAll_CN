		<msg @l="536" char="2" flags="0" index="368" line="536" name="紗々"
		   text="「でも、オーナーや礼羽さんに比べると、ちょっと\n:ちっちゃいかなって……」" timeout=""
		   voices="sas17060"/>
		<play_sound @l="538" buf="5" fadeout="0" intro="0" src="sas80017" type="6"
		   volume="1."/>
		<msg @l="539" char="11" flags="0" index="370" line="539" name="亮"
		   text="「あの２人の乳首ってもっとおっきいの！！？」" timeout="" voices=""/>
		<play_sound @l="540" buf="5" fadeout="0" type="1"/>
		<chgbg @l="542" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evsas04_04c" type="99"/>
		<msg @l="544" char="2" flags="0" index="372" line="544" name="紗々"
		   text="「ちちちちがいますっ！　おっきいのはおっぱいそのもので、\n:乳首は……たぶん、同じぐらいじゃないかと」" timeout=""
		   voices="sas17061"/>
		<play_sound @l="546" buf="5" fadeout="0" intro="0" src="sas80015" type="6"
		   volume="1."/>
		<msg @l="547" char="11" flags="0" index="374" line="547" name="亮"
		   text="「そうなんだ。ってことは――」" timeout="" voices=""/>
		<play_sound @l="548" buf="5" fadeout="0" type="1"/>
		<msg @l="551" char="2" flags="0" index="376" line="551" name="紗々"
		   text="「ああっ？！　ダメっ、想像しちゃダメですっ！　\n:いま他の女の人のこと考えないでくださいっ！」" timeout=""
		   voices="sas17062"/>
		<chgbg @l="553" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evsas04_04b" type="99"/>
		<msg @l="556" char="2" flags="0" index="378" line="556" name="紗々"
		   text="「いまは私のことだけ見て、さわって、考えてくれないと……\n:いや、です……」" timeout=""
		   voices="sas17063"/>
		<play_sound @l="558" buf="5" fadeout="0" intro="0" src="sas80016" type="6"
		   volume="1."/>
		<msg @l="559" char="11" flags="0" index="380" line="559" name="亮"
		   text="「あっ……」" timeout="" voices=""/>
		<msg @l="561" char="-1" flags="0" index="382" line="561" name=""
		   text="確かに一瞬想像しそうになったけど、紗々ちゃんの声が早かった\n:こともあり、それは未然に防がれた。" timeout=""
		   voices=""/>
		<msg @l="562" char="-1" flags="0" index="384" line="562" name=""
		   text="そりゃ他の子のおっぱいが気にならないといったら\n:ウソになるけど、それでも僕は……。" timeout="" voices=""/>
		<msg @l="564" char="11" flags="0" index="386" line="564" name="亮"
		   text="「僕が一番好きなのは紗々ちゃんのおっぱいだよ、\n:というか紗々ちゃんのしかもう見えない」" timeout="" voices=""/>
		<play_sound @l="565" buf="5" fadeout="0" type="1"/>
		<chgbg @l="567" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evsas04_04a" type="99"/>
		<msg @l="569" char="2" flags="0" index="388" line="569" name="紗々"
		   text="「ほんとですか？　いま、初めて見たのに？」" timeout="" voices="sas17064"/>
		<play_sound @l="571" buf="5" fadeout="0" intro="0" src="sas80017" type="6"
		   volume="1."/>
		<msg @l="572" char="11" flags="0" index="390" line="572" name="亮"
		   text="「一目惚れしたんだ」" timeout="" voices=""/>
		<play_sound @l="573" buf="5" fadeout="0" type="1"/>
		<chgbg @l="575" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evsas04_04b" type="99"/>
		<msg @l="577" char="2" flags="0" index="392" line="577" name="紗々"
		   text="「じゃあ、もっとさわったりとか、あと、その……\n:なめたりとかも」" timeout="" voices="sas17065"/>
		<msg @l="579" char="2" flags="0" index="394" line="579" name="紗々"
		   text="「全部、センパイのお好きなようにしてください……\n:このおっぱいはセンパイのものですから」" timeout=""
		   voices="sas17066"/>
		<play_sound @l="581" buf="5" fadeout="0" intro="0" src="sas80015" type="6"
		   volume="1."/>
		<msg @l="582" char="11" flags="0" index="396" line="582" name="亮"
		   text="「ごくっ……」" timeout="" voices=""/>
		<msg @l="584" char="-1" flags="0" index="398" line="584" name=""
		   text="舐める、という言葉に心臓がドキっと跳ねる。" timeout="" voices=""/>
		<msg @l="585" char="-1" flags="0" index="400" line="585" name=""
		   text="それは触れたり揉んだりってことよりも、ぐっとハードルが\n:あがった行為だ。" timeout="" voices=""/>
		<msg @l="587" char="11" flags="0" index="402" line="587" name="亮"
		   text="「それじゃあ、お言葉に甘えて……れろ、ちゅっ」" timeout="" voices=""/>
		<play_sound @l="588" buf="5" fadeout="0" type="1"/>
