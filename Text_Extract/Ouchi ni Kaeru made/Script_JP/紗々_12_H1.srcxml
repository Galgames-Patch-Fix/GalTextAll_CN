		<msg @l="446" char="-1" flags="0" index="304" line="446" name=""
		   text="女の子のおっぱいを守る為に、下着にこんなに堅いものが入ってるなんて知らなかった。" timeout="" voices=""/>
		<play_sound @l="447" buf="5" fadeout="0" type="1"/>
		<msg @l="449" char="2" flags="0" index="306" line="449" name="紗々"
		   text="「苦手な子も多いみたいですけど……でも、センパイには\n:少しでも良いおっぱいを提供したいなって……」" timeout=""
		   voices="sas17050"/>
		<chgbg @l="451" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evsas04_03b" type="99"/>
		<msg @l="453" char="2" flags="0" index="308" line="453" name="紗々"
		   text="「って、ああうっ！　私ってばなにヘンなこといってるんで\n:しょうかっ、ごめんなさいごめんなさいっ」" timeout=""
		   voices="sas17051"/>
		<play_sound @l="455" buf="5" fadeout="0" intro="0" src="sas80015" type="6"
		   volume="1."/>
		<msg @l="456" char="11" flags="0" index="310" line="456" name="亮"
		   text="「や、その気持ちがすっごく嬉しいよ」" timeout="" voices=""/>
		<msg @l="458" char="-1" flags="0" index="312" line="458" name=""
		   text="ワイヤーあるなしによる付け心地の変化っていうのは僕には\n:わからないけど、僕のため、という気持ちがなにより嬉しい。"
		   timeout="" voices=""/>
		<play_sound @l="459" buf="5" fadeout="0" type="1"/>
		<msg @l="461" char="2" flags="0" index="314" line="461" name="紗々"
		   text="「ですので、よかったらブラをとって、生でおっぱいを見て\n:いただいても、そのっ……」" timeout=""
		   voices="sas17052"/>
		<play_sound @l="463" buf="5" fadeout="0" intro="0" src="sas80016" type="6"
		   volume="1."/>
		<msg @l="464" char="11" flags="0" index="316" line="464" name="亮"
		   text="「あっ、お、おうっ……」" timeout="" voices=""/>
		<msg @l="466" char="-1" flags="0" index="318" line="466" name=""
		   text="生おっぱいという単語の強力さに脳がくらっとなる。" timeout="" voices=""/>
		<msg @l="467" char="-1" flags="0" index="320" line="467" name=""
		   text="けど、見せて頂けるというなら、ぜひ見たいしもちろん揉みたい。" timeout="" voices=""/>
		<msg @l="469" char="11" flags="0" index="322" line="469" name="亮"
		   text="「じゃあ、えと……ブラを……ん？　あれ、ええと」" timeout="" voices=""/>
		<msg @l="471" char="-1" flags="0" index="324" line="471" name=""
		   text="ブラジャーって確か、留め金？　が後ろについてるんだよな？　\n:ならこの場合はどうやればいいんだ？" timeout=""
		   voices=""/>
		<msg @l="472" char="-1" flags="0" index="326" line="472" name=""
		   text="背中に手を回す？　それとも別の外し方が？" timeout="" voices=""/>
		<play_sound @l="473" buf="5" fadeout="0" type="1"/>
		<chgbg @l="475" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evsas04_03a" type="99"/>
		<msg @l="477" char="2" flags="0" index="328" line="477" name="紗々"
		   text="「ブラもシャツと同じで、上にズラしてもらって大丈夫ですよ？」" timeout="" voices="sas17053"/>
		<play_sound @l="479" buf="5" fadeout="0" intro="0" src="sas80017" type="6"
		   volume="1."/>
		<msg @l="480" char="11" flags="0" index="330" line="480" name="亮"
		   text="「えっ？　それでいいのか？」" timeout="" voices=""/>
		<play_sound @l="481" buf="5" fadeout="0" type="1"/>
		<chgbg @l="483" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evsas04_03b" type="99"/>
		<msg @l="485" char="2" flags="0" index="332" line="485" name="紗々"
		   text="「はい。せっかく選んだ下着をすぐに外しちゃうのも、\n:もったいないかなぁ、なんて……」" timeout=""
		   voices="sas17054"/>
		<play_sound @l="487" buf="5" fadeout="0" intro="0" src="sas80015" type="6"
		   volume="1."/>
		<msg @l="488" char="-1" flags="0" index="334" line="488" name=""
		   text="照れなのか気遣いなのか……おそらくその両方だろうけど、\n:紗々ちゃんがもじもじと視線を泳がせる。" timeout=""
		   voices=""/>
		<msg @l="489" char="-1" flags="0" index="336" line="489" name=""
		   text="その表情にグッとくる。普段は控え目で大人しいのに、\n:エッチの時は積極的になってくれるなんて最高だ。" timeout=""
		   voices=""/>
		<msg @l="491" char="11" flags="0" index="338" line="491" name="亮"
		   text="「では、失礼して」" timeout="" voices=""/>
		<play_sound @l="492" buf="5" fadeout="0" type="1"/>
		<msg @l="494" char="2" flags="0" index="340" line="494" name="紗々"
