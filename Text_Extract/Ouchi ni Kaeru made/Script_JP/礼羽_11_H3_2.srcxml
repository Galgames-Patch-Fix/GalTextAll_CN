<?xml version="1.0" encoding="utf-8"?>
<xscript>
	<code>
		<play_sound @l="1451" buf="5" fadeout="0" intro="0" src="rai80017" type="6"
		   volume="1."/>
		<msg @l="1452" char="-1" flags="0" index="827" line="1452" name=""
		   text="お尻をペニスが抜けるギリギリまで腰を上げると、膣口から\n:カリ首のあたりが姿を現した。" timeout="" voices=""/>
		<play_sound @l="1454" buf="1" fadeout="0" type="1"/>
		<msg @l="1456" char="11" flags="0" index="829" line="1456" name="亮"
		   text="「お、おいっ！　いきなりそんな動いたら――」" timeout="" voices=""/>
		<play_sound @l="1457" buf="5" fadeout="0" type="1"/>
		<chgbg @l="1459" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evrai09_04e" type="99"/>
		<msg @l="1461" char="3" flags="0" index="831" line="1461" name="礼羽"
		   text="「はぁ、はぁ……大丈夫だよ、ちゃんと気持ち良いから……っ、ふぁっ、あんっ、私が感じてるのを、貴方も感じてっ」" timeout=""
		   voices="rai00930"/>
		<play_sound @l="1464" buf="5" fadeout="0" intro="0" src="rai80016" type="6"
		   volume="1."/>
		<msg @l="1465" char="-1" flags="0" index="833" line="1465" name=""
		   text="宣言すると同時に、礼羽は全身を大きく使って腰を沈める。" timeout="" voices=""/>
		<play_sound @l="1466" buf="5" fadeout="0" type="1"/>
		<play_sound @l="1468" buf="1" fadeout="0" intro="-1" src="hse108" type="1"
		   volume="1."/>
		<chgbg @l="1469" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evrai09_04d" type="99"/>
		<msg @l="1471" char="3" flags="0" index="835" line="1471" name="礼羽"
		   text="「んんああああっ！！　あっ、ああぁっ！　またズブズブって\n:おま○こかきわけてきて……っ、ふああぁ……っ！！」"
		   timeout="" voices="rai00931"/>
		<play_sound @l="1473" buf="1" fadeout="0" type="1"/>
		<play_sound @l="1475" buf="5" fadeout="0" intro="0" src="rai80019" type="6"
		   volume="1."/>
		<msg @l="1476" char="-1" flags="0" index="837" line="1476" name=""
		   text="踏ん張りをきかす為にがに股になっているのだろうが、大きく脚を開いているせいで結合部が丸見えだ。" timeout=""
		   voices=""/>
		<msg @l="1477" char="-1" flags="0" index="839" line="1477" name=""
		   text="ペニスを伝わり垂れてくる愛液の粘りけが、さらに増している。" timeout="" voices=""/>
		<play_sound @l="1478" buf="5" fadeout="0" type="1"/>
		<chgbg @l="1480" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evrai09_04c" type="99"/>
		<msg @l="1481" char="3" flags="0" index="841" line="1481" name="礼羽"
		   text="「イキたくなったら、いつでもイッていいからね……っ、はぁ、はぁ。貴方の好きにしていいから……っ」" timeout=""
		   voices="rai00932"/>
		<play_sound @l="1483" buf="5" fadeout="0" intro="0" src="rai80020" type="6"
		   volume="1."/>
		<msg @l="1484" char="11" flags="0" index="843" line="1484" name="亮"
		   text="「や、僕もお前を気持ちよくした――うっ、あっ、くうぅ……！」" timeout="" voices=""/>
		<play_sound @l="1486" buf="1" fadeout="0" intro="0" src="hse105" type="1"
		   volume="1."/>
		<msg @l="1488" char="-1" flags="0" index="845" line="1488" name=""
		   text="僕の言葉を遮るように、礼羽がパンパンとお尻の肉を僕の下腹部に打ち付けてくる。" timeout="" voices=""/>
		<play_sound @l="1489" buf="5" fadeout="0" type="1"/>
		<chgbg @l="1491" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evrai09_04d" type="99"/>
		<msg @l="1493" char="3" flags="0" index="847" line="1493" name="礼羽"
		   text="「ふあっ、ああぁん……！　いい、ち○ぽ、すっごくいいよぉ\x;heart:　んんっ！　動く度にカリがおま○この中、ひっかいて……\n:んはあぁっ！？」"
		   timeout="" voices="rai00933"/>
		<msg @l="1495" char="3" flags="0" index="849" line="1495" name="礼羽"
		   text="「それに、あっ、んあ……っ、根元から先っぽまで、あっ、\n:こんなに、ち○ぽが長いって実感出来てっ、はああぁぁ……\n:んくぅっ！！」"
		   timeout="" voices="rai00934"/>
		<msg @l="1497" char="3" flags="0" index="851" line="1497" name="礼羽"
		   text="「はっ、はぁっ、この体勢だと、んぅっ！　自分で自由に\n:動けるから……っ、ふあっ、あぅんっ、この角度……\n:きもちいい……っ\x;heart:\x;hearts:」"
		   timeout="" voices="rai00935"/>
		<play_sound @l="1499" buf="5" fadeout="0" intro="0" src="rai80019" type="6"
		   volume="1."/>
		<msg @l="1500" char="-1" flags="0" index="853" line="1500" name=""
		   text="快感を貪るように腰を上下に振って、自分の気持ちいいところに\n:ペニスをこすりつける礼羽。" timeout="" voices=""/>
		<msg @l="1501" char="-1" flags="0" index="855" line="1501" name=""
		   text="膣内は初めての時のようにただキツイだけでなく、粘膜の絡んだ\n:ヒダヒダが大きく動き、ギュッギュっと細かく締め付けてくる。"
		   timeout="" voices=""/>
		<msg @l="1502" char="-1" flags="0" index="857" line="1502" name=""
		   text="彼女が動く度により礼羽の匂いを強くはらんだ愛液が次々\n:溢れ出て、脳髄をシビレされる。" timeout="" voices=""/>
		<play_sound @l="1503" buf="5" fadeout="0" type="1"/>
		<chgbg @l="1505" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evrai09_04b" type="99"/>
		<msg @l="1507" char="3" flags="0" index="859" line="1507" name="礼羽"
		   text="「あ、っは……だめっ、んっ、私ばっかり、ふぁ、はぁ……っ、\n:気持ちよくなっちゃうぅっ\x;heart:」" timeout=""
		   voices="rai00936"/>
		<msg @l="1509" char="3" flags="0" index="861" line="1509" name="礼羽"
		   text="「んんっ、あっ、あんっ！　貴方は、どう……？\n:はぁ、はぁ……！　私のおま○こ、気持ちいい……？」" timeout=""
		   voices="rai00937"/>
		<play_sound @l="1511" buf="5" fadeout="0" intro="0" src="rai80020" type="6"
		   volume="1."/>
		<msg @l="1512" char="11" flags="0" index="863" line="1512" name="亮"
		   text="「素股もいいけど、はぁはぁ……これも気持ちいい！　最高だよ、礼羽のおま○こは……っ！」" timeout="" voices=""/>
		<play_sound @l="1513" buf="5" fadeout="0" type="1"/>
		<chgbg @l="1515" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evrai09_04e" type="99"/>
		<msg @l="1517" char="3" flags="0" index="865" line="1517" name="礼羽"
		   text="「良かったぁ、気に入ってもらえて……！　んっ、くうぅ……！　一緒に、いっぱい気持ち良くなろうね……ふあっ、ああぁ……！」"
		   timeout="" voices="rai00938"/>
		<chgbg @l="1519" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evrai09_04c" type="99"/>
		<msg @l="1521" char="3" flags="0" index="867" line="1521" name="礼羽"
		   text="「はっ、はっ、はぁっ！　貴方の上で腰を振って……っ、ふっ、\n:ふうぅんっ、まるでダンスしてるみたい……っ、恥ずかしいけど、\n:私のえっちな腰ふり、見てぇ……っ\x;heart:」"
		   timeout="" voices="rai00939"/>
		<msg @l="1523" char="3" flags="0" index="869" line="1523" name="礼羽"
		   text="「私も……あ、貴方が……気持ちよさそうにしてるの……んっ、\n:見たいからぁ、ンんッ！」" timeout=""
		   voices="rai00940"/>
		<play_sound @l="1525" buf="5" fadeout="0" intro="0" src="rai80018" type="6"
		   volume="1."/>
		<msg @l="1526" char="-1" flags="0" index="871" line="1526" name=""
		   text="より気持ち良い場所を探すように、無我夢中になって大振りに腰をふり続ける礼羽。" timeout="" voices=""/>
		<msg @l="1527" char="-1" flags="0" index="873" line="1527" name=""
		   text="乱れるツインテールが、汗と先ほどぶっかけた精液を飛びちらしてキラキラと周囲を光らせる。" timeout="" voices=""/>
		<play_sound @l="1528" buf="5" fadeout="0" type="1"/>
		<chgbg @l="1530" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evrai09_04b" type="99"/>
		<msg @l="1532" char="3" flags="0" index="875" line="1532" name="礼羽"
		   text="「ふあっ、あっあっ！　んんっ、ああぁっ！　はぁ、はぁっ！　\n:腰を下ろす度に、ち○ぽ、奥に当たって……っ、ゾクゾクって\n:してぇ……んっ、あぁぁっ\x;heart:　これ、癖になっちゃいそう\x;heart:\x;hearts:」"
		   timeout="" voices="rai00941"/>
		<play_sound @l="1534" buf="5" fadeout="0" intro="0" src="rai80019" type="6"
		   volume="1."/>
		<msg @l="1535" char="-1" flags="0" index="877" line="1535" name=""
		   text="こんなにもいやらしく僕を求めてくれることに、胸が熱く\n:なってくる。" timeout="" voices=""/>
		<msg @l="1537" char="11" flags="0" index="879" line="1537" name="亮"
		   text="（いやいや、ここで快楽に流されてどうする！！）" timeout="" voices=""/>
		<msg @l="1539" char="-1" flags="0" index="881" line="1539" name=""
		   text="そうだ！　ここで礼羽をめちゃんこ悦ばせて、僕なしではダメに\n:なるくらいにすれば、セフレから彼氏に昇格出来るんじゃないか？"
		   timeout="" voices=""/>
		<play_sound @l="1540" buf="5" fadeout="0" type="1"/>
		<chgbg @l="1542" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evrai09_04a" type="99"/>
		<msg @l="1544" char="3" flags="0" index="883" line="1544" name="礼羽"
		   text="「はぁ、はぁ……！　どうしたの……？」" timeout="" voices="rai00942"/>
		<play_sound @l="1546" buf="1" fadeout="300" type="1"/>
		<play_sound @l="1548" buf="1" fadeout="0" intro="0" src="hse106" type="1"
		   volume="1."/>
		<play_sound @l="1550" buf="5" fadeout="0" intro="0" src="rai80017" type="6"
		   volume="1."/>
		<msg @l="1551" char="-1" flags="0" index="885" line="1551" name=""
		   text="礼羽のしなやかな腰に手を伸ばし、しっかりと彼女を掴んで自分の腰を突き上げ始める。" timeout="" voices=""/>
		<msg @l="1552" char="-1" flags="0" index="887" line="1552" name=""
		   text="肉と肉のぶつかる音が、ふたりきりの教室内に響く。" timeout="" voices=""/>
		<play_sound @l="1554" buf="5" fadeout="0" type="1"/>
		<chgbg @l="1556" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evrai09_04d" type="99"/>
		<msg @l="1558" char="3" flags="0" index="889" line="1558" name="礼羽"
		   text="「あっ、動いちゃ……！　んぁっ、ダメだってぇ……っ、\n:んんんぅぅーーーーっ！？」" timeout=""
		   voices="rai00943"/>
		<msg @l="1560" char="3" flags="0" index="891" line="1560" name="礼羽"
		   text="「ふあっ、あぁんっ！？　だ、だめだよ……っ、わ、私に\n:任せてって言ったのにぃ……あっ、やあぁんっ！！」" timeout=""
		   voices="rai00944"/>
		<play_sound @l="1562" buf="5" fadeout="0" intro="0" src="rai80020" type="6"
		   volume="1."/>
		<msg @l="1563" char="11" flags="0" index="893" line="1563" name="亮"
		   text="「任せっぱなしよりも、一緒に動いたほうが気持ち良く\n:なれるかな？　って」" timeout="" voices=""/>
		<msg @l="1565" char="-1" flags="0" index="895" line="1565" name=""
		   text="さすがに本音を言うわけにはいかないので、\n:それっぽい理由を並べる。" timeout="" voices=""/>
		<msg @l="1567" char="11" flags="0" index="897" line="1567" name="亮"
		   text="「ほら、こういうのも気持ちいいだろ？」" timeout="" voices=""/>
		<play_sound @l="1568" buf="5" fadeout="0" type="1"/>
		<chgbg @l="1570" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evrai09_04c" type="99"/>
		<msg @l="1572" char="3" flags="0" index="899" line="1572" name="礼羽"
		   text="「んああぁっ！　あっ、ああぁっ！　あ、貴方がその気なら……\n:私だって、負けないから……\x;heart:　んっ、んくぅっ……！」"
		   timeout="" voices="rai00945"/>
		<play_sound @l="1574" buf="5" fadeout="0" intro="0" src="rai80019" type="6"
		   volume="1."/>
		<msg @l="1575" char="-1" flags="0" index="901" line="1575" name=""
		   text="彼女も負けじと、僕の動きと噛み合うように意識しつつ、\n:腰を揺さぶっていく。" timeout="" voices=""/>
		<msg @l="1576" char="-1" flags="0" index="903" line="1576" name=""
		   text="最初はぎこちなかったが、徐々にふたりの息があって\n:より小気味いいリズムを刻んでいく。" timeout="" voices=""/>
		<play_sound @l="1577" buf="5" fadeout="0" type="1"/>
		<chgbg @l="1579" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evrai09_04d" type="99"/>
		<msg @l="1581" char="3" flags="0" index="905" line="1581" name="礼羽"
		   text="「ふあっ、ああっ、んああぁっ！　こ、これ凄い……っ\x;heart:　\n:予想外のとこに当たってぇ！　んっ、あっ、ああぁっ！　\n:気持ち良すぎてぇ……！　ふあっ、あぁんっ\x;hearts:」"
		   timeout="" voices="rai00946"/>
		<msg @l="1583" char="3" flags="0" index="907" line="1583" name="礼羽"
		   text="「そこぉ……私も、あぁん、初めて知った、そこが……\n:気持ちいいってぇ……っ、自分じゃ絶対に気づかなかった、\n:とこなのぉ……っ、うあっ、はぁぁ……っ！」"
		   timeout="" voices="rai00947"/>
		<play_sound @l="1585" buf="5" fadeout="0" intro="0" src="rai80020" type="6"
		   volume="1."/>
		<msg @l="1586" char="11" flags="0" index="909" line="1586" name="亮"
		   text="「んっ……ここか……？」" timeout="" voices=""/>
		<play_sound @l="1587" buf="5" fadeout="0" type="1"/>
		<chgbg @l="1589" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evrai09_04b" type="99"/>
		<msg @l="1591" char="3" flags="0" index="911" line="1591" name="礼羽"
		   text="「うっんっ……そこぉ、おま○この入口の、ざらざらの上ぇ……\n:ふあっ！？　んああぁーーっ\x;heart:\x;hearts:」"
		   timeout="" voices="rai00948"/>
		<msg @l="1593" char="3" flags="0" index="913" line="1593" name="礼羽"
		   text="「くっ、んうぅぅ……っ、私がち○ぽをイカせたいのに……\n:はぁ、はぁっ！　このままじゃ、私が先に……っ！　\n:んっ、ひああぁっ！！」"
		   timeout="" voices="rai00949"/>
		<play_sound @l="1595" buf="5" fadeout="0" intro="0" src="rai80021" type="6"
		   volume="1."/>
		<msg @l="1596" char="-1" flags="0" index="915" line="1596" name=""
		   text="白く大きな乳房が、僕の腰の動き合わせてぶるんぶるんとボールのように弾んでいる。" timeout="" voices=""/>
		<msg @l="1597" char="-1" flags="0" index="917" line="1597" name=""
		   text="まるで僕が操っているかのような錯覚に、脳がトロけそうになる。" timeout="" voices=""/>
		<play_sound @l="1598" buf="5" fadeout="0" type="1"/>
		<chgbg @l="1600" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evrai09_04c" type="99"/>
		<msg @l="1602" char="3" flags="0" index="919" line="1602" name="礼羽"
		   text="「あっ、あぁん！　ち○ぽ……私の中で、またふくらんで\n:きて……っ！　ふあっ、ああぁんっ！　いい、凄くいいよぉ……\n:ふあっ、あっ、ふああぁっ！！」"
		   timeout="" voices="rai00950"/>
		<play_sound @l="1604" buf="5" fadeout="0" intro="0" src="rai80022" type="6"
		   volume="1."/>
		<msg @l="1605" char="-1" flags="0" index="921" line="1605" name=""
		   text="普段は勉強をしている教室で、学園でもトップクラスの人気を誇る彼女を、こんなにも乱れさせている。" timeout=""
		   voices=""/>
		<msg @l="1606" char="-1" flags="0" index="923" line="1606" name=""
		   text="そんな背徳感と優越感が入り混じって、自然と腰のストロークが\n:大きくなる。" timeout="" voices=""/>
		<play_sound @l="1607" buf="5" fadeout="0" type="1"/>
		<chgbg @l="1609" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evrai09_04d" type="99"/>
		<msg @l="1611" char="3" flags="0" index="925" line="1611" name="礼羽"
		   text="「んああああっ！！　あっあっ、あぁんっ！　ま、また腰の動きが速く、なってぇ……っ、んんっ、おま○こをゴシゴシしてぇっ、\n:ふあっ、ああぁぁっ\x;heart:\x;hearts:」"
		   timeout="" voices="rai00951"/>
		<msg @l="1613" char="3" flags="0" index="927" line="1613" name="礼羽"
		   text="「はぁ、はぁ……っ、私の動きと貴方の動きが一体になって……\n:あんっ、いやらしいっ\x;heart:　んぅんっ！　んああぁぁっ\x;heart:\x;hearts:」"
		   timeout="" voices="rai00952"/>
		<play_sound @l="1615" buf="5" fadeout="0" intro="0" src="rai80021" type="6"
		   volume="1."/>
		<msg @l="1616" char="-1" flags="0" index="929" line="1616" name=""
		   text="僕のたての動きと、礼羽の横の動き。" timeout="" voices=""/>
		<msg @l="1617" char="-1" flags="0" index="931" line="1617" name=""
		   text="まるでひとつに溶け合ったように、お互いの腰の動きが\n:シンクロしあう。" timeout="" voices=""/>
		<msg @l="1619" char="11" flags="0" index="933" line="1619" name="亮"
		   text="（すごい……けど、このままじゃ！？）" timeout="" voices=""/>
		<msg @l="1621" char="-1" flags="0" index="935" line="1621" name=""
		   text="一度達したとは言え、あまりの快感に再び射精感が押し\n:寄せてきた。" timeout="" voices=""/>
		<msg @l="1622" char="-1" flags="0" index="937" line="1622" name=""
		   text="このままだと、礼羽の中にそのまま出してしまいそうだ。" timeout="" voices=""/>
		<play_sound @l="1623" buf="5" fadeout="0" type="1"/>
		<play_sound @l="1625" buf="1" fadeout="300" type="1"/>
		<chgbg @l="1626" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evrai09_04a" type="99"/>
		<msg @l="1628" char="3" flags="0" index="939" line="1628" name="礼羽"
		   text="「んっ、んうぅぅ……っ？　はぁ、はぁっ……今度は、\n:どうする……つもりなの？」" timeout=""
		   voices="rai00953"/>
		<play_sound @l="1630" buf="5" fadeout="0" intro="0" src="rai80020" type="6"
		   volume="1."/>
		<msg @l="1631" char="11" flags="0" index="941" line="1631" name="亮"
		   text="「そろそろ……出そうだから、抜こうと思ったんだが……\n:はぁはぁっ」" timeout="" voices=""/>
