<?xml version="1.0" encoding="utf-8"?>
<xscript>
	<code>
		<label @l="6" id="scene_3_03"/>
		<play_sound @l="10" buf="0" fadeout="2500" type="0"/>
		<showmsg @l="11" duration="500" show="0"/>
		<loadimage @l="17" bg="0" fit="0" screen="1" src="evrai12_01a"/>
		<scroll @l="18" async="0" center_x="45" center_y="45" duration="0" flags="0"
		   rev="0." screen="1" transparency="0" v0="0." v1="0." wait_flags="0" x="0"
		   y="0" zoom="1."/>
		<chgscrn @l="19" duration="2500" flags="4608" flip="-1" flip_flags="0"
		   param="0" screen="1" type="10"/>
		<play_sound @l="20" buf="0" fadeout="500" intro="-1" src="bgm18" type="0"
		   volume="1."/>
		<msg @l="29" char="3" flags="0" index="0" line="29" name="礼羽"
		   text="「ねぇ、最後のお勤めなんだから、今までで一番、\n:激しく抱いてね……\x;heart:」" timeout=""
		   voices="rai13900"/>
		<play_sound @l="31" buf="1" fadeout="0" intro="-1" src="se560" type="1"
		   volume="0.7"/>
		<msg @l="33" char="-1" flags="0" index="2" line="33" name=""
		   text="ソファベッドの上で仰向けになり、両脚を大きく広げて\n:僕を誘う礼羽。" timeout="" voices=""/>
		<msg @l="34" char="-1" flags="0" index="4" line="34" name=""
		   text="彼女の秘部は既にしっとりと濡れているようで、パンツの\n:クロッチ部分にいやらしい染みが見える。" timeout=""
		   voices=""/>
		<play_sound @l="36" buf="1" fadeout="0" type="1"/>
		<msg @l="38" char="3" flags="0" index="6" line="38" name="礼羽"
		   text="「貴方の熱を、息づかいを、声を……フィンランドに帰っても\n:思い出せるように、全部刻みつけて欲しいの」" timeout=""
		   voices="rai13901"/>
		<msg @l="40" char="11" flags="0" index="8" line="40" name="亮"
		   text="「僕も同じ気持ちだ。いつでも思い出せるように、\n:全力でお前のことを抱きたい」" timeout="" voices=""/>
		<chgbg @l="42" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evrai12_01d" type="99"/>
		<msg @l="43" char="3" flags="0" index="10" line="43" name="礼羽"
		   text="「嬉しい……じゃあ、まずはキスからね。んっ……」" timeout="" voices="rai13902"/>
		<msg @l="45" char="-1" flags="0" index="12" line="45" name=""
		   text="瞳を閉じると彼女はくっと顎を上げ、僕の唇を待つ。" timeout="" voices=""/>
		<msg @l="46" char="-1" flags="0" index="14" line="46" name=""
		   text="込み上げてくる愛おしさをぶつけるように、差し出された唇に\n:自分の唇を押しつけた。" timeout="" voices=""/>
		<chgbg @l="48" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evrai12_01e" type="99"/>
		<msg @l="49" char="3" flags="0" index="16" line="49" name="礼羽"
		   text="「んんっ……ちゅ、ちゅぅ……れりゅ、んふぅ……いきなり舌、\n:入れてぇ……ふぁ、んっ……ちゅぅ……こんな激しいキス、\n:頭ボーッとしてきちゃう……」"
		   timeout="" voices="rai13903"/>
		<msg @l="51" char="3" flags="0" index="18" line="51" name="礼羽"
		   text="「ちゅぅ、ちゅっ、ちゅるる……はふぅ、キス\n:上手になったよね……ちゅ、ちゅぷ……んふぅ……最初にした時は\n:ぎこちなかったのに……」"
		   timeout="" voices="rai13904"/>
		<play_sound @l="53" buf="5" fadeout="0" intro="0" src="rai80028" type="6"
		   volume="1."/>
		<msg @l="54" char="11" flags="0" index="20" line="54" name="亮"
		   text="「いっぱいキスしたからな……んっ、ちゅ……舌を絡めて\n:唾液を交換するのが好きだろ？」" timeout="" voices=""/>
		<play_sound @l="55" buf="5" fadeout="0" type="1"/>
		<chgbg @l="57" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evrai12_01c" type="99"/>
		<msg @l="58" char="3" flags="0" index="22" line="58" name="礼羽"
		   text="「れりゅ、ちゅぅ……うん、好き……んはぁ、ちゅ、ちゅぷ……\n:この味、大好き\x;hearts:……」" timeout=""
		   voices="rai13905"/>
		<chgbg @l="60" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evrai12_01a" type="99"/>
		<msg @l="61" char="3" flags="0" index="24" line="61" name="礼羽"
		   text="「ふふっ、夢中になって舌を食べて……ちゅぅ、ちゅぷ……\n:貴方も私と一緒なんだね……んっ、ちゅっ……ちゅるぅ……」"
		   timeout="" voices="rai13906"/>
		<play_sound @l="63" buf="5" fadeout="0" intro="0" src="rai80029" type="6"
		   volume="1."/>
		<msg @l="64" char="-1" flags="0" index="26" line="64" name=""
		   text="くちゅくちゅと唾液の音を鳴らしながら積極的に舌を絡ませると、\n:口の中に甘い味が広がっていく。" timeout=""
		   voices=""/>
		<msg @l="65" char="-1" flags="0" index="28" line="65" name=""
		   text="好きと明確な言葉にしなくても、彼女の気持ちは\n:十分に伝わってくる。" timeout="" voices=""/>
		<play_sound @l="66" buf="5" fadeout="0" type="1"/>
		<chgbg @l="68" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evrai12_01d" type="99"/>
		<msg @l="69" char="3" flags="0" index="30" line="69" name="礼羽"
		   text="「んっ、ちゅうぅ……\x;heart:　んはぁ、はぁ……あぁ、切ないよ……\n:ちゅ、ちゅぅ……キスだけじゃ足りなくなってきたかも……」"
		   timeout="" voices="rai13907"/>
		<play_sound @l="71" buf="5" fadeout="0" intro="0" src="rai80015" type="6"
		   volume="1."/>
		<msg @l="72" char="-1" flags="0" index="32" line="72" name=""
		   text="少し唇を離しつーっと唾液の糸を滴らせながら\n:彼女の体を見てみると、キレイなピンク色の乳首が\n:すでにピンと立っていた。"
		   timeout="" voices=""/>
		<msg @l="74" char="11" flags="0" index="34" line="74" name="亮"
		   text="「キスだけでこんなになったのか？」" timeout="" voices=""/>
		<play_sound @l="75" buf="5" fadeout="0" type="1"/>
		<chgbg @l="76" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evrai12_01a" type="99"/>
		<msg @l="77" char="3" flags="0" index="36" line="77" name="礼羽"
		   text="「だって最初っから激しいんだもん……」" timeout="" voices="rai13908"/>
		<play_sound @l="82" buf="5" fadeout="0" intro="0" src="rai80016" type="6"
		   volume="1."/>
		<msg @l="83" char="-1" flags="0" index="38" line="83" name=""
		   text="ふるふると、触って欲しそうに眼前で揺れるふくよかな乳房。" timeout="" voices=""/>
		<msg @l="84" char="-1" flags="0" index="40" line="84" name=""
		   text="その魅力的なぷるぷるした動きに誘われるように、\n:僕は手を伸ばす。" timeout="" voices=""/>
		<play_sound @l="85" buf="5" fadeout="0" type="1"/>
		<chgbg @l="87" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evrai12_01e" type="99"/>
		<msg @l="88" char="3" flags="0" index="42" line="88" name="礼羽"
		   text="「んっ、あっ、はぁ……おっぱい、気持ちいいよ……\n:はぁ、あんっ……乳首、ぎゅってして……\x;hearts:」"
		   timeout="" voices="rai13909"/>
		<msg @l="90" char="3" flags="0" index="44" line="90" name="礼羽"
		   text="「ふあっ、あぁっ……！　それ、好きぃ……！\n:ふぅぅ……っ！　乳首、グリグリってされると、\n:ビリビリって気持ちいいのがきて……あっ、ああぁっ！」"
		   timeout="" voices="rai13910"/>
		<play_sound @l="92" buf="5" fadeout="0" intro="0" src="rai80018" type="6"
		   volume="1."/>
		<msg @l="93" char="-1" flags="0" index="46" line="93" name=""
		   text="すでに勃起していた乳首を指で摘むと、甲高い喘ぎ声を上げた。" timeout="" voices=""/>
		<msg @l="94" char="-1" flags="0" index="48" line="94" name=""
		   text="しっとりと汗ばんだ柔らかな胸で、ほんのりと香る汗のにおいと\n:コリコリとした乳首の感触を楽しむ。" timeout=""
		   voices=""/>
		<play_sound @l="95" buf="5" fadeout="0" type="1"/>
		<chgbg @l="97" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evrai12_01b" type="99"/>
		<msg @l="98" char="3" flags="0" index="50" line="98" name="礼羽"
		   text="「あっあっ、ああぁっ！　ふふっ。おっぱい触り始めると、\n:すぐに夢中になるよね……ふあっ、あぁっ……！」" timeout=""
		   voices="rai13911"/>
		<play_sound @l="100" buf="5" fadeout="0" intro="0" src="rai80019" type="6"
		   volume="1."/>
		<msg @l="101" char="11" flags="0" index="52" line="101" name="亮"
		   text="「こんなに大きくて、柔らかくて……エロいおっぱい。\n:夢中にならない方がおかしいだろ」" timeout="" voices=""/>
		<msg @l="103" char="-1" flags="0" index="54" line="103" name=""
		   text="むにゅむにゅと、手に力を入れるたびに形を変えていくおっぱい。" timeout="" voices=""/>
		<play_sound @l="104" buf="5" fadeout="0" type="1"/>
		<chgbg @l="106" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evrai12_01e" type="99"/>
		<msg @l="107" char="3" flags="0" index="56" line="107" name="礼羽"
		   text="「正直だね……はぁ、はぁ……！　や、んんっ……！\n:そんなに強く揉まれたら……ふあっ、ああぁっ！　おっぱい、\n:取れちゃうって……！」"
		   timeout="" voices="rai13912"/>
		<play_sound @l="109" buf="5" fadeout="0" intro="0" src="rai80018" type="6"
		   volume="1."/>
		<msg @l="110" char="11" flags="0" index="58" line="110" name="亮"
		   text="「激しくって言ったのは、そっちだろ？」" timeout="" voices=""/>
		<msg @l="112" char="-1" flags="0" index="60" line="112" name=""
		   text="自分でも気付かない内に胸を揉む手つきが\n:乱暴になってしまっていたようだ。" timeout="" voices=""/>
		<msg @l="113" char="-1" flags="0" index="62" line="113" name=""
		   text="それでも礼羽の身体を堪能出来るのが最後かもしれないと思うと、\n:まったく自制する気にならない。" timeout=""
		   voices=""/>
		<play_sound @l="114" buf="5" fadeout="0" type="1"/>
		<chgbg @l="116" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evrai12_01b" type="99"/>
		<msg @l="117" char="3" flags="0" index="64" line="117" name="礼羽"
		   text="「ああっん……ふぁっ、いいよ……はぁはぁっ……\n:もっと、好きなように触って」" timeout=""
		   voices="rai13913"/>
		<chgbg @l="119" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evrai12_01a" type="99"/>
		<msg @l="120" char="3" flags="0" index="66" line="120" name="礼羽"
		   text="「あ、っは……っ、貴方に触られるの……\n:あ、は、っひ……好きだから」" timeout="" voices="rai13914"/>
		<chgbg @l="124" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evrai12_02d" type="99"/>
		<msg @l="126" char="3" flags="0" index="68" line="126" name="礼羽"
		   text="「んはぁ、あぁっ……！　もう我慢できないよ……もう、あそこが\n:こんなにぐしょぐしょになっちゃってるの、あっ、あぁぁ……！」"
		   timeout="" voices="rai13915"/>
		<msg @l="128" char="3" flags="0" index="70" line="128" name="礼羽"
		   text="「はぁはぁ、早くおま○こにち○ぽ入れてぇ……\x;heart:」" timeout="" voices="rai13916"/>
		<play_sound @l="130" buf="5" fadeout="0" intro="0" src="rai80018" type="6"
		   volume="1."/>
		<msg @l="131" char="11" flags="0" index="72" line="131" name="亮"
		   text="「えっ、早すぎないか？」" timeout="" voices=""/>
		<play_sound @l="132" buf="5" fadeout="0" type="1"/>
		<chgbg @l="134" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evrai12_02a" type="99"/>
		<msg @l="135" char="3" flags="0" index="74" line="135" name="礼羽"
		   text="「あ、っは……っ、はぁ、エッチなキスや、\n:いやらしい触り方されちゃったからね」" timeout=""
		   voices="rai13917"/>
		<chgbg @l="138" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evrai12_02d" type="99"/>
		<msg @l="140" char="3" flags="0" index="76" line="140" name="礼羽"
		   text="「はぁ、はぁ……それにそっちだって、ズボンを履いてても\n:わかるくらいパンパンに膨らんでるじゃない」" timeout=""
		   voices="rai13918"/>
		<msg @l="142" char="3" flags="0" index="78" line="142" name="礼羽"
		   text="「あはっ、はぁぁんっ……貴方だって……\n:は、はぁ、我慢できないんでしょ？」" timeout=""
		   voices="rai13919"/>
		<play_sound @l="144" buf="5" fadeout="0" intro="0" src="rai80015" type="6"
		   volume="1."/>
		<msg @l="145" char="11" flags="0" index="80" line="145" name="亮"
		   text="「そりゃ、礼羽と繋がりたくて仕方ないからな」" timeout="" voices=""/>
		<play_sound @l="146" buf="5" fadeout="0" type="1"/>
		<chgbg @l="148" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evrai12_02e" type="99"/>
		<msg @l="149" char="3" flags="0" index="82" line="149" name="礼羽"
		   text="「うん……私のことが欲しくてたまらないんだなって\n:伝わってきたよ……」" timeout="" voices="rai13920"/>
		<play_sound @l="151" buf="1" fadeout="0" intro="-1" src="se511" type="1"
		   volume="0.6"/>
		<play_sound @l="153" buf="5" fadeout="0" intro="0" src="rai80016" type="6"
		   volume="1."/>
		<msg @l="154" char="-1" flags="0" index="84" line="154" name=""
		   text="そう言うと、彼女は自らパンツに手を伸ばし、スッと横にずらす。" timeout="" voices=""/>
		<play_sound @l="155" buf="5" fadeout="0" type="1"/>
		<play_sound @l="157" buf="1" fadeout="0" type="1"/>
		<chgbg @l="161" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evrai12_03a" type="99"/>
		<msg @l="163" char="3" flags="0" index="86" line="163" name="礼羽"
		   text="「遠慮はいらないから……はぁ、はぁ……きて、すぐにきて……」" timeout="" voices="rai13921"/>
		<play_sound @l="165" buf="5" fadeout="0" intro="0" src="rai80017" type="6"
		   volume="1."/>
		<msg @l="166" char="-1" flags="0" index="88" line="166" name=""
		   text="露わになった秘部は透明に輝く愛液を溢れさせ、\n:むわりと甘酸っぱい女の子の匂いが立ち篭めてきた。" timeout=""
		   voices=""/>
		<play_sound @l="167" buf="5" fadeout="0" type="1"/>
		<chgbg @l="169" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evrai12_03d" type="99"/>
		<msg @l="170" char="3" flags="0" index="90" line="170" name="礼羽"
		   text="「ほら、見て……おま○こヒクヒクしてるでしょ？はぁ、はぁ……\n:ち○ぽが欲しくて、発情しちゃってるんだよ……\x;hearts:」"
		   timeout="" voices="rai13922"/>
		<play_sound @l="172" buf="1" fadeout="0" intro="-1" src="hse102" type="1"
		   volume="0.7"/>
		<play_sound @l="174" buf="5" fadeout="0" intro="0" src="rai80016" type="6"
		   volume="1."/>
		<msg @l="175" char="-1" flags="0" index="92" line="175" name=""
		   text="下着をズラした手でそのまま陰唇を開き、そのまま膣口を\n:見せつけてくる。" timeout="" voices=""/>
		<msg @l="176" char="-1" flags="0" index="94" line="176" name=""
		   text="綺麗な朱色をした膣肉は言葉通りヒクヒクと収縮していて、\n:ペニスを今か今かと待ちわびているようだった。" timeout=""
		   voices=""/>
		<play_sound @l="178" buf="1" fadeout="0" type="1"/>
		<msg @l="180" char="11" flags="0" index="96" line="180" name="亮"
		   text="（いつにも増してエロい！）" timeout="" voices=""/>
		<msg @l="182" char="-1" flags="0" index="98" line="182" name=""
		   text="上気した肌。挑発的な身体。切なげに潤んだ瞳――\n:全ての要素が僕を煽ってくる。" timeout="" voices=""/>
		<msg @l="183" char="-1" flags="0" index="100" line="183" name=""
		   text="加熱していく欲望に急かされるままに僕はズボンと一緒に\n:パンツも降ろす。" timeout="" voices=""/>
		<msg @l="184" char="-1" flags="0" index="102" line="184" name=""
		   text="そしてそのままピンと反り返っているペニスを\n:濡れそぼっている入り口にあてがった。" timeout="" voices=""/>
		<play_sound @l="185" buf="5" fadeout="0" type="1"/>
		<play_sound @l="189" buf="1" fadeout="0" intro="-1" src="hse104" type="1"
		   volume="1."/>
		<chgbg @l="190" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evrai12_04d" type="99"/>
		<msg @l="192" char="3" flags="0" index="104" line="192" name="礼羽"
		   text="「んっ、ふあぁ……ち○ぽ、硬い……それに熱くて……\n:はぁ、あぁ……触れ合ってるだけで、ゾクゾクしてきちゃう……」"
		   timeout="" voices="rai13923"/>
		<play_sound @l="195" buf="1" fadeout="0" intro="-1" src="hse103" type="1"
		   volume="0.7"/>
		<chgbg @l="196" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evrai12_04e" type="99"/>
		<msg @l="198" char="3" flags="0" index="106" line="198" name="礼羽"
		   text="「や、あぁん……！　焦らさないで……ぬるぬる擦りつけてないで\n:早く入れてよぉ……？　はぁ、はぁ……！」" timeout=""
		   voices="rai13924"/>
		<play_sound @l="200" buf="1" fadeout="0" type="1"/>
		<play_sound @l="202" buf="5" fadeout="0" intro="0" src="rai80018" type="6"
		   volume="1."/>
		<msg @l="203" char="11" flags="0" index="108" line="203" name="亮"
		   text="「もどかしそうにしてる姿が可愛くって、つい……な？」" timeout="" voices=""/>
		<chgbg @l="206" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evrai12_04a" type="99"/>
		<msg @l="208" char="-1" flags="0" index="110" line="208" name=""
		   text="いつの間にか僕のモノは、カウパーと愛液で\n:ベトベトに濡れていた。" timeout="" voices=""/>
		<play_sound @l="209" buf="5" fadeout="0" type="1"/>
		<chgbg @l="211" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evrai12_04d" type="99"/>
		<msg @l="212" char="3" flags="0" index="112" line="212" name="礼羽"
		   text="「余裕見せちゃって……んうぅ……こんな鉄みたく硬くして、\n:ホントは入れたくって仕方ない癖に……」" timeout=""
		   voices="rai13925"/>
		<chgbg @l="214" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evrai12_04e" type="99"/>
		<msg @l="215" char="3" flags="0" index="114" line="215" name="礼羽"
