<?xml version="1.0" encoding="utf-8"?>
<xscript>
	<code>
		<msg @l="691" char="1" flags="0" index="404" line="691" name="汐"
		   text="「あっ……そ、そうなんだ。これが、精子……」" timeout="" voices="usi10898"/>
		<chgbg @l="695" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evusi04_04e" type="99"/>
		<msg @l="697" char="1" flags="0" index="406" line="697" name="汐"
		   text="「すんすん、れろ、あむっ……。ん、なんか、\n:想像してたのよりも、ずっと熱くて、どろっとしてる……\x;hearts:」"
		   timeout="" voices="usi10899"/>
		<msg @l="699" char="1" flags="0" index="408" line="699" name="汐"
		   text="「それにさっきの透明なのとは違って……\n:すっごくキミの匂いが濃い……\x;heart:」" timeout=""
		   voices="usi10900"/>
		<msg @l="701" char="1" flags="0" index="410" line="701" name="汐"
		   text="「味も……ぺろっ、ちゅっ……ちょっと苦いかな？」" timeout="" voices="usi10901"/>
		<msg @l="704" char="11" flags="0" index="412" line="704" name="亮"
		   text="「精子を想像したこと、あったんですね」" timeout="" voices=""/>
		<chgbg @l="707" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evusi04_04a" type="99"/>
		<msg @l="709" char="1" flags="0" index="414" line="709" name="汐"
		   text="「んぐっ……な、なんでキミはいじわる言うかなぁ、もう」" timeout="" voices="usi10902"/>
		<msg @l="711" char="1" flags="0" index="416" line="711" name="汐"
		   text="「……言っておくけど、私が想像したのは、\n:あくまでもキミの精子なんだからね？　……ちゅっ、れろっ」" timeout=""
		   voices="usi10903"/>
		<play_sound @l="713" buf="5" fadeout="0" intro="0" src="usi80000" type="6"
		   volume="1."/>
		<msg @l="714" char="11" flags="0" index="418" line="714" name="亮"
		   text="「にゃわっ？！　ちょっ、だ、だめですってば、\n:いまそんなに激しく舐めたらっ……」" timeout="" voices=""/>
		<play_sound @l="715" buf="5" fadeout="0" type="1"/>
		<chgbg @l="717" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evusi04_04b" type="99"/>
		<msg @l="719" char="1" flags="0" index="420" line="719" name="汐"
		   text="「くすくすっ。でも、イッてもらえたってことは、\n:気持ちよかったってことだよね？」" timeout=""
		   voices="usi10904"/>
		<msg @l="721" char="11" flags="0" index="422" line="721" name="亮"
		   text="「それは……はい。あ、でも、出過ぎちゃったせいで、\n:顔にかけちゃってすみません」" timeout="" voices=""/>
		<msg @l="723" char="1" flags="0" index="424" line="723" name="汐"
		   text="「謝ることなんてないよ。気持ちよかったから\n:いっぱいでたんでしょ？　なら、嬉しいもん」" timeout=""
		   voices="usi10905"/>
		<msg @l="727" char="1" flags="0" index="426" line="727" name="汐"
		   text="「……うん、すごく嬉しい。嬉しいなぁ、えへへっ\x;heart:」" timeout="" voices="usi10906"/>
		<msg @l="729" char="-1" flags="0" index="428" line="729" name=""
		   text="初めてのエッチで精子をぶっかけられたというのに、\n:それでもにこにこと笑う朝霞さんの健気さに、不覚にも――。" timeout=""
		   voices=""/>
		<chgbg @l="731" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evusi04_04c" type="99"/>
		<msg @l="733" char="1" flags="0" index="430" line="733" name="汐"
		   text="「……えっ？　あ、な、なんかあそこ、\n:またびくってしたんだけど……これって、もしかして？」" timeout=""
		   voices="usi10907"/>
		<msg @l="735" char="11" flags="0" index="432" line="735" name="亮"
		   text="「朝霞さんが可愛すぎて、\n:一回じゃ収まらないみたいで……あはは」" timeout="" voices=""/>
		<msg @l="737" char="1" flags="0" index="434" line="737" name="汐"
		   text="「なっ……あはっ、ふふっ。んもう、人のせいにしてぇ……\x;hearts:」" timeout=""
		   voices="usi10908"/>
		<chgbg @l="741" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evusi04_04b" type="99"/>
		<msg @l="743" char="1" flags="0" index="436" line="743" name="汐"
		   text="「……でも、よかった。\n:ここで終わってたら、私のほうがおさまらなくなっちゃうし……」" timeout=""
		   voices="usi10909"/>
		<msg @l="745" char="11" flags="0" index="438" line="745" name="亮"
		   text="「それって――」" timeout="" voices=""/>
		<msg @l="747" char="1" flags="0" index="440" line="747" name="汐"
		   text="「うっ、うん……。今度こそ、私が言う番だよ？」" timeout="" voices="usi10910"/>
		<msg @l="749" char="1" flags="0" index="442" line="749" name="汐"
		   text="「こんなにいっぱい射精して、私をドキドキさせた責任、\n:とってくれるよね……？」" timeout=""
		   voices="usi10911"/>
		<play_sound @l="757" buf="0" fadeout="2500" type="0"/>
		<showmsg @l="758" duration="500" show="0"/>
		<chgbg @l="759" duration="2500" flags="4608" flip="-1" flip_flags="0"
		   param="0" screen="1" src="evusi05_01a" type="10"/>
		<play_sound @l="760" buf="0" fadeout="500" intro="-1" src="bgm19" type="0"
		   volume="1."/>
		<msg @l="762" char="1" flags="0" index="444" line="762" name="汐"
		   text="「んっ……\x;heart:」" timeout="" voices="usi10912"/>
		<msg @l="764" char="-1" flags="0" index="446" line="764" name=""
		   text="仰向けにころんと寝転がった朝霞さんが、\n:足を開いて可愛らしい下着を見せてくれる。" timeout="" voices=""/>
		<msg @l="765" char="-1" flags="0" index="448" line="765" name=""
		   text="けど、大胆な下半身とは対照的に、上半身――\n:とくに顔は、頑なにこちらの視線をガードしていた。" timeout=""
		   voices=""/>
		<msg @l="767" char="1" flags="0" index="450" line="767" name="汐"
		   text="「ほ、ほら、はやくっ……」" timeout="" voices="usi10913"/>
		<msg @l="769" char="11" flags="0" index="452" line="769" name="亮"
		   text="「あの……ひょっとして」" timeout="" voices=""/>
		<chgbg @l="771" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evusi05_01b" type="99"/>
		<msg @l="773" char="1" flags="0" index="454" line="773" name="汐"
		   text="「は、恥ずかしいの。言わせないでよもう」" timeout="" voices="usi10914"/>
		<msg @l="775" char="-1" flags="0" index="456" line="775" name=""
		   text="腕で顔を隠しながら、こっちの様子を伺ってくる朝霞さん。" timeout="" voices=""/>
		<msg @l="776" char="-1" flags="0" index="458" line="776" name=""
		   text="あれほど積極的だった朝霞さんに恥じらわれると、\n:こっちとしても気恥ずかしいというか。" timeout="" voices=""/>
		<msg @l="777" char="-1" flags="0" index="460" line="777" name=""
		   text="でも、この恥じらいがより興奮を煽るというか。" timeout="" voices=""/>
		<msg @l="779" char="11" flags="0" index="462" line="779" name="亮"
		   text="「……あ」" timeout="" voices=""/>
		<msg @l="781" char="-1" flags="0" index="464" line="781" name=""
		   text="目の前に現れたしましまパンツを、なにもかも忘れて凝視。" timeout="" voices=""/>
		<msg @l="783" char="11" flags="0" index="466" line="783" name="亮"
		   text="「朝霞さん、パンツに染みが……」" timeout="" voices=""/>
		<chgbg @l="785" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evusi05_01a" type="99"/>
		<msg @l="787" char="1" flags="0" index="468" line="787" name="汐"
		   text="「そ、そういうことはわかってても言わないのっ……！」" timeout="" voices="usi10915"/>
		<msg @l="789" char="1" flags="0" index="470" line="789" name="汐"
		   text="「キミって時々イジワルだよね」" timeout="" voices="usi10916"/>
		<msg @l="791" char="11" flags="0" index="472" line="791" name="亮"
		   text="「好きな子のことはイジワルしたくなるもんなんです」" timeout="" voices=""/>
		<chgbg @l="793" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evusi05_01e" type="99"/>
		<msg @l="795" char="1" flags="0" index="474" line="795" name="汐"
		   text="「も、もう！　そんな言い方はズルイよ。\n:そう言われたら嬉しくて、なんでも許しちゃうじゃない」" timeout=""
		   voices="usi10917"/>
		<msg @l="797" char="11" flags="0" index="476" line="797" name="亮"
		   text="「じゃあ、パンツ見てもいいですよね？」" timeout="" voices=""/>
		<msg @l="799" char="1" flags="0" index="478" line="799" name="汐"
		   text="「う……うん\x;heart:」" timeout="" voices="usi10918"/>
		<showmsg @l="801" duration="500" show="0"/>
		<scroll @l="802" async="-1" center_x="600" center_y="360" duration="1000"
		   flags="0" rev="0." screen="0" transparency="0" v0="0." v1="0."
		   wait_flags="0" x="0" y="0" zoom="1.5"/>
		<msg @l="804" char="11" flags="0" index="480" line="804" name="亮"
		   text="（うわぁ……もうこんなに濡れてるんだ）" timeout="" voices=""/>
		<msg @l="806" char="-1" flags="0" index="482" line="806" name=""
		   text="恥じらう朝霞さんのあそこが微かにひくっと動いた気がして、\n:思わず生唾を飲んでしまう。" timeout="" voices=""/>
		<msg @l="807" char="-1" flags="0" index="484" line="807" name=""
		   text="この薄い布一枚の向こうに、朝霞さんのぷにぷにの割れ目が……。" timeout="" voices=""/>
		<msg @l="809" char="11" flags="0" index="486" line="809" name="亮"
		   text="「じゃ、じゃあ……さわります」" timeout="" voices=""/>
		<msg @l="811" char="1" flags="0" index="488" line="811" name="汐"
		   text="「ん……」" timeout="" voices="usi10919"/>
		<msg @l="813" char="-1" flags="0" index="490" line="813" name=""
		   text="短い承諾をもらったあと彼女の股間に顔を近寄せると、\n:それだけで濃厚な女の子の匂いが漂ってきた。" timeout=""
		   voices=""/>
		<msg @l="815" char="11" flags="0" index="492" line="815" name="亮"
		   text="「あぁ……いい匂い……はぁ、はぁ」" timeout="" voices=""/>
		<msg @l="819" char="1" flags="0" index="494" line="819" name="汐"
		   text="「そっ、それはだって、\n:ちゃんとお風呂でよーく洗ってきたもんっ……」" timeout="" voices="usi10920"/>
		<msg @l="821" char="11" flags="0" index="496" line="821" name="亮"
		   text="「ッ……」" timeout="" voices=""/>
		<msg @l="823" char="-1" flags="0" index="498" line="823" name=""
		   text="お風呂屋に行ったときから\n:エッチな展開を意識していたんだとわかって、\n:それだけで股間が熱くなる。" timeout=""
		   voices=""/>
		<msg @l="824" char="-1" flags="0" index="500" line="824" name=""
		   text="甘くて酸っぱくて、ちょっとしょっぱい……\n:シャンプーと汗と愛液と、いろんな匂いが混ざった匂いが愛しい。" timeout=""
		   voices=""/>
		<showmsg @l="827" duration="500" show="0"/>
		<loadimage @l="828" bg="0" fit="0" screen="1" src="evusi05_01b"/>
		<scroll @l="829" async="0" center_x="45" center_y="45" duration="0"
		   flags="0" rev="0." screen="1" transparency="0" v0="0." v1="0."
		   wait_flags="0" x="0" y="0" zoom="1."/>
		<chgscrn @l="830" duration="1000" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" type="99"/>
		<msg @l="834" char="1" flags="0" index="502" line="834" name="汐"
		   text="「あ、あんまりじろじろ見てないで、そ、その、はやくっ……」" timeout="" voices="usi10921"/>
		<msg @l="836" char="11" flags="0" index="504" line="836" name="亮"
		   text="「さっきの僕と同じこと言ってますけど？」" timeout="" voices=""/>
		<chgbg @l="838" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evusi05_01f" type="99"/>
		<msg @l="840" char="1" flags="0" index="506" line="840" name="汐"
		   text="「あっ……う、うんっ。じっと見つめられるのって、\n:こんなに恥ずかしかったんだね……はぁ、はぁ」" timeout=""
		   voices="usi10922"/>
		<play_sound @l="842" buf="5" fadeout="0" intro="0" src="usi80015" type="6"
		   volume="1."/>
		<msg @l="843" char="11" flags="0" index="508" line="843" name="亮"
		   text="「あ、すごい……」" timeout="" voices=""/>
		<play_sound @l="844" buf="5" fadeout="0" type="1"/>
		<chgbg @l="846" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evusi05_01a" type="99"/>
		<msg @l="848" char="1" flags="0" index="510" line="848" name="汐"
		   text="「な、なにっ？」" timeout="" voices="usi10923"/>
		<play_sound @l="850" buf="5" fadeout="0" intro="0" src="usi80016" type="6"
		   volume="1."/>
		<msg @l="851" char="11" flags="0" index="512" line="851" name="亮"
		   text="「いま、パンツのシミが、じわっと広がって――」" timeout="" voices=""/>
		<play_sound @l="852" buf="5" fadeout="0" type="1"/>
		<msg @l="854" char="1" flags="0" index="514" line="854" name="汐"
		   text="「っ……？！　き、キミがじっと見つめるからっ……！」" timeout="" voices="usi10924"/>
		<play_sound @l="856" buf="5" fadeout="0" intro="0" src="usi80017" type="6"
		   volume="1."/>
		<msg @l="857" char="11" flags="0" index="516" line="857" name="亮"
		   text="「嬉しいです、感じてくれて」" timeout="" voices=""/>
		<play_sound @l="858" buf="5" fadeout="0" type="1"/>
		<chgbg @l="860" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evusi05_01e" type="99"/>
		<msg @l="862" char="1" flags="0" index="518" line="862" name="汐"
		   text="「ううっ……恥ずかしいのに嬉しい。ほんとズルイよぉ……」" timeout="" voices="usi10925"/>
		<play_sound @l="864" buf="5" fadeout="0" intro="0" src="usi80015" type="6"
		   volume="1."/>
		<msg @l="865" char="-1" flags="0" index="520" line="865" name=""
		   text="弾む息と広がっていく染みを見て、\n:さっきの朝霞さんが大胆になってた理由が少しわかった気がした。" timeout=""
		   voices=""/>
		<msg @l="866" char="-1" flags="0" index="522" line="866" name=""
		   text="感じる相手を見ていると、もっと感じさせたくなるんだ。" timeout="" voices=""/>
		<play_sound @l="867" buf="5" fadeout="0" type="1"/>
		<chgbg @l="873" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evusi05_02e" type="99"/>
		<play_sound @l="875" buf="1" fadeout="0" intro="-1" src="hse101" type="1"
		   volume="1."/>
		<play_sound @l="876" buf="5" fadeout="0" intro="0" src="usi80016" type="6"
		   volume="1."/>
		<msg @l="877" char="11" flags="0" index="524" line="877" name="亮"
		   text="「あぁ……指でそっと押すと、じゅわって広がって……」" timeout="" voices=""/>
		<play_sound @l="878" buf="5" fadeout="0" type="1"/>
		<play_sound @l="879" buf="1" fadeout="0" type="1"/>
		<msg @l="881" char="1" flags="0" index="526" line="881" name="汐"
		   text="「んくっ、はぅ、はあっ、ああっ……」" timeout="" voices="usi10926"/>
		<play_sound @l="883" buf="5" fadeout="0" intro="0" src="usi80017" type="6"
		   volume="1."/>
		<msg @l="884" char="-1" flags="0" index="528" line="884" name=""
		   text="うっすら透けている縦筋に触れると、足がひくっと震える。" timeout="" voices=""/>
		<msg @l="885" char="-1" flags="0" index="530" line="885" name=""
		   text="水たまりみたいな染みが広がっていくと、\n:しまパンのラインの濃淡がくっきりしてきて、\n:さらにエッチになっていく。"
		   timeout="" voices=""/>
		<msg @l="887" char="11" flags="0" index="532" line="887" name="亮"
		   text="「割れ目、すっごいふにふにしてる……」" timeout="" voices=""/>
		<play_sound @l="888" buf="5" fadeout="0" type="1"/>
		<chgbg @l="890" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evusi05_02b" type="99"/>
		<msg @l="892" char="1" flags="0" index="534" line="892" name="汐"
		   text="「はあっ、あ、ああっ……\n:あそこ、さわられちゃってるよぅ、ああっ、はあっ」" timeout=""
		   voices="usi10927"/>
		<play_sound @l="894" buf="5" fadeout="0" intro="0" src="usi80018" type="6"
		   volume="1."/>
		<msg @l="895" char="11" flags="0" index="536" line="895" name="亮"
		   text="「おつゆがどんどん出てきて……あぁ、匂いがさらに、\n:むわっとして……くんくん、すぅ、はあっ……」" timeout=""
		   voices=""/>
		<msg @l="897" char="-1" flags="0" index="538" line="897" name=""
		   text="朝霞さんの秘所を撫で、匂いを嗅ぐ。\n:それだけで、射精したばかりの息子が\n:どんどんガチガチになっていく。" timeout=""
		   voices=""/>
		<play_sound @l="898" buf="5" fadeout="0" type="1"/>
		<msg @l="900" char="1" flags="0" index="540" line="900" name="汐"
		   text="「あ、あぅ……すごく、顔、近いよぅ……。\n:も、もうちょっと、離れてもいいんじゃないかなっ……？」" timeout=""
		   voices="usi10928"/>
		<play_sound @l="902" buf="5" fadeout="0" intro="0" src="usi80019" type="6"
		   volume="1."/>
		<msg @l="903" char="11" flags="0" index="542" line="903" name="亮"
		   text="「さっきの朝霞さんも、このぐらいでしたよ」" timeout="" voices=""/>
		<play_sound @l="904" buf="5" fadeout="0" type="1"/>
		<chgbg @l="906" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evusi05_02e" type="99"/>
		<msg @l="908" char="1" flags="0" index="544" line="908" name="汐"
		   text="「そっ、それはそうなんだけど……でも、するとされるとじゃ、\n:ぜんぜん違うからっ……ふぅ、はぁ、はあっ」" timeout=""
		   voices="usi10929"/>
		<play_sound @l="910" buf="5" fadeout="0" intro="0" src="usi80020" type="6"
		   volume="1."/>
		<msg @l="911" char="11" flags="0" index="546" line="911" name="亮"
		   text="「あ、あぁ……指を押し込むと、\n:じゅぶっと割れ目に指が沈んで、どんどん染みが広がって……」" timeout=""
		   voices=""/>
		<play_sound @l="912" buf="5" fadeout="0" type="1"/>
		<msg @l="914" char="1" flags="0" index="548" line="914" name="汐"
		   text="「んぐッ……ふぅ、はぁ……うぅ、恥ずかしいよぅ」" timeout="" voices="usi10930"/>
		<play_sound @l="916" buf="5" fadeout="0" intro="0" src="usi80018" type="6"
		   volume="1."/>
		<msg @l="917" char="11" flags="0" index="550" line="917" name="亮"
		   text="「パンツ、おもらししたみたいに濡れてきちゃいましたよ」" timeout="" voices=""/>
		<play_sound @l="918" buf="5" fadeout="0" type="1"/>
		<chgbg @l="920" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evusi05_02a" type="99"/>
		<msg @l="922" char="1" flags="0" index="552" line="922" name="汐"
		   text="「お、おもらしじゃないからっ。これは、感じてるだけで――……んッ、はぅ、はあっ」" timeout=""
		   voices="usi10931"/>
		<play_sound @l="924" buf="5" fadeout="0" intro="0" src="usi80020" type="6"
		   volume="1."/>
