		   param="16" screen="1" src="evusi01_01d" type="99"/>
		<msg @l="1179" char="1" flags="0" index="531" line="1179" name="汐"
		   text="「うん……私も一緒だよ」" timeout="" voices="usi12079"/>
		<msg @l="1181" char="-1" flags="0" index="533" line="1181" name=""
		   text="彼女と話すだけでいくらでも胸が高鳴っていく。\n:しかもこの高鳴りが心地良い。" timeout="" voices=""/>
