<?xml version="1.0" encoding="utf-8"?>
<xscript>
	<code>
		<msg @l="1107" char="1" flags="0" index="477" line="1107" name="汐"
		   text="「う、浮かべるわけないでしょ！　そんな子供みたいなこと……」" timeout="" voices="usi12068"/>
		<chgbg @l="1109" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evusi01_02b" type="99"/>
		<msg @l="1111" char="-1" flags="0" index="479" line="1111" name=""
		   text="自覚があるのかないのか、アヒルに突っ込まれると\n:朝霞さんが目をそらしてしまった。" timeout="" voices=""/>
		<msg @l="1113" char="11" flags="0" index="481" line="1113" name="亮"
		   text="「ぷくく……じゃあどうして持ってきてるんですか？」" timeout="" voices=""/>
		<msg @l="1115" char="1" flags="0" index="483" line="1115" name="汐"
		   text="「昔は花音と一緒にお風呂で遊んだりもしたから……\n:その名残で、つい持ってきちゃったのっ」" timeout=""
		   voices="usi12069"/>
		<chgbg @l="1117" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evusi01_01c" type="99"/>
		<msg @l="1119" char="1" flags="0" index="485" line="1119" name="汐"
		   text="「勘違いしないで欲しいんだけど、今はお家のお風呂でしか\n:入れてないからね？」" timeout=""
		   voices="usi12070"/>
		<msg @l="1121" char="11" flags="0" index="487" line="1121" name="亮"
		   text="「ぶふぅーーっ！！」" timeout="" voices=""/>
		<msg @l="1123" char="-1" flags="0" index="489" line="1123" name=""
		   text="お風呂でアヒルさんと戯れる朝霞さんを想像して、\n:今度こそ盛大に噴き出してしまった。" timeout="" voices=""/>
		<chgbg @l="1125" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evusi01_01a" type="99"/>
		<msg @l="1127" char="1" flags="0" index="491" line="1127" name="汐"
		   text="「あーー、ひどい笑ったぁ！　子供みたいだと思ったんでしょ？\n:そうなんでしょ？？」" timeout=""
		   voices="usi12071"/>
		<msg @l="1129" char="11" flags="0" index="493" line="1129" name="亮"
		   text="「すみません、吹き出していいのは\n:アヒルさんだけでしたね、ぷぷぷ……！」" timeout="" voices=""/>
		<chgbg @l="1131" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evusi01_01c" type="99"/>
		<msg @l="1133" char="1" flags="0" index="495" line="1133" name="汐"
		   text="「上手いこと言ったつもり！？　ああもうっ、\n:アヒルちゃん持ってきたの失敗したよ～」" timeout=""
		   voices="usi12072"/>
		<msg @l="1135" char="11" flags="0" index="497" line="1135" name="亮"
		   text="「朝霞さんらしいというか、可愛くて似合ってると\n:思いますよ……ぶふっ！」" timeout="" voices=""/>
		<chgbg @l="1137" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evusi01_02a" type="99"/>
		<msg @l="1139" char="1" flags="0" index="499" line="1139" name="汐"
		   text="「うぅ～～っ！　もう、笑いすぎ～っ！」" timeout="" voices="usi12073"/>
		<msg @l="1141" char="-1" flags="0" index="501" line="1141" name=""
		   text="拗ねてほっぺをぷくって膨らませてる顔もかわいい。" timeout="" voices=""/>
		<msg @l="1142" char="-1" flags="0" index="503" line="1142" name=""
		   text="お姉ちゃんしてるときも、こういう見た目相応も\n:どっちもかわいい。" timeout="" voices=""/>
		<msg @l="1144" char="11" flags="0" index="505" line="1144" name="亮"
		   text="（もう、朝霞さんならなんでもかわいいんじゃないか？）" timeout="" voices=""/>
		<msg @l="1146" char="-1" flags="0" index="507" line="1146" name=""
		   text="なーんて思えるほど、彼女の全てが魅力的に感じてしまう。" timeout="" voices=""/>
		<chgbg @l="1148" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evusi01_02e" type="99"/>
		<msg @l="1150" char="1" flags="0" index="509" line="1150" name="汐"
		   text="「あ、キミの髪、ところどころ水がしたたってるじゃない」" timeout="" voices="usi12074"/>
		<msg @l="1152" char="11" flags="0" index="511" line="1152" name="亮"
		   text="「ほんとだ、拭きが甘かったんですかね。でもこれくらいなら\n:歩いてるうちに乾きますから」" timeout="" voices=""/>
		<chgbg @l="1154" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evusi01_01d" type="99"/>
		<msg @l="1156" char="1" flags="0" index="513" line="1156" name="汐"
		   text="「ダメだよ、髪の毛が痛んじゃうから。はい、お姉ちゃんが\n:拭いてあげる」" timeout="" voices="usi12075"/>
		<msg @l="1158" char="-1" flags="0" index="515" line="1158" name=""
		   text="そう言うと、自分のバスタオルを取り出して、僕の髪の毛に\n:ポンポンと押し当てていく。" timeout="" voices=""/>
		<msg @l="1159" char="-1" flags="0" index="517" line="1159" name=""
		   text="いつも気にしてなかったけど、好きな人が間近にくる\n:この体勢の破壊力はヤバいな。" timeout="" voices=""/>
		<msg @l="1161" char="1" flags="0" index="519" line="1161" name="汐"
		   text="「はい終わったよ。ごめんね私のタオル使っちゃって、\n:でもこっちの方が吸水性がいいから」" timeout=""
		   voices="usi12076"/>
		<msg @l="1163" char="11" flags="0" index="521" line="1163" name="亮"
		   text="「朝霞さんの使用済みタオル、いい匂いがするんで最高でした」" timeout="" voices=""/>
		<chgbg @l="1165" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evusi01_02c" type="99"/>
		<msg @l="1167" char="1" flags="0" index="523" line="1167" name="汐"
		   text="「も、もう、変なこと言わないの」" timeout="" voices="usi12077"/>
		<msg @l="1169" char="-1" flags="0" index="525" line="1169" name=""
		   text="彼女もさっきまで近くにいたことを意識しているのか、\n:顔が赤らんでいる。" timeout="" voices=""/>
		<chgbg @l="1171" duration="300" flags="256" flip="-1" flip_flags="0"
		   param="16" screen="1" src="evusi01_02b" type="99"/>
		<msg @l="1173" char="1" flags="0" index="527" line="1173" name="汐"
		   text="「ふふっ、なんだか不思議な気分。今までとやってることは\n:変わらないはずなのに、今は全然違う風に感じるんだ」" timeout=""
		   voices="usi12078"/>
		<msg @l="1175" char="11" flags="0" index="529" line="1175" name="亮"
		   text="「そうですね。前までのドキドキとは違ったドキドキです」" timeout="" voices=""/>
