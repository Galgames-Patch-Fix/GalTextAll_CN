		<msg @l="1575" char="-1" flags="0" index="905" line="1575" name=""
		   text="よっぽど恥ずかしいのか、花音が足を閉じようとぴくっと動く。" timeout="" voices=""/>
		<msg @l="1576" char="-1" flags="0" index="907" line="1576" name=""
