		[136]={
			ja={
				{
					"綾希に遠ざけられたと思って冷えていた胸の内は、一瞬で熱くなった。",
				},
			},
		},
		[137]={
			ja={
				name={"name", name="琥太"},
				{
					"「めちゃ驚いたよ。それに小腹も空いてるし、めちゃ嬉しい！」",
				},
			},
		},
		[138]={
			vo={
				{"vo", file="fem_aki_01175", ch="aki" },
			},
			ja={
				name={"name", name="綾希"},
				{
					"「よかった。じゃ、食べてみて」",
				},
			},
		},
		[139]={
			ja={
				{
					"お皿に盛られているワッフルは、お店で出てきてもおかしくないくらい、綺麗に盛りつけもされていた。",
				},
			},
		},
		[140]={
			ja={
				{
					"一つは、ホイップクリームの上からチョコレートソースがかけられたもの。",
				},
			},
		},
		[141]={
			ja={
				{
					"もう一つは、ホイップクリームの上に、ブルーベリーやカットされたいちごが盛られている。",
				},
			},
		},
		[142]={
			ja={
				{
					"どちらも甘い匂いと、焼き菓子独特の香ばしさが立ち上り、小腹ではなく本格的にお腹が空いてきた。",
				},
			},
		},
		[143]={
			ja={
				name={"name", name="琥太"},
				{
					"「いただきます！」",
				},
			},
		},
		[144]={
			ja={
				{
					"チョコソースがかかった方から口に運ぶ。",
				},
			},
		},
		[145]={
			ja={
				name={"name", name="琥太"},
				{
					"「うわ、うまっ！　これ、ソースも甘すぎなくて俺好みだ！」",
				},
			},
		},
		[146]={
			vo={
				{"vo", file="fem_aki_01176", ch="aki" },
			},
			ja={
				name={"name", name="綾希"},
				{
					"「うん。琥太は甘すぎるの苦手だから、チョコもホイップも甘さ控えめにしてみたんだ」",
				},
			},
		},
		[147]={
			ja={
				{
					"綾希が嬉しそうにニッコリと笑った。",
				},
			},
		},
		[148]={
			ja={
				{
					"甘さを控えめにした、ということは、チョコソースも手作りということだろう。",
				},
			},
		},
		[149]={
			ja={
				{
					"チョコの甘い香りが漂ってくるものの、味は甘すぎず、ほろ苦さもあって、俺には嬉しい。",
				},
			},
		},
		[150]={
			ja={
				{
					"ワッフルも外がさっくりと仕上がっていて、ホイップクリームとチョコソースのなめらかさがよく合う。",
				},
			},
		},
		[151]={
			ja={
				name={"name", name="琥太"},
				{
					"「次はこっちも……！」",
				},
			},
		},
		[152]={
			ja={
				{
					"フルーツが乗せられた方も切り分け、頬張った。",
				},
			},
		},
		[153]={
			ja={
				{
					"こちらは、ワッフルの仄かな甘みを感じられ、表面のさくさくした食感と、中のしっとり感をよく味わえるものだった。",
				},
			},
		},
		[154]={
			ja={
				{
					"そこに生クリームのなめらかさが加わり、舌触りがとてもいい。",
				},
			},
		},
		[155]={
			ja={
				{
					"さらに、ブルーベリーの自然の甘さと、いちごの控えめな酸味が追いかけてきた。",
				},
			},
		},
		[156]={
			ja={
				name={"name", name="琥太"},
				{
					"「フルーツと一緒に食べるの、すごくいいよ。口の中がさっぱりして、これもめちゃうまだ！」",
				},
			},
		},
		[157]={
			vo={
				{"vo", file="fem_aki_01177", ch="aki" },
			},
			ja={
				name={"name", name="綾希"},
				{
					"「ふふ。ありがと。琥太だったら、こういう味にしてあげた方が、飽きずに食べられるだろうなって」",
				},
			},
		},
		[158]={
			ja={
				{
					"綾希のように甘味がそれほど得意ではない俺のために、彼女はわざわざ趣向を凝らして作ってくれていた。",
				},
			},
		},
		[159]={
			ja={
				{
					"俺のために、自分の時間を割いてくれる。",
				},
			},
		},
		[160]={
			ja={
				{
					"俺のために、おいしいものを作ってくれようとする。",
				},
			},
		},
		[161]={
			ja={
				{
					"俺のために、味付けや盛りつけまで工夫してくれる。",
				},
			},
		},
		[162]={
			ja={
				{
					"俺のことをここまで考えてくれることが、何よりも嬉しかった。",
				},
			},
		},
		[163]={
			ja={
				name={"name", name="琥太"},
				{
					"「ほんとおいしい！　一人で帰るから何かなって思ってたら、こんなサプライズを考えてくれてたなんてな」",
				},
			},
		},
		[164]={
			vo={
				{"vo", file="fem_aki_01178", ch="aki" },
			},
			ja={
				name={"name", name="綾希"},
				{
					"「琥太も、甘いものが苦手ってわけじゃないのがわかったから、一緒に食べられるメニューを考えてみようって思ったの」",
				},
			},
		},
		[165]={
			ja={
				{
					"パクパクと食べている俺を見て、綾希もニコニコとしていた。",
				},
			},
		},
		[166]={
			ja={
				name={"name", name="琥太"},
				{
					"「んじゃ、一緒に食べよう」",
				},
			},
		},
		[167]={
			vo={
				{"vo", file="fem_aki_01179", ch="aki" },
			},
			ja={
				name={"name", name="綾希"},
				{
					"「え、でもそれ、琥太のためにって作ったやつなのに」",
				},
			},
		},
		[168]={
			ja={
				name={"name", name="琥太"},
				{
					"「いいからいいから。はい、あーん」",
				},
			},
		},
		[169]={
			vo={
				{"vo", file="fem_aki_01180", ch="aki" },
			},
			ja={
				name={"name", name="綾希"},
				{
					"「う、うん……。あーん……」",
				},
			},
		},
		[170]={
			ja={
				{
					"軽く頬を染めながらも、綾希は俺の差し出すワッフルにパクついた。",
				},
			},
		},
		[171]={
			vo={
				{"vo", file="fem_aki_01181", ch="aki" },
			},
			ja={
				name={"name", name="綾希"},
				{
					"「ふふ。自分で作っといてなんだけど、ほんと、おいしいね。琥太が食べさせてくれたから、なおさらかな」",
				},
			},
		},
		[172]={
			ja={
				name={"name", name="琥太"},
				{
					"「て、照れること言うなよ……っ」",
				},
			},
		},
		[173]={
			vo={
				{"vo", file="fem_aki_01182", ch="aki" },
			},
			ja={
				name={"name", name="綾希"},
				{
					"「あたしだって、食べさせてもらうの、ちょっと恥ずかしかったし……おあいこよ」",
				},
			},
		},
		[174]={
			ja={
				{
					"恥じらってはいたものの、綾希はまたニッコリと笑顔をこぼした。",
				},
			},
		},
		[175]={
			ja={
				{
					"その微笑みにドキドキとしている俺は、誤魔化すようにワッフルを頬張る。",
				},
			},
		},
		[176]={
			vo={
				{"vo", file="fem_aki_01183", ch="aki" },
			},
			ja={
				name={"name", name="綾希"},
				{
					"「あ、もう。ほっぺたにクリームつけちゃって。しょうがないんだから……ちゅっ」",
				},
			},
		},
		[177]={
			ja={
				name={"name", name="琥太"},
				{
					"「……えっ」",
				},
			},
		},
		[178]={
			ja={
				{
					"綾希の唇が、俺の頬に触れた。",
					{"rt2"},
					"彼女は、俺の頬についていたホイップクリームを、口付けで綺麗にしてくれたのだ。",
				},
			},
		},
		[179]={
			ja={
				{
					"綾希の唇が触れた箇所が、蕩けて落ちてしまったように感じた。",
				},
			},
		},
		[180]={
			ja={
				{
					"ともすればホイップクリームよりも、彼女の唇は柔らかく、そしてプルプルとしていた。",
				},
			},
		},
		[181]={
			ja={
				{
					"その感触と同時に、綾希からキスされたという事実に、胸が高鳴った。",
				},
			},
		},
		[182]={
			ja={
				{
					"唇に受けたわけではないけど、それでも彼女から唇を重ねてもらった喜びが、心の中で溢れて口から飛び出しそうだった。",
				},
			},
		},
		[183]={
			vo={
				{"vo", file="fem_aki_01184", ch="aki" },
			},
			ja={
				name={"name", name="綾希"},
				{
					"「ふふふ。ぼぉっとしちゃって。またスケベさんになってるの？」",
				},
			},
		},
		[184]={
			ja={
				name={"name", name="琥太"},
				{
					"「いっ、いやいや違う違う！　キスしてもらって、夢見心地っていうか、なんというか……っ」",
				},
			},
		},
		[185]={
			vo={
				{"vo", file="fem_aki_01185", ch="aki" },
			},
			ja={
				name={"name", name="綾希"},
				{
					"「……そっか。でも、琥太一人だけ、ずるいよ……」",
				},
			},
		},
		[186]={
			ja={
				name={"name", name="琥太"},
				{
					"「そう、だなっ。じゃあ……お返し……っ」",
				},
			},
		},
		[187]={
			ja={
				{
					"そう。さっき綾希もお返しをしてきた。",
					{"rt2"},
					"だから俺も……。",
				},
			},
		},
		[188]={
			ja={
				{
					"俺は彼女の、頬にではなく、唇に、自分の口を押し付けた。",
				},
			},
		},
		[189]={
			vo={
				{"vo", file="fem_aki_01186", ch="aki" },
			},
			ja={
				name={"name", name="綾希"},
				{
					"「…………ちゅっ。ん……ふ……ちゅ……」",
				},
			},
		},
		[190]={
			ja={
				{
					"綾希の柔らかな唇に触れ、自身の口が溶けたようにも感じる。",
					{"rt2"},
					"さらにはプルンッと弾けるような感触と、艶めかしい潤いを覚えた。",
				},
			},
		},
		[191]={
			ja={
				{
					"唇を重ねていたのは一瞬のことだったのに、まるで時が止まったかのように、呼吸も、動きも、鼓動さえも、停止していた。",
				},
			},
		},
		[192]={
			ja={
				{
					"そしてそれらがうるさいほどに活動を再開して、一気に体温が上昇する。",
				},
			},
		},
		[193]={
			ja={
				{
					"頭の芯まで熱く、恍惚とした気分で、彼女から唇を離した。",
				},
			},
		},
		[194]={
			vo={
				{"vo", file="fem_aki_01187", ch="aki" },
			},
			ja={
				name={"name", name="綾希"},
				{
					"「ふ……あぁ……。キス……甘いね……」",
				},
			},
		},
		[195]={
			ja={
				{
					"それは、今、甘いものを食べたからだろう。",
				},
			},
		},
		[196]={
			ja={
				{
					"でも、俺も綾希も、甘さを感じているのはお互いの唇だけではなかった。",
				},
			},
		},
		[197]={
			ja={
				{
					"脳内では甘美な快感が溢れ出し、この部屋に漂う空気さえも、甘みを帯びているように感じていた。",
				},
			},
		},
		[198]={
			vo={
				{"vo", file="fem_aki_01188", ch="aki" },
			},
			ja={
				name={"name", name="綾希"},
				{
					"「あふぅ……はぁ～……っ」",
				},
			},
		},
		[199]={
			ja={
				{
					"綾希はいまだに、熱を帯びた溜め息を吐いていた。",
				},
			},
		},
		[200]={
			ja={
				{
					"うるうるとしている瞳は蕩け、焦点が定まっていないようにも見える。",
				},
			},
		},
		[201]={
			ja={
				{
					"多分俺も、同じようになっているだろう。",
				},
			},
		},
		[202]={
			ja={
				{
					"一回の口付けだけで、互いの気持ちを確かめ合うだけで、俺と綾希は官能に酔っていた。",
				},
			},
		},
		[203]={
			vo={
				{"vo", file="fem_aki_01189", ch="aki" },
			},
			ja={
				name={"name", name="綾希"},
				{
					"「こんなにもおいしくて、気持ちいいお返し、もらえるなら……毎日でも作ってあげたくなっちゃうよ」",
				},
			},
		},
		[204]={
			ja={
				name={"name", name="琥太"},
				{
					"「俺も……毎日こんなにおいしくて、気持ちよくなれるんだったら……毎日作ってほしいよ……」",
				},
			},
		},
		[205]={
			vo={
				{"vo", file="fem_aki_01190", ch="aki" },
			},
			ja={
				name={"name", name="綾希"},
				{
					"「ふふ。そしたら琥太、すんごく太ちゃうかもよ」",
				},
			},
		},
		[206]={
			ja={
				name={"name", name="琥太"},
				{
					"「それは困るな。綾希の恋人として、最低限の見栄えは保っときたいし」",
				},
			},
		},
		[207]={
			vo={
				{"vo", file="fem_aki_01191", ch="aki" },
			},
			ja={
				name={"name", name="綾希"},
				{
					"「あはは。あたしは太ってても気にしないけどね」",
				},
			},
		},
		[208]={
			vo={
				{"vo", file="fem_aki_01192", ch="aki" },
			},
			ja={
				name={"name", name="綾希"},
				{
					"「あ。あたし以外の女の子からモテなくなるって意味でも、太らせるのはいい作戦かも」",
				},
			},
		},
		[209]={
			ja={
				name={"name", name="琥太"},
				{
					"「もうとっくに胃袋をつかまれてるから、そんなことしなくても大丈夫だよ」",
				},
			},
		},
		[210]={
			vo={
				{"vo", file="fem_aki_01193", ch="aki" },
			},
			ja={
				name={"name", name="綾希"},
				{
					"「うわわっ。琥太がそんな恥ずかしいことをさらっと言うなんて」",
				},
			},
		},
		[211]={
			ja={
				{
					"綾希が顔を真っ赤にして頬を押さえている。",
				},
			},
		},
		[212]={
			ja={
				{
					"確かに、今の俺の発言は恥ずかしいものだった。",
					{"rt2"},
					"俺も自分で言っておいて、顔を熱くさせていた。",
				},
			},
		},
		[213]={
			vo={
				{"vo", file="fem_aki_01194", ch="aki" },
			},
			ja={
				name={"name", name="綾希"},
				{
					"「でも……嬉しいな」",
				},
			},
		},
		[214]={
			ja={
				{
					"はにかみつつ、綾希はちらりと俺の顔を覗いてきた。",
				},
			},
		},
		[215]={
			ja={
				{
					"その視線にまたしても顔が熱くなり、誤魔化すようにして残りのワッフルを平らげた。",
				},
			},
		},
		[216]={
			ja={
				name={"name", name="琥太"},
				{
					"「そ、それにしてもさ、綾希の料理の腕はほんとすごいよ。味はもちろんだけど、盛りつけもプロ並みだったし」",
				},
			},
		},
		[217]={
			ja={
				name={"name", name="琥太"},
				{
					"「桐原さんが言ってたみたいに、料理人とかパティシエールになれると思う」",
				},
			},
		},
		[218]={
			ja={
				name={"name", name="琥太"},
				{
					"「綾希が作るお菓子や料理を、みんな喜んで食べてくれると思うけどな」",
				},
			},
		},
		[219]={
			vo={
				{"vo", file="fem_aki_01195", ch="aki" },
			},
			ja={
				name={"name", name="綾希"},
				{
					"「そう言われるのは、実はまんざらじゃないんだ。でも……」",
				},
			},
		},
		[220]={
			ja={
				{
					"綾希はもう一度、上目遣いな目線をこちらに送ってきた。",
					{"rt2"},
					"彼女の頬は、桃色に染まったままだった。",
				},
			},
		},
		[221]={
			vo={
				{"vo", file="fem_aki_01196", ch="aki" },
			},
			ja={
				name={"name", name="綾希"},
				{
					"「今は、ね……みんなって言うより、琥太が食べて、喜んでくれる方が、嬉しいから」",
				},
			},
		},
		[222]={
			ja={
				name={"name", name="琥太"},
				{
					"「そ、それは……っ。俺も、嬉しい」",
				},
			},
		},
		[223]={
			ja={
				{
					"誰かをいい気分にさせるより、俺を喜ばせたい。",
				},
			},
		},
		[224]={
			ja={
				{
					"彼女のその気持ちに、本当に、ぐっときた。",
				},
			},
		},
		[225]={
			ja={
				{
					"俺と綾希は、幼馴染だった頃よりも、確実に心の距離が近付いている。",
				},
			},
		},
		[226]={
			ja={
				{
					"お互いに歩み寄って、お互いを感じ合っている。",
				},
			},
		},
		[227]={
			ja={
				{
					"恋人になるとは、こういうことなのかもしれない。",
				},
			},
		},
