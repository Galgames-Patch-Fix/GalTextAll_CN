		},
		[43]={
			vo={
				{"vo", file="fem_aki_01317", ch="aki" },
			},
			ja={
				name={"name", name="綾希"},
				{
					"「……ご休憩？　お城……？」",
				},
			},
		},
		[44]={
			ja={
				name={"name", name="琥太"},
				{
